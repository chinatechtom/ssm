<?xml version="1.0" encoding="gbk" ?>
<function id="3090102" name="将客户基础数据从集中交易系统传送到融资融券系统">
	<input>
		<item type="text"	id="vsmess"			name="信息"/>
		<item type="text"	id="vsvarstr0"		name="文件路径"/>	
		<item type="text"	id="snote"			name="文件名"/>	
		<item type="text"	id="scust_auth"		name="更新时间"/>	
	</input>
	<service type="procedure" id="p_import_data_filepath_search" process="911" style="1">
		<param id="user_id"			type="integer"/>	
		<param id="vsmess"			type="string"/>
		<param id="vsvarstr0"		type="string"/>
		<param id="snote"			type="string"/>
		<param id="scust_auth"		type="string"/>			
	</service>
	<output	target="3090103">
		<column name="导入方式"			type="string"	id="sstatus0"/>   
		<column name="路径"				type="string"	id="vsvarstr0"/>   
		<column name="文件名"			type="string"	id="saddr"/>  
		<column name="数据库表名"			type="string"	id="saddr2"/>	
		<column name="数据格式名"			type="string"	id="snote"/>   
		<column name="融资融券机构代码"	type="string"	id="sstation1"	display="false"/>   
		<column name="融资融券分支号"		type="string"	id="sdate3"		display="false"/>  
		<column name="融资融券柜员号"		type="string"	id="scheck_emp"	display="false"/>
		<column name="融资融券柜员密码"	type="string"	id="semp_pwd"	display="false"/>
	</output>
</function>