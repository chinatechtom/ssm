<?xml version="1.0" encoding="gbk" ?>
<function id="31102" name="账户钩稽" >
  <input>
	<item type="date"	id="begdate"					name="起始日期" default="last"/>
	<item type="date"	id="end_date"					name="终止日期" default="now"/>
	<item type="hidden" id="serial_no"					name="流水号"/>
	<item type="hidden" id="surveillance_sort_id"		name="风险指标类别id"/>
	<item type="select" id="surveillance_object_type"	name="监控对象类型" source="141"/>
	<item type="hidden" id="surveillance_object"		name="监控对象"/>
  </input>
  <service type="procedure" id="p_acc_audit" process="911" style="77">
	<param id="user_id"						type="integer"  way="in"/>
	<param id="begdate"						type="string"	way="in"/>
	<param id="end_date"					type="string"	way="in"/>
	<param id="serial_no"					type="integer" 	way="in"/>
	<param id="surveillance_sort_id"		type="integer" 	way="in"/>
	<param id="surveillance_object_type"	type="string" 	way="in"/>
	<param id="surveillance_object"			type="integer" 	way="in"/>
  </service>
  <output export="true">
    <column type="number" id="serial_no"				 name="流水号"  display="false"/>
	<column type="string" id="date"						 name="日期"/>
	<column type="string" id="time"						 name="时间"/>
	<column type="number" id="surveillance_sort_id"		 name="风险指标类别id"  display="false"/>
	<column type="string" id="surveillance_sort_name"	 name="风险指标类别名称"/>
	<column type="number" id="surveillance_level_id"	 name="风险指标id"	display="false"/>
	<column type="string" id="surveillance_level_name"	 name="风险指标名称"/>
	<column type="string" id="manage_status"			 name="处理状态"/>
	<column type="string" id="surveillance_object_type"  name="监控对象类型"/>
	<column type="string" id="surveillance_object"		 name="监控对象"/>
	<column type="string" id="result_remark"			 name="结果描述"/>
	<column type="number" id="level"					 name="指标值"	   precision="2"/>
	<column type="number" id="level_upper_limit"		 name="指标上限"	   precision="2"/>
	<column type="number" id="level_next_limit"			 name="指标下限"	   precision="2"/>
	<column type="number" id="current_num"				 name="当前值"	   precision="2"/>
	<column type="number" id="diff_num"					 name="差额值"	   precision="2"/>
	<column type="number" id="busi_diff_num"			 name="业务差额数据" precision="2"/>
	<column type="string" id="connect_status"			 name="联系状况"/>
	<column type="string" id="manage_idea"				 name="处理意见"/>
  </output>    
</function>