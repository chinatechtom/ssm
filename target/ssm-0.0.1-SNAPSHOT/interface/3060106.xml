<?xml version="1.0" encoding="gbk" ?>
<function id="3060106" name="客户资料同步" >  
	<input>
		<item type="text" id="cust_code" name="客户代码" nullable="false"/>
		<!--<item type="hidden" id="cert_no" name="证件号码"/-->
  	</input>
	<service type="procedure" id="p_cust_info_syn" process="911" style="77">
		<param id="user_id" 	type="integer"  way="in" /> 
		<param id="cust_code"	type="string"	way="in"/>
		<!--<param id="cert_no" /-->
  	</service> 
	<output>
    	<column type="number" id="cust_id" 				name="客户id"	display="false" />   
    	<column type="string" id="cust_code"   			name="客户代码"/>
		<column type="string" id="commness_cust_code"   name="普通资金账户"/>
    	<column type="string" id="cust_name"   			name="客户姓名"/>
		<column type="string" id="cert_type"   			name="证件类型1" 	display="false"/>
		<column type="string" id="cert_type_name"   	name="证件类型"/>
    	<column type="string" id="cert_no"   			name="证件号码"/>
		<column type="string" id="if_exsist"  			name="客户是否存在" display="false"/>
		<column type="string" id="if_exsist_name"   	name="客户是否存在"/>
		<column type="string" id="cust_type"   			name="客户类型1" 	display="false"/>
		<column type="string" id="cust_type_name"   	name="客户类型"/>
		<button type="send"   id="edit" 				name="客户资料同步" action="8041310"  extendaction="if_exsist"/>
   		<!-- <button type="edit" id="edit" name="新增合约" action="30626" extendaction="if_exsist" multiple="0"/>-->
  	</output>
	<assistant action="30103,3200301,30102,30104,30109,30111,30201,30202,30205,30206,30207,30215,30673,30672,30667,30668">
		<link src="cust_id" target="cust_id"/>
    </assistant>
</function>