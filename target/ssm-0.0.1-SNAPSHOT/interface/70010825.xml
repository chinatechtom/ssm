<?xml version="1.0" encoding="gbk" ?>
<function id="70010825" name="交易协议书议价" >
  <input>
	<group name="合同信息" >
		<item type="hidden" id ="cust_id" name="客户id" action="97000013"/>
		<item type="hidden" id ="institution_id" name="营业部id" />
		<item type="text" 	id ="institution_name" name="营业部名称" editable="false"/>
		<item type="text" 	id ="institution_code" name="营业部代码" editable="false"/> 
		<item type="text" 	id ="bond_no" name="合同编号" editable="false"/>
		<item type="text" 	id ="cust_name" name="客户姓名" editable="false"/>
		<item type="text" 	id ="cust_code" name="客户代码" editable="false"/> 	
	</group>  
	<item type="hidden" id="date" name="打印日期"/>	<!--从1888.xml获得的打印日期-->
	<item type="hidden" id="user_code" name="打印柜员代码"/><!--从1888.xml获得的打印柜员代码-->
	<item type="hidden" id="user_name" name="打印柜员名称"/><!--从1888.xml获得的打印柜员名称-->
	<item type="hidden" id="company" name="券商名称"/><!--从1888.xml获得的券商名称-->			
	<group name="交易协议书议价">
		<item type="hidden" id ="bond_id" name="bondid"/>
		<item type="text" 	id ="sbond_no" name="交易协议书编号" editable="false" nullable="false"/>
		<item type="select" id ="related_sbond_no" name="主关联交易协议书编号" nullable="false" source="97000019" filter="bond_id"/>
		<item type="select" id ="hg_code" name="初始交易代码" nullable="false" source="97000008" action="97000009"/>
		<item type="text" id ="hg_code2" name="购回交易代码" nullable="false" editable="false"/>
		<item type="select" id ="market_code" name="市场代码" nullable="false" source="039" action="97000023"/>
		<item type="text" 	id ="product_code" name="标的证券代码" nullable="false" action="97000023" minlen="6" maxlen="6" />
		<item type="text"	id ="product_name" name="标的证券名称" editable="false"/>
		<item type="text"	id ="product_type" name="标的证券类别" editable="false"/>
		<item type="text" 	id ="hg_num" name="标的证券数量" nullable="false" integer="true"/>
		<item type="text" 	id ="hg_convert_rate" name="折扣比率(%)" nullable="false" number="true"/>
		<item type="text" 	id ="app_price" name="标的证券价格" nullable="false" number="true"/>
		<item type="date" 	id ="engage_trade_date" name="初始交易日期" default="now" nullable="false" action="97000016,97000006"/>
		<item type="date" 	id ="bb_date" name="约定购回交易日期" nullable="false" action="97000016,97000006"/>
		<item type="text" 	id ="days" name="购回期限(天)" editable="false" nullable="false" integer="true" action="97000034"/>
		<!--<item type="text" 	id ="rate2" name="违约金比率(日利率%)" nullable="false" number="true"/>-->
		<item type="text" 	id ="para3016" name="违约金比率(日利率%)" nullable="false" number="true" editable="false"/>
		<item type="text"		id ="para3011" name="预警履约保障比率(%)" nullable="false" number="true"/>
		<item type="text" 	id ="para3010" name="最低履约保障比例(%)" nullable="false" number="true"/>
		<item type="text" 	id ="first_amt" name="初始交易金额小写" nullable="false" number="true" action="97000006,97000034"/>
		<item type="select" id ="if_hg_rate" name="是否按产品申请" nullable="false" source="728" default="0" action="97000034"/>
		<item type="text" 	id ="hg_rate" name="购回价格(每100元年收益)" nullable="false" number="true" filter="if_editable=1" action="97000006"/>
		<item type="text" 	id ="second_amt" name="购回交易金额小写" editable="false"/>
		<item type="text" 	id ="para3034" alias="fix_rate" name="固定交易费用比例(%)" nullable="false" number="true" action="97000006"/>
		<item type="textarea" id ="hg_num_big" name="标的证券数量大写" editable="false"/>
		<item type="textarea" id ="first_amt_big" name="初始交易金额大写" editable="false"/>
		<item type="textarea" id ="second_amt_big" name="购回交易金额大写" editable="false"/>
		<item type="textarea" id ="zjyt" name="资金用途"/>
		<item type="textarea" id ="remarks" name="备注"/>
	</group>
	<item type="hidden" id="abc"	   name="杂项ID"   default="1" action="97000005"/>
	<item type="hidden"	id="company"   name="公司名称" editable="false" />
	<item type="hidden" id="para3000"  name="杂项3000" editable="false"/>
	<item type="hidden" id="para3001"  name="杂项3001" editable="false"/>
	<item type="hidden" id="para3002"  name="杂项3002" editable="false"/>
	<item type="hidden" id="para3003"  name="杂项3003" editable="false"/>
	<item type="hidden" id="para3004"  name="杂项3004" editable="false"/>
	<item type="hidden" id="para3005"  name="杂项3005" editable="false"/>
	<item type="hidden" id="para3006"  name="杂项3006" editable="false"/>
	<item type="hidden" id="para3034"  name="杂项3034" editable="false"/>
	<item type="hidden" id="system_date"  name="系统日期" editable="false"/>
	<item type="hidden" id="cust_id"	name="cust_id"      action="70010102"/>
	<item type="hidden" id="stock_holder_sh"  name="沪A证券帐户" editable="false"/>
	<item type="hidden" id="stock_holder_sz"  name="深A证券帐户" editable="false"/>
	<item type="hidden" id="seat_code_sh"  name="沪A席位代码" editable="false"/>
	<item type="hidden" id="seat_code_sz"  name="深A席位代码" editable="false"/>
	<item type="hidden"	id="if_editable"   name="控制可编辑" editable="false" />
  </input>
  <service type="procedure" id="P70010825" process="822" style="1">
	<param type="integer" 	id="user_id"/>
	<param type="integer" 	id="cust_id"/>
	<param type="string" 	id="sbond_no"/>
	<param type="string"	id="hg_code"/>
	<param type="string"	id="market_code"/>
	<param type="string"	id="product_code"/>
	<param type="integer"	id="hg_num"/>
	<param type="float"		id="hg_convert_rate"/>
	<param type="float"		id="app_price"/>
	<param type="string"	id="engage_trade_date"/>
	<param type="integer"	id="days"/>
	<param type="float"		id="hg_rate"/>
	<param type="float"		id="para3016"/>
	<param type="float"		id="para3011"/>
	<param type="float"		id="para3010"/>
	<param type="string"	id="remarks"/>
	<param type="double"	id="first_amt"/>
	<param type="string" 	id="related_sbond_no"/>
	<param type="float" 	id="fix_rate"/>
	<param type="string" 	id="zjyt"/>
  </service>
  <output/>
</function>