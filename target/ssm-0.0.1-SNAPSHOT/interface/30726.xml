			<item type="text"	id="asset_gap"	name="资产差额"	editable="false"/> 
			<item type="text"	id="current_num"		name="触发时担保率"	editable="false"/>
			<item type="text"	id="cur_gua_rate"		name="实时担保率"	editable="false"/>
