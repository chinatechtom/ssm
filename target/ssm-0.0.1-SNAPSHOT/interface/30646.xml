<?xml version="1.0" encoding="gbk" ?>
<function id="30646" name="查看合同-联系资料" >
	<input>    
	 <item type="hidden"  id="bond_id" name="合同id"/> 
  </input>
  <service type="procedure" id="p_bond_connect_info_query" process="911" style="77">
	<param id="user_id"				type="integer" way="in"/> 
	<param id="bond_id"				type="ingeter" way="in"/>
	<param id="cust_id"				type="ingeter" way="in"/>
  </service>
  <output detail="true">
	<column type="number" id="bond_id" name="合同id"	display="false"/>
	<column type="string" id="move_tel" name="移动电话"/>
	<column type="string" id="fix_tel" name="固定电话" precision="4"/>
	<column type="string" id="meet_tel" name="应急电话"/>
	<column type="string" id="family_address" name="联系地址"/>
	<column type="string" id="ps_code" name="邮政编码"/>
	<column type="string" id="fax" name="传真"/>
	<column type="string" id="email" name="电子邮件"/>
	<column type="number" id="find_mode" name="合同id"	display="false"/>
	<column type="string" id="find_mode_name" name="主联系方式"/>
	<column type="string" id="find_mode_value" name="主联系方式2" display="false"/>
	<column type="string" id="emerg_contacts_man" name="应急联系人"/>
	<column type="string" id="emerg_contacts_tel" name="应急联系人电话"/>
  </output> 
</function>