<?xml version="1.0" encoding="gbk" ?>
<function id="3068101" name="查询融券上限" >
  <input>
	<item type="hidden" id="order_id" name="id"/>
  </input>
  <service type="procedure" id="p_order_borrow_stock_query" process="911" style="77">
	<param type="integer" id="user_id"/>	
	<param type="integer" id="order_id"/>	
  </service>
  <output>    
    <column type="number" id="order_id" name="合同id" display="false"/>
    <column type="string" id="market_code" name="市场代码" display="false"/>  
	<column type="string" id="market_code_name" name="市场名称"/>
	<column type="number" id="product_type" name="产品类型" display="false"/>  
	<column type="string" id="product_type_name" name="产品类型" display="false"/>
	<column type="number" id="product_id" name="产品id" display="false"/>
	<column type="string" id="product_code" name="证券代码" />  
	<column type="string" id="product_name" name="证券名称" />
	<column type="string" id="model" name="模式" display="false"/> 
	<column type="string" id="model_name" name="信用使用模式" display="false"/> 
	<column type="number" id="borrow_qty" name="证券数量" precision="0"/>
	<column type="string" id="orrow_gua_rate" name="融券保证金比例" />	
	<column type="string" id="begin_time" name="开始日期" display="false"/>
	<column type="string" id="return_time" name="到期日期" display="false"/>
	<column type="string" id="available_time" name="到期日期" display="false"/>
  </output> 
</function>