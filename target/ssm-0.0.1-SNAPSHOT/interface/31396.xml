<?xml version="1.0" encoding="gbk" ?>
<function id="31396" name="历史数据转移日志查询" >
	<input>
	    <item type="date"      id="date"        name="日期"      nullable="false"/>
		<item type="select"    id="if_main"     name="是否主库"  nullable="false" source="036"/>
		<item type="select"    id="trans_io"    name="操作方向"  source="518"/>
		<item type="text"      id="table_name"  name="表名"  />
		<item type="select"    id="if_success"  name="是否成功"  source="517"/>
	</input>
	<service type="procedure" id="p_history_table_log_query" process="911" style="77">
		<param id="user_id"    type="integer"/>
		<param id="if_main"    type="string"/>
		<param id="trans_io"   type="string"/>
		<param id="table_name" type="string"/>
		<param id="if_success" type="string"/>
		<param id="date"       type="string"/>
	</service>
	<output export="true">
		<column type="string" id="table_name"	name="处理表名"/>
		<column type="string" id="trans_io"	    name="操作方向"/>
		<column type="string" id="trans_type"	name="迁移目标"/>
		<column type="string" id="if_success"	name="是否成功"/>
		<column type="number" id="rows_count"	name="行数"/>
		<column type="string" id="remark"		name="备注"/>
		<column type="string" id="sysdate"		name="系统日期"/>
        <column type="string" id="begin_time"   name="起始时间"/>
		<column type="string" id="end_time"     name="结束时间"/>
		<column type="number" id="use_time"     name="耗时(秒)"/>
		<column type="string" id="user_code"	name="操作员代码"/>
		<column type="string" id="target_machine"	name="目标服务器"/>
	</output>
</function>