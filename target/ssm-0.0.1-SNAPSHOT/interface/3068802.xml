<?xml version="1.0" encoding="GBK" ?>
<function id="3068802" name="查找客户交易记录">
  <input>
  	<item type="text" id="commness_cust_code" name="普通资金账户号"/>
	<item type="date" id="begin_date"		  name="起始日期"/>
	<item type="date" id="end_date"			  name="截止日期"/>
  </input>
  <!--
  <service type="drtp" id="" process="711" style="77">
	  <param type="string" field="commness_cust_code1" id="scust_no"/>
  </service>
  -->
  <service type="procedure" id="KSTAR.P_KSSC_513102" process="511" style="77">
	<param type="string"  id="commness_cust_code"/>
	<param type="string"  id="begin_date"/>
	<param type="string"  id="end_date"/>
  </service>
  <output>
    <column name="客户代码"			id="cust_code"/>
    <column name="股票基金交易量"	id="trade_amt_all"	type="number"	precision="2"/>
    <column name="总佣金贡献" 		id="fee_amt"		type="number"	precision="2"/>
    <column name="周转率"			id="velocity_rate"	type="number"	precision="2"/>
	<column name="总盈亏"			id="profit_loss"	type="number"   precision="2"/>
    <column name="资金余额"			id="fund_amt"		type="number"   precision="2"/>
    <column name="股票市值"			id="asset_value"	type="number"	precision="2"/>
    <column name="交易量最大的股票"	id="one_a_stkcode"/>
	<column name="交易量第二股票"	id="two_a_stkcode"/>
    <column name="交易量第三股票"	id="three_a_stkcode"/>
    <column name="交易量第四股票"	id="four_a_stkcode"/>
    <column name="交易量第五股票"	id="five_a_stkcode"/>
	<button type="edit" id="edit" name="存入数据库" action="3068803" multiple="0"/>
  </output>
</function>