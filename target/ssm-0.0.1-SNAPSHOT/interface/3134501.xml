		<column type="string" id="sysdate"	name="操作交易日期"/>
		<column type="string" id="keyvalue"	name="操作对象"/>
		<column type="string" id="oldvalue"	name="修改前值"/>
		<column type="string" id="newvalue"	name="修改后值"/>
		<column type="string" id="remark"	name="备注信息"/>
		<column type="date"   id="time"		name="系统时间"/>
	</output>
</function>