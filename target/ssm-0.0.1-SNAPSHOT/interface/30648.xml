	<item type="hidden" id="bond_id" name="合同id"/>
  </input>
  <service type="procedure" id="p_bond_parameter_query" process="911" style="77">
	<param type="integer" id="user_id"/>	
	<param type="integer" id="bond_id"/>
  </service>
  <output>    
    <column type="number" id="bond_id" name="合同id"   display="false"/>
    <column type="string" id="credit_control_mode" name="信用控制模式"/>  
	<column type="string" id="account_fee_mode" name="计费模式"  display="false"/>
	<column type="string" id="pc_mode" name="平仓顺序"/>
    <column type="string" id="borrow_stock_pg" name="融券复权模式"/>  
	<column type="string" id="borrow_stock_hg" name="融券红股模式" display="false"/>
	<column type="string" id="borrow_stock_tspc" name="融券退市平仓日期" />
  </output> 
</function>