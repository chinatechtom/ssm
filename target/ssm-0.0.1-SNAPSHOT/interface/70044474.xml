			<item type="text" id="rz_pg" name="入账配股数量" editable="false"/>
			<item type="text" id="zt_hl_tax" name="在途红利税金" editable="false"/>
			<item type="text" id="rz_hl_tax" name="入账红利税金" editable="false"/>
