<?xml version="1.0" encoding="gbk" ?>
<function id="7001070801" name="交易协议书补仓资金查询" >
  <input>
	<item type="hidden" id="cust_id"  name="客户id"/>
	<item type="hidden" id="sbond_id" name="sbond_id"/>
	<item type="text"   id="sbond_no" name="交易协议书编号"/>
  </input>
  <service type="procedure" id="P7001070801" process="922" style="77">
	<param type="integer" id="user_id"/>	
	<param type="integer" id="cust_id"/>
	<param type="integer" id="sbond_id"/>
	<param type="string"  id="sbond_no"/>
  </service>
  <output  export="true">  
    <column type="number" id="bc_id"   name="补仓序号"   display="false"/>
    <column type="number" id="fund_amt"   name="补仓资金" precision="2"/>     
	<column type="string" id="bc_trade_date"   name="补仓日期"/>
  </output>
</function>