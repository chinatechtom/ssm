<?xml version="1.0" encoding="gbk" ?>
<function id="36000073" name="7.1.6营业部融资融券客户价格异常交易统计" > 
<input>
  <item type="select" id="institution_id" name="营业部名称"  source="95000008"/>
  <item type="text"   id="stock_code"     name="证券代码"  />
  <item type="select" id="market_code"    name="市场代码" source="039" />
  <item type="date"   id="begin_date"           name="起始日期"     default="last"/>
  <item type="date"   id="end_date"           name="结束日期"     default="now"/>
  <item type="select"  id="order_choice"    name="排序选择"  source="383"/>
</input>
<service type="procedure" id="P36000073" process="911" style="77">
  <param id="user_id"         type="integer" way="in" />
  <param id="institution_id"  type="integer" way="in" />
  <param id="market_code"     type="string"  way="in" />
  <param id="stock_code"      type="string"  way="in" />
  <param id="begin_date"            type="string"  way="in" />
  <param id="end_date"            type="string"  way="in" />
  <param id="order_choice"     type="string"  way="in"/>
</service>
<output  print="true">
  <column type="number" id="id"                name="序号"   />
  <column type="string" id="institution_name"  name="营业部" /> 
  <column type="string" id="cust_code"         name="客户代码"/>  
  <column type="string" id="cust_name"         name="客户名称"/>
  <column type="string" id="product_code"      name="证券代码"/>
  <column type="string" id="product_name"      name="证券名称"/>
  <column type="string" id="date"              name="日期"/>
  <column type="number" id="average_buy_amt"   name="买入均价"      precision="2" total="true"/>
  <column type="number" id="buy_vol"           name="买入成交数量"  precision="0" total="true"/>
  <column type="number" id="average_sell_amt"  name="卖出均价"      precision="2" total="true"/>
  <column type="number" id="sell_vol"          name="卖出成交数量"  precision="0" total="true"/> 
</output>
</function>