<?xml version="1.0" encoding="gbk" ?>
<function id="27050133" name="风险客户资产差额明细表">
	<input>
		<item type="select"		id="insitution_id"	name="营业部"	source="95000008"/>
		<item type="hidden"		id="id"				name="id"		action="31381" default="1"/>		
		<item type="date"		id="date"			name="日期"		editable="false"/>
	</input>
    <service type="procedure" id="P27050133"  process="911" style="77">
		<param id="user_id"	        type="integer" way="in"/>
		<param id="insitution_id"	type="integer" way="in"/>
    </service>
    <output export="true" autoload="false" print="true">
		<column type="string" id="status"						name="状态" />
		<column type="string" id="institution_code"				name="营业部分支号"/>
		<column type="string" id="institution_name"				name="营业部"/>
		<column type="string" id="cust_code"					name="客户号"/>
		<column type="string" id="cust_name"					name="客户名"/>
		<column type="string" id="risk_level_name"				name="风险类型"/>
		<column type="string" id="left_days"					name="到期剩余天数"/>
		<column type="string" id="assure_rate"					name="维持担保比例"/>
		<group name="资产总额">
			<column type="number" id="fund_value"				name="资金"			precision="2"	/>
			<column type="number" id="stock_value"				name="证券市值"		precision="2"	/>
			<column type="number" id="asset_value"				name="合计"			precision="2"	/>
		</group>
		<group name="可接受担保物价值">
			<column type="number" id="can_accept_fund_value"		name="资金"				precision="2"	/>
			<column type="number" id="can_accept_guarantee_value"	name="可接受证券市值"	precision="2"	/>
			<column type="number" id="can_accept_total_value"		name="合计"				precision="2"	/>
		</group>
		<column type="number" id="cust_total_debt"		name="负债总额"				precision="2"	/>
		<column type="number" id="debt_bal"				name="负债差额"				precision="2"	/>
		<column type="number" id="up_to_safe_value"		name="恢复到安全线差额"	precision="2"	/>
	</output>
</function>
