<?xml version="1.0" encoding="gbk" ?>
<function id="30681" name="查询融券上限" >
  <input>
	<item type="hidden" id="order_id" name="id"/>
  </input>
  <service type="procedure" id="p_order_borrow_stock_query" implement="p_order_borrow_stock_queryABCD" process="911" style="77">
	<param type="integer" id="user_id"/>	
	<param type="integer" id="order_id"/>	
  </service>
  <output> 
    <column type="number" id="institution_id" name="营业部ID" />
    <column type="string" id="institution_name" name="营业部名称" />
    <column type="number" id="order_id" name="合同id" display="false"/>
    <column type="string" id="market_code" name="市场代码" display="false"/>  
		<column type="string" id="market_code_name" name="市场名称"/>
		<column type="number" id="product_type" name="产品类型" display="false"/>  
		<column type="string" id="product_type_name" name="产品类型"/>
		<column type="number" id="product_id" name="产品id" display="false"/>  
		<column type="string" id="product_name" name="产品名称" />
		<column type="string" id="model" name="模式" display="false"/> 
		<column type="string" id="model_name" name="信用使用模式"/> 
		<column type="string" id="product_code" name="产品代码" display="false"/>  
		<column type="number" id="borrow_qty" name="融券上限" precision="2"/>
		<column type="string" id="big_borrow_qty" name="大写中文产品数量" display="false"/>
		<column type="string" id="small_borrow_qty" name="小写中文产品数量" display="false"/>
		<column type="number" id="bs_bond_qty" name="原融券上限额度" precision="2"/>
		<column type="string" id="big_bs_qty_bond" name="大写中文原融券上限" display="false"/>
		<column type="string" id="small_bs_qty_bond" name="小写中文原融券上限" display="false"/>		
		<column type="string" id="begin_time" name="起始日期"/>
		<column type="string" id="return_time" name="到期日期" />
		<column type="string" id="begin_time1" name="起始日期" display="false"/>
		<column type="string" id="return_time1" name="到期日期" display="false"/>
		<column type="number" id="availability_day_num"  name="有效天数" />
		<column type="string" id="application_time" name="申请日期" />
		<column type="string" id="available_time" name="到期日期"/>
		<column type="number" id="cust_factor" name="客户融券费率因子"/>
		<column type="number" id="app_days" name="融券申请天数"/>
		<column	type="number"	id="borrow_stock_product_id"	name="融券产品ID"/>
		<column	type="string"	id="borrow_stock_product_name"	name="融券产品名称"/>
  </output> 
</function>