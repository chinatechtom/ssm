<?xml version="1.0" encoding="gbk" ?>
<function id="33100" name="余券退还客户查询" >
	<input>
		<item type="text" 	id="cust_code" 		name="客户代码"/>
		<item type="text" 	id="acc_code" 		name="证券账户"/>	
		</input>
	<service type="procedure" id="p_remain_stock_return" process="911" style="77">
		<param id="user_id" type="integer"/>
		<param id="cust_code" />	
		<param id="acc_code"/>
	</service>
	<output print="ture">
		<column type="number" id="cust_id"   		name="客户ID"   	display="false"/>
		<column type="string" id="cust_code"   		name="客户代码"  />
    	<column type="string" id="cust_name"   		name="客户姓名"  />
		<column type="number" id="acc_id"   		name="账户ID"   	display="false"/>
    	<column type="string" id="acc_code"   		name="证券账户"  />
    	<column type="string" id="property_type"   	name="资产种类"  />
    	<column type="string" id="currency_type"   	name="货币类型"  />
    	<column type="string" id="market_code"   	name="市场代码"  />
		<column type="string" id="product_type"   	name="证券类型"  />
    	<column type="string" id="product_code"   	name="证券代码"  />
    	<column type="number" id="asset_qty"   		name="融券数量"  		precision="2"/>
    	<column type="number" id="trin_bal"   		name="客户已还券数量"  	precision="2"/>
    	<column type="number" id="needbe_returns"   name="券商应还券数量"  	precision="2"/>
    	<column type="number" id="yqhz"   		    name="券商实际还券数量"  	precision="2"/>
    	<column type="string" id="time"      		name="时间"  />
	</output>
</function>