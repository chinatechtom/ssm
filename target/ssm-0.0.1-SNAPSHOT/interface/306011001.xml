<?xml version="1.0" encoding="gbk" ?>
<function id="306011001" name="信用产品浏览" >	
  <input> 
		<item type="hidden"	id="bond_id"		 name="产品id" />
		<item type="hidden"	id="order_type"	 name="产品代码" />
		<item type="hidden"	id="order_id"		 name="定单ID" />
  </input>
  <service type="procedure" id="p_financing_feerate_query" implement="p_financing_feerate_query306011001"  process="911" style="77"> 
		<param id="user_id"			type="integer" 	way="in"/>
		<param id="bond_id"			type="integer" 	way="in"/>
		<param id="order_type"	    type="integer"  way="in" default="21"/>
		<param id="order_id"		type="integer"  way="in"/>
  </service>
  <output>
		<column type="number" id="credit_product_id_fin"			 name="产品id"			display="false"/>
		<column type="string" id="product_code_fin"					 name="产品代码"/>
		<column type="string" id="credit_product_name_fin"			 name="产品名称"/>
		<column type="string" id="product_sort_fin"					 name="产品类型1"		display="false"/>
		<column type="string" id="product_sort_name_fin"			 name="产品类型"/>
		<column type="number" id="basis_interest_id_fin"			 name="基准利率id"		display="false"/>
		<column type="number" id="financing_interest_rate"			 name="融资利息率"		precision="4" display="false"/>
		<column type="number" id="fix_borrow_fund_rate"				 name="融资固定额度费率"	precision="4" display="false"/>
		<column type="number" id="takeup_borrow_fund_rate"			 name="融资占用额度费率"	precision="4" display="false"/>
		<column type="number" id="bef_ret_com_charge_fin"	         name="提前还贷手续费"	precision="4"/>
		<column type="number" id="punish_interest_rate_fin"		     name="罚息率"			precision="4"/>
		<column type="number" id="bad_punish_interest_rate_fin"		 name="坏帐罚息率"		precision="4"/>
		<column type="number" id="basis_interest_rate"				 name="基准利率(年)(%)"	precision="4"/>		
  </output>		
</function>