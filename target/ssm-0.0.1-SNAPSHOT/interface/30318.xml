<?xml version="1.0" encoding="gbk" ?>
<function id="30318" name="设置VAR模型标志" >	
	<input>		
		<item type="hidden" id="product_id"	      			  name="产品id"/>				
		<item type="text"	id="stock_rate_var"				  name="证券折算率(%)"     nullable="false"	number="true"/>		
		<item type="select" id="if_financing_var"			  name="融资标的"   source="302" nullable="false"/>
		<item type="select" id="if_borrow_stock_var"		  name="融券标的"	   source="303" nullable="false"/>
		<item type="select" id="if_guarantee_var"			  name="担保品标的"  source="304" nullable="false"/>
	</input>
    <service type="procedure" id="p_product_flag_var_set" process="811" style="1"> 
		<param id="user_id"						 type="integer" way="in"/>		
		<param id="product_id"					 type="integer" way="in"/>		
		<param id="stock_rate_var"				 type="double"	way="in"/>	
		<param id="if_financing_var"			 type="string"	way="in"/>
		<param id="if_borrow_stock_var"			 type="string"	way="in"/>
		<param id="if_guarantee_var"			 type="string"	way="in"/>
	</service>	
</function>
