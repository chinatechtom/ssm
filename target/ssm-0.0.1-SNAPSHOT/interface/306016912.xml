<?xml version="1.0" encoding="gbk" ?>
<function id="306016912" name="客户持有流通股限售或解除限售查询" >
	<input>
		<item type="hidden" id="cust_id"			name="客户ID"/>
		<item type="hidden" id="commness_cust_code" name="普通资金账户"/>
		<item type="hidden" id="cust_name"			name="客户姓名"/>
		<item type="hidden" id="cert_no"			name="身份证号码"/>
	</input>
	<service type="procedure" id="p_hold_circulate_info" implement="p_hold_circulate_info306016912" process="911" style="1">
		<param id="user_id"			   type="integer" way="in"/> 
		<param id="cust_id"			   type="integer" way="in"/> 
		<param id="commness_cust_code" type="string" way="in"/> 
		<param id="cust_name"		   type="string" way="in"/> 
		<param id="cert_no"			   type="string" way="in"/> 
	</service> 
	<output>
		<column id="cust_id"			type="number" name="客户ID" display="false"/>
		<column id="commness_cust_code" type="string" name="普通资金账户" display="false"/> 
		<column id="cust_name"			type="string" name="客户姓名"   display="false"/> 
		<column id="cert_no"			type="string" name="身份证号码" display="false"/>
		<column id="product_name"		type="string" name="股票名称"/>
		<column id="product_code"		type="string" name="股票代码"/>
		<column id="market_code"		type="string" name="市场代码"/>
		<column id="stock_amt"			type="number" name="股份数量"/>
		<column id="product_type"		type="string" name="股票类型TYPE" display="false"/>
		<column id="product_type_name"		type="string" name="股票类型"/>
		<column id="relieve_date"		type="string" name="解禁日期"/>
	</output>
</function>