  <item type="select" id="market_code"     name="市场名称"    source="039" />
  <item type="date"  id="date"      name="日期"   default="now"/>
  </input>
  <service type="procedure" id="P36000017" process="911" style="77">
  <param id="user_id"      type="integer"    way="in"/>
  <param id="product_id"     type="integer"  way="in"/>
  <param id="date"       type="string"   way="in"/>
   <param id="market_code"    type="string"   way="in"/>
  </service>
  <output export="true" print="true">    
    <column type="number" id="id"          name="序号"                 />
    <column type="string" id="XYZQDM"      name="证券代码"                 />
    <column type="number" id="XYZRRZYE"     name="前日融资余额"       precision="2" />
    <column type="number" id="XYJRRZMR"     name="本日融资买入额"      precision="2" />
    <column type="number" id="XYJRRZCH"     name="本日融资偿还额"      precision="2" />
    <column type="number" id="XYJRZJHK"     name="本日直接还款额"      precision="2" display="false"/>
    <column type="number" id="XYJRMJHK"     name="本日卖券还款额"      precision="2" display="false"/>
    <column type="number" id="XYRZPC"      name="本日融资强制平仓额"    precision="2" display="false"/>
    <column type="number" id="XYJRRZZQ"     name="本日融资正权益调整量"   precision="2" display="false"/>
    <column type="number" id="XYJRRZFQ"     name="本日融资负权益调整量"   precision="2" display="false"/>
    <column type="number" id="XYZRRJYE"     name="前日融券余额"       precision="2" />
