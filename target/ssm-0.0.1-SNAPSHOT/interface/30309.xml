<?xml version="1.0" encoding="gbk" ?>
<function id="30309" name="证券风险评估" >	
	<input>		
		<item type="hidden" id="product_id"					name="产品id"/>				
		<item type="select" id="product_risk_level_id"		name="证券风险级别"		source="90000001" nullable="false"/>
		<item type="text"	id="stock_convert_rate"			name="证券折算率(%)"		nullable="false" number="true"/>
		<item type="text"	id="borrow_sec_rate"			name="融券冻结率(%)"		nullable="false" number="true"/>
		<item type="text"	id="fee_rate"					name="融券浮动费率(%)"		nullable="false" number="true"/>
		<item type="text"	id="stock_factor"				name="证券收费因子"		nullable="false" number="true"/>
		<item type="select" id="if_financing"				name="融资标的"				source="302" nullable="false"/>
		<item type="select" id="if_borrow_stock"			name="融券标的"				source="303" nullable="false"/>
		<item type="select" id="if_guarantee"				name="担保品标的"			source="304" nullable="false"/>
		<item type="select" id="bstk_allot_ret_flag"		name="融券配股偿还标志"	source="520" nullable="false"/>
		<item type="text"   id="beta_value"					name="证券负债利率(%)"		nullable="false"/>
		<item type="select" id="if_guarantee_sumbit_buy"	name="暂停标的券提交买入控制"	source="535" nullable="false"/>
		<item type="select" id="debt_ret_flag"				name="配债偿还标志"		source="520" nullable="false"/> <!--add 21040903-->
		<item type="select" id="payout_warrant_ret_flag"	name="派发权证偿还标志"	source="520" nullable="false"/> <!--add 21040903-->
		<item type="select" id="roll_over_flag"				name="展期标示"				source="302" nullable="false"/> <!--add 20141017-->
		<item type="select" id="rq_sell_price_limit"		name="融券卖出价格限制"	source="633" nullable="false"	editable="false"/> <!--add 20150504-->
		<item type="text"	id="submit_rate"				name="提交比例"/> <!--add 20141017-->
		<item type="select" id="if_concentration_control"	name="是否集中度控制"	source="302" nullable="false"/> 
	</input>
    <service type="procedure" id="p_product_pro_risk_level_modi" process="911" style="1"> 
		<param id="user_id"						type="integer"	way="in"/>		
		<param id="product_id"					type="integer"	way="in"/>		
		<param id="product_risk_level_id"		type="integer"	way="in"/>
		<param id="stock_convert_rate"			type="double"	way="in"/>	
		<param id="borrow_sec_rate"				type="double"	way="in"/>
		<param id="fee_rate"					type="double"	way="in"/>
		<param id="if_financing"				type="string"	way="in"/>
		<param id="if_borrow_stock"				type="string"	way="in"/>
		<param id="if_guarantee"				type="string"	way="in"/>
		<param id="stock_factor"				type="double"	way="in"/>
		<param id="bstk_allot_ret_flag"			type="string"	way="in"/>
		<param id="beta_value"					type="double"	way="in"/>
		<param id="if_guarantee_sumbit_buy"		type="string"	way="in"/>
		<param id="debt_ret_flag"				type="string"	way="in"/> <!--add 21040903-->
		<param id="payout_warrant_ret_flag"		type="string"	way="in"/> <!--add 21040903-->
		<param id="roll_over_flag"				type="string"	way="in"/> <!--add 20141017-->
		<param id="rq_sell_price_limit"			type="string"	way="in"/> <!--add 20150504-->
		<param id="submit_rate"					type="double"	way="in"/> <!--add 20141017-->
		<param id="if_concentration_control"	type="string"	way="in"/>
	</service>
	<output target="3030902">
		<column name="市场代码"					id="smarket_code"	type="string"	field="smarket_code"/>
		<column name="证券代码"					id="sstock_code"	type="string"	field="sstock_code"/>
		<column name="证券折算率"				id="drate0"			type="number"	field="drate0"/>
		<column name="证券负债利率"			id="drate1"			type="number"	field="drate1"/>
		<column name="是否融资标的"			id="sstatus1"		type="string"	field="sstatus1"/>
		<column name="是否融券标的"			id="sstatus2"		type="string"	field="sstatus2"/>
		<column name="是否担保品标的" 			id="sstatus3"		type="string"	field="sstatus3"/>
		<!--  20141017
		<column name="配股偿还标志" 			id="sstatus4"		type="string"	field="sstatus4"/>
		<column name="暂停标的券提交买入控制" 	id="smain_flag"		type="number"	field="smain_flag"/>	
		-->
		<column name="权益补偿方式"			id="sphone"			type="string"/> <!--add 20141017-->
		<column name="标的风险标示"			id="sphone2"		type="string"/> <!--add 20141017-->
		<column name="提交比例"					id="damt0"			type="number"/>	<!--add 20141017-->
		<column name="总股本"					id="damt1"			type="number"/>	<!--add 20141017-->
  </output>	
</function>
