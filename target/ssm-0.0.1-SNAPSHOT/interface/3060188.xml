<?xml version="1.0" encoding="gbk"?>
<function	id="3060188" name="系数与信用级别对应关系查询">
	<input>
		<item type="text" id="credit_level_code" name="信用级别代码"/>
		<item type="text" id="credit_level_name" name="信用级别名称"/>
	</input>
	<service type="produrce"	id="p_factor_credit_level_query"	implement="p_factor_credit_level_query3060188"	process="911"	style="77">
		<param id="user_id" 			type="integer" way="in"/>
		<param id="credit_level_code" 	type="string"  way="in"/>
		<param id="credit_level_name" 	type="string"  way="in"/>
	</service>
	<output>
		<column type="string"	id="credit_level_code"	name="信用级别代码"/>
		<column type="string"	id="credit_level_name"	name="信用级别名称"/>
		<column type="number"	id="factor"				name="授信系数" precision="2"/>
		<button type="edit"		id="edit"	name="修改"	multiple="0"	action="30601880"/>
		<button type="edit"		id="edit"	name="增加"	multiple="-1"	action="30601881"/>
		<button type="edit"		id="edit"	name="删除"	multiple="0"	action="30601882"/>
	</output>
</function>