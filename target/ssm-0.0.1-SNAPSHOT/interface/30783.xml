  </input>
  <service type="procedure" id="p_cust_pofit_monitor" process="911" style="77"> 
		<param type="integer" id="user_id"	way="in"/>
  </service>
  <output refresh="120" export="true">
		<column type="string" id="remark"				    name="盈亏监控类别"		  />
		<column type="string" id="institution_code" name="营业部代码"		    />
		<column type="string" id="institution_name" name="营业部名称"		    />
