<?xml version="1.0" encoding="gbk" ?>
<function id="77120111" name="冻结客户资金信息同步">
	<description>确实同步客户冻结资金信息吗?</description>
	<input>
		<item type="hidden" id="institution_code" alias="sbranch_code0" name="开户营业部代码"/>
		<item type="hidden" id="institution_code" alias="sbranch_code1" name="发生营业部代码"/>
		<item type="hidden" id="cust_code" alias="scust_no" name="客户号"/>
		<item type="hidden" id="currency_type" alias="scurrency_type" name="货币类型"/>
		<item type="hidden" id="second_amt" alias="damt9" name="临时冻结金额"/>
	</input>    
	<service type="drtp" id="120111" process="711" style="77">
		<param type="string" id="sbranch_code0" />
		<param type="string" id="sbranch_code1" />
		<param type="string" id="scust_no" />
		<param type="string" id="scurrency_type" default="01"/>
		<param type="string" id="scurrency_type2"/>
		<param type="string" id="lsafe_level"	default="0"/>
		<param type="number" id="damt9"	/>
		<param type="string" id="scheck_emp"/>
		<param type="string" id="sstation0"/>
		<param type="string" id="semp" default="SYSTEM"/>
		<param type="string" id="lwithdraw_flag" default="1"/>
	</service>
	<output target="78120111">
		<column name="发生日期"	id="tradedate"   type="string"  field="sdate0"/>		
		<column name="流水号"	id="serial_no"  type="string"  field="sserial0"/>
		<column name="客户号"	id="cust_code"  type="string"  field="scust_no"/>
		<column name="冻结金额"	id="frz_amt"  type="number"  field="damt9"/>
	</output>
</function>
