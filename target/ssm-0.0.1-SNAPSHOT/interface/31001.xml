<?xml version="1.0" encoding="gbk" ?>
<function id="31001" name="资金统计" >  
  <input>
	<item type="date" id="date" name="日期"  default="now"/>
	<item type="select" id="property_type" name="资产种类"  source="129"/>
	<item type="select" id="credit_level_id" source="90000002" name="信用级别" />
  </input>
   <service type="procedure" id="p_found_addup" process="911" style="77">
	<param id="user_id" type="integer" way="in" /> 
	<param id="date"/>
	<param id="property_type" />
	<param id="credit_level_id" />
  </service> 
  <output >
    <column type="string" id="date" name="日期"/>  
	<column type="number" id="institution_id" name="机构id" display="false"/>
	<column type="string" id="institution_code" name="机构代码"/>
    <column type="number" id="property_type"   name="资产种类" display="false"/>
	<column type="string" id="property_typename"   name="资产种类"/>
	<column type="nmumber" id="currency_type" name="币种" display="false"/>
	<column type="nmumber" id="currency_typename" name="币种"/>
    <column type="number" id="credit_level_id"   name="信用级别" display="false"/>
    <column type="string" id="credit_level_name"   name="信用级别名称"/>
	<column type="number" id="fund_amt"   name="资金余额" precision="2"/>
	<column type="number" id="deposit"   name="存入金额" precision="2"/>
    <column type="number" id="withdraw"   name="取出金额" precision="2"/>
    <column type="number" id="frozen_amt"   name="冻结资金" precision="2"/>
   	<column type="number" id="abnormal_frozen_amt"   name="异常冻结资金" precision="2"/>
    <column type="number" id="real_buy_amt"   name="实际买入金额" precision="2"/>
    <column type="number" id="buy_frozen_amt"   name="买入冻结金额" precision="2"/>
	<column type="number" id="sell_done_amt"   name="卖出解冻金额" precision="2"/>
    <column type="number" id="manual_frzn_amt"   name="手工冻结金额" precision="2"/>
    <column type="number" id="manual_unfrzn_amt"   name="手工解冻金额" precision="2"/>
	<column type="number" id="taday_qty"   name="当日变动数量" precision="2"/>
    <column type="number" id="credit"   name="信用额" precision="2"/>
    <column type="number" id="forbid_amt"   name="禁用资金" precision="2"/>
 	<column type="number" id="forbid_asset"   name="禁取资产" precision="2"/>
  </output>
</function>