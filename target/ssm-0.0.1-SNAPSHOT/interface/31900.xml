<?xml version="1.0" encoding="gbk" ?>
<function id="31900" name="合同查询" >
  <input export="true">
		<item type="select" id="institution_id" name="营业部名称"   source="95000008"/>
 		<item type="hidden" id="cust_id"    name="客户id"/>
		<item type="text"   id="cust_code"  name="客户代码" />
		<item type="hidden" id="cust_name" name="客户姓名"/>
		<item type="hidden" id="cert_no" name="证件号码"/>
		<item type="hidden" id="bond_id" name="合同id"/>
		<item type="hidden" id="bond_no" name="合同编号"/>
		<item type="hidden" id="bond_status" name="合同状态" source="022" />
		<item type="hidden" id="manage_status" name="处理状态" />
		<item type="date"   id="begin_date" name="合同签署起始日期" default="last"/>
		<item type="date"   id="end_date"   name="合同签署结束日期" default="now"/>
		<group name="分页查询" page="true">		
			<item type="text"   id="page_num"       name="页码"     integer="true" default="1"/>
			<item type="select" id="rows_num"       name="每页行数" integer="true" source="95000012" nullable="false"/>
		</group>
  </input>
  <service type="procedure" id="p_cust_bond_browse" process="911" style="77">
		<param type="integer" id="user_id"/>	
		<param id="page_num" type="integer"/>
		<param id="rows_num" type="integer"/>		
		<param type="integer" id="institution_id"/>
 		<param type="integer" id="cust_id"/>
		<param id="cust_code"/>
		<param id="cust_name"/>
		<param id="cert_no"/>
		<param type="integer" id="bond_id"/>
		<param id="bond_no"/>
		<param type="integer" id="bond_status"/>
		<param type="integer" id="manage_status"/>
		<param id="begin_date"/>
		<param id="end_date"/>		
  </service>
  <output export="true" >    
    <column type="string" id="institution_name" 	name="营业部名称" />
    <column type="string" id="CUST_CODE"  				name="客户代码" />
    <column type="string" id="CUST_NAME"  				name="客户姓名" />
    <column type="string" id="BOND_NO"  					name="合同编号" />
    <column type="string" id="bond_status_name"  	name="合同状态" />
    <column type="string" id="ddate"  						name="合同签署日期" />
    <column type="string" id="time"  							name="合同签署时间" display="false"/>
    <column type="string" id="begin_time"  				name="合同起始日期" />
    <column type="string" id="return_time"  			name="合同到期日期" />
    <column type="number" id="DC_MODE"  					name="借贷模式id" display="false"/>
    <column type="string" id="dc_mode_name"  			name="借贷模式" display="false"/>
    <column type="string" id="INSTITUTION_ID"  		name="营业部ID" display="false"/>
    <column type="number" id="CURRENCY_TYPE"  		name="货币类型" display="false"/>
    <column type="string" id="CURRENCY_TYPE_NAME" name="货币类型" display="false"/>
    <column type="string" id="AVAILABILITY_TIME"  name="合同有效日期" display="false"/>
    <column type="string" id="OVER_TIME"  				name="合同延长日期" display="false"/>
    <column type="string" id="REMARK"  						name="备注说明" display="false"/>
    <column type="number" id="BOND_STATUS" 				name="合同状态" display="false"/>
    <column type="number" id="REGI_STATE"  				name="处理状态" display="false"/>
    <column type="string" id="REGI_STATE_NAME"  	name="合同延长日期" display="false"/>
    <column type="number" id="ACCEPT_ID" 					name="受理员id" display="false"/>
    <column type="string" id="ACCEPT_NAME"  			name="受理员" display="false"/>
	<column type="string" id="ACCEPT_TIME" 				name="受理日期" display="false"/>
    <column type="number" id="MANAGE_ID"  				name="处理员id" display="false"/>
    <column type="string" id="MANAGE_NAME"  			name="处理员" display="false"/>
    <column type="string" id="MANAGE_TIME"  			name="处理日期" display="false"/>
    <column type="number" id="APPROVER_ID"  			name="审核员id" display="false"/>
    <column type="string" id="APPROVER_NAME"  		name="审核员" display="false"/>
    <column type="string" id="APPROVER_OPINION"  	name="审核意见" display="false"/>
    <column type="string" id="APPROVER_TIME"  		name="审核时间" display="false"/>
    <column type="number" id="ASSESSOR_ID"  			name="审批员id" display="false"/>
    <column type="string" id="assessor_name" 			name="审批员" display="false"/>
	<column type="string" id="assessor_opinion" 	name="审批意见" display="false"/>
	<column type="string" id="assessor_time" 			name="审批日期" display="false"/>
	<column type="number" id="bond_id"  					name="合同ID"  display="false"/>
    <column type="number" id="cust_id"						name="客户ID"  display="false"/>
    <button type="view" id="edit" name="查看合同详细" action="3065555510" multiple="0"/>
	</output> 	
  	<assistant action="30103,3200301,30102,30104,30109,30111,30201,30202,30205,30206,30207,30215,30673,30672,30667,30668,31905,31906">
		<link src="cust_id" target="cust_id"/>
    </assistant>
</function>