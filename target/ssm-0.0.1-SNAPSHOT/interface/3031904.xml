		<column type="string" id="product_code2"	 name="融券产品"/>
		<column type="number" id="credit_num2"		 name="授信额度" precision="2"/>
		<column type="number" id="financing_amt"	 name="融资上限" precision="2"/>
		<column type="number" id="bstock_amt"		 name="融券上限" precision="2"/>
		<column type="string" id="financing_product_code"    name="融资产品"/>
		<column type="string" id="borrow_stock_product_code" name="融券产品"/>
  </output>	
</function>
