    <column type="string" id="cust_name"   			name="客户姓名"  />
	<column type="string" id="full_name"   			name="客户全称"  display="false"/>
    <column type="string" id="cust_type"   			name="客户类型"  />
    <column type="string" id="cust_sort"   			name="客户类别"  />
    <column type="string" id="gender"   			name="性别"  />
    <column type="string" id="cert_type"   			name="证件类型" /> 
    <column type="string" id="cert_no"   			name="证件号码" /> 
