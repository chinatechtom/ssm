<?xml version="1.0" encoding="gbk" ?>
<function id="80880048" name="深圳市场违约处置报告查询">
	<input>		
		<item type="text"	id="cust_code" name="客户号"/>
		<item type="text"	id="cust_name" name="客户名称"/>
		<item type="text"	id="acc_code" name="股东账号"/>		
		<item type="date"	id="begin_date"	name="开始日期" default="last"/>
		<item type="date"	id="end_date"	name="结束日期" default="now"/>
	</input>
	<service type="procedure" id="P60880048" process="933" style="77">
		<param id="user_id"		type="integer"/>
		<param id="cust_code"	type="string"/>
		<param id="cust_name"	type="string"/>
		<param id="acc_code"	type="string"/>		
		<param id="begin_date"	type="string"/>
		<param id="end_date"	type="string"/>
	</service>
	<output export="true">
		<column id="trade_date" name="日期" type="string"/>
		<column id="origin_sqbh" name="初始交易合同序号" type="string"/>
		<column id="supplement_sqbh" name="补充质押合同序号（如有）" type="string"/>		
		<column id="engage_trade_date" name="初始交易日期" type="string"/>
		<column id="first_amt" name="初始交易金额" type="string"/>
		<column id="days" name="回购期限" type="string"/>
		<column id="bb_date" name="到期购回日" type="string"/>
		<column id="product_code" name="质押标的证券代码" type="string"/>
		<column id="product_name" name="质押标的证券简称" type="string"/>
		<column id="curr_stock_vol" name="质押标的证券数量" type="string"/>
		<column id="all_stock_percent" name="质押数量占总股本" type="string"/>		
		<column id="rrf_acc_code" name="融入方证券账户号码" type="string"/>
		<column id="rrf_name" name="融入方名称" type="string"/>
		<column id="rcf_acc_code" name="融出方证券账户号码" type="string"/>
		<column id="rcf_name" name="融出方名称" type="string"/>
		<column id="zjyt" name="资金用途" type="string"/>
		<column id="handle_date" name="违约处置申报日期" type="string"/>
		<column id="sell_date" name="处置卖出日期" type="string"/>
		<column id="sell_method" name="处置卖出方式" type="string"/>
		<column id="cjje" name="处置卖出金额" type="string"/>
		<column id="sell_vol" name="处置卖出数量" type="string"/>
		<column id="if_full_payback" name="融出方是否足额受偿" type="string"/>
		<column id="rcf_first_got" name="融出方优先受偿金额" type="string"/>		
		<column id="return_amt" name="其中融入方违约金金额" type="string"/>
		<column id="return_rrf_balance" name="返还融入方资金金额" type="string"/>
		<column id="return_method" name="返还融入方方式" type="string"/>		
		<column id="return_stock_code" name="处置剩余解除质押证券代码" type="string"/>
		<column id="return_stock_vol" name="处置剩余解除质押证券数量" type="string"/>		
		<column id="remark1" name="其他需说明的处置安排" type="string"/>
		<column id="remark2" name="其他需说明的处置结果" type="string"/>
	</output>
</function>