			<column type="number" id="THIS_SEASON_GUA_TRADE"		name="本季担保品买卖交易量"	  precision="2" 	 />
			<column type="number" id="THIS_SEASON_TRADE_AMT"		name="本季融资融券总交易量"	  precision="2" 	 />
			<column type="number" id="THIS_SEASON_TRADE_AMT_RATE"	name="本季融资融券总交易量公司占比" precision="6" />
