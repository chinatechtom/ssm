		<button type="mod"    id="chg"	name="评级批量复核生效" action="30601147"	 multiple="1"/>
		<!--button type="edit"   id="all"	name="评级征信批量生效" action="30601146"	 multiple="-1"/-->
		<button type="edit"   id="all"	name="二次征信批量生效" action="306014608"	 multiple="-1"/>
		
	</output>
	<assistant action="3042206">
		<link src="cust_id"	  target="cust_id"/>
		<link src="versionid" target="versionid"/>
	</assistant>
</function>