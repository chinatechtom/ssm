<?xml version="1.0" encoding="gbk" ?>
<function id="3092001" name="将交易所标的证券和违约资料从ftp服务器传送到BIX服务器">
	<input>
		<item type="text"	id="lvol0"			name="方向"			default="0"	nullable="false"/>
		<item type="text"	id="lvol1"			name="下载方式"		default="0"	nullable="false"/>	
		<item type="text"	id="snote"			name="远程文件名"		nullable="false"/>	
		<item type="text"	id="snote2"			name="本地文件名"		nullable="false"/>
		<item type="text"	id="lwithdraw_flag"	name="ftp连接号"		default="8888"/>
		<item type="hidden"	id="sstation1"		name="融资融券机构代码"/>
		<item type="hidden"	id="sdate3"			name="融资融券分支号"/>
		<item type="hidden"	id="scheck_emp"		name="融资融券柜员号"/>
	</input>
	<service type="drtp" id="413001" process="711" style="77">
		<param id="lvol0"			type="integer" default="0"/>
		<param id="lvol1"			type="string"  default="0"/>
		<param id="snote"			type="string"/>
		<param id="snote2"			type="string"/>
		<param id="lwithdraw_flag"	type="integer"	default="8888"/>
		<param id="sstation1"		type="string"/>
		<param id="sdate3"			type="string"/>
		<param id="scheck_emp"		type="string"/>		
	</service>
	<output	target="3092002">
		<column name="信息"		type="string"	id="vsmess"/>   
		<column name="文件路径"	type="string"	id="vsvarstr0"/>   
		<column name="文件名"	type="string"	id="snote"/>  
		<column name="更新时间"	type="string"	id="scust_auth"/>		
	</output>
</function>