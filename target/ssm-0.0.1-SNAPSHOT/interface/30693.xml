<?xml version="1.0" encoding="gbk" ?>
<function id="30693" name="可申请融券总额度限制" >
  <input>
  <item type="hidden" id="cust_id" name="客户id"/>
  <item type="hidden" id="market_code" name="市场代码"/>
  <item type="hidden" id="product_code" name="证券代码"/>
  </input>
  <service type="procedure" id="p_get_institution_total_stock_query" process="911" style="77">
		<param type="integer" id="user_id"/>	
		<param type="integer" id="cust_id"/>
		<param type="string" id="market_code"/>
		<param type="string" id="product_code" way="in"/>
  </service>
  <output>    
  <column type="string" id="c1" name="证券代码"/>  
	<column type="number" id="c2" name="证券公司融券总额度"/>
	<column type="number" id="c3" name="证券公司已发放融券上限"/>  
	<column type="number" id="c6" name="所有客户已发放待生效额度"/> 
	<column type="number" id="c4" name="证券公司可发放融券剩余额度"/>
	<column type="number" id="c5" name="当前客户可申请融券上限"/>  
	<column type="number" id="c7" name="冻结数量"/>  
  </output> 
</function>