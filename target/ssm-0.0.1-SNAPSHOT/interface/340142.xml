<?xml version="1.0" encoding="gbk" ?>
<function id="340142" name="计算保证金比例因子分数(日均资产规模,收益率,投资经验)">
	<input>
		<item type="hidden" id="crd_capital_account" name="信用资金账号"/>
		<item type="hidden" id="pt_capital_account" name="普通资金账号" />
		<item type="hidden" id="abs_profit_rate" name="年化收益率(%)" />
		<item type="hidden" id="avg_asset" name="日均资产规模(万元)" />
		<item type="hidden" id="tx_date" name="首次交易日期"/>	
		<item type="hidden" id="evaluate_dict" name="风险评测结果"/>	
	</input>
	<service type="procedure" id="P17000022" adapter="sysinfo" implement="P17000022" process="811" style="1">
		<param id="user_id" type="integer" way="in" /> 
		<param id="crd_capital_account" type="string"  alias="cust_code" way="in" />
		<param id="abs_profit_rate" type="number" alias="year_profit_rate" way="in" /> 
		<param id="avg_asset" type="number"  alias="daily_asset" way="in"/> 
		<param id="tx_date"  type="string" alias="first_trade_date" way="in"/> 
		<param id="evaluate_dict"  type="string" way="in"/> 
	</service>
	<output/>
</function>