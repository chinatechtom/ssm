			<column type="number" id="THIS_MONT_TRADE_AMT"			name="本月融资融券总交易量"	  precision="2" 	 />
			<column type="number" id="THIS_MONT_TRADE_AMT_RATE"	    name="本月融资融券总交易量公司占比" precision="6" />
		</group>
		<group name="本季占比">
