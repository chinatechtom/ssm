<?xml version="1.0" encoding="gbk" ?>
<function id="31804" name="融券明细查询" >	
  <input>		
  <item type="select" id="institution_id" name="营业部名称"  source="95000008"/>
	<item type="hidden" id="cust_id" name="客户id"/>
	<item type="text"   id="in_cust_code"    name="客户代码" />
	<item type="text"   id="in_product_code" name="证券代码" />
	<item type="date"   id="date" name="日期" />
  </input>  
  <service type="procedure" id="p_report_stock_query" process="911" style="77">
	<param id="user_id" type="integer" />
	<param id="cust_id" type="integer"/>
	<param id="institution_id" type="integer"/>
	<param id="in_cust_code"     type="string"	way="in"/>
	<param id="in_product_code"  type="string"  way="in"/>	
	<param id="date"  type="string"  way="in"/>
  </service>  
  <output export="true">  	
  <column type="string"  id="institution_id"             name="机构ID" display="false"/>
  <column type="string"  id="institution_name"           name="机构名字"/>
	<column type="string"  id="cust_code"                  name="客户代码"/>
	<column type="string"  id="cust_name"                  name="客户姓名"/>
	<column type="string"  id="product_code"               name="证券代码"/>    
	<column type="number"  id="yes_bs_vol"                 name="前日融券余量" precision="2"/>
	<column type="number"  id="today_bs_vol"               name="本日融券余量" precision="2"/>
	<column type="number"  id="today_bs_sell_vol"          name="本日融券卖出量" precision="2"/>
	<column type="number"  id="today_return_val"           name="本日融券偿还量" precision="2"/>	
	<column type="number"  id="today_buy_return_vol"       name="本日买券还券量" precision="2"/>	
	<column type="number"  id="today_direct_return_vol"    name="本日直接还券量" precision="2"/>
	<column type="number"  id="today_bs_pc_vol"            name="本日融券强制平仓量"  precision="2"/>
	<column type="number"  id="today_bs_vol_amt"           name="本日融券余量余额"	  precision="2"/>	
	<column type="number"  id="bs_unit"                    name="融券报送单位"/>
	<column type="string"  id="date"                       name="信用交易日期"/>   
  </output>
</function>