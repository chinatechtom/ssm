<?xml version="1.0" encoding="gbk" ?>
<function id="60220103" name="导入文件到质押回购系统">
	<input>
		<item type="text"	id="sstatus0"	name="导入方式"		default="0"		 nullable="false"/>
		<item type="text"	id="vsvarstr0"	name="路径"			nullable="false"/>	
		<item type="text"	id="saddr"		name="文件名"		nullable="false"/>	
		<item type="text"	id="saddr2"		name="数据库表名"	nullable="false"/>
		<item type="text"	id="snote"		name="数据格式名"	nullable="false"/>
		<item type="hidden"	id="sstation1"	name="融资融券机构代码"/>
		<item type="hidden"	id="sdate3"		name="融资融券分支号"/>
		<item type="hidden"	id="scheck_emp"	name="融资融券柜员号"/>
		<item type="hidden"	id="semp_pwd"	name="融资融券柜员密码"/>
	</input>
	<service type="drtp" id="413501" process="711" style="1">
		<param id="sstatus0"		type="string" default="0"/>
		<param id="vsvarstr0"		type="string"/>
		<param id="saddr"			type="string"/>
		<param id="saddr2"			type="string"/>
		<param id="snote"			type="string"/>
		<param id="sstation1"		type="string"/>
		<param id="sdate3"			type="string"/>
		<param id="scheck_emp"		type="string"/>
		<param id="semp_pwd"		type="string"/>
		<param id="sbank_code"		type="string" default="GPZY"/>
	</service>
	<output target="60220104">
		<column name="信息"		type="string"	id="vsmess"/>   
		<column name="记录个数"	type="number"	id="lvol1"/>   
		<column name="总记录个数"	type="number"	id="lvol2"/>
		<column name="文件名"	type="string"	id="saddr"/>   
		<column name="表名"	type="string"	id="saddr2"/>
		<column name="文件是否存在标志"	type="number"	id="lvol3"/> <!--20150624 SECGPZY-306-->
	</output>
</function>