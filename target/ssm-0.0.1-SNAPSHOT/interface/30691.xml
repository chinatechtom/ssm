  </input>
  <service type="procedure" id="p_get_institution_total_money_query" process="911" style="77">
	<param type="integer" id="user_id"/>	
	<param type="integer" id="cust_id"/>
  </service>
  <output>    
    <column type="number" id="model_id" name="申请模式id" display="false"/>
	<column type="string" id="model_name" name="申请模式"/>
    <column type="number" id="c1" name="公司最高可发放融资总额"  precision="2"/>  
	<column type="number" id="c2" name="公司最高可实际融资总额" precision="2"/>
	<column type="number" id="c3" name="公司已发放融资总额"  precision="2"/>  
	<column type="number" id="c4" name="客户已使用融资总额" precision="2"/>
	<column type="number" id="c5" name="公司可发放融资剩余额度"  precision="2"/>  
	<column type="number" id="c7" name="所有客户已发放待生效额度"  precision="2"/>  
	<column type="number" id="c6" name="客户可融资上限"  precision="2"/>  
	<column type="number" id="c8" name="专户冻结金额"  precision="2"/>  
