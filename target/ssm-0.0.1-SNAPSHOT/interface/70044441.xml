<?xml version="1.0" encoding="gbk" ?>
<function id="70044441" name="处理新增信用级别申请" >
	<input>
		<group name="合约申请信用级别" scope="group">
			<item type="hidden" id="order_id" name="合约id" action="70030605"/>
			<item type="text" id="order_no" name="合约编号" editable="false"/>
			<item type="hidden" id="formerly_voucher_no" name="原始凭证号" editable="false"/>
			<item type="text" id="consult_score" name="参考分值" editable="false"/>
			<item type="text" id="valuation_score" name="申请评估得分" editable="false"/>
			<item type="text" id="credit_level_name" name="申请信用级别"  editable="false"/>
		</group>
		<group name="处理后申请信用级别" >
			<item type="hidden" id="order_id" name="合约id" />
