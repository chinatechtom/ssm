<?xml version="1.0" encoding="gbk" ?>
<function id="30109" name="客户实时委托查询" >  
  <input export="true">
	<item type="select" id="institution_id" name="营业部名称" source="95000008"/>
	<item type="hidden" id="cust_id" 		name="客户号" />
	<item type="hidden" id="acc_id"         name="账户号"/>
	<item type="text"   id="cust_code"      name="客户代码"/>
	<item type="text"   id="product_code"   name="证券代码"/>
    <group name="分页查询" page="true">			
		<item type="text"    id="page_num"       name="页码"     integer="true" default="1"/>
		<item type="select"  id="rows_num"       name="每页行数" integer="true" source="95000012" nullable="false"/>
	</group>
  </input>
  <service type="procedure" id="p_cust_acc_real_entrust_query" process="911" style="77">
	<param id="user_id"        type="integer"  way="in"/>
	<param id="page_num"	   type="integer"  way="in"/> 			
 	<param id="rows_num"	   type="integer"  way="in"/> 		
	<param id="institution_id" type="integer"  way="in"/>
	<param id="cust_id"        type="integer"  way="in"/>
	<param id="acc_id"         type="integer"  way="in"/>
	<param id="cust_code"      type="string"   way="in"/>
	<param id="product_code"   type="string"   way="in"/> 
  </service>

  <output export="true"> 
    <column type="string" id="institution_code"	   name="营业部代码"/>    	
  	<column type="string" id="institution_name"    name="营业部名称"   />
	<column type="number" id="cust_id"             name="客户号"       display="false" />
    <column type="string" id="cust_code"           name="客户代码"  />
    <column type="string" id="cust_name"           name="客户姓名"  />
    <column type="string" id="acc_code"            name="证券账户"  />
	<column type="number" id="acc_id"              name="账户号"       display="false" />
	<column type="string" id="fund_acc_code"       name="资金帐号"     display="false"/>
	<column type="string" id="date"                name="委托日期"  />
    <column type="string" id="time"                name="委托时间"  />	
	<column type="string" id="entrust_type"        name="委托类型"      display="false"/>
	<column type="string" id="entrust_type_name"   name="委托类型"      display="false"/>
	<column type="string" id="entrust_no"          name="委托序号"  />
	<column type="string" id="contract_no"         name="合同号码"      display="false"/>
	<column type="string" id="market_code"         name="市场代码"      display="false"/>
	<column type="string" id="market_name"         name="市场名称"  />
	<column type="string" id="currency_type"       name="货币类型"      display="false"/>
	<column type="string" id="currency_type_name"  name="货币类型"      display="false"/>
	<column type="number" id="product_id"          name="产品id"  		 display="false"/>
	<column type="string" id="product_code"        name="证券代码"  />
	<column type="string" id="product_name"        name="证券名称"  />
	<column type="number" id="product_type"        name="产品类型"       display="false"/>
	<column type="string" id="product_type_name"   name="产品类型"       display="false"/>
	<column type="string" id="credit_flag"         name="信用交易标志id" display="false"/> 
	<column type="string" id="credit_flag_name"    name="信用交易标志"/> 
	<column type="string" id="in_out"              name="买卖方向"       display="false"/>
	<column type="string" id="in_out_name"         name="买卖方向"  />
	<column type="string" id="force_flag"          name="平仓标志id"     display="false"/>
	<column type="string" id="force_flag_name"     name="平仓标志"  />
    <column type="string" id="bs"                  name="交易类别"       display="false" />
	<column type="string" id="bs_name"             name="交易类别"       display="false"/>
	<column type="string" id="entrust_type"        name="委托类型"       display="false"/>
	<column type="string" id="entrust_type_name"   name="委托类型"       display="false"/>
	<column type="string" id="entrust_status"      name="委托状态"       display="false"/>
	<column type="string" id="entrust_status_name" name="委托状态"   	 display="false"/>
	<column type="string" id="entrust_flag"        name="委托标志"   	 display="false"/>
	<column type="string" id="entrust_flag_name"   name="委托标志"   	 display="false"/>
	<column type="string" id="rpt_status"          name="报盘状态"   	 display="false"/>
	<column type="string" id="rpt_status_name"     name="报盘状态"   	 display="false"/>
	<column type="string" id="sec_acc"             name="报盘代码"   	 display="false"/>
	<column type="string" id="report_seat"  	   name="报盘席位"   	 display="false"/>
	<column type="string" id="report_contract"     name="报盘合同号"  />
	<column type="string" id="withdraw_mark"       name="撤单标志"   	 display="false"/>
	<column type="string" id="withdraw_mark_name"  name="撤单标志"   	 display="false"/>
	<column type="number" id="entrust_price"       name="委托价格"   	 precision="3"/>		
    <column type="number" id="entrust_qty"         name="委托数量"   	 precision="2" total="true"/>
    <column type="number" id="entrust_amt"         name="委托金额"   	 precision="2" total="true"/>
    <column type="number" id="withdraw_qty"        name="撤单数量"   	 total="true"/>
    <column type="number" id="withdraw_amt"        name="撤单金额"   	 precision="2" total="true" display="false"/>
    <column type="number" id="trade_qty"           name="成交数量"   	 total="true"/>
    <column type="number" id="trade_amt"           name="成交金额"   	 precision="2" total="true"/>
	<column type="number" id="trade_price"         name="成交价格"   	 precision="2" />
	<column type="number" id="trade_counts"        name="成交笔数"   	 precision="2"/>
    <column type="number" id="qty_bal"             name="资产余额"   	 total="true" precision="2" display="false"/>
	<column type="number" id="frozen_qty"          name="冻结数量"   	 total="true" display="false"/>   
	<column type="number" id="no_frozen_qty"       name="解冻数量"   	 total="true" display="false"/> 
	<column type="number" id="this_qty"            name="本次余额"   	 display="false"/> 
  </output>
</function>