<?xml version="1.0" encoding="gbk" ?>
<function id="3064401" name="授信申请资产处理查询" >
  <input>
    <item type="text" id="total_asset_wy" name="信用资金台账总资产"/>
	<item type="text" id="total_debt_wy"  name="信用资金台账总负债"/>
	<item type="text" id="commness_stock_asset" name="普通证券账户资产总值"/>
	<item type="text" id="financial_asset" name="其他金融资产总值"/>
	<item type="text" id="estate_asset"    name="个人房产状况"/>
  </input>
  <service type="procedure" id="p_deal_credit_report_query" process="911" style="77">
	<param type="integer" id="user_id"/>	
	<param type="double"  id="total_asset_wy"/>
	<param type="double"  id="total_debt_wy"/>
	<param type="double"  id="commness_stock_asset"/>
	<param type="double"  id="financial_asset"/>
	<param type="double"  id="estate_asset"/>
  </service>
  <output>    
    <column type="number" id="financial_asset_half" name="金融资产的一半"    precision="4"/>
    <column type="number" id="asset_quarter"        name="总资产的四分之一"  precision="4" /> 
	<column type="number" id="net_assets"           name="净资产"    precision="4" /> 	
	<column type="string" id="asset_over_date"      name="以上资产截止时间"  />
    <column type="number" id="max_asset_deal"       name="上述两者谁大"      precision="4" /> 	
  </output> 
</function>