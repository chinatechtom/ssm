<?xml version="1.0" encoding="gbk" ?>
<function id="30721" name="融券到期监控" >
	<input>			
		<item type="hidden"  id="cust_id"	        name="客户id"/>
		<item type="text"    id="cust_code"	        name="客户代码"/>	
		<item type="text"    id="cust_name"	        name="客户姓名"/>	
		<item type="hidden"  id="product_id"	    name="融券产品id"/>
		<item type="text"    id="product_code"	    name="融券证券代码"/>
		<item type="date"    id="begin_date"	    name="起始时间"	default="last"/>	
		<item type="date"    id="end_date"	        name="终止时间"	default="now"/>
	</input>
    <service type="procedure" id=""  process="911" style="77">
		<param id="user_id"	            type="integer" way="in"/> 
		<param id="cust_id"				type="integer" way="in"/>
		<param id="cust_code"	        type="string"  way="in"/> 
		<param id="product_id"			type="integer" way="in"/>
		<param id="product_code"	    type="string"  way="in"/>
		<param id="begin_date"	        type="string"  way="in"/>
		<param id="end_date"	        type="string"  way="in"/>		
    </service>
    <output export="true" autoload="true" refresh="120">
		<column type="number" id="cust_id"			name="客户id"		display="false"/>
		<column type="string" id="cust_code"		name="客户代码"/>
		<column type="string" id="cust_name"		name="客户姓名"/>
		<column type="number" id="product_id"		name="融券产品id"		display="false"/>
		<column type="string" id="product_code"		name="融券证券代码"/>
		<column type="number" id=""				name="额度"	precision="3"	total="true"/>
		<column type="number" id=""				name="使用数量"	precision="3"	total="true"/>
		<column type="number" id=""				name="剩余数量"	precision="3"	total="true"/>
		<column type="string" id="begin_date"		name="起始日期"/>
		<column type="string" id="end_date"			name="终止日期"/>
		<column type="string" id="today"			name="当前日期"/>
		<column type="string" id="warning_level"	name="报警级别"	level="true"	display="false"/>
    </output>
	<assistant action="30103,3200301,30102,30104,30109,30111,30201,30202,30208,30673,30672,30667,30668,30669">
		<link src="cust_id"		target="cust_id"/>
		<link src="product_id"	target="product_id"/>
    </assistant>
</function>