<?xml version="1.0" encoding="gbk" ?>
<function id="3064403" name="授信申请获取信用上限_授信额度" >
  <input>	
    <item type="hidden" id="credit_limit_institution"   name="申请的授信额度(万元)" />
    <item type="hidden" id="net_assets"     name="客户净资产(万元)" />
	<item type="hidden" id="credit_limit_factor"     name="信用上限系数" />
	<item type="hidden" id="max_asset_deal" name="MAX(资产)(万元)" />
	<item type="hidden" id="credit_limit_factor_changed" name="信用上限系数(变更时)" />
	<item type="hidden" id="order_id" name="合约ID" />
  </input>
  <service type="procedure" id="p_order_crd_rpt_crd_num_query" process="911" style="77">
	<param id="user_id"  type="integer"/>
	<param id="credit_limit_institution"        type="double"/>
	<param id="net_assets"          type="double"/>
	<param id="credit_limit_factor" type="double"/>
	<param id="max_asset_deal" type="double"/>
	<param id="credit_limit_factor_changed" type="double"/>
	<param id="order_id" type="integer"/>
  </service>
  <output export="true">  
  	<column type="number" id="report_credit_num"     name="信用上限"   precision="4"/>  
	<column type="number" id="first_credit_limit"    name="客户初步授信额度"   precision="4"/>  
	<column type="number" id="first_financing_limit" name="客户初步融资上限"   precision="4"/>  
	<column type="number" id="first_bstock_limit"    name="客户初步融券上限"   precision="4"/>  
	<column type="number" id="net_assets_add"    name="净资产增加值"   precision="4"/> 
	<column type="number" id="last_net_assets"    name="上次授信净资产"   precision="4"/> 
  </output>
</function>