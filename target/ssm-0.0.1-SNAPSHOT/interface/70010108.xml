		<item name="开户人员"	id="open_oper"   type="text"	editable="false"/>                                                           
		<item name="开户日期"	id="open_date"   type="text"	editable="false"/>                                                           
		<item name="销户日期"	id="close_date"  type="text"	editable="false"/>                                                           
		<item name="销户人员"	id="close_oper"  type="text"	editable="false"/>                                                           
		<item name="记录状态"	id="regi_state"  type="text"	editable="false"/>                                                           
		<item name="机构代码"	id="institution_code"  type="text"	editable="false"/>                                                       
		<item name="移动电话"	id="move_tel"    type="text"	editable="false"/>                                                             
		<item name="固定电话"	id="fix_tel"     type="text"	editable="false"/>                                                             
		<item name="应急电话"	id="meet_tel"    type="hidden"	editable="false"/>                                                             
		<item name="邮件地址"	id="email"		 type="text"	editable="false"/>                                                               
		<item name="传真"	    id="fax"		 type="text"	editable="false"/>                                                                 
		<item name="客户状态" id="cust_status" type="hidden"		editable="false"/> 
		<item name="客户状态" id="cust_status_name" type="text"		editable="false"/>		
		<item name="证件有效年数" id="effective_years" type="text"	editable="false"/>                                                     
		<item name="证件起始日期" id="cert_start_date" type="text"	editable="false"/>                                                     
		<item name="证件终止日期" id="cert_end_date" type="text"	editable="false"/>                                                       
		<item name="上海证券账户" id="holder_acc_sh" type="text"	editable="false"/>                                                       
		<item name="深圳证券账户" id="holder_acc_sz" type="text"	editable="false"/> 
		<item name="上海席位代码" id="seat_code_sh" type="text"	editable="false"/>                                                       
		<item name="深圳席位代码" id="seat_code_sz" type="text"	editable="false"/> 
	</input>	
	
	<service type="procedure" id="p70010108" process="822" style="1">
		<param id="user_id" type="integer"/>
		<param id="cust_code"   type="string"/>
		<param id="cust_name"   type="string"/>
		<param id="cust_type"   type="string"/>
		<param id="cust_sort"   type="string"/>
		<param id="gender"      type="string"/>
		<param id="cert_type"   type="string"/>
		<param id="cert_no"     type="string"/>
		<param id="address"     type="string"/>
		<param id="postalcode"  type="string"/>
		<param id="open_oper"   type="string"/>
		<param id="open_date"   type="string"/>
		<param id="close_date"  type="string"/>
		<param id="close_oper"  type="string"/>
		<param id="regi_state"  type="string"/>
		<param id="institution_code"  type="string"/>
		<param id="move_tel"    type="string"/>
		<param id="fix_tel"     type="string"/>
		<param id="meet_tel"    type="string"/>
		<param id="email"		type="string"/>
		<param id="fax"		 	type="string"/>
