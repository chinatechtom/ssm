<?xml version="1.0" encoding="gbk" ?>
<function id="31367" name="将本系统日终数据上传到集中交易系统">
	<input>
		<item type="hidden"	id="id"		name="id" action="31381" default="1"/>		
		<item type="date"	id="date"	alias="sdate3"	name="日期" editable="false"/>				
	</input>
	<service type="procedure" id="p_synchronization_upload" implement="p_synchronization_upload31367" process="911" style="1">
		<param id="user_id"		type="integer"/>
		<param id="sdate3"		type="string"/>
		<param id="front_back"	type="string" default='1'/>
	</service>
	<output target="3090301">
		<column name="导出方式"			type="string"	id="sstatus0"/>   
		<column name="路径"				type="string"	id="vsvarstr0"/>  
		<column name="文件名"			type="string"	id="saddr"/>
		<column name="数据库表名"			type="integer"	id="saddr2"/>   
		<column name="数据格式名"			type="string"	id="snote"/>   
		<column name="融资融券机构代码"	type="string"	id="sstation1"	display="false"/>  
		<column name="融资融券分支号"		type="string"	id="sdate3"		display="false"/>
		<column name="融资融券柜员号"		type="string"	id="scheck_emp"	display="false"/>   
		<column name="融资融券柜员密码"	type="string"	id="semp_pwd"	display="false"/> <!--此四个隐藏字段作将来扩展用-->
		<!--<button type="edit"    id="edit"   name="将文件导入到融资融券数据库" action="3090101" multiple="0"/>-->
	</output>
</function>