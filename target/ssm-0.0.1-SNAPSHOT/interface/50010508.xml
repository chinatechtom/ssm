<?xml version="1.0" encoding="gbk" ?>
<function id="50010508" name="总融资额度参数修改" >
	<input>	
	<item type="text"     id="total_credit"    name="融资行权总额度" nullable="false"	number="true"  minlen="1" maxlen ="22"  />
    <item type="text"     id="one_max_credit"  name="单客户最大融资额度" nullable="false"	number="true" minlen="1" maxlen ="22" />
	<item type="text"   id="used_credit"     name="融资行权已使用额度" editable="false" number="true"/>
    <item type="text"   id="remain_credit"   name="融资行权剩余额度"   editable="false" number="true" />
	<item type="hidden"   id="func_id" 				 action="8801" 	default="50010508"/>
	<item type="hidden"   id="func_need_check" 		 name="是否支持复核" 	  default="0"/>		
	<group name="复核">   
    <item type="text" 		id="chk_user_code" 		 name="复核人员" filter="func_need_check=1"/>
    <item type="password"   id="chkuser_pwd" 		 name="密码" 	filter="func_need_check=1"/>
    </group>		
	</input>   
	<service type="procedure" id="P50010507"  implement="P50010507_50010508"  process="866" style="1"> 
		<param id="user_id"			type="integer" way="in"/>
		<param id="func_id"			type="integer" way="in"/>
		<param id="chk_user_code"	type="string"  way="in"/>
		<param id="chkuser_pwd"	  	type="string"  way="in"/>
		<param id="total_credit"  	type="float"   way="in"/>
		<param id="one_max_credit"	type="float"   way="in"/>
		<param id="used_credit"   	type="float"   way="in"/>
		<param id="remain_credit"   type="float"  way="in"/>
		<param id="operation"       type="integer" default="2"/> <!--修改-->		
	</service> 
	<output/>  
</function>    