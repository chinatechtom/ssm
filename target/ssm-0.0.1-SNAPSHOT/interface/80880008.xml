<?xml version="1.0" encoding="gbk" ?>
<function id="80880008" name="场外付息客户利息修正" >
	<input>		
		<item type="text" id="cust_code" name="客户代码" />
		<item type="text"  id="sbond_no" name="交易协议书编号"/>
		<item type="select" id="sbond_status" name="交易协议书状态" source="700" default="4"/>
		<item type="hidden" id="if_pay_outside" name="是否场外付息" default="1"/>
	</input>
	<service type="procedure" id="P60880008" process="933" style="77">
		<param type="integer" id="user_id"/>	
		<param type="string"  id="cust_code"/>
		<param type="string"  id="sbond_no"/>
		<param type="string"  id="sbond_status"/>
		<param type="string"  id="if_pay_outside"/>
	</service>
	<output export="true">
		<column type="number" id="sbond_id"   name="交易协议书id" display="false"/>     
		<column type="number" id="institution_id"   name="机构号" display="false"/>	
		<column type="string" id="institution_code"   name="营业部代码"/>
		<column type="string" id="institution_name"   name="营业部名称"/>
		<column type="number" id="cust_id"   name="客户id"  display="false"/>	
		<column type="string" id="cust_code"   name="客户代码" />
		<column type="string" id="cust_name"   name="客户姓名" />
		<column type="string" id="sbond_no"   name="交易协议书编号" />
		<column type="string" id="sbond_status"   name="交易协议书状态"/>
		<column type="number" id="related_sbond_id"   name="关联(主)交易协议书编号" display="false"/>
		<column type="string" id="related_sbond_no"   name="关联(主)交易协议书编号" />
		<column type="integer" id="product_id"   name="productid" display="false"/>
		<column type="string" id="market_code"   name="市场代码" display="false"/>
		<column type="string" id="market_code_name"   name="市场代码"/>
		<column type="string" id="product_code"   name="证券代码"/>
		<column type="string" id="product_name"   name="证券名称"/>
		<column type="number" id="curr_stk_vol"   name="当前证券数量" precision="0"/>
		<column type="number" id="curr_debt_amt"   name="当前负债金额" precision="2"/>
		<column type="number" id="real_interest"   name="实时借款利息" precision="2"/>
		<column type="number" id="fix_fee"   name="固定费用" precision="2"/>
		<column type="number" id="real_penalty"   name="实时违约金" precision="2"/>
		<column type="number" id="unreceived_interest"   name="已结未收利息" precision="2"/>
		<column type="number" id="received_interest"   name="已收利息" precision="2"/>
		<column type="number" id="unreceived_penalty"   name="已结未收违约金" precision="2"/>
		<column type="number" id="received_penalty"   name="已收违约金" precision="2"/>
		<column type="string" id="if_mand_nota"   name="是否强制公证" display="false"/>
		<column type="string" id="if_mand_nota_name"   name="是否强制公证" />
		<column type="string" id="if_per_interest"   name="是否个性化结息" display="false"/>
		<column type="string" id="if_per_interest_name"   name="是否个性化结息" />
		<column type="string" id="interest_set_dates"   name="个性化结息日期" />
		<column type="string" id="if_pay_outside"   name="是否场外付息" display="false"/>
		<column type="string" id="if_pay_outside_name"   name="是否场外付息" />
		<button type="view" id="edit" name="利息修正" action="80880009" multiple="0"/>
	</output>
</function>