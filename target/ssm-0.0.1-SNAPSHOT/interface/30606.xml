<?xml version="1.0" encoding="gbk" ?>
<function id="30606" name="查看合约-贷款">
  <input>    
	 <item type="text" id="order_id" name="合约id"/>	 
  </input>
  <service type="procedure" id="p_order_financing_query"  process="911" style="77"> 
	<param id="user_id"	  type="integer" way="in"/>    
    <param id="order_id"  type="integer" way="in"/>
  </service>
  <output export="true">	
	 <column type="number" id="order_id"	  name="合约id"	display="false"/> 
	 <column type="number" id="dc_amt"		  name="借贷金额"/>	
	 <column type="number" id="approve_amt"	  name="审批金额"/>	
	 <column type="string" id="begin_time"	  name="起始日期"/>	
	 <column type="string" id="return_time"	  name="到期日期"/>
	 <button type="add" id="zjdk" name="增加贷款" action="30616" multiple="-1" /> 
	<button type="edit" id="xgdk" name="修改贷款" action="30616" multiple="0" /> 
  </output>	
</function>
