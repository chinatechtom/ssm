<?xml version="1.0" encoding="gbk" ?>
<function id="3050206" name="新增通用产品" >	
  <input>
	<group name="产品基本信息">
		<item type="hidden"		id="product_id"     name="通用产品id"/>             
		<item type="text"		id="product_code"	name="通用产品代码" nullable="false"	maxlen="10"/>	                  
		<item type="text"		id="product_name"	name="通用产品名称" nullable="false"	maxlen="100"/>
		<item type="select" id="if_estop"     name="是否禁用"     source="340"      nullable="false"/>
		<item type="hidden" id="product_sort" name="信用产品类型" default="3"/>
		<!--<item type="select"		id="count_mode"		name="计费模式"    source="125" nullable="false"/>	
		<item type="text"		id="product_cyc"	name="产品周期"	  nullable="false"/>
		<item type="select"		id="cyc_unit"		name="周期单位"	  source="056" nullable="false"/>-->	
	</group>
	<group name="产品利率及费率">		
		<item type="select"	id="basis_interest_id"	name="基准利率名称" source="90000011" action="30503" nullable="false"/>
		<item type="text"	id="basis_interest_rate"	name="基准利率(年)(%)"	editable="false"/>
		<item type="select"	id="rateornum"							name="利息浮动方式" source="150"	nullable="false" />
		<item type="hidden"	id="financing_interest_rate"			name="融资利息率"		default="0"/>
		<item type="hidden"	id="borrow_stock_interest_rate"			name="融券利息率"		default="0"/>
		<item type="hidden"	id="fix_borrow_fund_rate"				name="融资固定额度费率"	default="0"/>
		<item type="hidden"	id="fix_borrow_stock_rate"				name="融券固定额度费率"	default="0"/>
		<item type="text"	id="before_return_commission_charge"	name="提前还贷手续费"	nullable="false"	number="true"/>
		<item type="text"	id="punish_interest_rate"				name="逾期罚息率"		nullable="false"	number="true"/>
		<item type="text"	id="bad_punish_interest_rate"			name="坏帐罚息率"		nullable="false"	number="true"/>
	</group>
	<item type="hidden"	id="takeup_borrow_fund_rate"			name="融资占用额度费率"	default="0"/>
	<item type="hidden"	id="takeup_borrow_stock_rate"			name="融券占用额度费率"	default="0"/>
	<item type="hidden"	id="borrow_stock_commission_charge"		name="融券违约手续费"		default="0"/>
  </input>
  <service type="procedure" id="P_credit_product_modi" implement="P_credit_product_modi3050206"  process="811" style="1"> 
		<param id="user_id"							 type="integer" way="in"/>
		<param id="product_id"     					 type="integer" way="in"/>             
		<param id="product_code"					 type="string"  way="in"/>	                  
		<param id="product_name"					 type="string"  way="in"/>
		<param id="product_sort"					 type="string"  way="in" default="3"/>
		<param id="count_mode"						 type="string"  way="in"	default="1"/>
		<param id="product_cyc"						 type="integer" way="in" default="1"/>
		<param id="cyc_unit"						 type="string" 	way="in" default="1"/>
		<param id="basis_interest_id"				 type="integer" way="in"/>
		<param id="basis_interest_rate"				 type="double"	way="in"/>
		<param id="rateornum"						 type="string"	way="in"/>	
		<param id="financing_interest_rate"			 type="double" 	way="in"/>
		<param id="borrow_stock_interest_rate"		 type="double" 	way="in"/>
		<param id="fix_borrow_fund_rate"			 type="double" 	way="in"/>
		<param id="fix_borrow_stock_rate"			 type="double" 	way="in"/>
		<param id="takeup_borrow_fund_rate"			 type="double" 	way="in"/>
		<param id="takeup_borrow_stock_rate"		 type="double" 	way="in"/>
		<param id="borrow_stock_commission_charge"	 type="double"	way="in"/>
		<param id="before_return_commission_charge"	 type="double"	way="in"/>
		<param id="punish_interest_rate"			 type="double" 	way="in"/>
		<param id="bad_punish_interest_rate"		 type="double" 	way="in"/>				            
		<param id="if_estop"											    type="string"  		way="in"/>
  </service>
  <output/>
