	 <item type="date"    id="engage_trade_date"	name="初始交易日期" default="now"/>
	 <item type="select"  id="market_code"			name="市场代码" source="039"/>
	 <item type="text"    id="product_code"			name="证券代码"/>
	 <item type="select"  id="order_type"	        name="合约类型" source="97000004"/>
	 <item type="hidden"  id="order_id"             name="合约id"/>
