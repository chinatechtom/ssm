<?xml version="1.0" encoding="gbk" ?>
<function id="31310" name="角色浏览">
	<input>
		<item type="hidden"	id="role_id"		name="角色id"/>
		<item type="text"   id="role_name"		name="角色名称"/>
	</input>
	<service type="procedure" id="p_role_query" implement="p_role_query31310"  process="911" style="77">
		<param id="user_id"			type="integer"  way="in"/>
		<param id="role_id"			type="integer"  way="in"/>
		<param id="role_name"		type="string"   way="in"/>
	</service>
	<output autoload="true">
		<column type="number"  id="role_id"			name="角色id" display="false"/>
		<column type="string"  id="role_name"		name="角色名称"/>
		<column type="string"  id="role_descript"	name="角色描述"/>
		<column type="string"  id="role_type"		name="角色类型1" display="false"/>	
		<column type="string"  id="role_type_name"	name="角色类型" />
		<button type="add"     id="add"				name="增加" action="3131001" multiple="-1"/>
		<button type="edit"    id="edit"			name="修改" action="3131001" multiple="0"/>
		<button type="edit"    id="set"				name="设置权限" action="31317" multiple="0"/>
		<!--<button type="edit"    id="set"				name="合约处理权限设置" action="31899" multiple="0"/>-->
	</output>
</function>