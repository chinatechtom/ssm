<?xml version="1.0" encoding="gbk" ?>
<function id="27050177" name="客户融资融券业务盈亏统计表">
	<input>
		<item type="select"	 id="insitution_id"			name="营业部名称" source="95000008"/>
		<item type="text"	 id="cust_code"			    name="客户代码" />
		<item type="date"    id="begin_date"	        name="起始日期"		default="last"/>	
		<item type="date"    id="end_date"	            name="结束日期"		default="now"/>	
		
	</input>
    <service type="procedure" id="P27050177"  process="911" style="77">
		<param id="user_id"	        type="integer" way="in"/>
		<param id="insitution_id"	type="integer" way="in"/>
		<param id="cust_code"	    type="string"  way="in"/>
		<param id="begin_date"		type="string"  way="in"/>
		<param id="end_date"		type="string"  way="in"/>
    </service>
    <output export="true" autoload="false" print="true">
		<column type="number" id="order_num"				    name="序号"		/>
		<column type="string" id="institution_code"				name="营业部代码"/>
		<column type="string" id="institution_name"				name="营业部名称"/>
		<column type="string" id="cust_code"				    name="客户代码"/>
		<column type="string" id="cust_name"				    name="客户名称"/>		
		<group name="融资盈亏">
		    <column type="number" id="fin_buy_amt"				name="融资买入金额"		precision="2" total="true"/>
			<column type="number" id="fin_buy_sec_value"	    name="融资买入证券市值"    precision="2" total="true"/>
			<column type="number" id="fin_fact_profit"			name="已实现盈亏"		    precision="2" total="true"/>
			<column type="number" id="fin_interest"			    name="融资利息"		        precision="2" total="true"/>	
			<column type="number" id="fin_tkup_intr"			name="融资额度占用费"      precision="2" total="true"/>
			<column type="number" id="fin_profit_total"			name="盈亏总额"            precision="2" total="true"/>
		</group>
		<group name="融券盈亏">
		    <column type="number" id="bs_sell_amt"				name="融券卖出金额"		precision="2" total="true"/>
			<column type="number" id="bs_sell_sec_value"	    name="融券卖出证券市值"    precision="2" total="true"/>
			<column type="number" id="bs_fact_profit"			name="已实现盈亏"		    precision="2" total="true"/>
			<column type="number" id="bs_interest"			    name="融券费用"		        precision="2" total="true"/>	
			<column type="number" id="bs_tkup_intr"			    name="融券额度占用费"      precision="2" total="true"/>
			<column type="number" id="bs_profit_total"			name="盈亏总额"            precision="2" total="true"/>
		</group>
		<column type="number" id="profit_total"				    name="盈亏合计"		        precision="2" total="true"/>
    </output>
</function>
