<?xml version="1.0" encoding="gbk" ?>
<function id="32001" name="客户信息信誉查询" >	
  <input>		
	<item type="select" id="institution_id"       name="营业部名称"   source="95000008" />
	<item type="text"   id="cust_code"	         name="客户代码" />
	<item type="text"   id="cust_name"            name="客户姓名" />
		
  </input>  
  <service type="procedure" id="p_business_report_32001_query" process="911" style="77">
	<param id="user_id"          type="integer" />
	<param id="institution_id" type="integer"/>
	<param id="cust_code"     type="string"  way="in"/>
	<param id="cust_name"  type="string"  way="in"/>	
  </service>  
  <output export="true">    
  	<column type="string" id="c1"   name="营业部名称"  />
    <column type="string" id="c2"   name="客户代码"  />
	<column type="string" id="c3"   name="客户姓名"  />
    <column type="number" id="c4"   name="账户资产"        precision="2"/>
    <column type="number" id="c5"   name="自有资金"        precision="2"/>
    <column type="number" id="c6"   name="融券卖出价款"     precision="2" display="false"/>
    <column type="number" id="c7"   name="账户股票市值"     precision="2"/>
    <column type="number" id="c8"   name="担保品证券市值"   precision="2"/> 
    <column type="number" id="c9"   name="非担保品证券市值"  precision="2"/> 
    <column type="number" id="c10"   name="最高(固定)信用上限"  precision="2"/>
    <column type="number" id="c11"   name="已用信用上限"   precision="2"/>
	<column type="number" id="c12"   name="融券市值"       precision="2"/>   
	<column type="number" id="c13"   name="维持担保比例"   precision="2"/>   
	<column type="number" id="c14"   name="应付融资利息"   precision="2"/>   
	<column type="number" id="c15"   name="应付融券费用"   precision="2"/>   
	<column type="number" id="c16"   name="应付其它费用"   precision="2"/>  
	<column type="number" id="c17"   name="利息费用小计"   precision="2"/>
	<column type="string" id="c18"   name="融资融券期限"   precision="2"/> 
	</output> 	
    <assistant/>
</function>