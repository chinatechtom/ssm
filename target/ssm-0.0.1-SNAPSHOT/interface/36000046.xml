<?xml version="1.0" encoding="gbk" ?>
<function id="36000046" name="4.3.1大客户融资融券额度分析(前X名)" >  
  <input>
      <item type="select"  id="choice"                name="查询选择"  source="362" nullable="false" />
      <item type="text"    id="num"                   name="选择数据"/>
      <item type="select"  id="institution_id"        name="营业部名称" source="95000008"/> 
      <item type="hidden"  id="date"                  name="查询日期"/>      
  </input>
 <service type="procedure" id="P36000046" process="911" style="77">
   <param id="user_id"        type="integer" way="in" /> 
   <param id="institution_id" type="integer" way="in" /> 
   <param id="choice"         type="integer" way="in" /> 
    <param id="num"            type="double" way="in" /> 
    <param id="date"           type="string"  way="in" /> 
  </service> 
  <output  print="true">
     <column type="number" id="id"        name="序号"/> 
     <column type="string" id="cust_code" name="客户代码"/> 
     <column type="string" id="cust_name" name="客户姓名"/> 
     <column type="string" id="institution_name" name="所属营业部"/> 
     <column type="number" id="fin_bs_limit"     name="已发放融资融券额度" precision="2" total="true"/>  
   <group name="已发放融资额度"> 
     <column type="number" id="fin_limit_fix"      name="固定额度"   precision="2" total="true"/> 
     <column type="number" id="fin_limit_float"    name="非固定额度" precision="2" total="true"/> 
   </group>  
   <group name="已融资金额"> 
     <column type="number" id="fin_amt_fix"      name="固定额度" precision="2" total="true"/> 
     <column type="number" id="fin_amt_float"    name="非固定额度" precision="2" total="true"/> 
   </group>
     <column type="number" id="unuse_fin_limit"  name="未使用融资额度" precision="2" total="true"/> 
     <column type="number" id="bs_limit"         name="已发放融券额度" precision="2" total="true"/> 
     <column type="number" id="use_bs_limit"     name="已使用融券额度" precision="2" total="true"/> 
  </output>
</function>