		<column type="number" id="age"					name="客户年龄"/>								<!-- modify by SECREQ-1427 20140711 -->
		<column type="number" id="already_open_month"	name="投资经验"/> 						<!-- modify by SECREQ-1427 20140711 -->
		<button type="edit"   id="edit"	name="修 订"			 action="30601143"	 multiple="0"/>	<!--modify by haofugang 20110331 -->
		<button type="mod"    id="chg"	name="复核生效"			 action="3060114201" multiple="0"/>
		<button type="del"    id="del"	name="作废"				 action="3060130"	 multiple="0"/>
