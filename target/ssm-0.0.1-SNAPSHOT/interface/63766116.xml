	<item type="hidden" id="date"		name="打印日期"/><!--从1888.xml获得的打印日期-->
	<item type="hidden" id="user_code"	name="打印柜员代码"/><!--从1888.xml获得的打印柜员代码-->
	<item type="hidden" id="user_name"	name="打印柜员名称"/><!--从1888.xml获得的打印柜员名称-->
	<item type="hidden" id="company"    name="券商名称"/><!--从1888.xml获得的券商名称-->			
	<group name="意见">
		<item type="textarea" id ="remark" 			 name="申请备注"/>
		<item type="textarea" id ="manage_opinion"	 name="处理意见"/>
		<item type="textarea" id ="approver_opinion" name="审核意见"/>
		<item type="textarea" id ="assessor_opinion" name="审批意见"/>
	</group>
  </input>
</function>