<?xml version="1.0" encoding="gbk" ?>
<function id="30608" name="查看合约-费率" >
	<input>    
	 <item type="hidden"  id="order_id" name="客户id"/> 
  </input>
  <service type="procedure" id="p_order_fl_query" process="911" style="77">
	<param id="user_id" type="integer" way="in"/> 
	<param id="order_id" type="ingeter" way="in"/>
  </service>
  <output detail="true">
	<column type="number" id="order_id" name="合约id"	display="false"/>
	<column type="number" id="loan_interest_rate" name="融资利息率(%)"/>
	<column type="number" id="borrow_stock_interest_rate" name="融券利息率(%)" precision="4"/>
	<column type="number" id="punish_interest_rate" name="罚息率(%)"/>
	<column type="number" id="commission_charge" name="手续费" precision="2"/>
	<column type="number" id="approve_financing_interest_rate" name="审批融资利息率(%)"/>
	<column type="number" id="approve_borrow_stock_interest_rate" name="审批融券利息率(%)"/>
	<column type="number" id="approve_punish_interest_rate2" name="审批罚息率(%)"/>
	<column type="number" id="approve_commission_charge" name="审批手续费" precision="2"/>
  </output> 
</function>