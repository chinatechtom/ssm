<?xml version="1.0" encoding="gbk" ?>
<function id="30121" name="客户代理人信息" >
	<input>	
		<item type="hidden" id="cust_id" name="客户号"/>
		<item type="hidden" id="cust_code" name="客户代码" />
		<item type="hidden" id="cust_name" name="客户姓名" />
		<item type="hidden" id="cert_no" name="证件号码" />
	</input>
	<service type="procedure" id="p_cust_agent_info_query" process="911" style="77">
		<param id="user_id" type="integer"/>
		<param id="cust_id" type="integer"/>	
		<param id="cust_code"/>
		<param id="cust_name"/>
		<param id="cert_no"/>
	</service>
	<output export="true">
		<column type="number" id="agent_id" name="代理人id"  display="false" />
		<column type="string" id="agent_name" name="代理人姓名" />
		<column type="string" id="nation_code" name="国籍代码"  />
		<column type="string" id="cert_type" name="证件类型"  />
		<column type="string" id="cert_no" name="证件号码"  />
		<column type="string" id="gender" name="性别"/>
		<column type="string" id="agent_status" name="代理状态"  />
		<column type="string" id="over_date" name="到期日期"  />
		<column type="string" id="post_code" name="邮政编码" /> 
		<column type="string" id="address" name="联系地址"/> 
		<column type="string" id="fix_phone" name="固定电话"/>
		<column type="string" id="cell_phone" name="手机"/>
		<column type="string" id="fax" name="传真"/>
		<column type="string" id="email" name="电子邮件"/>
		<column type="string" id="open_emp" name="开户人员"/>   
		<column type="string" id="open_date" name="开户日期" /> 
		<column type="string" id="open_time" name="开户时间" />   
		<column type="string" id="close_emp" name="销户人员"/>   
		<column type="string" id="close_date" name="销户日期"/>   
		<column type="string" id="close_time" name="销户时间"/>
	</output>
</function>