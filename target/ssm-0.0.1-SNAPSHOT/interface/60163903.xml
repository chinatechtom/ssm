<?xml version="1.0" encoding="gbk"?>
<function id="60163903" name="清算表（业务主管部门适用）">
	<input>   
	 <item type="date" id="trade_date" name="查询日期"/>   
  </input>
  <service type="procedure" id="p60163903" process="933" style="77" adapter="psr">
	<param id="user_id" type="integer"/>  
	<param id="trade_date" type="string"/>
  </service>
  <output export="true">
    <column type="string" id="trade_date" name="报表日期"/>
    <column type="string" id="report_items" name="报告项目" display="false"/>
	<column type="string" id="report_items_name" name="报告项目名称"/>
	<group name="支付成交金额">
		<column type="number" id="done_amt_out" name="1" precision="2"/>
	</group>
	<group name="收入成交金额">
		<column type="number" id="done_amt_in" name="2" precision="2"/>
	</group>
	<group name="成交净额">
		<column type="number" id="net_amt" name="3=1-2" precision="2"/>
	</group>
	<group name="证交所和登记结算公司费用">
		<group name="经手费">
			<column type="number" id="jsf" name="4" precision="2"/>
		</group>
		<group name="证管费">
			<column type="number" id="zgf" name="5" precision="2"/>
		</group>
		<group name="过户费">
			<column type="number" id="ghf" name="6" precision="2"/>
		</group>
		<group name="印花税">
			<column type="number" id="yhs" name="7" precision="2"/>
		</group>
		<group name="质押登记费">
			<column type="number" id="zyf" name="8" precision="2"/>
		</group>
		<group name="其他">
			<column type="number" id="qtf" name="9" precision="2"/>
		</group>
		<group name="合计">
			<column type="number" id="total_fee" name="10=4+5+6+7+8+9" precision="2"/>
		</group>
	</group>
	<group name="结算净额">
		<column type="number" id="settle_net_amt" name="11=3+10" precision="2"/>
	</group>
	<button type="print" id="print" name="打印" action="60163923" multiple="0"/> 
  </output>
</function>
