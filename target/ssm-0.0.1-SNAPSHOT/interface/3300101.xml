<?xml version="1.0" encoding="gbk" ?>
<function id="3300101" name="增加重点客户" >
	<input>	
	  <item type="text" 	id="cust_code" 	name="客户代码" action="30101"/>
		<item type="text" 	id="cust_name" 	name="客户姓名" />
		<item type="select" id="cert_type_code" 	name="证件类型" source="029"   />		
		<item type="text" 	id="cert_number" 	name="证件号码" />
		<item type="text" 	id="area_code" 	name="地址"		/>
		<item type="select" id="risk_level" name="风险级别" nullable="false" source="337"   />
		<item type="select" id="reason_code"	name="类别"  source="96000004" nullable="false"/>
		<item type="text"	id="in_value"	name="进入数值"	number="true" default="0"/>
        <item type="date"	id="in_date"	name="进入日期"/>		
		<item type="date"	id="out_date"	name="退出日期"/>
		<item type="select" id="issyscust"	name="是否系统客户"  source="391" nullable="false"/>
		<item type="select" id="is_valid"	name="是否有效"  source="392" nullable="false"/>
		<item type="textarea" 	id="remark" 		name="备注"		/>
	</input>
	<service type="procedure" id="p_risk_cust_list_add" process="811" style="1">
		<param id="user_id" type="integer"/>	
		<param id="cust_code"/>
		<param id="cert_type_code"/>
	  <param id="cust_name"/>
		<param id="cert_number"/>
		<param id="area_code"/>
		<param id="risk_level"/>
		<param id="in_date"/>
		<param id="reason_code"/>
		<param id="in_value"	type="double"/>
		<param id="out_date"/>
		<param id="issyscust"/>
		<param id="is_valid"/>
		<param id="remark"/>
	</service>
	<output/>
</function>


