<?xml version="1.0" encoding="gbk" ?>
<function id="60163909" name="未到期融出本金增减结存明细表" >
	<input>   
	 <item type="date"  id="trade_date"           name="查询日期"/>   
  </input>
  <service type="procedure" id="p60163909" process="933" style="77" adapter="psr">
	<param id="user_id"				type="integer"/>  
	<param id="trade_date"			type="string"/>
  </service>
  <output export="true">
    <column type="string" id="trade_date"			name="报表日期"	/>
    <column type="number" id="sbond_id"				name="初始交易合同编号" display="false"/>	
	<column type="string" id="sbond_no"	            name="初始交易合同（成交）编号" />
	<group name="日初结存应收利息">
	<column type="number" id="begin_receive_interest"		name="1" precision="2"/>
	</group>
	<group name="本日增加">
		<group name="本日应收利息">
			<column type="number" id="today_interest"				name="2" precision="2"/>
		</group>
		<group name="其他">
			<column type="number" id="others1"						name="3" precision="2"/>
		</group>
	</group>
	<group name="本日减少">
		<group name="本日实收利息">
			<column type="number" id="today_rece_interest"			name="4" precision="2"/>
		</group>
		<group name="违约处置收回利息">
			<column type="number" id="violate_interest"   				name="5" precision="2"/>
		</group>
		<group name="其他">
			<column type="number" id="others2" 						name="6" precision="2"/>
		</group>
	</group>
	<group name="日末结存应收利息">
		<column type="number" id="end_receive_interest" 		name="7=1+2+3-4-5-6" precision="2"/>
	</group>
	<group name="是否逾期">
		<column type="string" id="if_over" 		name="8"/>
	</group>
	<button type="print"   id="print" name="打印" action="60163929" multiple="0"/> 
  </output>
</function>