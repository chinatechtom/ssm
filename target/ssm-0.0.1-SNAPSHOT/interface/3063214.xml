<?xml version="1.0" encoding="gbk" ?>
<function id="3063214" name="授信额度查询" >
	<input >
		<item type="hidden" id="cust_id" 			name="客户id" />			
		<item type="text" 	id="total_capital_wy" 	name="普通账户总资产"/>
		<item type="text" 	id="credit_limit" 		name="授信额度"/>
	</input>
	<service type="procedure" id="p_credit_limit_query4" implement="p_credit_limit_query43063214"  process="911" style="77">
		<param id="user_id" 				type="integer" 	 	way="in" />
		<param id="cust_id"					type="integer" 		way="in" />
		<param id="datd_type" 				type="string" 		way="in" default="2"/>
		<param id="total_capital_wy" 		type="double" 		way="in"/>
		<param id="credit_limit" 			type="double" 		way="in"/>
	</service>
	<output>
		<column type="number" id="top_credit_limit"	            name="最高授信额度" />
		<column type="number" id="apply_credit_limit"			name="可授信额度"	/>
		<column type="number" id="approval_credit_limit"		name="审批授信"/>
		<column type="number" id="total_capital_wy"				name="普通账户总资产"/>
	</output>
</function>