<?xml version="1.0" encoding="gbk" ?>
<function id="30607" name="查看合约-信用上限" >
  <input>    	 
	 <item type="hidden"  id="order_id" name="合约id"/> 	 
  </input>
  <service type="procedure" id="p_order_credit_num_query" process="911" style="77">
	<param id="user_id"				type="integer" way="in"/>
    <param id="order_id"			type="integer" way="in"/>
  </service>
  <output refresh="600">	
	<column type="number" id="order_id"	name="合约id"	display="false"/>
	<column type="number" id="app_num" name="申请额度"/>
	<column type="string" id="app_num_chn" name="申请额度中文"/>
	<column type="number" id="approve_num" name="审批额度"/>
	<column type="string" id="approve_num_chn" name="审批额度中文"/>
	<column type="number" id="credit_limit" name="合同授信额度"/>
	<column type="string" id="credit_limit_chm_bond" name="合同授信额度中文"/>
	<column type="string" id="credit_date" name="申请日期"/>
	<column type="string" id="ori_begin_date" name="原额度起始日期"/>
  </output>  
</function>