<?xml version="1.0" encoding="gbk" ?>
<function id="70010121" name="取客户证券">
	<input>
		<item type="text"	id="cust_code"	 name="客户代码" nullable="false"/>
	</input>
	<service type="procedure" id="kingstar.p_bbs_get_cust_stock" process="311" style="77">
		<param type="string" id="cust_code"/>
	</service>
	<output target="70010122">
		<column name="客户代码"	id="capital_account"   type="string"/>
		<column name="市场代码"	id="market_code"   type="string"/>
		<column name="证券代码"	id="stock_code"   type="string"/>
		<column name="证券数量"	id="qty"   type="number"/>
		<column name="交易日期"	id="tradedate"   type="string"/>
	</output>
</function>