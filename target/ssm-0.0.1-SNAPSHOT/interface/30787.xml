<?xml version="1.0" encoding="gbk" ?>
<function id="30787" name="限售股交易监控" >
  <input>
	<item type="text"    id="cust_code"		name="客户代码"/>
	<item type="text"    id="holder_acc"	name="证券帐户"/>	
	<item type="text"    id="cert_no"		name="证件号码"/>
	<item type="select"  id="type"			name="类型" source="400"/>
	<item type="select"  id="market_code"	name="市场代码" source="039"/>
	<item type="text"    id="sec_code"		name="证券代码"/>
	<item type="date"    id="begin_date"	name="开始日期"	nullable="false"/>
	<item type="date"    id="end_date"		name="结束日期" nullable="false"/>
  </input>
  <service type="procedure" id="p_sjsfw_tzxx_trade_query"   process="911" style="77"> 
    <param id="user_id"		type="integer" way="in"/>
	<param id="cust_code"	type="string" way="in"/>
	<param id="holder_acc"	type="string" way="in"/>
	<param id="cert_no"		type="string" way="in"/>
	<param id="type"		type="string" way="in"/>
	<param id="market_code"	type="string" way="in"/>
	<param id="sec_code"	type="string" way="in"/>
	<param id="begin_date"	type="string" way="in"/>
	<param id="end_date"	type="string" way="in"/>
  </service>
	<output>
		<column type="string" id="xwdm"	name="席位代码" />
		<column type="string" id="gddm"	name="证券帐户"/>
		<column type="string" id="zqdm"	name="证券代码"/>
		<column type="string" id="product_name"	name="证券名称"/>
		<column type="string" id="market_name"	name="市场代码"/>	
		<column type="string" id="zqsl"	name="限售证券数量"/>
		<column type="string" id="sfzh"	name="证件号码"/>
		<column type="string" id="scdm"	name="市场代吗" display="false"/>
		<column type="string" id="sjlb"	name="类型"/>
		<column type="string" id="date"	name="交易日期"/>
		<column type="string" id="contract_no"	name="合同序号"/>
		<column type="string" id="done_no"	name="成交编号"/>
		<column type="string" id="bs"	name="买卖方向"/>
		<column type="string" id="credit_flag"	name="信用交易标志"/>
		<column type="number" id="trade_qty"	name="成交数量" precision="0"/>
	</output>
</function>
