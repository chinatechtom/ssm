<?xml version="1.0" encoding="gbk" ?>
<function id="30718" name="融资专户余额监控" >
	<input>			
		<item type="hidden"  id="institution_id"	    name="机构id"/>
		<item type="hidden"  id="acc_id"	            name="机构账户id"/>
		<item type="text"    id="acc_code"	            name="账户代码"/>		
	</input>
    <service type="procedure" id="p_institution_acc_money_query"  process="911" style="77">
		<param id="user_id"	            type="integer" way="in"/> 
		<param id="institution_id"	    type="integer" way="in"/> 
		<param id="acc_id"	            type="integer" way="in"/>
		<param id="acc_code"	        type="string"  way="in"/>		
    </service>
    <output export="true" autoload="true"	refresh="120">
		<column type="number" id="acc_id"					name="账户id"		display="false"/>
		<column type="string" id="acc_code"					name="账户代码"/>
		<column type="string" id="date"						name="日期"			display="false"/>
		<column type="string" id="property_type"			name="资产种类"		display="false"/>
		<column type="string" id="currency_type"			name="货币类型"/>
		<column type="number" id="fund_amt"					name="资金余额"	    precision="2"/>
		<column type="number" id="deposit"					name="存入金额"	    precision="2"/>
		<column type="number" id="withdraw"					name="取出金额"	    precision="2"/>
		<column type="number" id="frozen_amt"				name="冻结资金"	    precision="2" display="false"/>
		<column type="number" id="abnormal_frozen_amt"		name="异常冻结资金"	precision="2" display="false"/>
		<column type="number" id="real_buy_amt"				name="融资买入金额"	precision="2"/>
		<column type="number" id="buy_frozen_amt"			name="买入冻结金额"	precision="2" display="false"/>
		<column type="number" id="sell_done_amt"			name="卖券还款金额"	precision="2"/>
		<column type="number" id="un_used_fix_qty"			name="未使用固定融资上限总和"	precision="2"/>
		<column type="number" id="un_frozen_amt"			name="未冻结款余额"	precision="2"/>
		<column type="number" id="manual_frzn_amt"			name="手工冻结金额"	precision="2" display="false"/>
		<column type="number" id="manual_unfrzn_amt"		name="手工解冻金额"	precision="2" display="false"/>
		<column type="number" id="credit"					name="信用额"		precision="2" display="false"/>
		<column type="number" id="forbid_amt"				name="禁用资金"	    precision="2" display="false"/>
		<column type="number" id="forbid_asset"				name="禁取资产"	    precision="2" display="false"/>
		<column type="number" id="interest"					name="利息"			precision="2" display="false"/>
		<column type="number" id="interest_count"			name="利息积数"	    precision="2" display="false"/>
		<column type="number" id="punish_interest"			name="罚息"			precision="2" display="false"/>
		<column type="number" id="punish_interest_count"	name="罚息积数"	    precision="2" display="false"/>
		<column type="number" id="user_id"					name="操作员id"	    display="false"/>
		<column type="string" id="time"						name="时间戳"        display="false"/>
    </output>
</function>
