<?xml version="1.0" encoding="gbk" ?>
<function id="31308" name="机构设置">
	<input>
		<!--<group name="机构基本信息">-->
		<item type="hidden" id="institution_id"				name="机构id"/>
		<item type="text"   id="institution_code"			name="机构代码"	nullable="false"	maxlen="20"/>
		<item type="text"   id="institution_name"			name="机构名称"	nullable="false"	maxlen="50"/>
		<item type="text"   id="department_code"			name="部门代码"	nullable="false"  maxlen="20"/>
		<item type="select" id="father_sort_id"				name="上级机构名称" nullable="false" source="90000006" />		
		<item type="select" id="institution_type0"			name="机构类型" 	source="016"/>
		<!-- 新添加的added by ming.shan -->
		<item type="select" id="system_type"			    name="营业部类型" 		nullable="false"  source="521"/>
		<item type="text"   id="branchid"				    name="BRANCH_ID"		maxlen="5"/>
		<item type="text"   id="branch_sbm"				    name="营业部识别码"	maxlen="2"/>
		<!--<item type="select" id="institution_type"			name="机构类型" source="016" />-->
		<item type="select" id="institution_kind"			name="机构性质" 	source="017"/>
		<item type="text"   id="enrol_address"				name="注册地址"		maxlen="100"/>
		<item type="text"   id="business_address"			name="营业地址"		maxlen="100"/>
		<item type="date"   id="begin_time"					name="成立时间" default="now"/>
		<item type="text"   id="enrol_amt"					name="注册资金"		number="true"/>
		<item type="text"   id="business_no"				name="工商执照号码"	 maxlen="30"/>
		<item type="text"   id="country_tax_no"				name="国税登记号"	 	maxlen="30"/>
		<item type="text"   id="place_tax_no"				name="地税登记号"	 	maxlen="30"/>
		<item type="text"   id="principal"					name="负责人"		maxlen="10"/>
		<item type="text"   id="linkman"					name="联系人"		maxlen="10"/>
		<item type="text"   id="contact_tel"				name="联系电话"		maxlen="30"/>
		<item type="text"   id="web_address"				name="网站地址"	maxlen="50"/>
		<item type="text"   id="email"						name="邮件地址"	maxlen="50"/>
		<item type="text"   id="postalcode"					name="邮政编码"	minlin="6"	maxlen="6"/>
		<!--</group>
		<group name="机构席位信息">
			<item type="select" id="market_code"				name="市场" source="039" />
			<item type="text"   id="seat"						name="席位"/>
			<item type="text"   id="report_seat"				name="报盘席位"/>
		</group>-->
	</input>
	<service type="procedure" id="p_institution_set" implement="p_institution_set31308"  process="811" style="1">
		<param type="integer" id="user_id"					way="in"/>
		<param type="integer" id="institution_id"			way="in"/>
		<param type="string"  id="institution_code"			way="in"/>
		<param type="string"  id="institution_name"			way="in"/>
		<param type="string"  id="department_code"			way="in"/>
		<param type="integer" id="father_sort_id"			way="in"/>		
		<param type="string"  id="institution_type0"		way="in"/>
		<param type="string"  id="institution_kind"			way="in"/>
		<param type="string"  id="enrol_address"			way="in"/>
		<param type="string"  id="business_address"			way="in"/>
		<param type="string"  id="begin_time"				way="in"/>
		<param type="string"  id="enrol_amt"				way="in"/>
		<param type="string"  id="business_no"				way="in"/>
		<param type="string"  id="country_tax_no"			way="in"/>
		<param type="string"  id="place_tax_no"				way="in"/>
		<param type="string"  id="principal"				way="in"/>
		<param type="string"  id="linkman"					way="in"/>
		<param type="string"  id="contact_tel"				way="in"/>
		<param type="string"  id="web_address"				way="in"/>
		<param type="string"  id="email"					way="in"/>
		<param type="string"  id="postalcode"				way="in"/>
		<param type="string"  id="market_code"				way="in"	default="1"/>
		<param type="string"  id="seat"						way="in"	default="1"/>
		<param type="string"  id="report_seat"				way="in"	default="1"/>
		<!-- added by ming.shan-->
		<param type="string"  id="system_type"				way="in"/>
		<param type="string"  id="branch_sbm"				way="in"/>
		<param type="string"  id="branchid"					way="in"/>
	</service>
	<output/>		
</function>