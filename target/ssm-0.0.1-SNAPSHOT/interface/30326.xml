<?xml version="1.0" encoding="GBK" ?>
<function id="30326" name="停牌证券处理">
	<input >
		<item type="select"	id="market_code"     name="市场代码"   source="039"/>		
		<item type="text"	id="product_code"    name="证券代码" />
		<item type="date"	id="begin_date"      name="停牌起始日期" default="null"/>
	</input>  
	<service type="procedure" id="p_suspension_stock_query" implement="p_suspension_stock_query30326" process="911" style="77">
		<param id="user_id"	            type="integer" way="in"/> 
		<param id="market_code"         type="string"  way="in"/> 
		<param id="product_code"        type="string"  way="in"/> 
		<param id="begin_date"          type="string"  way="in"/> 
	</service>
	<output export="true">
		<column type="number"     id="product_id"             name="证券ID" display="false"/>
		<column type="string"     id="market_code"            name="市场代码"   display="false"/>
		<column type="string"     id="market_code_name"       name="市场代码"/>
		<column type="string"     id="product_code"           name="证券代码" />
		<column type="string"     id="product_name"           name="证券名称"/>
		<column type="string"     id="begin_date"             name="停牌起始日期"/>
		<column type="string"     id="remark"                 name="备注"/>
		<column type="string"     id="set_date"               name="设置日期"/>
		<column type="string"     id="set_time"               name="设置时间"/>
		<column type="string"     id="set_user"               name="设置人员"/>
		<button type="edit"       id="add"                    name="新增"    action="3032601"  multiple="-1"/>
		<button type="edit"       id="edit"                   name="修改"    action="3032602"  multiple="0"/>
		<button type="edit"       id="delete"                 name="删除"    action="3032603"  multiple="0"/>
		<button type="edit"       id="import"                 name="停牌证券批量导入"    action="30327"  multiple="-1"/>
	</output>
	</function>