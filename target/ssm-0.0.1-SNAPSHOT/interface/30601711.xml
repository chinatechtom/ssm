		<param id="cust_id"				 type="integer"	way="in"/>
		<param id="declare_id"			 type="integer"	way="in"/>
		<param id="status"				 type="string"	way="in" default="2"/>
		<param id="if_modify"			 type="string"	way="in" default="2"/>
		<param id="composite_optition"   type="string"	way="in"/>
		<param id="approver_opinion"	 type="string"	way="in"/>
		<param id="assessor_opinion"	 type="string"	way="in"/>
		<param id="reviser_opinion"		 type="string"	way="in"/>
	</service>
	<output/>	
</function>