	<group name="交易协议书变更">
		<item type="text" 	id ="hg_num" alias="hg_num1"	name="标的证券数量"	integer="true" editable="false"/>
		<item type="text" 	id ="hg_convert_rate" 	name="折扣比率(%)" 		number="true" editable="false"/>
		<item type="hidden" id ="app_price" 	name="标的证券价格" 		number="true" editable="false"/>
		<item type="date" 	id ="engage_trade_date" 	alias="engage_trade_date1" name="初始交易日期" action="97000026,97000025"/>
		<item type="date" 	id ="bb_date2" 	alias="bb_date1"		name="购回交易日期" action="97000026,97000025"/>
