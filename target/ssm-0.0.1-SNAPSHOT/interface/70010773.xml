		<param type="integer" id="sbond_id" way="in"/>
		<param type="string" id="market_code1" way="in"/>
		<param type="string" id="product_code1" way="in"/>
		<param type="integer" id="stock_vol1" way="in" default="0"/>
		<param type="string" id="remark1"  way="in"/>
	</service>
	<output/>
</function>