	</input>
	<service type="procedure" id="p70100151" process="911" style="1">
		<param id="user_id"		type="integer"/>	
	</service>
	<output target="78120611">
		<column type="string" field="sys_date"   name="系统日期" id="sdate2"/>
		<column type="string" field="institution_code"   name="营业部代码" id="sbranch_code0"/>
		<column type="string" field="cust_code"   name="客户代码" id="scust_no"/>
		<column type="string" field="sha_acc"  name="上海证券账户" id="sholder_ac_no"/>
