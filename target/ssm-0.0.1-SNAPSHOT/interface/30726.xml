  <service type="procedure" id="p_pc_record_generate" process="811" style="1">
		<param id="user_id" type="integer" way="in"/>
		<param id="cust_id"	type="integer"  way="in"/>
		<param id="guarantee_rate_id_ctl" type="integer" way="in"/>
		<param id="product_id" type="integer" way="in"/>
		<param id="bond_id" type="integer" way="in"/>
		<param id="approve_idea" type="string" way="in"/>
		<param id="bc_begin_date" type="string" way="in"/>
		<param id="bc_end_date" type="string" way="in"/>
		<param id="pc_reason" type="string" way="in"/>
		<param id="pc_begin_date" type="string" way="in"/>
		<param id="bond_no" type="string" way="in"/>
  </service>
  <output/>
</function>