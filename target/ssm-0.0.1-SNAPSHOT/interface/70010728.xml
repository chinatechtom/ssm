		<item type="text" 	id ="second_amt1"	name="购回交易金额" 	editable="false"/>
		<item type="text" 	id ="penalty_rate" 	name="违约金比率(日利率%)" number="true" default="0.05" editable="false"/>
		<item type="text" 	id ="prewarning_price" 	name="预警履约保障比例(%)" number="true" default="145" editable="false"/>
		<item type="text" 	id ="lowest_price" 	name="最低履约保障比例(%)" number="true" default="130" editable="false"/>
		<item type="textarea" id ="first_amt_big"	alias="first_amt_big1" name="初始交易金额大写" editable="false"/>
		<item type="textarea" id ="second_amt_big1" name="购回交易金额大写" editable="false"/>
		<item type="text" 	id ="fix_rate" 	alias="fix_rate1"	name="固定交易费用比例(%)"	nullable="false" number="true" action="97000025"/>
		<item type="text" id="manual_chg"  alias="manual_chg1" name="购回金额手工调整" nullable="false" number="true" action="97000025"/>
		<item type="textarea" id ="remark2" 		name="备注"/>
