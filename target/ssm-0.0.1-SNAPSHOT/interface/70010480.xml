<?xml version="1.0" encoding="gbk" ?>
<function id="70010480" name="批量征信任务查询" >  
	<input>
		<item type="date"     id="begin_date"       name="起始日期"/>
		<item type="date"     id="end_date"         name="结束日期"/>
		<item type="text"     id="task_id"          name="任务编号"/>
		<item type="select"   id="task_type"        name="任务类型" source="526" editable="false" default="A"/>
		<item type="select"   id="task_status"      name="任务状态" source="527"/>
		<item type="select"   id="export_status"    name="导出状态" source="524"/>
		<item type="select"   id="import_status"    name="导入状态" source="525"/>
	</input>
	<service type="procedure" id="p70010480" process="922" style="77">
		<param id="user_id" type="integer"/>
		<param id="task_id" type="integer"/>
		<param id="task_type" />
		<param id="task_status" />
		<param id="begin_date"/>
		<param id="end_date"/> 
		<param id="export_status" />
		<param id="import_status" />
	</service> 
	<output>
	    <column type="string" id="task_id"         name="任务编号"/>
	    <column type="string" id="task_id_out"         name="任务编号导出"/>
		<column type="string" id="task_type"       name="任务类型" display="false"/>
		<column type="string" id="task_type_name"  name="任务类型"/>
		<column type="string" id="task_status"     name="任务状态" display="false"/>
		<column type="string" id="task_status_name" name="任务状态" />
		<column type="string" id="task_exe_status"      name="任务执行状态" display="false"/>
		<column type="string" id="task_exe_status_name" name="任务执行状态" />
	    <column type="string" id="export_status"   name="导出状态" display="false"/>
		<column type="string" id="export_status_name"   name="导出状态" />
		<column type="string" id="export_date"     name="导出日期"/>
		<column type="string" id="export_time"     name="导出时间"/>
		<column type="string" id="export_user_id"  name="导出人员"/>
		<column type="string" id="import_status"   name="导入状态" display="false"/>
		<column type="string" id="import_status_name"   name="导入状态"/>
		<column type="string" id="import_date"     name="导入日期"/>
		<column type="string" id="import_time"     name="导入时间"/>
		<column type="string" id="import_user_id"  name="导入人员"/>
		<column type="string" id="occur_date"      name="设置日期"/>
		<column type="string" id="occur_time"      name="设置时间"/>
		<column type="string" id="occur_user_id"   name="设置人员"/>
		<column type="string" id="remark"          name="备注"/>
		<button type="add"    id="add"             name="同步任务到征信"   action="70010481" multiple="0"/>
		<button type="view"   id="view"            name="更新任务状态"     action="70010475" multiple="0"/>
		<button type="ipt"    id="ipt"             name="批量导入征信数据" action="70010487" multiple="0"/>
		<button type="delete" id="delete"          name="任务删除"          action="70010477" multiple="0"/>
		<assistant action="70010490">
			<link src="task_id" target="task_id"/>
		</assistant>
	</output>
</function>