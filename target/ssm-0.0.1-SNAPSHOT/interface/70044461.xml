			<item type="textarea" id ="hg_num_big" name="标的证券数量大写" editable="false"/>
			<item type="textarea" id ="first_amt_big" name="初始交易金额大写" editable="false"/>
			<item type="textarea" id ="second_amt_big" name="购回交易金额大写" editable="false"/>
			<item type="textarea" id ="zjyt" name="资金用途"/>
