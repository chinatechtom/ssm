<?xml version="1.0" encoding="gbk" ?>
<function id="32326" name="清算后融券对帐报表" >
	<input>
		<item type="select"  id="institution_id"	name="营业部名称"	source="95000008"/>	
		<item type="text"    id="cust_code"			name="客户代码"/>
		<item type="select"  id="market_code"		name="市场代码"		source="039"/>
		<item type="text"    id="product_code"		name="证券代码"/>
		<item type="date"    id="date1"				name="开始时间"/>	
		<item type="date"    id="date2"				name="结束时间"/>	
	</input>
    <service type="procedure" id="p_balance_sheet_bstock"  process="911" style="77">
		<param id="user_id"	        type="integer" way="in"/> 
		<param id="institution_id"	type="integer" way="in"/>
		<param id="cust_code"	    type="string"  way="in"/>
		<param id="market_code"		type="string"  way="in"/>
		<param id="product_code"	type="string"  way="in"/>
		<param id="date1"			type="string"  way="in"/>
		<param id="date2"			type="string" way="in"/>		
    </service>
    <output export="true" print="true">
		<column type="string" id="institution_id"	name="营业部id"		display="false"/>
		<column type="string" id="institution_code"	name="营业部代码"/>
		<column type="string" id="institution_name"	name="营业部名称"/>
		<column type="string" id="cust_id"			name="客户id"		display="false"/>
		<column type="string" id="cust_code"		name="客户代码"/>
		<column type="string" id="cust_name"		name="客户姓名"/>
		<column type="string" id="product_id"		name="证券id"		display="false"/>
		<column type="string" id="product_code"		name="证券代码"/>
		<column type="string" id="product_name"		name="证券名称"/>
		<column type="string" id="market_code"		name="市场代码"/>
		<column type="string" id="begin_date"		name="开始时间"/>
		<column type="string" id="end_date"			name="结束时间"/>
		<column type="number" id="last_vol"			name="期初融券余量"	precision="0" total="true"/>
		<column type="number" id="this_vol"			name="期末融券余量"	precision="0" total="true"/>
		<column type="number" id="sell_vol"			name="期间融券卖出"	precision="0" total="true"/>
		<column type="number" id="return_vol"		name="期间融券归券"	precision="0" total="true"/>
		<column type="number" id="buy_return"		name="期间买券还券"	precision="0" total="true"/>
		<column type="number" id="direct_return"	name="期间直接还券"	precision="0" total="true"/>
    </output>
</function>