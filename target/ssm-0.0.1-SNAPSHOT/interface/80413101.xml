<?xml version="1.0" encoding="GBK" ?>
<function id="80413101" name="取客户资料">
  <input>
  	<item type="text" id="scust_no" name="客户号"/>
  </input>
  <service type="drtp" id="413101" process="711" style="77">
	  <param type="string" id="scust_no"/>
  </service>
  <output target="81413101">
    <column name="客户代码"	id="cust_code"   type="string"  field="scust_no"   />
    <column name="客户普通帐号代码"	id="cust_code1"   type="string"  field="scust_no2"   />
    <column name="客户姓名"	id="cust_name"   type="string"  field="sname"      />
	<column name="客户全称"	id="full_name"   type="string"  field="snote2"      />
    <column name="客户类型"	id="cust_type"   type="string"  field="sorder0"    />
    <column name="客户类别"	id="cust_sort"   type="string"  field="scust_type" />
    <column name="性别"     id="gender"      type="string"  field="sstatus3"      />
    <column name="证件类型"	id="cert_type"   type="string"  field="lcert_code" />
    <column name="证件号码"	id="cert_no"     type="string"  field="scert_no"     />
	<column name="证件地址"	id="cert_addr"   type="string"  field="scert_addr"   />
    <column name="联系地址"	id="area_code"   type="string"	 field="saddr"     />
    <column name="邮政编码"	id="postalcode"  type="string"  field="spost_code"/>
    <column name="开户人员"	id="open_oper"   type="string"  field="sopen_emp"  />
    <column name="开户日期"	id="open_date"   type="string"  field="sdate1"     />
    <column name="销户日期"	id="close_date"  type="string"  field="sdate3"    />
    <column name="销户人员"	id="close_oper"  type="string"  field="scheck_emp"/>
    <column name="记录状态"	id="regi_state"  type="string"	 field="sstatus1"  />
	<column name="机构代码"	id="institution_code"  type="string"	 field="sbranch_code1"  />
	<column name="机构名称"	id="institution_name"  type="string"	 field="sbankname"  />
	<column name="移动电话"	id="move_tel"    type="string"	 field="sphone3"  />
	<column name="固定电话"	id="fix_tel"     type="string"	 field="sphone"  />
	<!--<column name="应急电话"	id="meet_tel"    type="string"	 field="snote2"  />413101不存在meet_tel-->
	<column name="邮件地址"	id="email"		 type="string"	 field="semail"  />
	<column name="传真"	    id="fax"		 type="string"	 field="sphone2"  />
	<column name="客户状态" id="cust_status" type="string"   field="sstatus0"/>
	<column name="证件有效年数" id="effective_years" type="string"   field="sserial0"/>
	<column name="证件起始日期" id="cert_start_date" type="string"   field="sserial1"/>
	<column name="证件终止日期" id="cert_end_date" type="string"   field="sserial2"/>
  </output>
</function>