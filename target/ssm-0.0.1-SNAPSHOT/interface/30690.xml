<?xml version="1.0" encoding="gbk" ?>
<function id="30690" name="查询合同融券上限" >
  <input>
		<item type="hidden" id="bond_id" name="id"/>
		<item type="text" id="cust_code" name="客户代码"/>
		<item type="text" id="bond_no" name="合同编号"/>
		<item type="hidden" id="stock_code" name="证券代码"/>	
  </input>
  <service type="procedure" id="p_bond_borrow_stock_query" process="911" style="77">
		<param type="integer" id="user_id"/>	
		<param type="integer" id="bond_id"/>
		<param id="bond_no"/>	
		<param id="cust_code"/>
		<param id="stock_code"/>
  </service>
  <output >    
		<column type="sting" id="institution_name" name="营业部名称" display="false"/>
		<column type="number" id="institution_id" name="营业部ID" display="false"/>
		<column type="number" id="bond_id" name="合同id" display="false"/>
		<column type="string" id="cust_code" name="客户代码"/>
		<column type="string" id="market_code" name="市场代码" display="false"/>		
		<column	type="number"	id="borrow_stock_product_id"	name="融券产品ID" display="false"/>
		<column	type="string"	id="borrow_stock_product_code"	name="融券信用产品代码"/>		
		<column	type="string"	id="borrow_stock_product_name"	name="融券产品"/>
		<column type="string" id="market_code_name" name="市场名称"/>
		<column type="number" id="product_type" name="证券类型" display="false"/>  
		<column type="string" id="product_type_name" name="证券类型"/>
		<column type="number" id="product_id" name="证券id" display="false"/>  
		<column type="string" id="product_code" name="证券代码"/>  
		<column type="string" id="product_name" name="证券名称" />	
		<column type="string" id="model" name="模式" display="false"/> 
		<column type="string" id="model_name" name="信用使用模式"/> 
		<column type="number" id="borrow_qty" name="融券申请上限" precision="2"/>
		<column type="number" id="actual_borrow_qty" name="实际融券数量" precision="2"/>
		<column type="number" id="return_borrow_qty" name="返还数量" 	precision="2" display="false"/>
		<column type="number" id="surplas_borrow_qty" name="剩余可融券数量" 	precision="2" display="false"/>
		<column type="string" id="begin_time" name="起始日期"/>
		<column type="string" id="return_time" name="到期日期"/>
		<column type="string" id="begin_time1" name="开始日期" display="false"/>
		<column type="string" id="return_time1" name="到期日期" display="false"/>
		<column type="number" id="availability_day_num"  name="有效天数" display="false"/>
		<column type="number" id="cust_factor" name="客户融券费率因子" precision="2"/>
		<column type="string" id="order_no" name="合约编号"/>
		<column type="number" id="interest_rate" name="利息率" precision="4"/>
		<column type="string" id="order_status" name="合约状态"/>
		<button type="send" id="edit" name="同步融券数据" action="80413107" multiple="0"/>
  </output> 
</function>