<?xml version="1.0" encoding="gbk" ?>
<function id="36000052" name="5.1.3融资融券业务客户贡献度分析" >  
  <input>
  <item type="select" id="institution_id"       name="营业部名称"  source="95000008"/>
  <item type="text"   id="cust_code"             name="客户代码"  /> 
  <item type="date"   id="begin_date"           name="起始日期"  default="last"/>
  <item type="date"   id="end_date"             name="结束日期"  default="now"/> 
  </input>
   <service type="procedure" id="P36000052" process="911" style="77">
   <param id="user_id" type="integer" way="in" />  
   <param id="institution_id" type="integer" way="in" />
   <param id="begin_date"    type="string" way="in" />
   <param id="end_date"    type="string" way="in" />
   <param id="cust_code"    type="string" way="in" />
  </service> 
  <output  print="true">
    <column type="number" id="no"                 name="序号"/>
    <column type="string" id="institution_name"   name="营业部"/> 
    <column type="string" id="cust_code"       name="客户代码"  /> 
    <column type="string" id="cust_name"       name="客户名称"  />
   <group name="本期融资"> 
    <column type="number" id="financing_fee"          name="融资利息"  precision="2"  total="true"/>
    <column type="number" id="financing_other_fee"    name="其他收入" precision="2" total="true"/>
   </group>
   <group name="本期融券"> 
    <column type="number" id="borrow_stock_fee"            name="融券收入" precision="2" total="true"/>
    <column type="number" id="borrow_stock_other_fee"      name="其他收入" precision="2" total="true"/>
   </group>
    <column type="number" id="fee_all"       name="收入合计" precision="2" total="true"/>
    <column type="number" id="fee_rate"       name="贡献度(%)" precision="4" total="true"/>
  </output>
</function>