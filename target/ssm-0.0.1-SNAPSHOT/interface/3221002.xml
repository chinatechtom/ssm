<?xml version="1.0" encoding="gbk" ?>
<function id="3221002" name="邮件收件人查询" >
	<input>
		<item type="hidden"	id="c1"	name="信息编号"	/>		
	</input>
	<service type="procedure" id="p_msg_receiver_query" process="911" style="77">
		<param id="user_id"	type="integer"  way="in"/>
		<param id="c1"	type="integer"  way="in"/>
	</service>
	<output>
		<column type="number"	id="cust_id"			name="客户ID"		display="false"	/>
		<column type="string"	id="cust_code"			name="客户代码"		display="true"	/>
		<column type="string"	id="cust_name"			name="客户名称"		display="true"	/>
		<column type="string"	id="send_flag"			name="发送标志"		display="false"	/>
		<column type="string"	id="send_flag_name"		name="发送标志"		display="true"	/>
		<column type="number"	id="user_id"			name="用户ID"		display="false"	/>
		<column type="string"	id="user_code"			name="用户代码"		display="true"	/>
		<column type="string"	id="user_name"			name="用户名称"		display="true"	/>
		<column type="number"	id="group_id"			name="组ID"			display="false"	/>
		<column type="string"	id="group_code"			name="组代码"		display="true"	/>
		<column type="string"	id="group_name"			name="组名称"		display="true"	/>
		<column type="number"	id="institution_id"		name="营业部ID"		display="false"	/>
		<column type="string"	id="institution_name"	name="营业部名称"	display="true"	/>
		<column type="string"	id="email_address"		name="邮件地址"		display="true"	/>
	</output>
</function>