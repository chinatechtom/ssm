<?xml version="1.0" encoding="gbk" ?>
<function id="30401" name="客户信用浏览" >
  <input>
    <item type="select" id="institution_id" name="营业部"  source='95000008'/>
    <item type="hidden" id="cust_id"		name="客户id" />
    <item type="text"	id="cust_code"	    name="客户代码" />
    <item type="text"	id="cust_name"      name="客户姓名" />
    <item type="text"	id="cert_no"        name="证件号码" />
	<item type="select" id="credit_level_id" name="信用级别"  source="90000002"/>
	<item type="select" id="type" name="信用级别调整方向"  source="395"/>
  </input>
  <service type="procedure" id="p_cust_credit_browse" implement="p_cust_credit_browse401"  process="911" style="77"> 
    <param id="user_id"		   type="integer" way="in"/>
    <param id="institution_id" type="integer" way="in"/>
    <param id="cust_id"		   type="integer" way="in"/>
    <param id="cust_code"      type="string"  way="in"/>
    <param id="cust_name"	   type="string"  way="in"/>
	<param id="cert_no"		   type="string"  way="in"/>
	<param id="credit_level_id" type="integer" way="in"/>
	<param id="type"		   type="string" way="in"/>
  </service>
  <output export="true">
  	<column type="number" id="institution_id"		name="institutionid"    display="false"/>
	<column type="string" id="institution_code"		name="营业部代码"/>
	<column type="string" id="institution_name"		name="营业部名称"/>
	<column type="number" id="cust_id"				name="客户id"    display="false"/>
	<column type="string" id="cust_code"			name="客户代码" />
	<column type="string" id="cust_name"			name="客户姓名" />
	<column type="number" id="credit_level_id"		name="信用级别id" display="false"/>
	<column type="string" id="credit_level_code"	name="信用级别代码" />	
	<column type="string" id="credit_level_name"	name="信用级别名称" />
	<column type="string" id="risk_level"			name="风险级别1"	  display="false"/>
	<column type="string" id="risk_level_name"		name="风险级别"/>
	<column type="string" id="if_estop"				name="是否禁止1"  display="false"/>
	<column type="string" id="if_estop_name"		name="是否禁止" />
	<column type="number" id="credit_integral"		name="信用积分" display="false"/>
	<column type="number" id="credit_level_id_old"		name="信用级别idold" display="false"/>
	<column type="string" id="credit_level_code_old"	name="上次信用级别代码" />	
	<column type="string" id="credit_level_name_old"	name="上次信用级别名称" />
	<column type="string" id="risk_level_old"			name="上次风险级别1"	  display="false"/>
	<column type="string" id="risk_level_name_old"		name="上次风险级别"/>	
  </output>
  <assistant action="30103,3200301,30102,30104,30109,30111,30201,30202,30208,30673,30672,30667,30668,30669">
	<link src="cust_id" target="cust_id"/>
  </assistant>
</function>