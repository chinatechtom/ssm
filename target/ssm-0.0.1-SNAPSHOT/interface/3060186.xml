<?xml version="1.0" encoding="gbk" ?>
<function id="3060186" name="客户持有流通股限售或解除限售查询" >
	<input>
		<item type="hidden" id="cust_id"	name="客户ID"/>
		<item type="hidden"	id="declare_id"	name="申报信息编号"/>
	</input>
	<service type="procedure" id="p_hold_circulate_info"  implement="p_hold_circulate_info3060186" process="911" style="1">
		<param id="user_id"		type="integer" way="in"/> 
		<param id="cust_id"		type="integer" way="in"/> 
		<param id="declare_id"	type="integer" way="in"/>
	</service> 
	<output>
		<column id="cust_id"			type="number" name="客户ID" display="false"/>
		<column id="declare_id"			type="number" name="申报信息编号" display="false"/>
		<column id="commness_cust_code"	type="string" name="普通资金账号" display="false"/>
		<column id="cust_code"			type="string" name="客户代码" display="false"/>
		<column id="cust_name"			type="string" name="客户姓名" display="false"/>
		<column id="product_name"		type="string" name="股票名称" display="false"/>
		<column id="product_code"		type="string" name="股票代码" display="false"/>
		<column id="stock_amt"			type="string" name="股份数量" display="false"/>
		<column id="product_type"		type="string" name="股票类型TYPE" display="false"/>
		<column id="product_type_name"		type="string" name="股票类型"/>
		<column id="relieve_date"		type="string" name="解禁日期" display="false"/>
	</output>
</function>