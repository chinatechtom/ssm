<?xml version="1.0" encoding="gbk" ?>
<function id="37424" name="获取客户原始保证金比例" >	
  <input> 
		<item type="hidden" id="cust_id"	name="客户id"/>
		<item type="hidden"	id="order_id"   name="合约id"/>
		<item type="hidden"	id="sbank_acc"	name="调整值"/> <!--add 20140905-->
  </input>
  <service type="procedure" id="p_cust_original_guarantee_rate"  implement="p_cust_original_guarantee_rate37424" process="911" style="77"> 
		<param id="user_id"		type="integer"/>
		<param id="cust_id"		type="integer"/>
		<param id="order_id"	type="integer"/>
		<param id="sbank_acc"	type="double"/><!--add 20140905-->
  </service>
  <output>
		<column type="number" id="CREDIT_LEVEL_ID"			name="信用级别ID"/>
		<column type="string" id="CREDIT_LEVEL_NAME"			name="信用级别"/>
		<column type="number" id="CREDIT_LEVEL_GUARANTEE_RATE_13"	name="初始保证金比例"		precision="2"/>
		<column type="number" id="CREDIT_LEVEL_GUARANTEE_RATE_5"	name="融券卖出保证金比例"	precision="2"/>
		<column type="number" id="CREDIT_LEVEL_GUARANTEE_RATE_6"	name="融资买入保证金比例"	precision="2"/>
		<column type="number" id="NOW_GUARANTEE_RATE_13"		name="当前初始保证金比例"	precision="2"/>
		<column type="number" id="NOW_GUARANTEE_RATE_5"			name="当前融券卖出保证金比例"	precision="2"/>
		<column type="number" id="NOW_GUARANTEE_RATE_6"			name="当前融券卖出保证金比例"	precision="2"/>
		<column type="number" id="apply_lower_guarantee_rate"	name="申请最低保证金比例"	precision="2"/>
		<column type="number" id="current_guarantee_rate"		name="当前保证金比例"		precision="2"/> <!--add 20140905-->
		<column type="number" id="origin_lower_guarantee_rate"	name="原始最低保证金比例"	precision="2"/>
		<column type="number" id="credit_num"					name="当前授信额度"			precision="2"/> <!--add 20140909-->
  </output>		
</function>