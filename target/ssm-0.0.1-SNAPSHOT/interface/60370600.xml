<?xml version="1.0" encoding="gbk" ?>
<function id="60370600" name="分段计息区间浏览" >	
  <input> 
		<item type="hidden"	id="sbond_id" name="子合同id" />
		<item type="hidden"	id="section_id" name="分段计息区间id" />
  </input>
  <service type="procedure" id="P60370600" process="977" style="77"  adapter="psr"> 
	    <param id="user_id"	type="integer"/>		
		<param id="sbond_id" type="integer"/>	
	    <param id="section_id" type="integer"/>
  </service>
  <output>
		<column type="number" field="section_id"        id="sg_section_id"        name="区间ID"   display="false"/>
		<column type="string" field="section_name"      id="sg_section_name"      name="区段"   />
		<column type="string" field="begin_date"        id="sg_begin_date"        name="计息起始日期" 		     />
		<column type="string" field="end_date"          id="sg_end_date"          name="计息截至日期" 			 />
		<column type="string" field="end_date_true"     id="sg_end_date_true"     name="计息截至日期(实际)" display="false" />
		<column type="number" field="days" 		        id="sg_days" 		      name="区间天数"				 />
		<column type="number" field="hg_rate"	        id="sg_hg_rate"	          name="购回价格(每100元)" precision="4" />
		<column type="number" field="interest_count"    id="sg_interest_count"    name="计息积数"  precision="2" />
		<column type="number" field="interest"		    id="sg_interest"		  name="未结利息"  precision="2" />
		<column type="number" field="received_interest" id="sg_received_interest" name="已收利息"  display="false"/>
		<column type="string" field="remarks"           id="sg_remarks"           name="备注"/>
  </output>		
</function>