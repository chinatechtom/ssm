<?xml version="1.0" encoding="gbk" ?>
<function id="60170137" name="电话通知记录查询" >
  <input>
	<item type="select" id="institution_id" name="营业部名称" source="97000001"/>
	<item type="text" id="cust_code" name="客户代码" />
	<item type="date" 	id="record_date" name="通知日期" default="now"/>
	<item type="select" id="if_connection" name="是否通知到客户" source="921"/>
  </input>
  <service type="procedure" id="P60170136" process="933" style="77"> 
    <param id="user_id"		    way="in"/>
	<param id="institution_id" way="in"/>
	<param id="cust_code"     way="in"/>
	<param id="record_date"     way="in"/>
    <param id="if_connection"   way="in"/>  
  </service>
  <output>
  	<column type="number" id="record_id"   name="记录id号" display="false"/>
  	<column type="string" id="institution_name"   name="营业部名称" />
	<column type="string" id="institution_code"   name="营业部代码" />  
	<column type="string" id="cust_id"			name="客户id" display="false"/>
	<column type="string" id="cust_code"		name="客户代码"/>
	<column type="string" id="cust_name"		name="客户名称"/>
	<column type="string" id="record_date"		name="通知日期"/>	
	<column type="number" id="record_times"		name="通知次数"/>		
	<column type="string" id="record_content"		name="通知内容"/>
	<column type="string" id="if_connection"		name="是否通知到客户" display="false"/>		
	<column type="string" id="if_connection_name"		name="是否通知到客户"/>		
	<column type="string" id="remark"			name="备注"/>	
	<column type="string" id="set_code"			name="设置人员"/>
	<column type="string" id="set_date"			name="设置日期"/>	
	<column type="string" id="upd_code"			name="更新人员"/>
	<column type="string" id="upd_date"			name="更新日期"/>	
	<button type="add"    id="add"				name="增加信息" action="60170138" multiple="-1"/>
	<button type="edit"   id="edit"				name="设置信息" action="60170139" multiple="0"/>
	<button type="delete"   id="delete"			name="删除信息" action="60170140" multiple="0"/>		
  </output>
</function>
