		<item type="text" 	    id="cust_code" 		     name="客户代码"	        editable="false"		/>
		<item type="select" 	id="if_send_prompt_msg"  name="是否发送提示信息"	source="340" nullable="false"/>	
		<item type="hidden"		id="checkbill_type"		 name="对账单服务类型"		default="1" />
	</input>
	<service type="procedure" id="p_cust_prompt_msg_set"  process="811" style="1">
		<param id="user_id" type="integer"/>
		<param id="cust_code"/>
		<param id="if_send_prompt_msg"/>
		<param id="checkbill_type"/>
	  </service>
</function>
		