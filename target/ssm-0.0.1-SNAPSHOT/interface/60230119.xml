<?xml version="1.0" encoding="gbk" ?>
<function id="p60230119" name="查询业务监管报表" >
  <input>
	<item type="year" id="year_id" name="查询年份" nullable="false" default="now"/>
	<item type="month" id="month_id" name="查询月份" nullable="false" default="last"/>
  </input>
  <service type="procedure" id="p60230119" process="933" style="77">
	<param type="integer" id="user_id"/>
	<param type="string"  id="year_id"/>
	<param type="string"  id="month_id"/>
  </service>
  <output  export="true">  
	<column type="string" id="SUBJECT_NAME"   name="项目名称" />
	<column type="number" id="line_num"   name="行次" precision="0"/>
	<column type="string" id="qc_value"   name="期初数"/>	
	<column type="string" id="inc_value"   name="本期增加"/>
	<column type="string" id="dec_value"   name="本期减少"/>
	<column type="string" id="qm_value"   name="期末数"/>
  </output>
</function>