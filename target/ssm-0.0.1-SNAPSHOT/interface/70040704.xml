<?xml version="1.0" encoding="gbk" ?>
<function id="70040704" name="证券标的设置" >
  <input>
	<item type="hidden" id="product_id" name="productid"/>
	<item type="text" 	id="product_code" name="产品代码" editable="false"/>
	<item type="text" 	id="product_name" name="产品名称" editable="false"/>
	<item type="select" id="hg_if_hg" name="是否回购标的证券" source="723"/>
	<group name="回购标的证券设置"> 
	  <item type="text" id="hg_convert_rate" name="回购折算率(%)" number="true" filter="hg_if_hg=1"/>
	  <item type="text" id="hg_concentration_rate" name="回购集中度(%)占总股本" number="true" filter="hg_if_hg=1"/>	  
	  <item type="text" id="one_stock_pca" name="回购集中度(%)占流通A" number="true" filter="hg_if_hg=1"/>
	  <item type="text" id="one_stock_pna" name="回购集中度(%)占净资本" number="true" filter="hg_if_hg=1"/>
	</group>  
  </input>
  <service type="procedure" id="P70040704" process="822" style="1">
	<param id="user_id" 				type="integer"/> 			
	<param id="product_id" 				type="integer"/> 
	<param id="hg_if_hg" 				type="string"/> 
	<param id="hg_convert_rate" 		type="float"/>
	<param id="hg_concentration_rate"	type="float"/>
	<param id="one_stock_pca"			type="float"/>
	<param id="one_stock_pna"			type="float"/>
  </service>
  <output/>
</function>