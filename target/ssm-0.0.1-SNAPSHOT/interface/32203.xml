<?xml version="1.0" encoding="gbk" ?>
<function id="32203" name="强制平仓客户数量和金额" >
	<input>	
		<item type="hidden" id="date" name="日期"  default="now"/>
		<item type="select" id="institution_id" name="机构"  source="95000008"/>
		<item type="text" id="cust_code" name="客户代码" />
  	</input>
  	<service type="procedure" id="p_test_04" process="911" style="77">
		<param id="user_id" type="integer" way="in" /> 
		<param id="date"/>
		<param id="institution_id" />
		<param id="cust_code" />
  	</service>
  	<output  print="true">
    <column type="string" id="c1" name="机构"/>  
	<column type="string" id="c2" name="信用资金账户"/>
	<column type="string" id="c3" name="账户姓名"/>
	<group name="平仓情况">
	<column type="number" id="c4" name="应平仓"/>
	<column type="number" id="c5" name="平仓证券名称"/>
	<column type="number" id="c6" name="平仓证券数量"/>
	<column type="number" id="c7" name="平仓金额"/>
	</group>
	<column type="string" id="c8" name="备注"/>
  </output>
 </function>