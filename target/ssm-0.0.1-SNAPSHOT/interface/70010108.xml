<?xml version="1.0" encoding="gbk" ?>
<function id="70010108" name="客户基本资料查询" >
	<description>确定要同步客户资料吗？</description>
	<input>
		<item type="hidden" id="cust_code" alias="scust_no" name="客户代码" default="11832085" action="70010107"/>
		<item name="客户代码"	id="cust_code"   type="text"	editable="false"/>                                                             
		<item name="客户姓名"	id="cust_name"   type="text"	editable="false"/>
		<item name="客户全称"   id="full_name"   type="text"	editable="false"/>                                                           
		<item name="客户类型"	id="cust_type"   type="hidden"	editable="false"/>
