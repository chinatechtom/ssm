<?xml version="1.0" encoding="gbk" ?>
<function id="30417" name="信用级别担保率设置" >
	<input>   
		<item type="select"	id="credit_level_id"	name="信用级别名称"	source="90000002"	nullable="false"/>
		<item type="select" id="guarantee_rate_id"	name="担保率名称"		source="90000012"	nullable="false"/>
		<item type="text"	id="guarantee_rate"		name="担保率"			nullable="false"	number="true"/>	
	</input>
	<service type="procedure" id="p_credit_guarantee_rate_set" implement="p_credit_guarantee_rate_set417"  process="911" style="1"> 
		<param id="user_id"				type="integer"	way="in"/>
		<param id="credit_level_id"		type="integer"	way="in"/>
		<param id="guarantee_rate_id"	type="integer"	way="in"/>
		<param id="guarantee_rate"		type="double"	way="in"/>
	</service>
	<output target="@func_no">
		<column name="信用级别代码"	id="sserial0"	type="string"/>
		<column name="交易指令"		  	id="sserial1"	type="string"/>
		<column name="担保率"	  		id="drate0"		type="double"/>
		<column name="信用级别名称"	id="sbank_acc"	type="string"/>
		<column name="融资上限标准"	id="damt0"		type="double"/>
		<column name="融券上限标准"	id="damt1"		type="double"/>
		<column name="备注"				id="scusttypes"	type="string"/>
		<column name="功能号"			id="func_no"	type="string"/>
	</output>
</function>