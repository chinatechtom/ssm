		<column type="string" id="institution_code"  name="营业部代码"/>
		<column type="string" id="credit_level_code" name="信用级别代码"/>
		<column type="number" id="credit_num"        name="信用上限" precision="2"/>
		<column type="string" id="pc_mode"           name="平仓标志"/>
		<column type="string" id="credit_usemode"    name="信用上限使用模式"/>
		<column type="string" id="if_blacklist"      name="是否黑名单"/>
		<column type="string" id="hk_mode"			 name="还款模式"/>
		<column type="string" id="bond_no"			 name="合约编号"/>
		<column type="string" id="bond_status"		 name="合约状态"/>
		<column type="number" id="fix_qty"           name="融资数额" precision="2"/>
		<column type="string" id="begin_time"        name="起始日期"/>
		<column type="string" id="return_time"       name="结束日期"/>
		<column type="string" id="market_code"       name="市场代码"/>  
		<column type="string" id="product_code"      name="证券代码"/>
		<column type="number" id="borrow_qty"        name="证券数量" precision="2"/>
