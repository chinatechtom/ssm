<?xml version="1.0" encoding="gbk" ?>
<function id="340662" name="逐日盯市(只含担保品)" >	
	<input>
	  <item type="select" id="institution_id"		name="营业部代码" source="95000008"/>
		<item type="text"	  id="cust_code" 	name="客户代码"/>		
		<item type="select" id="qzpc_mark"	name="强平标志"	source="263"/>
		<item type="select" id="if_all"  name="是否全量" default="1" source="002"/>  
	</input>
	<service type="procedure" id="P17000064"  implement="P17000064_340662" process="911" style="77">
		<param id="user_id"				type="integer"	way="in"/>
		<param id="institution_id"			type="integer"	way="in"/>
		<param id="cust_code"			type="string"	way="in"/> 
		<param id="qzpc_mark"     type="string"   way="in"/>
		<param id="if_all"     type="string"   way="in"/>
	</service>
	<output	export="true">
	  <column type="string" id="institution_code"			name="营业部代码"/>
		<column type="string" id="institution_name"			name="营业部名称"/> 
		<column type="number" id="cust_id"					name="客户id"		display="false"/>
		<column type="string" id="cust_code"				name="客户代码"/>
		<column type="string" id="cust_name"				name="客户姓名"/>  
		<column type="string" id="qzpc_mark"				name="强平标志" display="false"/>	
		<column type="string" id="qzpc_mark_name"   name="强制平仓标志"/>	
		<column type="number" id="guarantee_rate_dbp"  name="担保证券维持担保比例"	precision="6"/> 
		<column type="number" id="guarnatee_rate"      name="所有证券维持担保比例"	precision="6"/>
		<column type="number" id="guarantee_rate_zsh"  name="担保证券折算后维持担保比例"  precision="6"/>			
		<column type="number" id="debt_value"		name="总负债" precision="4"/>
		<column type="number" id="borrow_stock_value"		name="融券负债"		precision="4"/>
		<column type="number" id="pure_financing_amt"		name="融资负债"  precision="4"/>		
		<column type="number" id="interest_fee"		name="息费"  precision="4"/>	
		<column type="number" id="fund_amt"   name="资金"	precision="3"/> 
		<column type="number" id="gua_stock_value" name="担保证券市值"		precision="3"/> 
		<column type="number" id="guaran_goods_value" name="担保证券市值+资金"		precision="3"/> 
		<column type="number" id="stock_value" 	name="所有证券市值" precision="3"/> 
		<column type="number" id="asset_value" name="所有证券市值+资金+场外担保品" precision="3"/> 
		<column type="number" id="stock_convert_value"  name="担保证券折算市值" precision="3"/> 
		<column type="number" id="convert_asset_value" name="担保证券折算市值+资金" precision="3"/> 
		<column type="number" id="mortgage_value" name="场外担保品" precision="3"/>  
	</output>
</function>