<?xml version="1.0" encoding="gbk" ?>
<function id="3063224" name="信用级别、授信额度查询" >
	<input>
		<item type="hidden" id="valuation_score"	name="评估总分"/>
		<item type="hidden" id="total_capital"		name="客户总资产"/>
	</input>
	<service type="procedure" id="p_credit_level_num_query" implement="p_credit_level_num_query3063224"  process="911" style="77">
		<param id="user_id"			type="integer"	way="in"/>
		<param id="valuation_score"	type="double"	way="in"/>
		<param id="total_capital"	type="double"	way="in"/>
	</service>
	<output>
		<column type="integer"	id="auto_credit_level"	name="信用级别"/>
		<column type="number"	id="auto_credit_num"	name="授信额度"			precision="2"/>
		<column type="number"	id="credit_num_wy"		name="授信额度(万元)"	precision="2"/>
		<column type="number"	id="chinese"			name="授信额度中文大写"/>
	</output>
</function>