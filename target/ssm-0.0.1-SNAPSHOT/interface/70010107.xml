<?xml version="1.0" encoding="GBK" ?>
<function id="70010107" name="取客户资料">
	<input/>
	<service type="procedure" id="kingstar.p_bbs_get_cust_info" process="311" style="77">
		<param type="string" id="scust_no"/>
	</service>
	<output>
		<column name="客户代码"	id="cust_code"   type="string"/>
		<column name="客户姓名"	id="cust_name"   type="string"/>
		<column name="客户类型"	id="cust_type"   type="string"	display="false"/>
		<column name="客户类型"	id="cust_type_name"   type="string"/>
		<column name="客户类别"	id="cust_sort"   type="string"/>
		<column name="性别"     id="gender"      type="string"	display="false"/>
		<column name="性别"     id="gender_name"      type="string"/>
		<column name="证件类型"	id="cert_type"   type="string"	display="false"/>
		<column name="证件类型"	id="cert_type_name"   type="string"/>
		<column name="证件号码"	id="cert_no"     type="string"/>
		<column name="联系地址"	id="address"     type="string"/>
		<column name="邮政编码"	id="postalcode"  type="string"/>
		<column name="开户人员"	id="open_oper"   type="string"/>
		<column name="开户日期"	id="open_date"   type="string"/>
		<column name="销户日期"	id="close_date"  type="string"/>
		<column name="销户人员"	id="close_oper"  type="string"/>
		<column name="记录状态"	id="regi_state"  type="string"/>
		<column name="机构代码"	id="institution_code"  type="string"/>
		<column name="机构名称"	id="institution_name"  type="string"/>
		<column name="移动电话"	id="move_tel"    type="string"/>
		<column name="固定电话"	id="fix_tel"     type="string"/>
		<column name="应急电话"	id="meet_tel"    type="string" display="false"/>
		<column name="邮件地址"	id="email"		 type="string"/>
		<column name="传真"	    id="fax"		 type="string"/>
		<column name="客户状态" id="cust_status" type="string" display="false"/>
		<column name="客户状态" id="cust_status_name" type="string"/>
		<column name="证件有效年数" id="effective_years" type="string"/>
		<column name="证件起始日期" id="cert_start_date" type="string"/>
		<column name="证件终止日期" id="cert_end_date" type="string"/>
		<column name="上海证券账户" id="holder_acc_sh" type="string"/>
		<column name="深圳证券账户" id="holder_acc_sz" type="string"/>
		<column name="上海席位代码" id="seat_code_sh" type="string"/>
		<column name="深圳席位代码" id="seat_code_sz" type="string"/>
		<column name="客户全称" id="full_name" type="string"/>
	</output>

</function>