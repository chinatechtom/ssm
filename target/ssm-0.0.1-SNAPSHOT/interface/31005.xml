<?xml version="1.0" encoding="gbk" ?>
<function id="31005" name="融券统计" >  
  <input>
	<item type="date" id="date" name="日期" default="now"/>
	<item type="select" id="mark_code" source="039" name="市场代码" />
  </input>
   <service type="procedure" id="p_borrow_stock_addup" process="911" style="77">
	<param id="user_id" type="integer" way="in" /> 
	<param id="date"/>
	<param id="mark_code" />
  </service> 
  <output >
    <column type="string" id="date" name="日期"/>   
	<column type="number" id="institution_id" name="机构id" display="false"/>
	<column type="string" id="institution_code" name="机构代码"/>
    <column type="number" id="currency_type"   name="货币类型" display="false"/>
    <column type="number" id="market_code"   name="市场代码" display="false"/>
	<column type="number" id="product_type" name="产品类型" display="false"/>   
	<column type="string" id="currency_typename"   name="货币类型"/>
    <column type="string" id="market_codename"   name="市场代码"/>
	<column type="string" id="product_typename" name="产品类型"/> 
    <column type="number" id="product_id"   name="产品id" display="false"/>
    <column type="string" id="product_code"   name="产品代码"/>
    <column type="number" id="asset_qty"   name="资产余额" precision="2"/>
	<column type="number" id="cur_buy_vol"  name="当日买入数量" precision="2"/>
    <column type="number" id="cur_buy_amt"   name="当日买入金额" precision="2"/>
    <column type="number" id="cur_sell_vol"   name="当日卖出数量" precision="2"/>
    <column type="number" id="cur_sell_amt"   name="当日卖出金额" precision="2"/>
    <column type="number" id="trin_bal"   name="存入数量" precision="2"/>
    <column type="number" id="trout_bal"   name="取出数量" precision="2"/>
    <column type="number" id="taday_qty"   name="当日变动数量" precision="2"/>
  </output>
</function>