<?xml version="1.0" encoding="gbk" ?>
<function id="27050107" name="单个客户融资仓位集中度、占净资本比例统计" >
     <input>
      <item type="select"  id="institution_id"        name="营业部名称" source="95000008"/> 
      <item type="text"  id="cust_num"        name="客户数量*" default="1000"/>            
     </input>
  <service type="procedure" id="P27050107"  process="911" style="77">
      <param id="user_id"               type="integer" way="in"/> 
       <param id="institution_id"      type="integer" way="in"/>      
      <param id="cust_num"      type="integer" way="in"/>            
   </service>
 <output print="true">
        <column type="number" id="row_id" name="序号" />        
        <column type="string" id="cust_code" name="客户号" />        
        <column type="string" id="cust_name"   name="客户姓名"/>
        <column type="string" id="institution_name"   name="营业部名称"/>        
        <column type="number" id="fin_num" name="融资总额" precision="2" />   
        <column type="number" id="fin_total"   name="单位融资总额" precision="2"/>   
        <column type="number" id="rate1"   name="占单位融资比例" precision="6"/> 
        <column type="number" id="asset_total"   name="净资本额" precision="2"/>    
        <column type="number" id="rate2"   name="占净资本比例" precision="6"/>  
        <column type="string" id="remark"   name="备注"/>      
</output>
</function>  