<?xml version="1.0" encoding="gbk" ?>
<function id="27050131" name="客户贡献度分析" >
  <input export="true">
    <item type="select" id="institution_id" name="营业部"   source="95000008"/>
    <item type="text"   id="cust_code"      name="客户代码"/>
    <item type="year"   id="begin_year"     name="起始年度" nullable="false"/>
    <item type="month"  id="begin_month"    name="起始月份" nullable="false"/>
	  <item type="year"   id="end_year"       name="结束年度" nullable="false"/>
    <item type="month"  id="end_month"      name="结束月份" nullable="false"/>
    <item type="select" id="cust_detail"    name="客户明细" source="036" nullable="false" default="2"/>
    <group name="分页查询" page="true"> 		
	   <item type="text"  id="page_num"       name="页码"     integer="true" default="1"/>
	   <item type="text"  id="rows_num"       name="每页行数" integer="true"  default="50000" nullable="false" editable="false"/>
	  </group>
  </input>
  <service type="procedure" id="p27050131" process="955" style="77" implement="p2705013127050131">
    <param id="user_id"        type="ingeter"	way="in"/>
	<param type="integer" id="page_num"/>	
	<param type="integer" id="rows_num"/>
    <param id="institution_id" type="ingeter"	way="in"/>
    <param id="cust_code"      type="string"	way="in"/>
    <param id="begin_year"     type="string"	way="in"/>
    <param id="begin_month"    type="string"	way="in"/>
	<param id="end_year"       type="string"	way="in"/>
    <param id="end_month"      type="string"	way="in"/>
    <param id="cust_detail"    type="string"	way="in"/>
  </service>
  <output export="true">
    <column id="institution_id"         name="营业部id"   type="number"  display="false" precision="2"/>
    <column id="institution_code"       name="营业部代码" type="string" />
    <column id="institution_name"       name="营业部名称" type="string" />
    <column id="cust_code"              name="客户代码"   type="string" />
    <column id="cust_name"              name="客户名称"   type="string" />
    <column id="interest_amt"           name="融资利息"   type="number" precision="2"/>
    <column id="other_interest_amt"     name="融资其他收入" type="number" precision="2"/>
    <column id="bs_interest_amt"        name="融券费用"  type="number" precision="2"/>
    <column id="other_bs_interest_amt"  name="融券其他收入" type="number" precision="2"/>
    <column id="interest_sum"           name="融资融券利费收入合计" type="number" precision="2"/>    <!-- 增加分页去掉total="true"-->
  </output>
</function>