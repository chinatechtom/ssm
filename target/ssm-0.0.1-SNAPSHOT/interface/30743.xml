<?xml version="1.0" encoding="gbk" ?>
<function id="30743" name="增加补仓通知" >
  <input>
	<item type="hidden" id="cust_id"	  name="客户id"/>
	<item type="text"   id="cust_code"	  name="客户代码" nullable="false"	maxlen="20" action="30101"/>	
	<item type="text"   id="cust_name"	  name="客户姓名" editable="false"	maxlen="40"/>	
	<item type="date"   id="date"		  name="日期"	 nullable="false"/>
	<item type="text"   id="time"		  name="时间"	 maxlen="8" nullable="false"/>	
	<item type="text"   id="mode"		  name="通知方式" maxlen="40"	nullable="false"/>	
	<item type="select" id="if_success"	  name="是否通知到客户"		source="326" nullable="false"/>	
	<item type="text"   id="name"		  name="通知人员" maxlen="10" nullable="false"/>	
	<item type="textarea" id="mark"		  name="备注"/>	
  </input>
  <service type="procedure" id="p_bc_notice_add"  process="811" style="1"> 
    <param id="user_id"		type="integer" way="in"/>
	<param id="cust_id"		type="integer" way="in"/> 
	<param id="cust_code"	type="string"  way="in"/> 
	<param id="cust_name"	type="string"  way="in"/> 
	<param id="date"		type="string"  way="in"/>
	<param id="time"		type="string"  way="in"/> 
	<param id="mode"		type="string"  way="in"/> 
	<param id="if_success"	type="string"  way="in"/> 
	<param id="name"		type="string"  way="in"/> 
	<param id="mark"		type="string"  way="in"/> 	
  </service>
	<output/>
</function>