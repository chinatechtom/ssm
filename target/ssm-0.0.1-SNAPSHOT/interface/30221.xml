<?xml version="1.0" encoding="gbk" ?>
<function id="30221" name="融券明细历史查询" >	
  <input>		
  <item type="hidden" id="in_cust_id" name="客户id"/>
	<item type="text"   id="in_cust_code"       name="客户代码" />
	<item type="text"   id="in_product_code"	name="证券代码" />
	<item type="date"   id="in_date" name="日期"  nullable="false" default="now"/>
		
  </input>  
  <service type="procedure" id="p_report_stock_his_query" process="911" style="77">
	<param id="user_id" type="integer" />
	<param id="in_cust_id" type="integer"/>
	<param id="in_cust_code"     type="string"  way="in"/>
	<param id="in_product_code"  type="string"  way="in"/>	
	<param id="in_date" type="string" way="in"/>
  </service>  
  <output export="true">  	
	<column type="string"  id="cust_code"                  name="客户代码"/>
	<column type="string"  id="cust_name"                  name="客户姓名"/>
	<column type="string"  id="product_code"               name="证券代码"/>    
	<column type="number"  id="yes_bs_vol"                 name="前日融券余量" precision="3"/>
	<column type="number"  id="today_bs_vol"               name="本日融券余量" precision="3"/>
	<column type="number"  id="today_bs_sell_vol"          name="本日融券卖出量" precision="3"/>
	<column type="number"  id="today_return_val"           name="本日融券偿还量" precision="3"/>	
	<column type="number"  id="today_buy_return_vol"       name="本日买券还券量" precision="3"/>	
	<column type="number"  id="today_direct_return_vol"    name="本日直接还券量" precision="3"/>
	<column type="number"  id="today_bs_pc_vol"            name="本日融券强制平仓量"  precision="3"/>
	<column type="number"  id="today_bs_vol_amt"           name="本日融券余量余额"	  precision="3"/>	
	<column type="number"  id="bs_unit"                    name="融券报送单位"/>
	<column type="string"  id="date"                       name="信用交易日期"/>   
  </output>
</function>