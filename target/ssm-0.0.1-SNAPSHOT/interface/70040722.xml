<?xml version="1.0" encoding="gbk" ?>
<function id="70040722" name="增加证券权益信息" >
	<input>
	<group name="证券基本信息">
		<item type="text" 	id="product_code"	name="证券代码" nullable="false" minlen="6" maxlen="6"/>
		<item type="select" id="market_code"	name="市场代码"  default="2" source="039"/>
	</group>
	<group name="证券权益信息">
		<item type="text" 	id="send_off_10"	name="每10股送股"	number="true" nullable="false" default="0"/>
		<item type="text" 	id="send_qz_10"		name="每10股送配股权证数"	number="true" nullable="false" default="0"/>
		<item type="hidden" 	id="allot_10"		name="每10股配股"	default="0"/>
		<item type="text" 	id="every_bonus_10"	name="每10股红利"		number="true" nullable="false" default="0"/>
		<item type="date" 	id="bonus_arrive"	name="红利到账日"/>
		<item type="date" 	id="register_date"	name="登记日期"		nullable="false"/>
		<item type="date" 	id="remove_date"	name="除权日期"		nullable="false"/>
		<item type="text" 	id="pg_code"		name="权证代码"/>
		<item type="date" 	id="ss_date"		name="红股上市日" default="null"/>
		<item type="date" 	id="pg_arrive_date"	name="配股权到账日" default="null"/>	
		<item type="select" id="tax_unit"		name="代扣税主体" source="736" nullable="true" default="2"/>
		<item type="text" 	id="every_bonus_10_after_tax"		name="每10股税后红利"	 number="true" nullable="false"/>
	</group>   
	</input>
	<service type="procedure" id="p70040722"  process="822" style="1"> 
		<param id="user_id"			type="integer" way="in"/>
		<param id="product_code"	type="string"	/>
		<param id="market_code"		type="string"	/>
		<param id="send_off_10"		type="float"  way="in"/> 
		<param id="send_qz_10"		type="float"  way="in"/> 
		<param id="allot_10"		type="float"  way="in"/>  	 
		<param id="every_bonus_10"		type="float"  way="in"/>  
		<param id="register_date"	type="string"  way="in"/>  
		<param id="remove_date"		type="string"  way="in"/>
		<param id="pg_code"			type="string"  way="in"/>
		<param id="ss_date"			type="string"  way="in"/>
		<param id="pg_arrive_date"	type="string"  way="in"/> 
		<param id="tax_unit"		type="string"  way="in"/>
		<param id="every_bonus_10_after_tax"		type="float"  way="in"/>
		<param id="bonus_arrive"	type="string"  way="in"/>
	</service>
	<output/>
</function>