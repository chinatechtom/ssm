<?xml version="1.0" encoding="gbk" ?>
<function id="304261" name="机构客户评级因子设置" >	
	<input> 
		<item type="text" id="cust_code" name="客户代码" editable="false"/> 
		<item type="text" id="cust_name" name="客户姓名" editable="false"/>		
		<item type="hidden" id="cust_type" name="客户类型"/>		
		<item type="hidden"     id="paraid"             action="32311" default="lrmmjy"/>
		<item type="hidden"       id="paravalue"         name="是否需要校验密码" default="0"/>
		<item type="hidden"	id="cust_id"/>
		<group name="密码校验">
			<item type="password"   id="chk_cust_pwd" 	name="交易密码"  pwdtype="2"  point="cust_code"	filter="paravalue=1"/>
		</group >
		<group name="基本情况">		
			<item type="hidden"	id="ratingfactor_sort_201"  default="201"/>
			<item type="hidden"	id="ratingfactor_sort_204"  default="204"/>
			<item type="hidden"	id="ratingfactor_sort_210"  default="210"/>
			<item type="radio"	id="data_code_201"	filter="ratingfactor_sort_201"	name="成立时间"           	source="95000007" />
			<item type="radio"	id="data_code_204"	filter="ratingfactor_sort_204"	name="规模"           		source="95000007" />
			<!--item type="radio"	id="data_code_210"	filter="ratingfactor_sort_210"	name="是否上市公司"         source="95000007" /-->
		</group>
		<group name="财务状况">			
			<item type="hidden"	id="ratingfactor_sort_213"  default="213"/>
			<item type="hidden"	id="ratingfactor_sort_216"  default="216"/>
			<item type="hidden"	id="ratingfactor_sort_264"  default="264"/>
			<item type="hidden"	id="ratingfactor_sort_265"  default="265"/>
			
			<item type="radio"	id="data_code_265"	filter="ratingfactor_sort_265"	name="财务报表提供情况"	source="95000007" />	
			<item type="radio"	id="data_code_213"	filter="ratingfactor_sort_213"	name="上年度净资产收益率"	source="95000007" />
			<item type="radio"	id="data_code_216"	filter="ratingfactor_sort_216"	name="最近一年资产负债率"	source="95000007" />
			<item type="radio"	id="data_code_264"	filter="ratingfactor_sort_264"	name="金融资产"         	source="95000007" />
			
		</group>	
		<group name="证券投资(普通账户)">
            <item type="hidden"	id="ratingfactor_sort_240"  default="240"/>
			<item type="hidden"	id="ratingfactor_sort_234"  default="234"/>		
			<item type="hidden"	id="ratingfactor_sort_245"  default="245"/>		
			
			<item type="radio"	id="data_code_240"	filter="ratingfactor_sort_240"	name="账户绝对收益水平(一年又一期平均年化收益率)"         source="95000007" />
			<item type="radio"	id="data_code_234"	filter="ratingfactor_sort_234"	name="投资经验"         source="95000007" />	
			<item type="radio"	id="data_code_245"	filter="ratingfactor_sort_245"	name="日均资产规模"     source="95000007" />
		</group>	
		<group name="信用记录">	
			<item type="hidden"	id="ratingfactor_sort_259"  default="259"/>
			<item type="hidden"	id="ratingfactor_sort_260"  default="260"/>	
			<item type="radio"	id="data_code_260"	filter="ratingfactor_sort_260"	name="考试成绩"         source="95000007" />
			<item type="radio"	id="data_code_259"	filter="ratingfactor_sort_259"	name="其他机构记录"     source="95000007" />
		</group>
		<group name="风险偏好">	
      <item type="hidden"	id="ratingfactor_sort_277"  default="277"/>
			<item type="radio"	id="data_code_277"	filter="ratingfactor_sort_277"	name="风险评测级别" source="95000007" nullable="false"/>
		</group>
	</input>
	<service type="procedure" id="304261"  process="888" style="1"> 
	  <param type="string" id="function_id" default="3042602"/>
	  <param type="string" id="function_id" default="3042601"/>
	  <param type="string" id="function_id" default="304262"/>
	</service>
	<output/>	
</function>