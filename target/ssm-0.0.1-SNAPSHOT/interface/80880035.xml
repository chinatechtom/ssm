<?xml version="1.0" encoding="gbk" ?>
<function id="80880035" name="质押直接扣款-扣款情况查询">
	<input>
		<item type="select" id="institution_id" name="营业部代码" source="95000008"/>
		<item type="text"	id="cust_code"	name="客户代码" />
		<item type="text"	id="cust_name"	name="客户姓名"/>
		<item type="text"	id="sbond_no"	name="交易协议书编号"/>		
		<item type="select"	id="if_full_deducted"	name="是否已足额扣款" source="1001"/>
	</input>
	<service type="procedure" id="P60880035" process="933" style="77">
		<param type="integer" id="user_id"/>
		<param type="integer" id="institution_id"/>
		<param type="string"  id="cust_code"/>
		<param type="string"  id="cust_name"/>
		<param type="string"  id="sbond_no"/>
		<param type="string"  id="if_full_deducted"/>
	</service>
	<output export="true">		
		<column type="string" id="institution_code"   name="营业部代码"/>
		<column type="string" id="institution_name"   name="营业部名称"/>		
		<column type="string" id="cust_code"   name="客户代码" />
		<column type="string" id="cust_name"   name="客户姓名" />		
		<column type="string" id="sbond_no"   name="交易协议书编号"/>
		<column type="string" id="capital"   name="本金"/>
		<column type="string" id="interest"   name="利息"/>
		<column type="string" id="penalty_interest"   name="违约金"/>
		<column type="string" id="capital_deducted"   name="已扣本金"/>
		<column type="string" id="interest_deducted"   name="已扣利息"/>
		<column type="string" id="penalty_deducted"   name="已扣违约金"/>
		<column type="string" id="capital_rest"   name="剩余待扣本金"/>
		<column type="string" id="interest_rest"   name="剩余待扣利息"/>
		<column type="string" id="penalty_interest_rest"   name="剩余待扣违约金"/>
		<column type="string" id="all_rest"   name="总剩余待扣款额"/>
		<column type="string" id="last_deduct_date"   name="最近扣款时间"/>
		<column type="string" id="if_full_deducted"   name="是否已足额扣款"/>
	</output>
</function>