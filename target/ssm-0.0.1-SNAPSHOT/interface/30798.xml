<?xml version="1.0" encoding="gbk"?>
<function id="30798" name="资产评估-在公司内净资产总值获取">
	<input>
		<item type="hidden" id="order_id"  name="合约id"/>
	</input>
	<service type="procedure" id="p_fund_net_asset_query" implement="p_fund_net_asset_query30798" process="911" style="77">
		<param type="integer" id="user_id" way="in"/>
		<param type="integer" id="order_id" way="in"/>
	</service>
	<output>
		<column type="number" id="total_asset"        name="总资产"/>
		<column type="number" id="fund_amt"           name="资金金额"/>
		<column type="number" id="stock_market_value" name="证券市值"/>
		<column type="number" id="stock_convert_amt"  name="证券折算金额"/>
		<column type="number" id="gage_value"         name="担保品折算价格"/>
		<column type="number" id="credit_num"         name="信用上限"/>
		<column type="number" id="total_debt"         name="总负债"/>
		<column type="number" id="financing_amt"      name="实际融资总额"/>
		<column type="number" id="borrow_stock_amt"   name="实际融券总额"/>
		<column type="number" id="fee_all"            name="总费用"/>
		<column type="number" id="guan_rate"          name="维持担保比例"/>
		<column type="number" id="common_fund_amt"     name="普通A股账户资产总值"/>
		<column type="number" id="other_fund_amt"     name="其他金融资产总值"/>
		<column type="number" id="all_debt_amt"       name="回购等负债总值"/>
		<column type="number" id="net_asset_amt"      name="在公司内净资产总值"/>
		<column type="string" id="if_allow"           name="是否允许"       display="false"/>
		<column type="string" id="if_blacklist"       name="是否黑名单"     display="false"/>
		<column type="string" id="if_allow_name"      name="是否允许"       display="false"/>
		<column type="string" id="if_blacklist"       name="是否黑名单"     display="false"/>
	</output>
</function>