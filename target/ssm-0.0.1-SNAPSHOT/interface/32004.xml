<?xml version="1.0" encoding="gbk" ?>
<function id="32004" name="负债明细表" >
  <input>	
  <item type="select" id="institution_id" name="营业部名称" source="95000008"/>
	<item type="text" id="cust_code" name="客户代码" />
	<item type="hidden" id="acc_code" name="信誉账户" />	
  </input>
  <service type="procedure" id="p_business_report_32004_query" process="911" style="77">
	<param id="user_id" 		type="integer"/>
	<param id="cust_code" 		type="string"/>
	<param id="acc_code" 		type="string"/>	
	<param id="institution_id" 	type="string"/>
  </service>
  <output export="true" print="true">    
  	<column type="string" id="c1"   name="营业部名称"  />
    <column type="string" id="c2"   name="客户代码"  />
		<column type="string" id="c3"   name="客户姓名"  />
		<column type="number" id="c4"   name="授信额度"  precision="2" total="true"/>
    <column type="number" id="financing_value"   name="总融资"  precision="2" total="true"/>
    <column type="number" id="borrow_stock_value"   name="融券市值" precision="2"  total="true"/>
    <column type="number" id="borrow_stock_rate"   name="融券占总负债比例" precision="2"  />
    <column type="number" id="c7"   name="应付融资利息"  precision="2" total="true"/>
    <column type="number" id="c8"   name="应付融券费用"  precision="2" total="true"/>
    <column type="number" id="c9"   name="应付其它费用"  precision="2" total="true"/> 
    <column type="number" id="c10"   name="利息费用小计"  precision="2" total="true"/> 
    <column type="number" id="c11"   name="维持担保比例"  precision="4" />
    <column type="number" id="c12"   name="合同剩余到期时间(天)"  />
	</output> 	
</function>