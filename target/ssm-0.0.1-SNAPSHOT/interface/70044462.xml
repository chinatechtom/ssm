<?xml version="1.0" encoding="gbk" ?>
<function id="70044462" name="修改合同紧急联系人申请处理">
  <input print="70044462.jsp">
	<group name="合同信息" >
		<item type="hidden" id="cust_id"  	name="客户id"/>			
		<item type="text" 	id="bond_id" 	name="合同id" 	editable="false"/>
		<item type="text"	id="cust_name"	name="客户姓名" editable="false"/>
		<item type="text" 	id="cust_code"	name="客户代码" editable="false"/> 			
	</group>
	<item type="hidden" id="order_id"	name="合约ID" action="70044562"/>			
	<item type="hidden" id="date" 		name="打印日期"/>
	<item type="hidden" id="user_code"	name="打印柜员代码"/>
	<item type="hidden" id="user_name"	name="打印柜员名称"/>
	<item type="hidden" id="company"	name="券商名称"/>	
	<group name="原紧急联系人信息">
	  <item type="text" id="emerg_contacts_man_old"		name="紧急联系人" editable="false"/>
	  <item type="text" id="emerg_contacts_tel_old"		name="紧急联系人电话" editable="false"/>
	</group>
	<group name="变更急联系人信息">
	  <item type="text" id="emerg_contacts_man"		name="紧急联系人" nullable="false"/>
	  <item type="text" id="emerg_contacts_tel"		name="紧急联系人电话" nullable="false"/>	
	</group>
	<group name="意见">
	  <item type="textarea" id ="remark"			name="申请意见" editable="false"/>
	  <item type="textarea" id ="manage_opinion" 	name="处理意见"/>
	</group>
  </input>
  <service type="procedure" id="P70044462" process="822" style="1">
	<param type="integer" 	id="user_id"/>
	<param type="integer" 	id="cust_id"/>
	<param type="integer" 	id="order_id"/>
	<param type="integer" 	id="bond_id"/>
	<param type="string" 	id="emerg_contacts_man"/>	
	<param type="string" 	id="emerg_contacts_tel"/>
	<param type="string"  	id="manage_opinion"/>
  </service>
  <output/>
</function>