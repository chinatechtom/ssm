<?xml version="1.0" encoding="gbk" ?>
<function id="3139002" name="清算后备份还原">	
	<input>
		<description>是否进行清算后备份还原?
		</description>
		<item type="text"	id="ddate"	name="还原日期" editable="false"/>	
		<item type="text"	id="backup_file_name"	name="备份名称" editable="false"/>	
	</input>		
	<service type="procedure" id="p_database_restore"   process="811" style="1">
		<param id="user_id"		        type="integer"/>	
		<param id="ddate"		        type="string"  />	
		<param id="resotre_point"		type="string"  default="2"/>
		<param id="backup_file_name"	type="string"  />		
	</service>
</function>