<?xml version="1.0" encoding="gbk" ?>
<function id="3100605" name="信用类―客户资产统计明细表（融资）" >  
  <input>	
	<item type="select" id="institution_id" name="营业部名称"  source="95000008"/>
	<item type="hidden" id="cust_code" name="客户代码" />
	<item type="text" id="stock_code" name="证券代码" />
	<item type="hidden" id="date" name="日期"  default="now"/>
  </input>
   <service type="procedure" id="p_statistics_3100605_query" process="911" style="77">
	<param id="user_id" type="integer" way="in" /> 
	<param id="date"/>
	<param id="institution_id"/>
	<param id="cust_code" />
	<param id="stock_code" />
  </service>  
  <output  print="true">
	<column type="string" id="c2" name="客户代码"/>			
	<column type="string" id="c3" name="信用账户"/>	
	<column type="string" id="c1" name="统计日期"/>
	<column type="number" id="c4" name="融资金额" precision="2"/>
	<column type="string" id="c5" name="交易市场"/>
	<column type="string" id="c6" name="证券类别"/>
	<column type="string" id="c7" name="证券代码"/>
	<column type="string" id="c8" name="证券名称"/>	
	<column type="number" id="c9" name="持仓数量" />
	<column type="number" id="c10" name="证券市值" precision="2"/>
  </output>
</function>