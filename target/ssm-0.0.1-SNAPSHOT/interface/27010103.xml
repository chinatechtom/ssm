<?xml version="1.0" encoding="gbk" ?>
<function id="27010103" name="担保品集中度查询">
	<input>
		<item type="hidden"  id="cust_id"	            name="客户id" />
		<item type="text"    id="product_code"          name="证券代码" />
		<item type="text"    id="product_name"          name="证券名称" />
		<item type="date"    id="begin_date"	        name="起始日期" default="null"/>	
		<item type="date"    id="end_date"	            name="终止日期" default="null"/>	
		
	</input>
    <service type="procedure" id="P27010103"  process="911" style="77">
		<param id="user_id"	        type="integer" way="in"/> 
		<param id="product_code"    type="string"  way="in"/>
		<param id="product_name"    type="string"  way="in"/>
		<param id="begin_date"		type="string"  way="in"/>
		<param id="end_date"		type="string"  way="in"/>
    </service>
    <output export="true" autoload="false" print="true">
		<column type="string" id="product_id"		name="证券id"		display = "false"/>
		<column type="string" id="product_code"		name="证券代码"/>
		<column type="string" id="product_name"		name="证券名称"/>
		<column type="number" id="total_guarantee_value"		name="公司总担保品市值" precision="2"/>
		<column type="number" id="asset_value"		name="该证券总市值"		precision="2"/>
		<column type="number" id="circulated_value"	name="该证券流通市值"	precision="2"/>
		<column type="number" id="avg_trade_qty"	name="该证券上月日均成交量"		precision="2"/>
		<group name="担保品集中度">
			<column type="number" id="guarantee_value"	name="担保品市值"		precision="2" />
			<column type="number" id="guarantee_value_percent"	name="该担保品市值占总公司担保品市值比例%" precision="6"/>
			<column type="number" id="asset_value_percent"		name="该担保品市值占该证券总市值比例%" precision="6"/>
			<column type="number" id="circulated_value_percent"	name="该担保品市值占该证券流通市值比例%" precision="6"/>
			<column type="number" id="avg_trade_qty_percent"	name="该担保品市值占该证券上月日均成交量比例%" precision="6"/>
		</group>
    </output>
</function>
