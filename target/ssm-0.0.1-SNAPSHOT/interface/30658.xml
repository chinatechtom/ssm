<?xml version="1.0" encoding="gbk" ?>
<function id="30658" name="查询合约" >
	<input>    
	 <item type="hidden"  id="cust_id"              name="客户id"/> 
	 <item type="text"    id="cust_code"  name="客户代码"/> 
	 <item type="text"    id="cust_name"  name="客户姓名"/>
	 <item type="text"    id="cert_no"  name="证件号码"/> 
	 <item type="hidden"  id="order_id"	            name="合约id"/> 
	 <item type="text"    id="order_no"	            name="合约编号"/> 
	 <item type="hidden"    id="formerly_voucher_no"  name="原始凭证号"/> 
  </input>
  <service type="procedure" id="p_order_1_query" process="911" style="77">
	<param id="user_id"				type="integer" way="in"/> 
	<param id="cust_id"				type="ingeter" way="in"/>
	<param id="cust_code"				type="string" way="in"/>
	<param id="cust_name"				type="string" way="in"/>
	<param id="cert_no"				type="string" way="in"/>
    <param id="order_id"			type="integer" way="in"/>
    <param id="order_no"			type="string"  way="in"/>
	<param id="formerly_voucher_no" type="string"  way="in"/>
  </service>
  <output export="true">
	<column type="number" id="order_type"	            name="合约类型" display="false"/>
	<column type="string" id="order_type_name"			name="合约类型名称"/>
	<column type="number" id="manage_status"			name="处理状态" display="false"/>
    <column type="string" id="manage_status_name"		name="处理状态名称"/>
	<column type="string" id="date"						name="日期"/>
	<column type="string" id="time"						name="时间"/>
	<column type="number" id="cust_id"					name="客户id"	display="false"/>
	<column type="string" id="cust_name"	       		name="客户姓名"/>
	<column type="string" id="order_id" name="合约id" display="false"/>
	<column type="string" id="order_no"	name="合约编号"/>
	<column type="string" id="formerly_voucher_no" name="原始凭证号" display="false"/>
	<column type="number" id="product_id"	       		name="产品号" display="false"/>
	<column type="string" id="product_code"	       		name="产品代码"/>
	<column type="string" id="product_cyc"	       		name="产品周期"/>
	<column type="string" id="begin_time"				name="起始日期"/>
	<column type="string" id="return_time"				name="到期日期"/>
  </output> 
</function>