<?xml version="1.0" encoding="gbk" ?>
<function id="70044440" name="处理合同签署申请" >
  <input>	
	<item type="hidden" id="order_id" name="合约id" action="70030673"/>
	<item type="hidden" id="cust_id" name="客户id1" action="70010430"/>
  <group name="合约基本信息" >
	<item type="text"   id ="cust_name" name="客户姓名" editable="false"/>
	<item type="text"   id ="cust_code" name="客户代码" editable="false"/>
	<item type="text"   id ="cert_type" name="证件类型" editable="false"/>
	<item type="text"   id ="cert_no" name="证件号码" editable="false"/>  	
	<item type="text" 	id="order_no" name="合约编号" editable="false"/>
	<item type="text" id="formerly_voucher_no" name="合同编号"  editable="false"/>
	<item type="text" 	id="currency_type_name" name="货币类型"  editable="false"/>
	<item type="text" 	id="institution_name" name="营业部名称"  editable="false"/>
	<item type="hidden" id="availability_time" name="有效日期"   editable="false"/>
	<item type="hidden" id="begin_time" name="起始日期"  editable="false"/>
	<item type="hidden" id="return_time" name="到期日期" editable="false"/>
	<item type="text" 	id="app_years" name="申请年数" editable="false"/>
	<item type="text" id="agent_name" name="代理人姓名" editable="false"/>
	<item type="text" id="agent_cert_type" name="代理人证件类型" editable="false"/>
	<item type="text" id="agent_cert_no" name="代理人证件号码" editable="false"/>
	<item type="date"   id="over_time" name="客户协议签署日期" editable="false"/>	
	<item type="hidden" id="remark1" name="备注说明"  editable="false"/>
  </group>
  <group name="联系资料">	
	<item type="text" id="move_tel" name="移动电话"  editable="false"/>
	<item type="text" id="fix_tel" name="固定电话"  editable="false"/>
	<item type="hidden" id="meet_tel" name="应急电话"  editable="false"/>
	<item type="text" id="family_address" name="联系地址"  editable="false"/>
	<item type="text" id="email" name="电子邮件" editable="false"/>
	<item type="text" id="ps_code" name="邮政编码" editable="false"/>	
	<item type="text" id="fax" name="传    真" editable="false"/>
	<item type="text" id="find_mode_name" name="主联系方式"  editable="false"/>
	<item type="text" id="emerg_contacts_man" name="应急联系人"/>
	<item type="text" id="emerg_contacts_tel" name="应急联系人电话"/>	
  </group>   
  <group name="自然情况" scope="group">
	<item type="grid" id="cust_id" source="70030422" name="自然情况"/>	
	<item type="text" id="total_score" name="自然情况总分"  editable="false" default="0.0"/>	
  </group>
  <!--<group name="客户征信信息" scope="group">
	<item type="text" id="credit_level_name" name="信用级别"   editable="false"/>
  </group>-->
  <!--
  <group name="客户授信信息">
	<item type="text" id="credit_num"	name="信用上限"	number="true" editable="false" />
	<item type="text" id="credit_limit"	name="最大交易额度"	number="true" editable="false" />
  </group>
  -->
  <group name="处理意见">
	<item type="textarea" id="notes" name="处理意见"/>
  </group>
  </input>  
  <service type="procedure" id="P70044440" process="822" style="1">
	<param type="integer" id="user_id"/>
	<param type="integer" id="order_id"/>
	<param type="string"  id="notes"/>
  </service>
  <output/>
</function>
