<?xml version="1.0" encoding="gbk" ?>
<function id="30209" name="机构资金查询" >
	<input>			
		<item type="hidden"  id="institution_id"	    name="营业部名称" source="95000008"/>
		<item type="hidden"  id="acc_id"	            name="机构账户id"/>
		<item type="text"  id="acc_code"				name="资金账户"/>		
	</input>
    <service type="procedure" id="p_institution_acc_money_query"  implement="p_institution_acc_money_query30209"  process="911" style="77">
		<param id="user_id"	            type="integer" way="in"/> 
		<param id="institution_id"	    type="integer" way="in"/> 
		<param id="acc_id"	            type="integer" way="in"/>
		<param id="acc_code"	        type="string"  way="in"/>		
    </service>
    <output export="true">
		<column type="string" id="institution_name"			name="营业部名称" display="false"/>
		<column type="number" id="acc_id"					name="账户id"		display="false"/>
		<column type="string" id="acc_code"					name="资金账户"/>
		<column type="string" id="date"						name="日期"			display="false"/>
		<column type="string" id="property_type"			name="资产种类"		display="false"/>
		<column type="string" id="currency_type"			name="货币类型"/>
		<column type="number" id="fund_amt"					name="资金余额"	    precision="2" total="true"/>
		<column type="number" id="deposit"					name="当日转入金额"	precision="2" total="true"/>
		<column type="number" id="withdraw"					name="当日转出金额"	precision="2" total="true"/>
		<column type="number" id="frozen_amt"				name="冻结资金"	    precision="2" display="false"/>
		<column type="number" id="abnormal_frozen_amt"		name="异常冻结资金"	display="false"/>
		<column type="number" id="real_buy_amt"				name="融资买入金额"	precision="2" total="true"/>
		<column type="number" id="buy_frozen_amt"			name="买入冻结金额"	precision="2" total="true" display="false"/>
		<column type="number" id="sell_done_amt"			name="卖券还款金额"	precision="2" total="true"/>
		<column type="number" id="un_used_fix_qty"			name="未使用固定融资上限总和"	precision="2" total="true" display="false"/>
		<column type="number" id="un_frozen_amt"			name="未冻结款余额"	            precision="2" total="true" display="false"/>
		<column type="number" id="overdue_money"			name="逾期融出资金余额"	    precision="2" total="true"/>
		<column type="number" id="unoverdue_money"			name="未逾期融出资金余额"	precision="2" total="true"/>
		<column type="number" id="manual_frzn_amt"			name="手工冻结金额"	precision="2" display="false"/>
		<column type="number" id="manual_unfrzn_amt"		name="手工解冻金额"	precision="2" display="false"/>
		<column type="number" id="credit"					name="信用额"		precision="2" display="false"/>
		<column type="number" id="forbid_amt"				name="禁用资金"	    precision="2" display="false"/>
		<column type="number" id="forbid_asset"				name="禁取资产"	    precision="2" display="false"/>
		<column type="number" id="interest"					name="利息"			precision="2" display="false"/>
		<column type="number" id="interest_count"			name="利息积数"	    precision="2" display="false"/>
		<column type="number" id="punish_interest"			name="罚息"			precision="2" display="false"/>
		<column type="number" id="punish_interest_count"	name="罚息积数"	    precision="2" display="false"/>
		<column type="number" id="user_id"					name="操作员id"	    display="false"/>
		<column type="string" id="time"						name="时间戳"        display="false"/>
		<column type="number" id="all_credit_amt"			name="可申请总额度"	    precision="2" display="false"/>
		<column type="number" id="credit_amt_used"			name="融资申请已用额度"	precision="2" display="false"/>
    </output>
</function>
