<?xml version="1.0" encoding="gbk" ?>
<function id="27010115" name="融资融券持仓量调查" >
  <service type="procedure" id="P27010115"  process="911" style="77">
      <param id="user_id"               type="integer" way="in"/> 
   </service>
 <output print="true">
        <column type="number" id="row_id"   name="序号"/>
        <column type="string" id="product_code" name="证券代码"/>        
        <column type="string" id="product_name"   name="证券名称" />
        <column type="number" id="fin_total" name="市场上的融资总量"   precision="2"/>   
        <column type="number" id="bs_total"   name="市场上的融券总量"   precision="2"/>
        <column type="number" id="mini" name="轧差"           precision="2"/>  
        <column type="number" id="fin_ht" name="公司融资余额"           precision="2"/>  
        <column type="number" id="bs_ht" name="公司融券余额"           precision="2"/>  
        <column type="number" id="rate1" name="公司融资余额占市场比例"           precision="6"/>     
        <column type="number" id="rate2" name="公司融资余额占市场比例"           precision="6"/>     
</output>
</function>  