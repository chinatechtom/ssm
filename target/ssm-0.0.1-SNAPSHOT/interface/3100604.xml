<?xml version="1.0" encoding="gbk" ?>
<function id="3100604" name="信用类―客户资产统计汇总表" >  
  <input>	
	<item type="select" id="institution_id" name="营业部名称"  source="95000008"/>
	<item type="hidden" id="cust_code" name="客户代码" />
	<item type="hidden" id="date" name="日期"  default="now"/>
  </input>
   <service type="procedure" id="p_statistics_3100604_query" process="911" style="77">
	<param id="user_id" type="integer" way="in" /> 
	<param id="date"/>
	<param id="institution_id"/>
	<param id="cust_code" />
  </service>  
  <output  print="true"> 
	<column type="string" id="c2" name="客户代码"/>	
	<column type="string" id="c1" name="统计日期"/>
	<column type="number" id="c3" name="资产总额" precision="2"/>
	<column type="number" id="c4" name="信用上限" precision="2"/>
	<column type="number" id="c5" name="融资金额" precision="2"/>
	<column type="number" id="c6" name="融资相关总资产" precision="2"/>
	<column type="number" id="c7" name="融券价值" precision="2"/>	
	<column type="number" id="c8" name="融券相关总资产" precision="2"/>
	<column type="number" id="c9" name="维持担保比例" precision="2"/>
	<column type="number" id="c10" name="信用剩余期限" precision="2"/>
  </output>
</function>