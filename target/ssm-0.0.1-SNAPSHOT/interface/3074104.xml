<?xml version="1.0" encoding="gbk" ?>
<function id="3074104" name="设置信用违约资料">
	<description>席位代码提交时不校验,请谨慎操作!</description>
	<input>
		<item type="hidden" id="record_id"	name="记录id"/>
		<item type="text"   id="wyxwdm"		name="席位代码"			maxlen="6"			nullable="false"/>
		<item type="text"   id="wyzqmc"		name="券商名称"			maxlen="40"			nullable="false"/>
		<item type="select"	id="wyscdm"		name="市场"				source="039"		nullable="false"/>
		<item type="text"   id="wysfzh"		name="身份证号"			maxlen="30"			nullable="false"/>
		<item type="text"   id="wygddm"		name="违约者证券账户"	maxlen="12"			nullable="false"/>
		<item type="text"   id="wygdxm"		name="违约者姓名"		maxlen="60"			nullable="false"/>
		<item type="text"   id="wywyje"		name="违约金额"			number="true"		nullable="false"/>
		<item type="select" id="wywylb"		name="违约类别"			source="327"		nullable="false"/>
		<item type="select" id="wysblb"		name="申报类别"			source="328"		nullable="false"/>
		<item type="date"   id="wysbrq"		name="申报日期"			default="now"		nullable="false"/>
		<item type="hidden" id="wymark"		name="备用"/>
	</input>
	<service type="procedure" id="p_credit_violation_set"  process="811" style="1"> 
		<param id="user_id"		type="integer"	way="in"/>
		<param id="record_id"   type="integer"	way="in"/>
		<param id="wyxwdm"		type="string"	way="in"/>
		<param id="wyzqmc"		type="string"	way="in"/>
		<param id="wygddm"		type="string"	way="in"/>
		<param id="wygdxm"		type="string"	way="in"/>
		<param id="wysfzh"		type="string"	way="in"/>
		<param id="wywyje"		type="double"	way="in"/>
		<param id="wywylb"		type="string"	way="in"/>
		<param id="wysblb"		type="string"	way="in"/>
		<param id="wysbrq"		type="string"	way="in"/>
		<param id="wymark"		type="string"	way="in"/>
		<param id="wyscdm"		type="string"	way="in"/>
	</service>
	<output/>
</function>