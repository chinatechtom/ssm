<?xml version="1.0" encoding="gbk" ?>
<function id="36000082" name="8.2.3融券卖出证券集中度统计" >  
  <input>
 <item type="date"   id="date"           name="日期"  default="now"/>
 <item type="select"  id="order_choice"    name="排序选择"  source="377"/>
  </input>
   <service type="procedure" id="P36000082" process="911" style="77">
 <param id="user_id" type="integer" way="in" /> 
 <param id="date"    type="string" way="in" />
 <param id="order_choice"     type="string"  way="in"/>
  </service> 
  <output  print="true">
    <column type="number" id="id"                 name="序号"/>
    <column type="string" id="product_code"       name="证券代码"/> 
    <column type="string" id="product_name"       name="证券名称"/>  
   <column type="number" id="bs_stock_num"      name="客户持有数量" total="true" precision="0"/>
   <column type="number" id="circulated_a"       name="流通股数量"   total="true"     precision="0"  />
    <column type="number" id="circulated_rate"    name="占流通股比例"  precision="6" total="true"/> 
   <column type="number" id="all_stock"          name="总股本"  total="true" precision="0"/>
    <column type="number" id="all_stock_rate"     name="占总股本比例"  precision="6" total="true"/> 
  </output>
</function>