<?xml version="1.0" encoding="gbk" ?>
<function id="70010501" name="客户最大交易额度报告书查询" >
  <input>	
	<item type="select" id="insitution_id" name="营业部名称" source="97000001"/>
	<item type="hidden" id="cust_id" name="客户号"/>
	<item type="text" id="cust_code" name="客户代码" />
	<item type="text" id="cust_name" name="客户姓名" />
	<item type="text" id="cert_no" name="证件号码" />
	<item type="hidden" id="order_id" name="合约id"/>
  </input>
  <service type="procedure" id="p70010501" process="922" style="77">
	<param id="user_id" type="integer"/>
	<param id="insitution_id" type="integer"/>
	<param id="cust_id" type="integer"/>	
	<param id="cust_code" type="string"/>
	<param id="cust_name" type="string"/>
	<param id="cert_no" type="string"/>
	<param id="order_id" type="integer"/>
  </service>
  <output>
	<column type="string" id="institution_name" name="营业部名称"/>
	<column type="string" id="institution_code" name="营业部代码"/> 
  	<column type="string" id="cust_code" name="客户代码"/>
	<column type="string" id="xy_cust_code" name="信用客户代码"/>
    <column type="string" id="cust_name" name="客户姓名"/>
	<column type="string" id="full_name" name="客户全称"/>
    <column type="string" id="cust_type" name="客户类型"/>
	<column type="string" id="cert_type" name="证件类型"/>
	<column type="string" id="cert_no" name="证件号码"/>
	<column type="number" id="cust_num" name="客户净资产(万元)" precision="4"/>
	<column type="number" id="pt_asset" name="客户普通账户总资产(万元)" precision="4"/>
	<column type="number" id="xy_asset" name="客户信用账户总资产(万元)" precision="4"/>
	<column type="number" id="xy_debt" name="客户信用账户总负债(万元)" precision="4"/>
	<column type="number" id="credit_num_rate" name="最大交易额度系数(%)" precision="2"/>
	<column type="number" id="credit_calc" name="资产乘额度系数(万元)" precision="4"/>	
	<column type="number" id="approve_num" name="审批最大交易额度(万元)" precision="4"/>
	<column type="number" id="total_score" name="征信总分" precision="2"/>
	<column type="string" id="credit_level_code" name="信用级别代码"/>	
	<column type="string" id="credit_level_name" name="信用级别名称"/>
	<column type="number" id="fund_credit" name="公司授权规模(万元)" precision="4"/>
	<column type="number" id="one_max" name="单客户最大交易额度(万元)" precision="4"/>	
	<column type="number" id="total_credit" name="已发放额度(万元)" precision="4"/>
	<column type="number" id="total_used" name="已使用额度(万元)" precision="4"/>
	<column type="number" id="total_unused" name="未使用额度(万元)" precision="4"/>
	<column type="number" id="credit_min" name="计算最大交易额度(万元)" precision="4"/>
	<column type="string" id="request_date" name="申请日期前一天"/>
	<column type="string" id="system_date" name="系统日期"/>
	<column type="string" id="asset_date" name="资产日期"/>
	<column type="string" id="active_date" name="评级生效日期"/>
	<column type="string" id="credit_type" name="授信类型"/>
	<column type="string" id="begin_date" name="额度开始日期"/>
	<column type="string" id="end_date" name="额度结束日期"/>
	<column type="string" id="remark" name="备注"/>
	<column type="string" id="company" name="公司名称" display="false"/>
	<button type="edit" id="edit" name="打印" action="70010504" multiple="0"/>
  </output>
</function>