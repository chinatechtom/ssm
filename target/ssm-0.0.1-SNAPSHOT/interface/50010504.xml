<?xml version="1.0" encoding="gbk" ?>
<function id="50010504" name="融资行权参数删除" >
<description>
		确定删除？
</description> 
	<input>	
	<item type="hidden" 	  id="company_no"   	name="上市公司编号" />
    <item type="hidden" 	  id="plan_no"   		name="激励计划编号"/>
	<item type="hidden" 	  id="batch_no"     	name="批次号"/>
    <item type="hidden" 	  id="cust_no"   		name="客户号"/>	
	<item type="hidden" 	  id="interest_rate"	name="行权融资利率（%）" />
	<item type="hidden"	 	  id="gpzy_rate"	    name="质押融资利率（%）" />
	<item type="hidden"	 	  id="comm_rate"	    name="质押佣金率" />
	<item type="hidden" 	  id="days"	 			name="自动质押宽限期限" />
	<item type="hidden" 	  id="gpzy_days"	 	name="默认股票质押期限" />
	<item type="hidden"	 	  id="employee_type"	name="激励员工类型"  source="950" nullable = "false"  default="0" />
	<item type="hidden" 	  id="touch_rate"	 	name="预警履约保障比例（%）" />
	<item type="hidden" 	  id="lowest_rate"	 	name="最低履约保障比例（%）" />
	<item type="hidden" 	  id="loan_rate"	 	name="最高融资比例（%）" />
	<item type="hidden"	      id="status"           name="批次状态"  source="955" nullable = "false" default="1"/>
	<item type="hidden"   	  id="func_id" 				 action="8801" 	default="50010504"/>
	<item type="hidden"   	  id="func_need_check" 		 name="是否支持复核" 	  default="0"/>		
		<group name="复核">   
    <item type="text" 		id="chk_user_code" 		 name="复核人员" filter="func_need_check=1"/>
    <item type="password"   id="chkuser_pwd" 		 name="密码" 	filter="func_need_check=1"/>
    </group>
	</input>   
	<service type="procedure" id="P50010502"  implement="P50010502_50010504"  process="866" style="1"> 
		<param id="user_id"			type="integer" way="in"/>
		<param id="func_id"			type="integer" way="in"/>
		<param id="chk_user_code"	type="string"  way="in"/>
		<param id="chkuser_pwd"	  	type="string"  way="in"/>
		<param id="company_no" 		type="string"  way="in"/>
		<param id="plan_no"   		type="string"  way="in"/>
		<param id="batch_no"     	type="integer" way="in"/>
		<param id="cust_no"   		type="string" way="in"/>
		<param id="interest_rate"	type="float"  way="in"/> 
		<param id="gpzy_rate"	   type="float"  way="in"/> 
		<param id="comm_rate"	   type="float"  way="in"/> 
		<param id="days"		   type="integer"  way="in"/> 
		<param id="gpzy_days"		type="integer"  way="in"/> 
		<param id="employee_type"	type="string"  way="in"/> 		
		<param id="touch_rate"		type="float"  way="in"/>  	
		<param id="lowest_rate"	    type="float"  way="in"/> 
		<param id="loan_rate"		type="float"  way="in"/> 
		<param id="status"	        type="string"  way="in"/> 	
		<param id="operation"       type="integer" default="3"/> <!--删除-->		
	</service> 
	<output/>  
</function>    