	<service type="procedure" id="P60010502" process="833" style="1">
		<param type="integer" id="user_id" way="in"/>
		<param type="integer" id="sbond_id" way="in"/>
		<param type="string" id="violate_type" way="in"/>
		<param type="string" id="penalty_fee_type" way="in"/>
		<param type="string" id="begin_date" way="in"/>
		<param type="string" id="end_date" way="in"/>
		<param type="string" id="remark1" way="in"/>
	</service>
	<output/>
</function>