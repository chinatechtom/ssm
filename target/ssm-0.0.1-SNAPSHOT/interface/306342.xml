	</input>
	<service type="procedure" id="p_follow_valuation_score_select" process="911" style="77">
		<param id="user_id"		type="integer"/>
		<param id="cust_code"	type="string"/>
	</service>
	<output>
		<column type="string"	id="cust_code"			name="客户代码"/>
		<column type="number"	id="valuation_score"	name="后续评级得分" precision="2"/>
		<button type="edit"		id="edit"				name="客户后续评级结果导入"	action="30634203"		multiple="-1"/>
