<?xml version="1.0" encoding="gbk" ?>
<function id="60170109" name="产品属性信息查询" >
  <input>
	<item type="text" id="stx_pwd" name="产品代码"/>
  </input>
  <service type="procedure" id="P60170109" process="933" style="77">
    <param id="user_id"		type="integer"    way="in"/>
	<param id="stx_pwd" type="string"/>
  </service>
  <output export="true">
	<column type="number" id="section_days1" name="默认期限(天)"/>
	<column type="string" id="sdate0" name="初始交易日"/>	
	<column type="string" id="sdate1" name="购回交易日"/>
	<column type="string" id="section_begin_date1" name="起息日期1"/>
	<column type="string" id="earliest_bb_date" name="被允许的最早购回时点"/>	
	<column type="string" id="latest_bb_date" name="被允许的最晚购回时点"/>
	<column type="number" id="days" name="期限(天)"/>	
	<column type="number" id="deadline_lower" name="期限下限(天)" />
	<column type="number" id="deadline_upper" name="期限上限(天)" />
	<column type="number" id="hg_rate" name="默认利率2(%)" precision="2"/>
