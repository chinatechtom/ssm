<?xml version="1.0" encoding="gbk" ?>
<function id="31919" name="客户授信额度查询" >	
  <input>	
		<item type="select" id="institution_id" name="营业部名称"  source="95000008"/>
		<item type="hidden" id="order_id" 		name="合约ID"/>
		<item type="hidden" id="cust_id" 		name="客户ID"/>
		<item type="text" 	id="cust_code" 		name="客户代码"/>
		<item type="select" id="report_type" 	name="报告类型" source="505"/>
		<item type="date" 	id="sdate" 			name="起始日期" default="last" nullable="false"/>
		<item type="date" 	id="edate" 			name="结束日期" default="now"  nullable="false"/>
  </input>  
  <service type="procedure" id="p_cust_credit_report_query" process="911" style="77"> 
		<param id="user_id"	 type="integer" way="in"/>
		<param id="institution_id" type="integer"/> 
		<param id="order_id" type="integer" way="in"/>
		<param id="cust_id"	 type="integer" way="in"/>
		<param id="cust_code"/>
		<param id="report_type"/>
		<param id="sdate"/>
		<param id="edate"/>
  </service>  
  <output export="true">
        <column type="string" id="institution_code"   	name="营业部代码"/>
		<column type="string" id="institution_name"   	name="营业部名称"/>
		<column type="string" id="cust_code"   			name="客户代码"/>
		<column type="string" id="commness_cust_code"   name="普通资金账户"/>
		<column type="string" id="cust_name"   			name="客户姓名"/>
		<column type="number" id="app_crd_limit"   		name="客户申请授信额度(万元)"	    precision="4"/>
		<column type="number" id="agree_crd_limit"   	name="正式核准授信额度(万元)"	    precision="4"/>
		<column type="number" id="financing_limit"   	name="融资上限(万元)"	            precision="4"/>
		<column type="number" id="bs_limit"   		    name="融券上限(万元)"	            precision="4"/>
		<column type="number" id="general_asset"   	    name="客户普通账户总资产(万元)"	precision="4"/>
		<column type="number" id="credit_asset"   		name="客户信用账户总资产(万元)"	precision="4"/>
		<column type="number" id="credit_debts"   		name="客户信用账户总负债(万元)"	precision="4"/>
		<column type="number" id="net_assets"         	name="客户海通账户净资产(万元)"	precision="4"/>
		<column type="number" id="financial_asset"      name="其它金融资产(万元)"	        precision="4"/>
		<column type="number" id="estate_asset"   	    name="客户房产价值(万元)"	        precision="4"/>
		<column type="number" id="financial_asset_half" name="金融资产的一半(万元)"	    precision="4"/>
		<column type="number" id="asset_quarter"   		name="总资产的四分之一(万元)"	    precision="4"/>
		<column type="number" id="overall_score"   	    name="授信综合得分"	            precision="2"/>
    	<column type="string" id="active_date"          name="报告生效日期"/>
    	<column type="string" id="credit_manage_user"   name="授信管理岗"/>
    	<column type="string" id="report_type"   		name="报告类型"/>
  </output>
</function>