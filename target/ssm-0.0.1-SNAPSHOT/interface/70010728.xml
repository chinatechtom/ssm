  <service type="procedure" id="P70010728" process="822" style="1">
	<param type="integer" 	id="user_id"/>
	<param type="integer" 	id="cust_id"/>
	<param type="integer" 	id="sbond_id"/>
	<param type="string"	id="hg_code"/>
	<param type="string"	id="market_code"/>
	<param type="string"	id="product_code"/>
	<param type="integer"	id="hg_num1"/>
	<param type="float"		id="hg_convert_rate"/>
	<param type="float"		id="app_price" default="0"/>
	<param type="string"	id="engage_trade_date1"/>
	<param type="integer"	id="days1"/>
	<param type="float"		id="hg_rate1"/>
	<param type="float"		id="penalty_rate"/>
	<param type="float"		id="prewarning_price"/>
	<param type="float"		id="lowest_price"/>
	<param type="string"	id="remark2"/>
	<param type="float"		id="first_amt1"/>
	<param type="string"	id="bb_date1"/>
	<param type="integer"	id="related_sbond_id"/>
	<param type="float"		id="fix_rate1"/>
	<param type="float"		id="manual_chg1"/>
  </service>
  <output/>
</function>