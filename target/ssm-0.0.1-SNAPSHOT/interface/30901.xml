<?xml version="1.0" encoding="gbk" ?>
<function id="30901" name="从集中交易系统中下载其日终数据到本系统">
	<input>
		<item type="hidden"	id="id"		name="id" action="31381" default="1"/>		
		<item type="date"	id="date"	alias="sdate3"	name="日期" editable="false"/>		
	</input>
	<service type="procedure" id="p_synchronization_download" process="911" style="1">
		<param id="user_id"		type="integer"/>
		<param id="sdate3"		type="string"/>		
	</service>
	<output target="3090101">
		<column name="方向"				type="integer"	id="lvol0"/>   
		<column name="下载方式"			type="string"	id="lvol1"/>   
		<column name="远程文件名"			type="string"	id="snote"/>  
		<column name="本地文件名"			type="string"	id="snote2"/>
		<column name="ftp连接号"			type="integer"	id="lwithdraw_flag"/>   
		<column name="融资融券机构代码"	type="string"	id="sstation1"	display="false"/>   
		<column name="融资融券分支号"		type="string"	id="sdate3"		display="false"/>  
		<column name="融资融券柜员号"		type="string"	id="scheck_emp"	display="false"/>
		<!--<button type="edit"    id="edit"   name="将文件导入到融资融券数据库" action="3090101" multiple="0"/>-->
	</output>
</function>