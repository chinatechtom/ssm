		<param id="days" way="in" type="integer"/>
		<param id="hg_rate" way="in" type="float"/>
		<param id="penalty_rate" way="in" type="float"/>
		<param id="prewarning_price" way="in" type="float"/>
		<param id="lowest_price" way="in" type="float"/>
		<param id="first_amt" way="in" type="float"/>
		<param id="remarks" type="string" way="in"/>
		<param id="fix_rate" way="in" type="float"/>
		<param id="zjyt" type="string" way="in"/>
		<param id="manage_opinion" type="string" way="in"/>
