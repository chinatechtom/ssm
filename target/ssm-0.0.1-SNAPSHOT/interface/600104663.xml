<?xml version="1.0" encoding="gbk" ?>
<function id="600104663" name="同步冻结资金并报盘" >
	<description>确定要同步冻结客户资金并报盘?</description>
	<input>
		<item type="hidden" id="sbranch_code1" name="开户营业部"/>
		<item type="hidden" id="scust_no" name="客户号"/>
	</input>
    <service type="drtp" id="100313" process="711" style="1">
		<param type="string" id="sbranch_code1"/>
		<param type="string" id="sbranch_code0" default="000"/>
		<param type="string" id="scust_no"/>
		<param type="string" id="scurrency_type2" default="01"/>
		<param type="string" id="scurrency_type" default="01"/>
		<param type="string" id="semp" default="SYSTEM"/>
	</service>
    <output target="61010103">
		<column name="开户营业部"	id="branch_code"   type="string"  field="sbranch_code1"/>		
		<column name="客户号"	id="cust_code"  type="string"  field="scust_no"/>
		<column name="可用金额"	id="avail_balance"  type="number"  field="damt5"/>
	</output>
</function>