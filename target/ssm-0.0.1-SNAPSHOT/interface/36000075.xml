<?xml version="1.0" encoding="gbk" ?>
<function id="36000075" name="8.1.1客户资产集中度统计(前X名客户)" >  
  <input>
 <item type="date"   id="date"   name="查询日期"  default="now"   />
 <item type="text"   id="cust_num"       name="客户数量"  default="50"  />
  </input>
   <service type="procedure" id="P36000075" process="911" style="77">
 <param id="user_id"   type="integer" way="in" /> 
 <param id="date"      type="string" way="in" />
 <param id="cust_num"  type="double" way="in" />
  </service> 
  <output  print="true">
    <column type="number" id="id"               name="序号"/>
    <column type="string" id="content"          name="内容"/>
    <column type="number" id="amt"              name="金额"      precision="2" />
    <column type="number" id="rate"             name="集中度"    precision="4" />
  </output>
</function>