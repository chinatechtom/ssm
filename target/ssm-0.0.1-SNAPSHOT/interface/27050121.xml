<?xml version="1.0" encoding="gbk" ?>
<function id="27050121" name="单证券客户融资融券集中度">
	<input>
		<item type="select"	 id="insitution_id"			name="营业部" source="95000008"/>
		<item type="select"  id="market_code"			name="市场" source="039" nullable="false"/>
		<item type="text"	 id="product_code"			name="证券代码" nullable="false"/>
		<item type="text"	 id="cust_code"				name="客户代码" />
	</input>
    <service type="procedure" id="P27050121"  process="911" style="77">
		<param id="user_id"	        type="integer" way="in"/>
		<param id="market_code"		type="string"  way="in"/>
		<param id="product_code"	type="string"  way="in"/>
		<param id="insitution_id"	type="integer" way="in"/>
		<param id="cust_code"		type="string"  way="in"/>
    </service>
    <output export="true" autoload="false" print="true">
		<column type="string" id="row_no"						name="序号" />
		<column type="string" id="institution_code"				name="营业部分支号"/>
		<column type="string" id="institution_name"				name="营业部"/>
		<column type="string" id="cust_code"					name="客户号"/>
		<column type="string" id="cust_name"					name="客户名"/>
		<group name="该证券客户融资集中度">
			<column type="number" id="rz_qty"					name="融资数量"	precision="2"	/>
			<column type="number" id="rz_amt"					name="该券种融资金额"	precision="2"	/>
			<column type="number" id="rz_cust_total_amt"		name="客户融资总额"	precision="2"	/>
			<column type="number" id="rz_single_total_percent"	name="融资金额占客户融资总额比例"	precision="4"	/>
			<column type="number" id="rz_company"				name="该证券公司总融资量"	precision="2"	/>
			<column type="number" id="rz_single_company_percent" name="融资金额占公司总融资量比例"	precision="4"	/>
		</group>
		<group name="该证券客户融券集中度">
			<column type="number" id="rq_qty"					name="融券数量"	precision="2"	/>
			<column type="number" id="rq_amt"					name="融券市值"	precision="2"	/>
			
			<column type="number" id="rq_cust_total_amt"		name="客户融券总市值"	precision="2"	/>
			<column type="number" id="rq_single_total_percent"	name="融券市值占客户融券总市值比例"	precision="4"	/>
			<column type="number" id="rq_company"				name="公司该证券总融券量"	precision="2"	/>
			<column type="number" id="rq_single_company_percent" name="融券数量占公司该证券总融券量比例"	precision="4"	/>
		</group>
		<group name="担保证券集中度">
			<column type="number" id="guarantee_qty"					name="该担保证券数量"	precision="2"	/>
			<column type="number" id="guarantee_amt"					name="该担保证券市值"	precision="2"	/>
			<column type="number" id="guarantee_total_amt"				name="客户担保证券总市值"	precision="2"	/>
			<column type="number" id="guarantee_single_total_percent"	name="该担保证券市值占客户担保证券总市值比例"	precision="4"	/>
			<column type="number" id="company_guarantee_single_amt"		name="该担保证券公司总市值"	precision="2"	/>
			<column type="number" id="guarantee_single_company_percent" name="该担保证券市值占公司该担保证券总市值比例"	precision="4"	/>
			<column type="number" id="company_guarantee_total_amt"				name="公司担保证券总市值"	precision="2"	/>
			<column type="number" id="guarantee_single_company_total_percent" name="该担保证券市值占公司担保证券总市值比例"	precision="4"	/>
		</group>
    </output>
</function>
