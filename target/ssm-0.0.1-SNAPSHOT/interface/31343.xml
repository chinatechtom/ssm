<?xml version="1.0" encoding="gbk"?>
<function id="31343" name="复核操作日志查看">
	<input>
		<item type="text" id="oper_user_code" name="操作人员"/>
		<item type="text" id="chk_user_code"  name="复核人员"/>
		<item type="text" id="func_id"        name="业务功能代码"/>
		<item type="text" id="sno"  		  name="操作流水号"/> 
		<item type="date" id="begin_date"     name="操作起始日期" default="last"/>
		<item type="date" id="end_date"       name="操作结束日期" default="now" />
	</input>
	<service type="procedure" id="p_write_chk_log_query" implement="p_write_chk_log_query31343" process="911" style="77">
		<param id="user_id"			    type="integer"      way="in"/>
		<param id="oper_user_code" 		type="string" 		way="in"/>
		<param id="chk_user_code"  		type="string" 		way="in"/>
		<param id="func_id" 			type="integer" 		way="in"/>
		<param id="begin_date"  		type="string" 		way="in"/>
		<param id="end_date" 	 	    type="string" 		way="in"/>
		<param id="sno" 			    type="integer" 	    way="in"/>
  </service>
	<output export="true">
		<column type="string" id="sno"					name="操作流水号"/>
		<column type="string" id="user_id"				name="操作人员ID"     display="false"/>
		<column type="string" id="user_code1"			name="操作人员" />
		<column type="string" id="chk_user_id"		    name="复核人员ID" 	   display="false"/>
		<column type="string" id="user_code"			name="复核人员"/>
		<column type="string" id="func_id"				name="业务功能号"/>
		<column type="string" id="func_id_name"		    name="业务功能"/>
		<column type="string" id="digestid"				name="复核功能的摘要" display="false"/>
		<column type="string" id="digestname"			name="复核功能的摘要"/>
		<column type="string" id="operdate"				name="操作日期" />
		<column type="string" id="opertime"				name="操作时间"/>
  </output> 
</function>
