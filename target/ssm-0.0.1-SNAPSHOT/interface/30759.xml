<?xml version="1.0" encoding="gbk" ?>
<function id="30759" name="逐日盯市处理" >	
	<input>
		<item type="select" id="institution_id" name="营业部名称"  source="95000008"/>
		<item type="text"	id="cust_code"		name="客户代码"/>
		<item type="select"	id="status"	name="处理状态"	source="351" nullable="false"/>
	</input>
	<service type="procedure" id="p_guarantee_rate_needdeal_control"  process="911" style="77">
		<param id="user_id"			type="integer"	way="in"/>
		<param id="institution_id"	type="integer"	way="in"/>
		<param id="cust_code"		type="string"	way="in"/>
		<param id="status"			type="string"	way="in"/>
	</service>
	<output	export="true" autoload="true" refresh="60">
		<column type="number" id="record_id"				name="记录id"		display="false"/>
		<column type="number" id="institution_id"			name="营业部id"		display="false"/>
		<column type="string" id="institution_code"			name="营业部代码"/>
		<column type="string" id="institution_name"			name="营业部名称"/>
		<column type="number" id="cust_id"					name="客户id"		display="false"/>
		<column type="string" id="cust_code"				name="客户代码"/>
		<column type="string" id="cust_name"				name="客户姓名"/>
		<column type="string" id="credit_level_id"			name="信用级别id"		display="false"/>
		<column type="string" id="credit_level_code"		name="信用级别代码"	display="false"/>
		<column type="string" id="credit_level_name"		name="信用级别"/>	
		<column type="number" id="cur_gua_rate"				name="实时担保率"	precision="6"/>
		<column type="number" id="cur_gua_rate_without_mortgage" name="实时担保率(不计入场外担保品)"	precision="6"/>
		<column type="number" id="level"					name="担保率监控线"	precision="3"/>
		<column type="number" id="current_num"				name="触发时担保率"		precision="6"/>			
		<column type="number" id="guarantee_rate_id"		name="担保率id"		display="false"/>
		<column type="string" id="guarantee_rate_name"		name="报警类别"		/>
		<column type="string" id="guarantee_rate_use"		name="报警类别" display="false"/>		
		<column type="number" id="asset_amt"				name="当前资产总额(不计入场外担保品)"	precision="3"/>
		<column type="number" id="asset_amt_mortgage" name="当前资产总额(计入场外担保品)"	precision="3"/>
		<column type="number" id="out_mortgage_amt"  name="场外担保品总额"	precision="3"/>
		<column type="number" id="asset_gap"				name="资产差额"		precision="3"/>
		<column type="number" id="pc_amt"					name="强制平仓额度"		precision="3"/>
		<column type="number" id="guarantee_rate_id_ctl"	name="补仓线担保率id"		display="false"/>
		<column type="string" id="guarantee_rate_id_ctl_name"		name="补仓线担保率名称"	display="false"/>
		<column type="string" id="guarantee_rate_id_ctl_use"		name="补仓线"	display="false"/>
		<column type="string" id="result_remark"			name="结果描述"/>		
		<column type="string" id="touch_date"				name="触发平仓日期"/>
		<column type="string" id="touch_time"				name="触发平仓时间"/>
		<column type="string" id="bc_begin_date"			name="补仓起始日期"/>
		<column type="string" id="bc_end_date"				name="补仓结束日期"/>
		<column type="string" id="ddate"					name="当前系统日期"/>
		<column type="string" id="pc_begin_date"			name="拟平仓日期"/>
		<column type="string" id="manage_status"			name="处理状态1"		display="false"/>	
		<column type="string" id="manage_status_name"		name="处理状态"/>
		<column type="string" id="pc_mode"					name="平仓模式"		display="false"/>
		<column type="string" id="pc_reason"				name="平仓原因"		display="false"/>
		<column type="string" id="pc_reason_name"			name="平仓原因1"	display="false"/>
		<column type="string" id="warning_level"			name="报警级别"	level="true"	display="false"/>
		<column type="string" id="qzpc_mark"				name="强制平仓标志"/>
		<button type="ok"     id="ok"						name="生成平仓预案" action="30726" multiple="0"/>
		<button type="ok"     id="batok"					name="批量生成平仓预案" action="30773" multiple="-1"/>
	</output>
</function>