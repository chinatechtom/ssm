<?xml version="1.0" encoding="gbk" ?>
<function id="60660601" name="分段计息区间浏览" >	
  <input> 
		<item type="hidden"	id="sbond_id" name="子合同id" />
		<item type="hidden"	id="section_id" name="分段计息区间id" />
  </input>
  <service type="procedure" id="P60660600" implement="P60660600" process="933" style="77"> 
	    <param id="user_id"	type="integer"/>		
		<param id="sbond_id" type="integer"/>	
	    <param id="section_id" type="integer"/>
  </service>
  <output>
		<column type="number" field="section_id"        id="section_id"        name="区间ID"   display="false"/>
		<column type="string" field="section_name"      id="section_name"      name="区段"     display="false"/>
		<column type="string" field="begin_date"        id="section_begin_date"        name="计息起始日期" 		     />
		<column type="string" field="end_date"          id="section_end_date"          name="计息截至日期" 			 />
		<column type="string" field="end_date_true"     id="section_end_date_true"     name="计息截至日期" display="false" />
		<column type="number" field="days" 		        id="section_days" 		       name="区间天数"				 />
		<column type="number" field="engage_days" 		id="section_engage_days" 	   name="起始日期据初始交易日天数"	/>
		<column type="number" field="hg_rate"	        id="section_hg_rate"	       name="购回价格(每100元)"	 precision="2"     />
		<column type="number" field="interest_count"    id="section_interest_count"    name="计息积数" display="false"  precision="2" />
		<column type="number" field="interest"		    id="section_interest"		   name="未结利息"   precision="2" />
		<column type="number" field="received_interest" id="section_received_interest" name="已收利息" display="false"  precision="2" />
		<column type="string" field="remarks"           id="section_remarks"           name="备注"/>
  </output>		
</function>