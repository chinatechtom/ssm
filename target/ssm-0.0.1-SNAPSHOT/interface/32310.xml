		<item type="text" id="paraid"	name="参数符号"/>
		<item type="text" id="paraname"	name="参数名字"/>
	</input>
	<service type="procedure" id="p_mixedconfig_query" implement="p_mixedconfig_query32310" process="911" style="77">
		<param id="user_id" type="integer"/>
		<param id="paraid" />
		<param id="paraname"/>
	</service>
	<output export="true">
		<column type="string" id="paraid"		name="参数符号"/>
		<column type="string" id="paraname"		name="参数名字"/>
		<column type="string" id="paravalue"	name="参数值"/>
		<column type="string" id="datatype"		name="参数数据类型"		display="false"/>
		<column type="number" id="datalen"		name="数据长度"			display="false"/>
		<column type="number" id="declen"		name="小数点长度"		display="false"/>
		<column type="string" id="charset"		name="允许取值字符集"	display="false"/>
		<column type="string" id="ctrltype"		name="控件类型"			display="false"/>	
		<column type="string" id="Show"			name="控件中显示内容"	display="false"/>
		<column type="string" id="biztype"		name="业务大类"			display="false"/>
		<column type="string" id="paralevel"	name="参数分类"			display="false"/>
		<column type="string" id="status"		name="参数状态"			display="false"/>
		<column type="string" id="remark"		name="备注信息"/>
	</output>
</function>