	<param id="report_status"/>
	<param id="market_code"/>
	<param type="integer" id="page_num"   way="in"/>
	<param type="integer" id="rows_num"   way="in"/>
  </service>
  <output export="true">  
  	<column type="string" id="institution_name"   name="营业部名称" />
	<column type="string" id="institution_code"   name="营业部代码" />  
  	<column type="number" id="cust_id"   name="客户号"  display="false" />
    <column type="string" id="cust_code"   name="客户代码"  />
    <column type="string" id="cust_name"   name="客户姓名"  />
    <column type="string" id="cust_type"   name="客户类型" display="false"/>
	<column type="string" id="cust_type_name"   name="客户类型"/>
    <column type="string" id="cust_sort"   name="客户类别"  />
