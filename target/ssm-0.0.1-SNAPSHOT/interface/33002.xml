<?xml version="1.0" encoding="gbk" ?>
<function id="33002" name="重点客户监控" >
	<input export="true">	
	    <item type="text" 	id="cust_code" 	name="客户代码" />
		<item type="text" 	id="cust_name" 	name="客户姓名" />
		<item type="text" 	id="cert_no"	name="证件号码" />
		<item type="select" id="risk_level" name="风险级别"  source="337"/>		
		<item type="select" id="in_reason"	name="类别"  source="96000004"/>
		<item type="date"	id="in_date"	name="进入日期"  default="null"/>
		<item type="date"	id="out_date"	name="退出日期"  default="null"/>
		<item type="hidden" id="issyscust"	name="是否系统客户"  source="391"/>
		<item type="select" id="isvalid"	name="是否有效"  source="392" default="1"/>
		<group name="分页查询" page="true">		
			<item type="text"   id="page_num"       name="页码"     integer="true" default="1"/>
			<item type="select" id="rows_num"       name="每页行数" integer="true" source="95000012" nullable="false"/>
		</group>		
	</input>
	<service type="procedure" id="p_risk_cust_list_query" implement="p_risk_cust_list_query33002" process="911" style="77">
		<param id="user_id" type="integer"/>
		<param id="page_num"            type="integer" way="in"/>
		<param id="rows_num"            type="integer" way="in"/>		
		<param id="cust_code"/>
		<param id="cust_name"/>
		<param id="cert_no"/>
		<param id="risk_level"/>
		<param id="in_date"/>
		<param id="in_reason"/>
		<param id="out_date"/>
		<param id="issyscust"/>
		<param id="isvalid"/>
	</service>
	<output>
		<column type="number" id="record_id"   	name="记录编号"/>
	 	<column type="number" id="cust_id"   	name="客户id"	display="false"/>
		<column type="string" id="cust_code"   	name="客户代码"	 />
    	<column type="string" id="cust_name"   	name="客户姓名"/>
		<column type="string" id="reason_code"   name="重点客户分类1" display="false"/>
		<column type="string" id="reason_name"   name="重点客户分类"/>
		<column type="string" id="risk_level_name"   name="风险级别"/>
		<column type="string" id="in_date"     name="进入日期"/>
		<column type="string" id="out_date"    name="退出日期"/>
        <column type="string" id="is_valid"   name="是否有效1"	display="false"/>
		<column type="string" id="is_valid_name"   name="是否有效"/>		
		<column type="number" id="bc_notice_days"   	name="补仓通知天数" precision="0"/>
		<column type="number" id="pc_notice_days"   	name="平仓通知天数" precision="0"/>
		<column type="string" id="move_tel"	name="客户手机号码"/>
		<column type="string" id="fix_tel"	name="客户固定电话"/>
		<column type="string" id="user_name"	name="柜员姓名"/>
		<column type="string" id="telephone"	name="柜员联系电话"/>
		<column type="string" id="cert_type"	name="证件类型名称"	display="false"/>
		<column type="string" id="cert_type_name"	name="证件类型"/>
    	<column type="string" id="cert_no"   	name="证件号码"/>
    	<column type="string" id="area_code"   	name="地址"/>
    	<column type="string" id="risk_level"   name="风险级别名称"	display="false"/>
		<column type="number" id="in_value"   name="进入值" precision="2"/>
		<column type="number" id="index_value"   name="指标值" precision="2"/>
		<column type="string" id="issyscust"   name="是否系统客户1"	display="false"/>
		<column type="string" id="issyscust_name"   name="是否系统客户"/>
    	<column type="string" id="remark"   	name="备注"/>
		<button type="view"   id="view"         name="查看详细信息" 	action="3300104"  multiple="0"/>
	</output>
	<assistant action="30103,30109,30215,30202,3300201,3300202,31905,31906">
		<link src="cust_id" target="cust_id"/>
    </assistant>
  </function>
  
