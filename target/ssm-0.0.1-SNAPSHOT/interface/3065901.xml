<?xml version="1.0" encoding="gbk" ?>
<function id="3065901" name="客户结清利息查询" >  
	<input>
		<item type="hidden" id="order_id" name="客户id" />
	</input>
	<service type="procedure" id="p_compute_interest_query" process="911" style="77">
		<param id="user_id" type="integer" way="in" /> 
		<param id="order_id"/>	
	</service> 
	<output >  
		<column type="number" id="cust_id" display="false" name="客户id"/>
		<column type="string" id="over_interest_date" name="结息日期"/>
		<column type="number" id="financing_interest_sum" name="融资利息合计" precision="2"/> 
		<column type="number" id="borrow_stock_interest_sum" name="融券利息合计" precision="2"/> 
		<column type="number" id="fix_borrow_fund_sum"  name="融资固定额度费合计" precision="2"/> 
		<column type="number" id="fix_borrow_stock_sum" name="融券固定额度费合计" precision="2"/> 
		<column type="number" id="takeup_borrow_fund_sum" name="融资占用额度费合计" precision="2"/> 
		<column type="number" id="takeup_borrow_stock_sum" name="融券占用额度费合计" precision="2"/> 
		<column type="number" id="before_return_commission_charge_sum" name="融券违约手续费合计" precision="2"/> 
		<column type="number" id="bsk_bef_rtn_commission_sum" name="提前还贷手续费合计" precision="2"/> 
		<column type="number" id="financing_punish_interest_sum"     name="融资逾期罚息合计" precision="2"/> 
		<column type="number" id="bstk_punish_interest_sum"          name="融券逾期罚息合计" precision="2"/> 
		<column type="number" id="financing_bad_punish_interest_sum" name="融资坏账罚息合计" precision="2"/> 
		<column type="number" id="bstk_bad_punish_interest_sum"      name="融券坏账罚息合计" precision="2"/> 		
	</output>
</function>