<?xml version="1.0" encoding="gbk" ?>
<function id="340141" name="获取征信系统数据(日均资产规模,收益率,投资经验)">
	<input>
		<item type="hidden" id="cust_code" name="信用资金账号"/> 
	</input>
	<service type="procedure" id="kingstar.P37815013" adapter="newzhengxin" implement="P37815013" process="155" style="1">
		<param id="cust_code"	type="string"  />
		<param id="user_code"	type="string" default="rzrqgl"/>
	</service>
	<output target="340142"> 
	  <column type="string" id="branch_code" name="营业部代码"/> 
	  <column type="string" id="crd_capital_account" name="信用资金账号"/>
	  <column type="string" id="pt_capital_account" name="普通资金账号"/>
	  <column type="string" id="begin_date" name="征信开始日期"/>
	  <column type="string" id="end_date" name="征信结束日期"/>
	  <column type="number" id="abs_profit_rate" name="年化收益率(%)" precision="2"/>
	  <column type="number" id="avg_asset" name="日均资产规模(万元)" precision="2"/>
	  <column type="string" id="tx_date" name="首次交易日期"/> 
	  <column type="string" id="tx_years" name="投资经验"/> 
	  <column type="string" id="evaluate_dict" name="风险评测等级"/> 
	</output> 
</function>