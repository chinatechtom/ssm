<?xml version="1.0" encoding="gbk" ?>
<function id="36000050" name="5.1.2融资融券业务收支分析" >
	<input>	
		<item type="select"  id="institution_id"	    name="营业部名称"  source="95000008"/>
	  <item type="date"   id="begin_date"           name="起始日期"  default="last"/>
    <item type="date"   id="end_date"             name="结束日期"  default="now"/>		
	</input>
    <service type="procedure" id="P36000050"    process="911" style="77">
		<param id="user_id"	            type="integer" way="in"/> 
		<param id="institution_id"	    type="integer" way="in"/> 
    <param id="begin_date"    type="string" way="in" />
    <param id="end_date"     type="string" way="in" />		 		
   </service>
    <output export="true" print="true">
    <column type="string" id="institution_name"					name="营业部名称"/>
		<column type="number" id="financing_interest"	name="融资利息"	precision="2" total="true"/>		
		<column type="number" id="borrow_stock_interest"	name="融券利息"	precision="2" total="true"/>
		<column type="number" id="fix_borrow_fund"	name="融资固定额度费"	precision="2" total="true"/>
		<column type="number" id="fix_borrow_stock"	name="融券固定额度费"	precision="2" total="true"/>
		<column type="number" id="takeup_borrow_fund"	name="融资占用额度费"	precision="2" total="true"/>
		<column type="number" id="takeup_borrow_stock"	name="融券占用额度费"	precision="2" total="true"/>
		<column type="number" id="borrow_stock_commission_charge"	name="融券违约手续费"	precision="2" total="true"/>
		<column type="number" id="before_return_commission_charge"	name="提前还贷手续费"	precision="2" total="true"/>
		<column type="number" id="punish_interest"			name="逾期罚息"		precision="2" total="true"/>
		<column type="number" id="bad_punish_interest"		name="坏帐罚息"	precision="2" total="true"/>	
		<column type="number" id="trade_commission_charge"		name="交易佣金"	precision="2" total="true"/>	
    </output>
</function>
