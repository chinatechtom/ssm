<?xml version="1.0" encoding="gbk" ?>
<function id="90000017" name="角色合约处理权限查询">
	<input>
		<item type="text"		id="role_id"		name="角色id" />
		<item type="text" 	id="order_type"	name="合约类型"/>			
	</input>
	<service type="procedure" id="p_role_order_authority_query"   process="911" style="77">
		<param id="user_id"				type="integer"  way="in"/>
		<param id="role_id"				type="integer"  way="in"/>
		<param id="order_type"		type="string"   way="in"/>
	</service>
	<output>
		<column type="number" id="role_id"   							name="角色ID"/>
		<column type="string" id="data_code"	 		   			name="订单类型"/>
		<column type="string" id="order_type_name"	 		  name="订单类型名称"/>
    </output>
</function>