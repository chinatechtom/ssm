<?xml version="1.0" encoding="gbk" ?>
<function id="60070704" name="证券标的查询" >
  <input>
    <item type="hidden" id="product_id"     name="证券id"/>
	<item type="select" id="market_code"    name="市场"	source="039"/>
	<item type="text"   id="product_code"   name="证券代码"/>
    <item type="text"   id="product_name"   name="证券名称"/>	
	<item type="select"	id="if_zy_target"   name="是否质押标的证券" source="804"/>	
  </input>
  <service type="procedure" id="P60070701" implement="P60070701_60070704" process="933" style="77"> 
    <param id="user_id"		   type="integer" way="in"/>
    <param id="product_id"	   type="integer" way="in"/>
	<param id="market_code"    type="string"  way="in"/>
    <param id="product_code"   type="string"  way="in"/>  
	<param id="product_name"   type="string"  way="in"/>
	<param id="if_zy_target"   type="string"  way="in"/>
  </service>
  <output>
	<column type="number" id="product_id"				name="证券id" display="false"/>
	<column type="string" id="market_code"				name="市场代码"/>	
	<column type="string" id="product_code"				name="证券代码"/>
	<column type="string" id="product_name"				name="证券名称"/>
	<column type="string" id="product_type"				name="证券类别"/>
	<column type="string" id="if_zy_target"				name="是否质押标的证券" display="false"/>
	<column type="string" id="if_zy_target_name"		name="是否质押标的证券"/>
	<column type="number" id="circulated_a"				name="流通A" precision="0"/>
	<column type="number" id="zy_stock_pca"				name="质押集中度(%)占流通A" precision="6"/>
	<column type="number" id="credit_vol_pca"			name="可接受数量占流通A" precision="0"/>
	<column type="number" id="all_stock"				name="总股本" precision="0"/>
	<column type="number" id="zy_concentration_rate"	name="质押集中度(%)占总股本" precision="6"/>
	<column type="number" id="credit_vol"				name="可接受数量占流通A" precision="0"/>
	<column type="number" id="zy_stock_pna"				name="质押集中度(%)占净资本" precision="6"/>
	<column type="number" id="fund_amt"					name="净资本" precision="2"/>
	<column type="number" id="credit_amt_pna"			name="可接受金额占净资本" precision="2"/>
	<column type="number" id="zy_convert_rate"			name="质押折算率(%)" precision="6"/>
	<column type="string" id="zy_user_code"				name="更新人员" />
	<column type="string" id="zy_upd_date"				name="更新日期" />
  </output>
  <assistant action="30304,30305,30306,30315">		
	<link src="product_id" target="product_id"/>
  </assistant>
</function>
