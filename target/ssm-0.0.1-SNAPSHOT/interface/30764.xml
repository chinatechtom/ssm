<?xml version="1.0" encoding="gbk" ?>
<function id="30764" name="设置应平证券信息" >
	<input>
		<item type="text"    id="record_id"        name="平仓报告编号"	editable="false"/>		
		<item type="hidden"  id="product_id"       name="证券id"/>
		<item type="text"    id="product_code"     name="证券代码" editable="false"/>
		<item type="text"    id="market_code_name" name="市场代码" editable="false"/>
		<item type="text"    id="asset_qty"        name="证券数量" editable="false"/>
		<item type="text"    id="asset_value"      name="证券总价值" editable="false"/>
		<item type="text"    id="sell_qty"         name="卖出数量" nullable="false"	integer="true"/>
		<item type="text"    id="sell_price"       name="卖出价格" nullable="false" number="true"/>
		<item type="hidden"  id="position"         name="位置"/>
		<item type="text"	 id="stock_convert_rate"	name="折算率"	editable="false"/>
	</input>
    <service type="procedure" id="p_cust_should_sell_stock_set"  process="811" style="1">
		<param id="user_id"	            type="integer"  way="in"/> 
		<param id="record_id"	        type="integer"  way="in"/> 
		<param id="product_id"			type="integer"  way="in"/>
		<param id="sell_qty"			type="integer"  way="in"/>
		<param id="sell_price"			type="double"   way="in"/>
		<param id="position"			type="integer"  way="in"/>
    </service>
    <output/>
</function>
