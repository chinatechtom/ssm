<?xml version="1.0" encoding="gbk" ?>
<function id="70044445" name="处理券商首次授予最大交易额度申请" >
  <input print="70010504.jsp">
	<item type="hidden" 	id="cust_id" 	name="cust_id"/>
	<item type="hidden" 	id="order_id" 	name="合约id" action="70010501"/>
	<group name="客户信息">
		<item type="text" id="institution_code" name="营业部代码" editable="false"/>
		<item type="text" id="institution_name" name="营业部名称" editable="false"/>
		<item type="text" id="cust_code" name="客户代码" editable="false"/>
		<item type="text" id="cust_name" name="客户姓名" editable="false"/>
		<item type="text" id="full_name" name="客户全称" editable="false"/>
		<item type="text" id="xy_cust_code" name="信用客户代码" editable="false"/>
		<item type="text" id="cust_type" name="客户类型" editable="false"/>
		<item type="text" id="cert_type" name="证件类型" editable="false"/>
		<item type="text" id="cert_no" name="证件号码" editable="false"/>
		<item type="text" id="credit_level_code" name="信用级别代码" editable="false"/>
		<item type="text" id="credit_level_name" name="信用级别名称" editable="false"/>
		<item type="text" id="total_score" name="征信总分" editable="false"/>
	</group>
	<group name="公司额度信息">
		<item type="text" id="fund_credit" name="公司授权规模(万元)" editable="false"/>
		<item type="text" id="one_max" name="单客户最大交易额度(万元)" editable="false"/>
		<item type="text" id="total_credit" name="已发放额度(万元)" editable="false"/>
		<item type="text" id="total_used" name="已使用额度(万元)" editable="false"/>
		<item type="text" id="total_unused" name="未使用额度(万元)" editable="false"/>
	</group>
	<item type="hidden" id="credit_calc" name="资产乘额度系数(万元)" editable="false"/>
	<item type="hidden" id="credit_min" name="计算最大交易额度(万元)" editable="false"/>
	<item type="hidden" id="request_date" name="合约日期" editable="false"/>
	<item type="hidden" id="system_date" name="系统日期" editable="false"/>
	<item type="hidden" id="active_date" name="评级生效日期" editable="false"/>
	<item type="hidden" id="credit_type" name="授信类型" editable="false"/>
	<item type="hidden" id="company" name="公司名称"/>
	<group name="调整最大交易额度">
	  <item type="grid" 	id="cust_id" 	name="证券信息"			source="70010125"/>
	  <item type="hidden" 	id="order_id" 	name="合约id"			action="70044544"/>
	  <item type="text"		id="pt_asset" name="客户普通账户总资产(万元)" editable="false"/>
	  <item type="text"		id="xy_asset" name="客户信用账户总资产(万元)" editable="false"/>
	  <item type="text"		id="xy_debt" name="客户信用账户总负债(万元)" editable="false"/>
	  <item type="text"		id="asset_date" name="资产日期" editable="false"/>
	  <item type="text" 	id="order_no" 	name="合约编号"			editable="false"/>
	  <item type="text" 	id="cust_num" name="客户资产(万元)"	editable="false" action="7003065005"/>
	  <item type="text" 	id="credit_num" name="申请最大交易额度"	editable="false" action="7003065005"/>
	  <item type="text" 	id="credit_num_rate" name="授信系数(%)"	editable="false" action="7003065005"/>
	  <item type="textarea" id="credit_num_c" name="最大交易额度大写" 	editable="false"/>
	  <item type="text" 	id="credit_num_net" name="申请网上最大交易额度"	editable="false"/>
	  <item type="textarea" id="credit_num_net_c" name="网上最大交易额度大写" 	editable="false"/>
	  <item type="text" 	id="remark" 	name="备注"			editable="false"/>
	</group>
	<group name="处理后修改最大交易额度" >
	  <item type="text" 	id="approve_num" name="审批最大交易额度" nullable="false" number="true" action="7003065005"/>
	  <item type="textarea" id="approve_num_c" name="审批最大交易额度大写" 	editable="false"/>
	  <item type="text" 	id="approve_num_net" name="审批网上最大交易额度" nullable="false" number="true" />
	  <item type="textarea" id="approve_num_net_c" name="审批网上最大交易额度大写" 	editable="false"/>
	  <item type="date" id="end_date" name="额度有效截止日" 	nullable="false"/>
	</group>
	<group name="自然情况" 	scope="group">
	  <item type="grid" 	id="cust_id" source="70030422" name="自然情况"/>
	</group>
	<group name="处理意见">
	  <item type="textarea"	id="manage_idea" name="处理意见"/>
	</group>  
  </input>
  <service type="procedure"	id="P70044444" process="822" style="1">
	<param type="integer"	id="user_id" 			way="in"/>
	<param type="integer" 	id="order_id" 			way="in" />
	<param type="string" 	id="approve_num" way="in"/>
	<param type="string" 	id="approve_num_net" way="in"/>
	<param type="string" 	id="manage_idea" 		way="in"/>
	<param type="string" 	id="end_date" 		way="in"/>
  </service>
  <output/>
</function>