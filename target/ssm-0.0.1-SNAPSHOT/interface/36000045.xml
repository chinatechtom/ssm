<?xml version="1.0" encoding="gbk" ?>
<function id="36000045" name="4.2.3融券业务额度汇总统计表" >  
  <input>
 <item type="date" id="begin_date" name="起始日期"  default="last"/>
 <item type="date" id="end_date" name="结束日期"  default="now"/>
  </input>
 <service type="procedure" id="P36000045" process="911" style="77">
 <param id="user_id" type="integer" way="in" /> 
 <param id="begin_date"    type="string"  way="in" />
  <param id="end_date"      type="string"  way="in" />
  </service> 
  <output  print="true">
     <column type="number" id="id" name="序号"/>  
   <group name="项目"> 
     <column type="string" id="product_code"   name="证券代码"/> 
     <column type="string" id="product_name"   name="证券名称"/> 
   </group>  
   <group name="融券可申请总数量"> 
     <column type="number" id="borrow_stock_vol_all"      name="数量"   precision="0" total="true"/> 
     <column type="number" id="bs_app_mkval"              name="市值"   precision="2" total="true"/>
   </group> 
   <group name="已融出证券额度"> 
     <column type="number" id="app_bs_num"      name="数量"   precision="0" total="true"/> 
     <column type="number" id="app_bs_mk_value" name="市值"   precision="2" total="true"/> 
   </group>   
   <group name="已卖出证券"> 
     <column type="number" id="sell_stock_num"      name="数量" precision="0" total="true"/> 
     <column type="number" id="sell_stock_mk_value" name="市值" precision="2" total="true"/> 
   </group> 
   <group name="未卖出证券"> 
     <column type="number" id="unsell_stock_num"      name="数量" precision="0" total="true"/> 
     <column type="number" id="unsell_stock_mk_value" name="市值" precision="2" total="true"/> 
   </group>  
   <group name="可融出证券"> 
     <column type="number" id="can_bs_num"      name="数量" precision="0" total="true"/> 
     <column type="number" id="can_bs_mk_value" name="市值" precision="2" total="true"/> 
   </group>  
  </output>
</function>