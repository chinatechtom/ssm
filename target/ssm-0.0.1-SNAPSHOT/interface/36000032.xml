<?xml version="1.0" encoding="gbk" ?>
<function id="36000032" name="3.1.1融资融券风险准备比例统计" >
  <input> 
  <item type="date"  id="date"      name="日期"   default="now"/>
  <item type="text"  id="rate"      name="占净资本比例%"  />
  </input>
  <service type="procedure" id="P36000032" process="911" style="77">
  <param id="user_id"      type="integer"    way="in"/>
  <param id="date"       type="string"   way="in"/>
   <param id="rate"          type="double"   way="in"/>
  </service>
  <output export="true" print="true">    
    <column type="string" id="content"                  name="项目"                 />
    <column type="number" id="day_amt"                  name="报告日余额"       precision="2" />
    <column type="number" id="institution_fund_amt"     name="公司净资本"       precision="2" />
    <column type="number" id="rate"                     name="占净资本比例"       precision="6" />
    <column type="string" id="remark"                   name="备注"                 />
  </output>
</function>