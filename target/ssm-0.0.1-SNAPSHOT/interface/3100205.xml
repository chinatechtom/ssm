  <item type="hidden" id="cust_code" name="客户代码" />
	<item type="hidden" id="date" name="日期"  default="now"/>
	
  </input>
   <service type="procedure" id="p_statistics_3100205_query" process="911" style="77">
	<param id="user_id" type="integer" way="in" /> 
	<param id="date"/>
	<param id="institution_id" />
	<param id="cust_code" />
  </service> 
  <output  print="true">
  <column type="string" id="c1" name="营业部名称"/>  
	<column type="string" id="c2" name="客户代码"/>
	<column type="string" id="c3" name="客户姓名"/>
	<group name="平仓情况">
	<column type="number" id="c4" name="当前应补仓差额" precision="2" />
	<column type="string" id="c5" name="已平仓证券代码" />
	<column type="number" id="c6" name="已平仓证券数量" precision="2" />
	</group>
	<column type="string" id="c7" name="备注"/>
  </output>
</function>