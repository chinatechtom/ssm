<?xml version="1.0" encoding="gbk" ?>
<function id="3220804" name="邮件发送状态修改">
	<input export="true">
		<item type="hidden" 	id="msg_id" 		name="信息编号" />	
		<item type="text" 		id="cust_code" 		name="客户代码" />
		<item type="hidden" 	id="stock_code" 	name="证券代码" />
		<item type="select" 	id="cont_obj" 		name="信息通知对象"	source="332"/>
		<item type="select" 	id="msg_type" 		name="信息类型"			source="333"/>
		<item type="select" 	id="send_flg" 		name="发送标志"			source="334"/>
		<item type="hidden" 	id="email_recv" 	name="收件人邮箱"  	/>
		<item type="hidden" 	id="mobil_recv" 	name="手机号码"  	/>		
		<item type="date"		id="sc_date" 		name="信息生成起始日期"  	default="last"/>
		<item type="hidden" 	id="ss_date" 		name="信息发送起始日期*"  />
		<item type="hidden" 	id="es_date" 		name="信息发送结束日期*"  />
		<item type="select" 	id="real_flag" 		name="实时标志"  		source="335"/>
		<item type="text" 	    id="retry_times" 	name="重试次数"  		/>
		<item type="text" 	    id="gua_rate_lower" name="担保率下限≥"  	/>
		<item type="text" 	    id="gua_rate_upper" name="担保率上限＜"  	/>
		<group name="分页查询" page="true">		
			<item type="text"    id="page_num"       name="页码"     integer="true" default="1"/>
			<item type="select"  id="rows_num"       name="每页行数" integer="true" source="95000012" nullable="false"/>
		</group>
	</input>
	<service type="procedure" id="p_send_email_query" implement="p_send_email_query3220804" process="911" style="77">
		<param id="user_id" type="integer"/>
		<param id="page_num"	        type="integer"  way="in"/> 			
		<param id="rows_num"	        type="integer"  way="in"/> 		
		<param id="msg_id" type="integer"/>	
		<param id="cust_code"/>
		<param id="stock_code"/>
		<param id="cont_meth"/>
		<param id="cont_obj"/>
		<param id="msg_type"/>
		<param id="send_flg"/>
		<param id="email_recv"/>
		<param id="mobil_recv"/>
		<param id="real_flag"/>
		<param id="sc_date"/>
		<param id="sc_date"/> <!--信息生成结束日期跟起始日期一致-->
		<param id="ss_date"/>
		<param id="es_date"/>
		<param id="retry_times" type="integer"/>
		<param id="gua_rate_lower"/>		
		<param id="gua_rate_upper"/>
	  </service>
	<output export="true">
	 	<column type="number" 	id="c1"   		name="信息编号"  					/>
    	<column type="string" 	id="c2"   		name="信息生成日期"  				/>
    	<column type="string" 	id="c12"   		name="信息生成时间"  				/>		
		<column type="string" 	id="cust_code"  name="收件人代码"  					/>
    	<column type="string" 	id="cust_name"  name="收件人名称"  					/>
    	<column type="string" 	id="c5"   		name="信息通知方式"  	 			/>
    	<column type="string" 	id="c6"   		name="信息通知对象"  				/>
    	<column type="string" 	id="c7"   		name="信息类型"  	 				/>
		<column type="string" 	id="c11"   		name="发送标志"  					/>
    	<column type="string" 	id="c13"   		name="收件人邮箱"  	 				/>
    	<column type="string" 	id="c14"   		name="收件人手机号码"  			/>
    	<column type="string" 	id="c15"   		name="信息发送日期"  	 			/>
    	<column type="string" 	id="c16"   		name="信息发送时间"  				/>
		<column type="string" 	id="c21"   		name="实时重试次数"				/>	
		<column type="string" 	id="c22"   		name="备注"						    />	
	</output>
</function>
		
		