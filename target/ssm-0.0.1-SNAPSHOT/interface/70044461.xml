			<item type="text" id ="product_code" name="标的证券代码" editable="false"/>
			<item type="text" id="product_name"   name="标的证券名称" editable="false"/>
			<item type="text" id="product_type"   name="标的证券类型" editable="false"/>
			<item type="text" id ="hg_num" name="标的证券数量" nullable="false" integer="true"/>
			<item type="text" id="first_amt" name="初始交易金额" nullable="false" number="true" action="97000006"/>
			<item type="text" id ="hg_convert_rate" name="折扣比率(%)" nullable="false" number="true"/>
			<item type="text" id ="app_price" name="标的证券价格" nullable="false" number="true" precision="2"/>
			<item type="date" id="engage_trade_date" name="初始交易日期" 	default="last" nullable="false"	action="97000016,97000006"/>
			<item type="text" id ="days" name="购回期限(天)" nullable="false" editable="false" action="97000006"/>
			<item type="date" id="bb_date" name="购回交易日期" 	default="last" nullable="false"	action="97000016,97000006"/>
			<item type="text" id ="hg_rate" name="回购价格(每百元%)" nullable="false" number="true" action="97000006"/>
			<item type="text" id ="penalty_rate" name="违约金比率(日利率%)" nullable="false" number="true"/>
			<item type="text" id ="prewarning_price" name="预警履约保障比率(%)" nullable="false" number="true"/>
			<item type="text" id ="lowest_price" name="最低履约保障比例(%)" nullable="false" number="true"/>
			<!--<item type="text" 	id ="second_amt" name="购回交易金额小写" editable="false"/>-->
			<item type="text" id="second_amt" name="购回交易金额" nullable="false" number="true"/>
			<item type="text" id="fix_rate" name="固定交易费用比例(%)" nullable="false" number="true" action="97000006"/>
