<?xml version="1.0" encoding="gbk" ?>
<function id="60100220" name="批量结息日维护" >
  <input>
      <item type="year" id="year_id" name="查询年份"  default="now"/>
  </input>
  <service type="procedure" id="P60100220" process="933" style="77">
	<param id="user_id" type="integer"/>
	<param id="year_id" type="integer"/>	
  </service>
  <output export="true">   
    <column type="integer" id="id"  name="id" display="false"/>
  	<column type="date"   id="bat_jx_date"        name="当前季度结息日" />
  	<column type="date"   id="last_bat_jx_date"   name="上一季度结息日" />
    <column type="string" id="time"   name="结息日设置时间"/>
	<button type="add"    id="add"    name="增加" action="60100221" multiple="-1"/>
	<button type="set"    id="set"    name="修改" action="60100222" multiple="0"/>
	<button type="delete" id="delete" name="删除" action="60100223" multiple="0"/>
   </output>
</function>