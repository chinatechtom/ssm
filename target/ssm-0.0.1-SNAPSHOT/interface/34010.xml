<?xml version="1.0" encoding="gbk" ?>
<function id="34010" name="获取客户相关信息" >
	<input>
		<item type="hidden"	id="cust_id" name="客户ID"/> 
		<item type="hidden"	id="order_id" name="合约ID"/> 
	</input>
	<service type="procedure" id="P17000016" process="911" style="77" implement="P1700001634010">
	<param id="user_id" type="integer" way="in" />  
	<param id="cust_id" type="integer" way="in" />
	<param id="order_id" type="integer" way="in" /> 
  </service> 	
  <output>
		<column type="number" id="cust_id"  name="客户id" display="false"/>   
		<column type="number" id="order_id" name="合约编号" display="false"/>	 
		<column type="string" id="cust_code" name="客户代码"/>
		<column type="string" id="cust_name" name="客户姓名"/>
		<column type="string" id="commness_cust_code"	name="普通资金账号"/>   
		<column type="string" id="institution_code"   	name="营业部代码"/>
		<column type="string" id="institution_name"		name="营业部名称"/>
		<column type="string" id="trade_date" name="交易日期" display="false"/>  
		<column type="string" id="status_name"		name="处理状态"/>
		<column type="string" id="now_guarantee_rate_12" name="当前初始保证金比例"/>
		<column type="string" id="applay_guarantee_rate_12" name="申请初始保证金比例"/>
		<column type="string" id="now_guarantee_rate_4" name="当前融券卖出保证金比例"/>
		<column type="string" id="applay_guarantee_rate_4" name="申请融券卖出保证金比例"/>
		<column type="string" id="now_guarantee_rate_5" name="当前融资买入保证金比例"/>
		<column type="string" id="applay_guarantee_rate_5" name="申请融资买入保证金比例"/>
		<column type="string" id="station"		name="站点信息"/>
		<column type="string" id="remark"		name="备注"/> 
		<column type="string" id="time"		name="申请时间"/>
		<column type="string" id="source_flag" name="来源标志" />
		<column type="string" id="commness_total_asset_wy"		name="普通资产"/>   
		<column type="string" id="credit_debit_wy"		name="信用负债"/>   
		<column type="string" id="credit_asset_wy"		name="信用资产"/>   
		<column type="string" id="total_asset_wy"		name="净资产"/>   
		<column type="string" id="total_asset"		name="净资产"/>   
		<column type="string" id="credit_limit"		name="授信额度"/>   
		<column type="string" id="year_earn_ratio"		name="收益率"/>   
		<column type="string" id="age" name="年龄"/> 
		<column type="string" id="suggest_guarantee_rate"		name="建议值"/> 
		<column type="string" id="asset_guarantee_rate"		name="资产建议值"/>
	</output>
</function>