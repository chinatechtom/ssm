<?xml version="1.0" encoding="gbk" ?>
<function id="3100101" name="规模类―额度汇总统计" >  
  <input>
  <item type="select" id="institution_id" name="营业部名称"  source="95000008"/>  
	<item type="date" id="date" name="开始日期"  default="last" nullable="false"/>
	<item type="date" id="date2" name="结束日期"  default="now" nullable="false"/>
  </input>
   <service type="procedure" id="p_statistics_3100101_query" process="911" style="77">
	<param id="user_id" type="integer" way="in" /> 
	<param id="date"/>
	<param id="date2"/>
	<param id="institution_id" />
  </service> 
  <output print="true">
    <column type="string" id="c1" name="营业部名称"/>  
	<column type="number" id="c2" name="信用资金账户数"/>
	<group name="融资融券审批额度(元)">
	<column type="number" id="c3" name="融  资" precision="2"/>
    <column type="number" id="c4"   name="融  券" precision="2"/>
	<column type="number" id="c5" name="合  计" precision="2"/>
	</group>
	<group name="融资融券实际使用额度(元)">
	<column type="number" id="c6"   name="融  资" precision="2"/>
	<column type="number" id="c7" name="融  券" precision="2"/>
	<column type="number" id="c8" name="合  计" precision="2"/>
	</group>
  </output>
</function>