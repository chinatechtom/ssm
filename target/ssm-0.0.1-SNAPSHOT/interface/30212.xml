<?xml version="1.0" encoding="gbk"?>
<function id="30212" name="机构净资本设置" >  
  <input>
	<item type="hidden" id="institution_id" name="机构id"  />
	<item type="select" id="currency_type"	name="货币类型"	source="018" editable="false"/>
	<item type="text"   id="yestoday_amt"	name="上日余额"			nullable="false"	number="true"/>
	<item type="text"	id="fund_amt"		name="净资本余额"			nullable="false"	number="true"/>
	<item type="text"	id="deposit"		name="单笔融资占比(%)"		nullable="false"	number="true"/>
	<item type="text"	id="withdraw"		name="单个客户融资占比(%)"	nullable="false"	number="true"/>
	<item type="text"	id="frozen_amt"		name="总融资占比(%)"			nullable="false"	number="true"/>
	<item type="text"	id="bs_amt"		name="融券总规模"			nullable="false"	number="true"/>
  </input>
  <service type="procedure" id="p_institution_net_asset_set"  process="811" style="1">
	<param id="user_id"			type="integer"	way="in"/>	
	<param id="institution_id"  type="integer"	way="in"/>
	<param id="currency_type"	type="string"	way="in"/>
	<param id="yestoday_amt"	type="double"	way="in"/>
	<param id="fund_amt"		type="double"	way="in"/>
	<param id="deposit"			type="double"	way="in"/>
	<param id="withdraw"		type="double"	way="in"/>
	<param id="frozen_amt"		type="double"	way="in"/>
	<param id="bs_amt"		type="double"	way="in"/>
  </service>
  <output>
  </output>
</function>