	<column type="string" id="app_time"					name="生成合约时间"/>
	<column type="string" id="remarks"					name="备注说明"/>
	<column type="number" id="bond_id"					name="合同id" display="false"/>
	<column type="string" id="product_type"				name="证券类型"/>
	<column type="string" id="manage_status"			name="处理状态id" display="false"/>
