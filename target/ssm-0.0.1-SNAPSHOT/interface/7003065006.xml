<?xml version="1.0" encoding="gbk" ?>
<function id="7003065006" name="授信系数*客户资产额度=最大交易额度" >
	<input>	
		<item type="hidden" id="cust_num" name="客户资产额度"/>
		<item type="hidden" id="credit_num_rate" name="授信系数"/>
	</input>
	<service type="procedure" id="P7003065006"  process="922" style="77">
		<param id="user_id" type="integer"/>	
		<param type="float" id="cust_num"/>
		<param type="float" id="credit_num_rate"/>
	</service>
	<output export="true">  
		<column type="number" id="credit_num"   name="最大交易额度"/>
		<column type="number" id="credit_num_net"   name="网上最大交易额度"/>
	</output>
</function>