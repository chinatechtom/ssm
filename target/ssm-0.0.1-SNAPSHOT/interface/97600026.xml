  <service type="procedure" id="P97600026" process="933" style="77">
    <param id="user_id"		type="integer"    way="in"/>
	<param id="stx_pwd" type="string"/>
	<param id="order_type" type="string"/>
	<param id="sstock_code" type="string"/>
	<param id="smarket_code" type="string"/>
