<?xml version="1.0" encoding="gbk" ?>
<function id="31381" name="系统状态切换">	
	<input>
		<item type="hidden"	id="id"		name="id"/>	
	</input>		
	<service type="procedure" id="p_system_status"   process="911" style="77">
		<param id="user_id"		type="integer"/>	
	</service>
	<output>
		<column type="string" id="date"                field="date"	              name="当前交易日期"/>
		<column type="string" id="next_day"            field="next_day"	          name="下一交易日"/>
		<column type="string" id="system_status_name"	 field="system_status_name" name="当前系统状态"/>
		<column type="string" id="system_status"	     field="system_status"      name="当前系统状态" display="false"/>
		<button type="edit" id="edit" name="切换系统状态" action="3138101"  multiple="0"/>
		<button type="edit" id="edit" name="设置下一交易日" action="3138102"  multiple="0"/>
  </output>
</function>