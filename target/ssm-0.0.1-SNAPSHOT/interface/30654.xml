<?xml version="1.0" encoding="gbk" ?>
<function id="30654" name="新增延长还贷日期申请" >
  <input>	
  <group name="原合同延长还贷日期">  	
  	<item type="text" id="over_time" name="延长日期" editable="false"/>
	<item type="textarea" id="remark" name="备注说明" editable="false"/>
  </group>  	
  <group name="延长还贷日期申请">
    <item type="hidden" id="bond_id" name="合同id"  nullable="false"/>
	<item type="hidden" id="cust_id" name="客户id" nullable="false"/>
    <item type="text" id="order_no" name="定单编号" nullable="false"/>
    <item type="hidden" id="formerly_voucher_no" name="原始凭证号" /> 	
	<item type="date" id="over_time1" name="延长日期" default="now" nullable="false"/>
	<item type="textarea" id="remark1" name="备注说明"/>
  </group>
  </input>
  <service type="procedure" id="p_over_time_insert" process="811" style="1">
	<param type="integer" id="user_id" way="in"/>
	<param type="string" id="order_no"  way="in"  />
	<param type="string" id="formerly_voucher_no" way="in" />
	<param type="integer" id="cust_id" way="in"/>
	<param type="string" id="remark1" way="in"/>
	<param type="integer" id="bond_id"/>
	<param type="string" id="over_time1" way="in"/>
  </service>
  <output/>
</function>