<?xml version="1.0" encoding="GBK" ?>
<function id="60880003" name="取客户股东账户">
  <input>
  	<item type="text" id="scust_no" name="客户号"/>
  </input>
  <service type="drtp" id="100306" process="711" style="77">
	  <param type="string" id="scust_no"/>
	  <param type="string" id="lvol0" default="1"/>	 
	  <param type="string" id="semp" default="SYSTEM"/>	 
  </service>
  <output target="60880004">
    	<column name="客户代码"	id="cust_code"   type="string"  field="scust_no"/>
    	<column name="市场代码"	id="market_code" type="string"  field="smarket_code"/>
	<column name="股东代码"	id="acc_code"    type="string"  field="sholder_ac_no"/>
	<column name="席位代码"	id="seat_code"   type="string"  field="sstock_code"/>
	<column name="股东姓名"	id="acc_name"    type="string"  field="sname"/>
  </output>
</function>