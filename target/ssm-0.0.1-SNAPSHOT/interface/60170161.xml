<?xml version="1.0" encoding="gbk" ?>
<function id="60170161" name="产品属性信息查询" >
  <input>
	<item type="text" id="project_code" name="产品代码"/>
  </input>
  <service type="procedure" id="P60170161" process="933" style="77">
	<param id="user_id" type="integer"/>
	<param id="project_code" type="string"/>
  </service>
  <output export="true">  
	<column type="string" id="project_name" name="产品名称"/>
	<column type="hidden" id="project_type" name="产品使用方式" display="false"/>
	<column type="string" id="project_type_name" name="产品使用方式"/>
	<column type="string" id="order_type" name="指令类型"/>	
	<column type="hidden" id="order_type_name" name="指令类型" display="false"/>	
	<column type="number" id="deadline" name="默认期限(天)"  />
	<column type="number" id="deadline_lower" name="期限下限(天)" />
	<column type="number" id="deadline_upper" name="期限上限(天)" />
	<column type="hidden" id="sect_interest_mode" name="分段计息模式" display="false"/>
	<column type="string" id="sect_interest_name" name="分段计息模式"/>
	<column type="number" id="hg_rate" name="默认利率(%)" precision="2"/>
	<column type="number" id="hg_rate_lower" name="利率下限(%)" precision="2"/>
	<column type="number" id="hg_rate_upper" name="利率上限(%)" precision="2"/>
	<column type="number" id="project_limit_amt" name="产品限额" precision="2"/>
	<column type="number" id="project_balance" name="产品余额" precision="2"/>	
	<column type="string" id="begin_date" name="产品开始日"/>
	<column type="string" id="end_date" name="产品到期日"/>
	<column type="number" id="trade_amt_lower" name="交易起点金额(元)" precision="0"/>
	<column type="number" id="trade_amt_unit" name="交易金额取整单位(元)" precision="0"/>
	<column type="number" id="convert_rate" name="折算率加成百分点点(%)" precision="2"/>
	<column type="hidden" id="if_auto_repur" name="到期是否自动购回" display="false"/>
	<column type="string" id="auto_repur_name" name="到期是否自动购回"/>
	<column type="hidden" id="if_earlier_allow" name="是否允许提前归还" display="false"/>
	<column type="string" id="earlier_allow_name" name="是否允许提前归还" />
	<column type="hidden" id="if_later_allow" name="-是否允许延期归还" display="false"/>
	<column type="string" id="later_allow_name" name="-是否允许延期归还" />
	<column type="string" id="interest_set_mode" name="结息模式" />	
	<column type="hidden" id="interest_set_name" name="结息模式" display="false"/>	
	<column type="number" id="prewarning_price" name="预警履约保障比例(%)" precision="3"/>
	<column type="number" id="lowest_price" name="平仓履约保障比例(%)" precision="3"/>
	<column type="number" id="collect_price" name="提取履约保障比例(%)" precision="3"/>
	<column type="number" id="earliest_deadline" name="最早归还期限(天)" />
	<column type="number" id="longest_penalty_days" name="最长违约天数(天)" />
	<column type="number" id="earlier_penalty_rate" name="提前归还违约金率(%)" precision="3"/>
	<column type="number" id="interest_over_rate" name="利息逾期违约金率(%)" precision="3"/>
	<column type="number" id="capital_over_rate" name="本金逾期违约金率(%)" precision="3"/>
	<column type="hidden" id="if_effective" name="-产品是否启用有效" display="false"/>	
	<column type="string" id="effective_name" name="-产品是否启用有效" />	
	<column type="hidden" id="if_use_share_amt" name="是否使用共享资金" display="false"/>
	<column type="string" id="use_share_name" name="是否使用共享资金" />
	<column type="number" id="amt_level" name="资金层级"  />
	<column type="number" id="interest_extend_days" name="付息宽限期限(天)"   default="6"/>
	<column type="string" id="remark" name="备注"/>
</output>
</function>