<?xml version="1.0" encoding="gbk" ?>
<function id="36000070" name="7.1.2营业部融资融券客户资产负债明细表" >  
  <input>
  <item type="select" id="institution_id" name="营业部名称"  source="95000008"/>
  <item type="text"   id="cust_code"           name="客户代码"  />
  <item type="date"   id="date"           name="查询日期"  />
  </input>
   <service type="procedure" id="P36000070" process="911" style="77">
  <param id="user_id" type="integer" way="in" /> 
  <param id="institution_id" type="integer" way="in" />
  <param id="cust_code"    type="string" way="in" />
  <param id="date"    type="string" way="in" />
  </service> 
  <output  print="true">
    <column type="number" id="id"               name="序号"/>
    <column type="string" id="institution_name" name="营业部"/> 
    <column type="string" id="cust_code"        name="客户代码"/>  
   <column type="string" id="cust_name"        name="客户名称"/>
   <group name="担保资产总额">
    <column type="number" id="gua_fund_amt"     name="资金"          precision="2" total="true"/>
     <column type="number" id="gua_stock_amt"    name="证券市值"  precision="2" total="true"/>
     <column type="number" id="gua_goods_total"  name="合计"  precision="2" total="true"/>
    </group> 
   <group name="客户保证金总额">  
    <column type="number" id="used_guarantee_amt"   name="已使用保证金"  precision="2" total="true"/>
     <column type="number" id="unuse_guarantee_amt"  name="未使用保证金"  precision="2" total="true"/> 
    <column type="number" id="guarantee_amt_total"  name="合计"  precision="2" total="true"/>
  </group> 
  <group name="衍生资产市值"> 
   <column type="number" id="other_fund"           name="资金"  precision="2" total="true"/>
   <column type="number" id="other_market_values"  name="证券市值"  precision="2" total="true"/>
   <column type="number" id="other_total"          name="合计" precision="2" total="true"/>
  </group> 
  <group name="负债总额">
   <column type="number" id="financing_amt"         name="融资"  precision="2" total="true"/>
   <column type="number" id="bs_amt"                name="融券"  precision="2" total="true"/>
   <column type="number" id="bs_fin_amt"          name="合计" precision="2" total="true"/>
  </group>
  </output>
</function>