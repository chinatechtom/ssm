<?xml version="1.0" encoding="gbk" ?>
<function id="30689" name="查询合同融资" >
  <input>
	<item type="hidden" id="bond_id" name="合同id"/>
	<item type="text" id="cust_code" name="客户代码"/>
	<item type="text" id="bond_no" name="合同编号"/>
  </input>
  <service type="procedure" id="p_bond_fix_qty_query" process="911" style="77">
	<param type="integer" id="user_id"/>	
	<param type="integer" id="bond_id"/>	
	<param id="bond_no"/>	
	<param id="cust_code"/>
  </service>
  <output >    
    <column type="number" id="bond_id" name="合同id" display="false"/>
	<column type="string" id="cust_code" name="客户代码"/>
	<column type="string" id="financing_product_id" name="融资产品id" display="false"/>
	<column type="string" id="financing_product_code" name="融资产品代码"/>
	<column type="string" id="financing_product_name" name="融资产品名称"/>
    <column type="number" id="fix_qty" name="申请上限" precision="2"/> 
	<column type="number" id="actual_fix_qty" name="当前未还融资金额" precision="2"/>
	<column type="number" id="return_fix_qty" name="累计归还融资金额" precision="2" />
	<column type="string" id="model" name="模式" display="false"/> 
	<column type="string" id="model_name" name="信用使用模式"/> 
	<column type="string" id="begin_time" name="起始日期"/>
	<column type="string" id="return_time" name="结束日期"/>  
	<column type="number" id="institution_id" 	name="营业部id" display="false"/> 
	<column type="string" id="institution_name" name="营业部名称" display="false"/>
	<column type="number" id="availability_day_num" name="有效天数" display="false"/>
	<column type="string" id="application_time" name="申请日期" display="false"/>
	<column type="number" id="cust_factor" name="客户融资费率因子" precision="2"/>
	<column type="string" id="order_no" name="合约编号"/>
	<column type="number" id="interest_rate" name="利息率" precision="4"/>
	<column type="string" id="currency" name="币种" display="false" />
	<column type="string" id="currency_name" name="货币类型"/>
	<column type="string" id="order_status" name="合约状态"/>
	<button type="send" id="edit" name="同步融资数据" action="80413106" multiple="0"/>
  </output> 
</function>