<?xml version="1.0" encoding="gbk" ?>
<function id="30808" name="融券专户历史数据查询">	
  <input>		
  	<item type="text" 	id="acc_code" 	    name="账户代码"/>
  	<item type="text" 	id="product_code" 	name="证券代码"/>
  	<item type="select" id="market_code"	name="市场代码" source="039"/>
	<item type="date" 	id="date" 			name="日期" default="last" nullable="false"/>	
  </input>  
  <service type="procedure" id="p_bs_account_data_hisquery" implement="p_bs_account_data_hisquery30808"  process="955" style="77">
	<param id="user_id"	     type="integer" way="in"/> 
	<param id="acc_code"     type="string"  way="in"/>
	<param id="product_code" type="string"  way="in"/>		
	<param id="market_code"  type="string"  way="in"/>
	<param id="date"         type="string"  way="in"/>
  </service>  
  <output export="true">
	<column type="string" id="ACC_CODE"   			name="账户代码"/>
	<column type="string" id="MARKET_CODE_NAME"		name="市场名称"/>			
	<column type="number" id="PRODUCT_ID"			name="证券ID" display="false"/>
	<column type="string" id="PRODUCT_CODE"			name="证券代码"/>
	<column type="string" id="PRODUCT_NAME"			name="证券名称"/>
	<column type="number" id="ASSET_QTY"	        name="证券数量"	    precision="0" total="true" />
	<column type="number" id="REMAIN_BORROW_QTY"	name="融券余券数量"	    precision="0" total="true" />
	<column type="number" id="CAN_BORROW_OUT_QTY"	name="可融证券数量"	    precision="0" total="true" />
	<column type="number" id="SELF_INOUT_VOL"		name="自有证券转入(转出)净额数量"   precision="0" total="true"/>
	<column type="number" id="SELF_INOUT_RATE" 		name="占总股本比例%"		precision="2" display="false"/>
	<column type="number" id="SELF_INOUT_BAL"		name="自有证券转入市值"	precision="2" total="true"/>
	<column type="number" id="FROZEN_CAN_BSTK_VOL"	name="冻结可融证券数量"	precision="0" total="true" display="false"/>
	<column type="number" id="FROZEN_CAN_BSTK_RATE"	name="占总股本比例%"		precision="2" display="false"/>
	<column type="number" id="FROZEN_CAN_BSTK_BAL"	name="冻结可融证券市值"	precision="2" total="true" display="false"/>
	<column type="number" id="UNFROZEN_CAN_BSTK_VOL" name="未冻结可融证券数量"	precision="0" total="true" display="false"/>
	<column type="number" id="UNFROZEN_CAN_BSTK_RATE" name="占总股本比例%"		precision="2" display="false"/>
	<column type="number" id="UNFROZEN_CAN_BSTK_BAL" name="未冻结可融证券市值"	precision="2" total="true" display="false"/>
	<column type="number" id="OVER_BSTK_VOL"		name="逾期融出证券数量"	precision="0" total="true"/>
	<column type="number" id="OVER_BSTK_RATE"		name="占总股本比例%"		precision="2" display="false"/>
	<column type="number" id="OVER_BSTK_BAL"		name="逾期融出证券市值"	precision="2" total="true"/>
	<column type="number" id="UNOVER_BSTK_VOL"		name="未逾期融出证券数量"	precision="0" total="true"/>
	<column type="number" id="UNOVER_BSTK_RATE"		name="占总股本比例%"		precision="2" display="false"/>
	<column type="number" id="UNOVER_BSTK_BAL"		name="未逾期融出证券市值"	precision="2" total="true"/>
	<column type="number" id="BS_ALL_VOL"			name="融券总数量"			precision="0" total="true"/> 
	<column type="number" id="BS_ALL_BAL"			name="融券总市值"			precision="2" total="true"/>
	<column type="number" id="CUR_AMT"				name="融券规模"				precision="2" total="true"/>	
	<column type="string" id="DATE"   				name="日期"/>
  </output>
</function>