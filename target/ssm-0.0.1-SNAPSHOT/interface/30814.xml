<?xml version="1.0" encoding="gbk" ?>
<function id="30814" name="客户历史结息查询" >
	<input export="true">	
		<item type="select"  id="statistics_type"	  name="查询方式" source="515" nullable="false"/>
		<item type="select"  id="institution_id"	  name="营业部名称"  source="95000008"/>
		<item type="hidden"  id="cust_id"	          name="客户id"/>		
		<item type="text"    id="cust_code"	          name="客户代码"/>	
		<item type="select"  id="order_type"	      name="利费类型" source="394"/>
		<item type="date"    id="date"	              name="日期选择"/>	
		<group name="分页查询" page="true">		
			<item type="text"   id="page_num"       name="页码"     integer="true" default="1"/>
			<item type="select" id="rows_num"       name="每页行数" integer="true" source="95000012" nullable="false"/>
		</group>
	</input>
    <service type="procedure" id="p_cust_over_credit_charge_his"  implement="p_cust_over_credit_charge_his30814"  process="955" style="77">
		<param id="user_id"	            type="integer" way="in"/> 
		<param id="statistics_type"	    type="integer" way="in"/> 
		<param id="page_num"            type="integer" way="in"/>
		<param id="rows_num"            type="integer" way="in"/>	
		<param id="institution_id"	    type="integer" way="in"/> 
		<param id="cust_id"	            type="integer" way="in"/> 
		<param id="cust_code"	        type="string"  way="in"/>
		<param id="order_type"	        type="string"  way="in"/>
		<param id="date"	            type="string"  way="in"/> 		 			
    </service>
    <output export="true">
		<column type="string" id="institution_name"					name="营业部名称"/>
		<column type="string" id="cust_code"					      name="客户代码"/>
		<column type="string" id="cust_name"					      name="客户姓名"/>
		<column type="string" id="order_type"			          name="利费类型"/>
		<column type="string" id="statis_date"			        name="统计日期"/>		
		<column type="number" id="interest_knot"	          name="已结利息"	      precision="2" total="true"/>		
		<column type="number" id="fix_borrow_knot"	        name="已结固定额度费"	precision="2" total="true"/>
		<column type="number" id="takeup_borrow_knot"       name="已结占用额度费"	precision="2" total="true"/>
		<column type="number" id="bef_rtn_commission_knot"  name="已结提前还贷手续费"	precision="2" total="true"/>
		<column type="number" id="punish_knot"	            name="已结逾期罚息"	  precision="2" total="true"/>
		<column type="number" id="bad_punish_knot"	        name="已结坏帐罚息"	  precision="2" total="true"/>
		<column type="number" id="interest_pay"	            name="已付利息"	      precision="2" total="true"/>
		<column type="number" id="fix_borrow_pay"	          name="已付固定额度费"	precision="2" total="true"/>
		<column type="number" id="takeup_borrow_pay"        name="已付占用额度费"	precision="2" total="true"/>
		<column type="number" id="bef_rtn_commission_pay"   name="已付提前还贷手续费"		precision="2" total="true"/>
		<column type="number" id="punish_pay"		            name="已付逾期罚息"	  precision="2" total="true"/>	
		<column type="number" id="bad_punish_pay"		        name="已付坏帐罚息"	  precision="2" total="true"/>	
    </output>
</function>
