<?xml version="1.0" encoding="gbk" ?>
<function id="60044561" name="子合约详细" >
	<input>	
        <item type="hidden" id="order_id"     name="合约ID"/>
		<item type="hidden" id="order_no"     name="合约no"/>
	</input>
	<service type="procedure" id="P60044561" process="933" style="77">
		<param type="integer" id="user_id" way="in" />
		<param type="integer" id="order_id" way="in" />
		<param type="string"  id="order_no" way="in"/>
	</service>
	<output>
        <column type="string" id="order_no"   name="合约编号"/>	
		<column type="string" id="bond_no"   name="合同编号"/>	
		<column type="string" id="sbond_no"   name="交易协议书编号"/>
		<column type="string" id="sbond_status_name"   name="交易协议书状态"/>
		<column type="string" id="market_code"   name="市场代码"/>
		<column type="string" id="product_code"   name="标的证券代码"/>
		<column type="number" id="hg_num"   name="标的证券数量" precision="0"/>
		<column type="number" id="first_amt"   name="初始交易金额" precision="2"/>
		<column type="number" id="second_amt"   name="购回交易金额" precision="2"/>
		<column type="number" id="hg_convert_rate"   name="折扣比率(%)" precision="3"/>
		<column type="string" id="engage_trade_date"   name="初始交易日期"/>
		<column type="string" id="bb_date"   name="购回交易日期"/>
		<column type="number" id="days"   name="购回期限(天)" precision="0"/>
<!--		<column type="number" id=""hg_rate""   name="购回价格(每100元年收益)" precision="4"/>-->
		<column type="number" id="penalty_rate"   name="违约金比率(日利率%)" precision="4"/>
		<column type="string" id="remarks"   name="备注"/>
		<column type="number" id="prewarning_price"   name="预警履约保障比率(%)" precision="3"/>
		<column type="number" id="lowest_price"   name="最低履约保障比例(%)" precision="3"/>
		<column type="number" id="app_price"   name="标的证券价格" precision="2"/>
		<column type="string" id="related_sbond_no"   name="主关联交易协议书编号"/>
		<column type="string" id="product_name"   name="标的证券名称"/>
		<column type="string" id="product_type"   name="标的证券类型"/>
		<column type="string" id="cust_code"   name="客户代码"/>	
		<column type="string" id="cust_name"   name="客户名称"/>	
		<column type="string" id="institution_code"   name="营业部代码"/>	
		<column type="string" id="institution_name"   name="营业部名称"/>
		<column type="string" id="lender_id"   name="融出方ID" display="false"/>
		<column type="string" id="lender_code"   name="融出方代码"/>
		<column type="string" id="lender_name"   name="融出方名称"/>
		<column type="string" id="lender_acc_code"   name="融出方证券账户"/>
		<column type="string" id="lender_type"   name="融出方类型" display="false"/>
		<column type="string" id="lender_type_name"   name="融出方类型"/>
		<column type="string" id="mortgagee_type"   name="质权人类型" display="false"/>
		<column type="string" id="mortgagee_type_name"   name="质权人类型"/>
		<column type="string" id="hg_num_big"   name="标的证券数量大写"/>
		<column type="string" id="first_amt_big"   name="初始交易金额大写"/>
		<column type="string" id="second_amt_big"   name="购回交易金额大写"/>
		<column type="number" id="fix_rate"   name="固定交易费用比例(%)" precision="2"/>
		<column type="number" id="manual_chg"   name="购回金额手工调整" precision="2"/>
		<column type="string" id="zjyt"   name="资金用途"/>
		<column type="number" id="wghjyye"   name="客户未购回交易余额" precision="2"/>
		<column type="number" id="yspwjyje"   name="客户已审批未交易金额" precision="2"/>
		<column type="number" id="credit_num"   name="客户最大交易额度" precision="2"/>
		<column type="number" id="unused_credit"   name="本次交易前剩余额度" precision="2"/>
	</output>
</function>