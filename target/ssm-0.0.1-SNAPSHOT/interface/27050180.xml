<?xml version="1.0" encoding="gbk" ?>
<function id="27050180" name="担保品转入转出统计表">
	<input>
		<item type="select"	 id="insitution_id"			name="营业部名称" source="95000008"/>
		<item type="text"	 id="cust_code"			    name="客户代码" />
		<item type="text"	 id="product_code"			name="证券代码" />
		<item type="select"	 id="market_code"			name="市场代码"    source="039"/>
		<item type="date"    id="begin_date"	        name="起始日期"		default="last" nullable="false"/>	
		<item type="date"    id="end_date"	            name="结束日期"		default="now"  nullable="false"/>	
		
	</input>
    <service type="procedure" id="P27050180"  process="911" style="77">
		<param id="user_id"	        type="integer" way="in"/>
		<param id="insitution_id"	type="integer" way="in"/>
		<param id="cust_code"	    type="string"  way="in"/>
		<param id="product_code"	type="string"  way="in"/>
		<param id="market_code"	    type="string"  way="in"/>
		<param id="begin_date"		type="string"  way="in"/>
		<param id="end_date"		type="string"  way="in"/>
    </service>
    <output export="true" autoload="false" print="true">
		<column type="number" id="row_id"				        name="序号"		/>
		<column type="string" id="institution_code"				name="营业部代码"/>
		<column type="string" id="institution_name"				name="营业部名称"/>
		<column type="string" id="cust_code"				    name="客户代码"/>
		<column type="string" id="cust_name"				    name="营业名称"/>		
		<column type="number" id="assure_rate"				    name="维持担保比例" precision="4"/>
		<column type="number" id="debt_amt_chg"					name="负债变动金额" precision="2" total="true"/>
		<column type="string" id="risk_level_name"			name="风险类别"	 />
		<column type="string" id="product_code"				    name="证券代码" />
		<column type="string" id="product_name"				    name="证券名称" />	
		<group name="客户担保品价值">
		  <group name="证券">
		    <column type="number" id="all_stk"				name="证券数量"		precision="0" total="true"/>
			<column type="number" id="all_stk_bal"		    name="证券市值"		precision="2" total="true"/>
		  </group>
			<column type="number" id="fund_amt"			    name="资金"		    precision="2" total="true"/>
			<column type="number" id="asset_amt"			name="合计"		    precision="2" total="true"/>			
		</group>
		<group name="担保品转入价值">
		  <group name="担保证券转入">
		    <column type="number" id="sec_chg_in"			name="证券数量"		precision="0" total="true"/>
			<column type="number" id="sec_chg_in_amt"		name="证券市值"		precision="2" total="true"/>
		  </group>
			<column type="number" id="fund_chg_in"			name="担保资金转入" precision="2" total="true"/>
			<column type="number" id="gua_in"		        name="合计"		     precision="2" total="true"/>			
		</group>
		<group name="担保品转出价值">
		  <group name="担保证券转出">
		    <column type="number" id="sec_chg_out"			name="证券数量"		precision="0" total="true"/>
			<column type="number" id="sec_chg_out_amt"		name="证券市值"		precision="2" total="true"/>
		  </group>
			<column type="number" id="fund_chg_out"			name="担保资金转出" precision="2" total="true"/>
			<column type="number" id="gua_out"		        name="合计"		     precision="2" total="true"/>			
		</group>		
    </output>
</function>
