<?xml version="1.0" encoding="gbk" ?>
<function id="3031501" name="证券权益信息浏览" >
  <input>
    <item type="hidden" id="product_id"     name="证券id" />
	<item type="text"   id="product_code"   name="证券代码" />
    <item type="text"   id="product_name"   name="证券名称" />
	<item type="date"   id="reg_date"   name="股权登记日" />
    <item type="select"   id="real_flag"   name="实时标志" source="335"/>	
  </input>
  <service type="procedure" id="p_product_division_query"  process="911" style="77"> 
    <param id="user_id"		   type="integer" way="in"/>
    <param id="product_id"	   type="integer" way="in"/>
    <param id="product_code"   type="string"  way="in"/>  
	  <param id="product_name"   type="string"  way="in"/>  
	  <param id="real_flag"   type="string"  way="in"/> 
	<param id="reg_date"   type="string"  way="in"/> 
  </service>
	<output>
		<column type="string" id="real_flag"			   name="实时标志" />
		<column type="number" id="product_id"			   name="证券id" display="false"/>
		<column type="string" id="product_code"			   name="证券代码"/>
		<column type="string" id="product_name"			   name="证券名称"/>	
		<column type="string" id="market_code"		       name="市场代码"/>				
		<column type="number" id="send_off_10"			   name="每10股送股" precision="4"/>
		<column type="number" id="allot_10"				   name="每10股配股" precision="4"/>
		<column type="number" id="allot_price"			   name="配股价格" precision="4"/>
		<column type="number" id="every_bonus"			   name="每股红利" precision="4"/>
		<column type="string" id="begin_date"			   name="配股起始日期"/>
		<column type="string" id="end_date"				   name="配股结束日期"/>
		<column type="string" id="register_date"		   name="登记日期"/>
		<column type="string" id="remove_date"			   name="除权日期"/>
		<column type="string" id="off_date"				   name="退市日期"/>
		<column type="string" id="post_date"			   name="要约收购或退市公告日期"/>
		<column type="string" id="division_kind"		   name="权益类型1" display="false"/>
		<column type="string" id="division_type"		   name="权益类别1" display="false"/>
		<column type="string" id="division_kind_name"	   name="权益类型"  display="false"/>
		<column type="string" id="division_type_name"	   name="权益类别"  display="false"/>
		<column type="string" id="qzdm"					   name="权证代码"/>
		<column type="number" id="send_qz_10"			   name="每10股送权证" precision="4"/>
		<!--<button type="edit"   id="edit"					   name="设置权益信息" action="30316" multiple="0"/>-->
		<!--<button type="edit"   id="edit"					   name="同步标的" action="30309" multiple="0"/>-->
	</output>
	<assistant action="30304,30305,30306">		
		<link src="product_id" target="product_id"/>
    </assistant>
</function>