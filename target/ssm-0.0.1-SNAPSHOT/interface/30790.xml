<?xml version="1.0" encoding="gbk" ?>
<function id="30790" name="限售股东及董监高信用账户持有限售股票持仓统计" >
  <input>
	<item type="text"    id="cust_code"		name="客户代码"/>
	<item type="text"    id="holder_acc"	name="证券帐户"/>	
	<item type="text"    id="cert_no"		name="证件号码"/>
	<item type="select"  id="type"			name="类型" source="400"/>
	<item type="select"  id="market_code"	name="市场代码" source="039"/>
	<item type="text"    id="sec_code"		name="证券代码"/>
	<item type="date"    id="date"		    name="日期选择"/>
  </input>
  <service type="procedure" id="p_sjsfw_tzxx_xy_hold_query"   process="911" style="77"> 
    <param id="user_id"		type="integer" way="in"/>
	<param id="cust_code"	type="string" way="in"/>
	<param id="holder_acc"	type="string" way="in"/>
	<param id="cert_no"		type="string" way="in"/>
	<param id="type"		type="string" way="in"/>
	<param id="market_code"	type="string" way="in"/>
	<param id="sec_code"	type="string" way="in"/>
	<param id="date"	    type="string" way="in"/>
  </service>
	<output>
		<column type="string" id="xwdm"	name="席位代码" />
		<column type="string" id="gddm"	name="证券帐户"/>
		<column type="string" id="zqdm"	name="证券代码"/>
		<column type="string" id="product_name"	name="证券名称"/>
		<column type="string" id="market_name"	name="市场代码"/>	
		<column type="string" id="zqsl"	name="限售证券数量" />
		<column type="string" id="sfzh"	name="证件号码"/>
		<column type="string" id="scdm"	name="市场代吗" display="false"/>
		<column type="string" id="sjlb"	name="类型"/>
		<column type="number"  id="asset_qty" 				name="证券余量"		precision="0" total="true"/>
		<column type="number"  id="cur_buy_vol"				name="当日买入数量"	precision="0" total="true"/>
		<column type="number"  id="cur_buy_amt"				name="当日买入金额"	precision="0" total="true"/>        
		<column type="number"  id="cur_sell_vol"			name="当日卖出数量"	precision="0" total="true"/>
		<column type="number"  id="cur_sell_amt"			name="当日卖出金额"	precision="0" total="true"/>           
		<column type="number"  id="abnormal_frzn_vol"		name="异常冻结数量"	display="false"/>          
		<column type="number"  id="buy_unfrzn_vol"			name="买入解冻数量"	precision="0" total="true" display="false"/>            
		<column type="number"  id="sell_frozen_vol"			name="卖出冻结数量"	precision="0" total="true" display="false"/>             
		<column type="number"  id="manual_frozen_vol"		name="手工冻结数量"	display="false"/>
		<column type="number"  id="manual_unfrzn_vol"		name="手工解冻数量"	display="false"/>
		<column type="number"  id="trin_bal"				name="当日转入数量"		precision="0" total="true"/>
		<column type="number"  id="trout_bal"				name="当日转出数量"		precision="0" total="true"/>
		<column type="number"  id="uncome_buy_vol"			name="买入未过户数量"	display="false"/>
		<column type="number"  id="uncome_buy_amt"			name="买入未过户金额"	display="false"/>
		<column type="number"  id="uncome_sell_vol"			name="卖出未过户数量"	display="false"/>
		<column type="number"  id="uncome_sell_amt"			name="卖出未过户金额"	display="false"/>
		<column type="string"  id="sjrq"	                name="输入日期" />
	</output>
</function>
