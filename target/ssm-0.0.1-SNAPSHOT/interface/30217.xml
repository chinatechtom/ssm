<?xml version="1.0" encoding="gbk" ?>
<function id="30217" name="融券专户额度设置" >
	<input>
		<item type="hidden"  id="acc_id"   name="账户id"	nullable="false"/>
		<item type="text"	 id="acc_code"   name="账户代码"	editable="false"/>
		<item type="hidden"  id="product_id"	name="证券id" nullable="false"/>
		<item type="text"	 id="product_code"   name="证券代码"	editable="false"/>
		<item type="text"	 id="product_name"   name="证券名称"	editable="false"/>
		<item type="text"	 id="borrow_stock_vol_all"	name="融券可申请上限" nullable="false" integer="true"  maxlen="14"/>
		<item type="text"	 id="credit_frozen_vol"	name="冻结数量" nullable="false" integer="true"/>
	</input>
    <service type="procedure" id="p_borrow_stock_quantum_set"  process="811" style="1">
		<param id="user_id"	    type="integer" way="in"/>
		<param id="acc_id"	    type="integer" way="in"/>
		<param id="product_id"	type="integer"  way="in"/>
		<param id="borrow_stock_vol_all" type="integer"  way="in"/>
		<param id="credit_frozen_vol" type="integer"  way="in"/>
    </service>
    <output/>   
</function>