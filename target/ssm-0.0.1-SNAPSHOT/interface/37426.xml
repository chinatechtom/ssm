<?xml version="1.0" encoding="gbk" ?>
<function id="37426" name="申请最低保证金比例变更" >	
  <input> 
		<item type="hidden"	id="cust_id"						name="客户id" />
		<item type="hidden"	id="origin_lower_guarantee_rate"	name="原始最低保证金比例"/>
		<item type="hidden" id="apply_lower_guarantee_rate" 	name="申请最低保证金比例"/>
		<item type="hidden" id="add_score"						name="投资者属性额外加分项"/>
		<item type="text"	id="cp_profit_score"				name="相对收益率得分"			editable="false"/>
		<item type="text"	id="cur_asset_score"				name="净资产得分"				editable="false"/>
		<item type="text"	id="rzrq_commission_score"			name="信用交易佣金得分"		editable="false"/>
		<item type="text"	id="rzrq_fee_score"					name="信用交易息费收入得分"	editable="false"/>
  </input>
  <service type="procedure" id="p_total_score_query"  implement="p_total_score_query37426" process="911" style="77"> 
		<param id="user_id"						type="integer"/>
		<param id="origin_lower_guarantee_rate"	type="double"/>
		<param id="apply_lower_guarantee_rate"	type="double"/>
		<param id="add_score"					type="double"/>
		<param id="cp_profit_score"				type="double"/>
		<param id="cur_asset_score"				type="double"/>
		<param id="rzrq_commission_score"		type="double"/>
		<param id="rzrq_fee_score"				type="double"/>
  </service>
  <output>
  		<column type="number" id="total_score" 					name="总得分"					precision="2"/>
		<column type="number" id="suggest_lower_guarantee_rate" name="建议融资/融券保证金比例"	precision="2"/>
		<column type="number" id="lower_guarantee_rate"			name="最低融资/融券保证金比例"	precision="2"/>
		<column type="number" id="adjuest_value"				name="调整值"					precision="2"/>		
  </output>		
</function>