<?xml version="1.0" encoding="gbk" ?>
<function id="30416" name="信用级别担保率浏览" >
  <input>
	<item type="select"	id="credit_level_id"		name="信用级别名称"	source="90000002"/>
    <item type="hidden"	id="guarantee_rate_id"		name="担保率类id"	/>
	<item type="text"	id="guarantee_rate_name"	name="担保率名称"/>	
    <item type="select"	id="instruction"			name="交易指令"		source="310"/>	
  </input>
  <service type="procedure" id="p_credit_guarantee_rate_browse" implement="p_credit_guarantee_rate_browse416"  process="911" style="77"> 
    <param id="user_id"				type="integer" way="in"/> 
	<param id="credit_level_id"		type="integer" way="in"/>
    <param id="guarantee_rate_id"	type="integer" way="in"/>
    <param id="guarantee_rate_name"	type="string"  way="in"/>
	<param id="instruction"			type="string"  way="in"/>	
  </service>
  <output autoload="true">
	<column type="number" id="credit_level_id"		name="信用级别id" display="false"/>
	<column type="string" id="credit_level_code"	name="信用级别代码" display="false"/>	
	<column type="string" id="credit_level_name"	name="信用级别名称" />
	<column type="string" id="risk_level"			name="风险级别1"	  display="false"/>
	<column type="string" id="risk_level_name"		name="风险级别"/>
	<column type="string" id="if_estop"				name="是否禁止1"  display="false"/>
	<column type="string" id="if_estop_name"		name="是否禁止"	display="false"/>	
	<column type="number" id="guarantee_rate_id"	name="担保率类id" display="false"/>	
	<column type="number" id="guarantee_rate"		name="担保率"	  precision="4"/>	
	<column type="string" id="guarantee_rate_name"	name="担保率名称"/>
	<column type="string" id="guarantee_rate_use"	name="担保率说明"/>
	<column type="string" id="instruction"			name="交易指令1"  display="false"/>
	<column type="string" id="instruction_name"		name="担保率用途"/>
	<column type="number" id="fin_limit_standard"	name="融资上限标准"      precision="4" display="false"/>	
	<column type="number" id="bs_limit_standard"	name="融券上限标准"	  precision="4" display="false"/>	
	<column type="string" id="remark"		        name="备注" display="false"/>
	<button type="send"   id="add"					name="同步所有信用级别担保率"	action="3041602"	multiple="-1"/>
	<button type="add"    id="add"					name="新增信用级别担保率"	action="30417"	multiple="-1"/>
	<button type="edit"   id="edit"					name="修改信用级别担保率"	action="30418"	multiple="0"/>
	<button type="ok"	  id="ok"					name="同步信用级别担保率"	action="81413108"	multiple="0"/>
  </output>
</function>