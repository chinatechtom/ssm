<?xml version="1.0" encoding="gbk" ?>
<function id="30316" name="证券权益信息设置" >
  <input>
    <item type="hidden" 	id="product_id"			   name="证券id"	/>
	<item type="hidden" 	id="market_code"		   name="证券id"	action="3031602"/>
	<group name="证券基本信息">
		<item type="text" 	id="product_code"		   name="证券代码"	editable="false" />
		<item type="text" 	id="product_name"		   name="证券名称"	editable="false" />
	</group>			
	<group name="证券权益信息">
		<item type="text" 	id="send_off_10"		   name="每10股送股"	number="true" />
		<item type="text" 	id="allot_10"			   name="每10股配股或认购"	number="true"/>
		<item type="text" 	id="allot_price"		   name="配股价格"		number="true"/>
		<item type="text" 	id="every_bonus"		   name="每股红利"		number="true" />
		<item type="date" 	id="begin_date"			   name="配股起始日期" default="null"/>
		<item type="date" 	id="end_date"			   name="配股结束日期" default="null"/>
		<item type="date" 	id="register_date"		   name="登记日期"		nullable="false" default="null" />
		<item type="date" 	id="remove_date"		   name="除权日期"		nullable="false" default="null"/>
		<item type="date" 	id="off_date"		       name="收购截止日或退市日期"   default="null"/>
		<item type="date" 	id="post_date"		       name="要约收购或退市公告日期" default="null"/>
		<item type="hidden" id="division_kind"		   name="权益类别"/>
		<item type="hidden" id="division_type"		   name="权益类型"/>	
		<item type="text" 	id="qzdm"				   name="权证代码"		mixlen = "6" maxlen="6"/>
		<item type="text" 	id="send_qz_10"			   name="每10股送权证"	number="true"/>
	</group>	
  </input>
  <service type="procedure" id="p_product_division_set"  process="811" style="1"> 
    <param id="user_id"		   type="integer" way="in"/>
    <param id="product_id"	   type="integer" way="in"/>
    <param id="send_off_10"    type="double"  way="in"/>  
	<param id="allot_10"	   type="double"  way="in"/>  
	<param id="allot_price"    type="double"  way="in"/>  
	<param id="every_bonus"	   type="double"  way="in"/>  
	<param id="begin_date"	   type="string"  way="in"/> 
	<param id="end_date"	   type="string"  way="in"/>  
	<param id="register_date"  type="string"  way="in"/>  
	<param id="remove_date"	   type="string"  way="in"/>
	<param id="off_date"	   type="string"  way="in"/>
	<param id="post_date"	   type="string"  way="in"/>
	<param id="division_kind"  type="string"  way="in"	default="1"/>  
	<param id="division_type"  type="string"  way="in"	default="1"/>  
	<param id="qzdm"		   type="string"  way="in"/> 
	<param id="send_qz_10"    type="double"  way="in"/>
  </service>
	<output/>
</function>