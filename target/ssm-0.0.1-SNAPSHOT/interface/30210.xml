<?xml version="1.0" encoding="gbk" ?>
<function id="30210" name="机构证券查询" >
	<input>			
		<item type="hidden"  id="institution_id"	    name="机构id"/>
		<item type="hidden"  id="acc_id"	            name="机构账户id"/>
		<item type="text"  id="acc_code"	            name="证券账户"/>		
		<item type="select"  id="market_code"	 name="市场代码" source="039"/>		
		<item type="text"  id="product_code"	            name="证券代码"/>		
	</input>
    <service type="procedure" id="p_institution_acc_sec_query"  implement="p_institution_acc_sec_query30210"  process="911" style="77">
		<param id="user_id"	            type="integer" way="in"/> 
		<param id="institution_id"	    type="integer" way="in"/>
		<param id="acc_id"	            type="integer" way="in"/> 
		<param id="acc_code"	        type="string"  way="in"/>		
		<param id="market_code"	        type="string"  way="in"/>		
		<param id="product_code"	        type="string"  way="in"/>		
    </service>
    <output export="true">
		<column type="number" id="acc_id"				name="账户id"		display="false"/>
		<column type="string" id="acc_code"				name="证券账户"/>
		<column type="string" id="date"					name="日期"/>
		<column type="string" id="property_type"		name="资产种类"		display="false"/>
		<column type="string" id="currency_type"		name="货币类型"/>
		<column type="string" id="market_code"			name="市场"/>
		<column type="string" id="product_type"			name="证券类型"/>
		<column type="number" id="product_id"			name="证券id"		display="false"/>
		<column type="string" id="product_code"			name="证券代码"/>
		<column type="number" id="begin_asset_qty"		name="期初证券余额"	precision="2" total="true"/>
		<column type="number" id="asset_qty"			name="证券余额"		    precision="2" total="true"/>
		<column type="number" id="fact_borrow_stock"	name="实际融券余额"	precision="2" total="true"/>
		<column type="number" id="remain_borrow_qty"	name="余券数量"     	precision="2" total="true"/>
		<column type="number" id="can_borrow_out_qty"	name="可融证券数量"    precision="2" total="true"/>
		<column type="number" id="cur_buy_vol"			name="买券还券数量"	precision="2" total="true"/>
		<column type="number" id="cur_buy_amt"			name="当日买入金额"	precision="2" total="true" display="false"/>
		<column type="number" id="cur_sell_vol"			name="融券卖出数量"	precision="2" total="true"/>
		<column type="number" id="cur_sell_amt"			name="当日卖出金额"	precision="2" total="true" display="false"/>
		<column type="number" id="abnormal_frzn_vol"	name="异常冻结数量"	display="false"/>
		<column type="number" id="buy_unfrzn_vol"		name="买入解冻数量"	precision="2" total="true" display="false"/>
		<column type="number" id="sell_frozen_vol"		name="卖出冻结数量"	precision="2" total="true" display="false"/>
		<column type="number" id="manual_frozen_vol"	name="手工冻结数量"	display="false"/>
		<column type="number" id="manual_unfrzn_vol"	name="手工解冻数量"	display="false"/>
		<column type="number" id="trin_bal"				name="当日调入数量"		precision="2" total="true"/>
		<column type="number" id="trout_bal"			name="当日调出数量"		precision="2" total="true"/>
		<column type="number" id="uncome_buy_vol"		name="买入未过户数量"	display="false"/>
		<column type="number" id="uncome_buy_amt"		name="买入未过户金额"	display="false"/>
		<column type="number" id="uncome_sell_vol"		name="卖出未过户数量" display="false"/>
		<column type="number" id="uncome_sell_amt"		name="卖出未过户金额"	display="false"/>
		<column type="number" id="last_price"			name="最新价格"		precision="3"/>
		<column type="number" id="asset_value"			name="证券市值"		precision="2" total="true"/>
		<column type="number" id="buy_cost_price"		name="持仓成本"		display="false"/>
		<column type="number" id="profit_loss"			name="浮动盈亏"		display="false"/>
		<column type="number" id="stock_convert_rate" 	name="证券折算率"		display="false"/>
		<column type="number" id="stock_convert_amt"	name="证券折算价值"	precision="2" total="true"/>
		<column type="number" id="borrow_stock_vol_all"	name="融券可申请上限"	precision="2" display="false"/>
		<column type="number" id="borrow_stock_req_used"	name="融券申请已用额度"	precision="2" display="false"/>
		<column type="number" id="un_used_credit"		name="未使用融券总量" display="false"/>
		<column type="number" id="un_frozen_vol"		name="未冻结证券余量" display="false"/>
    </output>
</function>