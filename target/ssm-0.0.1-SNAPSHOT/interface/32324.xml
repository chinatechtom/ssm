<?xml version="1.0" encoding="gbk" ?>
<function id="32324" name="一周内或当日累计转入股份市值大于500万" >
  <input>	
	<item type="select" id="insitution_id" name="营业部名称" source="95000008"/>
	<item type="text" id="cust_code" name="客户代码" />
	<item type="date" id="date" name="日期" />
	<item type="text" id="asset" name="资产阀值" default="5000000"/>
  </input>
  <service type="procedure" id="p_weekly_trin_bal_stock_query" process="911" style="77">
	<param id="user_id" type="integer"/>
	<param id="insitution_id" type="integer"/>
	<param id="cust_code"/>
	<param id="date"/>
	<param id="asset" type="integer"/>

  </service>
  <output export="true">  
  	<column type="string" id="year"   		 name="年份"  />
  	<column type="string" id="week"   		 name="周"  />
  	<column type="string" id="institution_name"   name="营业部名称" />  
    <column type="string" id="cust_code"   name="客户代码"  />
    <column type="string" id="cust_name"   name="客户姓名"  />
    <column type="number" id="fund_chg"  	 name="变动金额"  />   
	</output>
  	<assistant/>
</function>