<?xml version="1.0" encoding="gbk" ?>
<function id="31920" name="保证金比例调整值查询" >	
	<input>	
		<item type="text"	id="cust_code"	name="信用资金账户"/>
		<item type="text" id="commness_cust_code"	name="普通资金账户"/>
		<item type="text"	id="cust_name"	name="客户姓名"/>
		<item type="text"	id="cert_no"	name="证件号码"/>
	</input>  
	<service type="procedure" id="p_cust_margin_rate_query" process="911" style="77"> 
		<param id="user_id"	type="integer" way="in"/>
		<param id="cust_code"/>
		<param id="commness_cust_code"/>
		<param id="cust_name"/>
		<param id="cert_no"/>
	</service>  
	<output>
		<column type="string"	id="institution_code"   			name="营业部代码"/>
		<column type="string"	id="institution_name"   			name="营业部名称"/>
		<column type="string"	id="cust_id"						name="客户ID" display="false"/>
		<column type="string"	id="cust_code"						name="信用资金账户"/>
		<column type="string"	id="cust_name"   					name="客户姓名"/>
		<column type="string"	id="cert_no" name="证件号码"/>
		<column type="string"	id="commness_cust_code" name="普通资金账号"/>  
		<column type="string"	id="opertype" name="操作类别" display="false"/>  
		<column type="string"	id="instruction" name="保证金类型" display="false"/>  
		<column type="string"	id="GUARANTEE_RATE_NAME" name="保证金类型"/>  
		<column type="string"	id="user_code" name="操作员代码"/>  
		<column type="string"	id="date" name="日期"/>  
		<column type="string"	id="time" name="时间"/>  
		<column type="string"	id="remark" name="备注"/> 
		<column type="number"	id="guarantee_rate"		name="当前保证金比例" precision="2"/> 
		<button type="edit"	 id="edit"	 name="同步保证金比例"  action="80100117"/>
	</output>
</function>