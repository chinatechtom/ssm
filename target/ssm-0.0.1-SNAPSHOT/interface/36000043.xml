<?xml version="1.0" encoding="gbk" ?>
<function id="36000043" name="4.2.2融券卖出证券余额统计表" >  
  <input>
 <item type="date" id="date" name="日期"  default="now"/>
 <item type="text" id="scale_rate" name="市值占融券总规模比例"  />
 <item type="text" id="fund_rate" name="市值占净资本比例"  />
 <item type="select"    id="order_choice"        name="排序选择"  source="382"/>
  </input>
 <service type="procedure" id="P36000043" process="911" style="77">
 <param id="user_id" type="integer" way="in" /> 
 <param id="date"    type="string"  way="in" />
 <param id="scale_rate" type="double" way="in" /> 
 <param id="fund_rate" type="double" way="in" /> 
 <param id="order_choice" type="string"  way="in"/>
  </service> 
  <output  print="true">
   <column type="number" id="id" name="序号"/>  
   <column type="string" id="product_code"   name="证券代码"/> 
   <column type="string" id="product_name"   name="证券名称"/> 
   <column type="number" id="product_num"    name="证券数" precision="0" total="true"/> 
   <column type="number" id="fin_scale_rate" name="市值占融券总规模比例" precision="4"   /> 
   <column type="number" id="fund_rate"      name="市值占净资本比例" precision="6" /> 
   <column type="number" id="mission"        name="市值与卖出金额差额" precision="2" total="true"/> 
  </output>
</function>