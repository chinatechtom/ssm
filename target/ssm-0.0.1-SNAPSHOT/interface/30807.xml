<?xml version="1.0" encoding="gbk" ?>
<function id="30807" name="融资专户历史数据查询" >	
  <input>		
  	<item type="text" 	id="acc_code" 	name="账户代码"/>
		<item type="date" 	id="date"	name="日期" default="last" nullable="false"/>	
  </input>  
  <service type="procedure" id="p_fund_account_data_hisquery" process="911" style="77">
		<param id="user_id"	 type="integer" way="in" /> 
		<param id="acc_code" type="string"  way="in"/>		
		<param id="date"    type="string"  way="in"/>
  </service>  
  <output export="true">
			<column type="string" id="ACC_CODE"   				   name="账户代码"/>
			<column type="number" id="SELF_INOUT_AMT"   	   name="自有资金转入(出)净额" precision="2"/>
			<column type="number" id="CAN_FUND_AMT"   			 name="可融资金余额"         precision="2"/>
			<column type="number" id="FROZEN_CAN_FUND_AMT"   name="冻结可融资金"         precision="2" display="false"/>
			<column type="number" id="UNFROZEN_CAN_FUND_AMT" name="未冻结可融资金"       precision="2" display="false"/>
    	<column type="number" id="FUND_AMT"   					 name="融出资金余额"         precision="2"/>
    	<column type="number" id="UNOVER_FUND_AMT"   	   name="未逾期融出资金余额"	 precision="2"/>
    	<column type="number" id="OVER_FUND_AMT"   		   name="逾期融出资金余额"		 precision="2"/>
    	<column type="string" id="DATE"   						   name="日期"/>
  </output>
</function>