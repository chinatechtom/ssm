<?xml version="1.0" encoding="gbk" ?>
<function id="30793" name="集中度与维持担保比例之比监控" >
	<input>
		<item type="text"    id="cust_code"	    name="客户代码"/>
		<item type="select"  id="market_code"	name="市场"	   source="039"/>
		<item type="text"    id="product_code"	name="证券代码"/>	
		<item type="text"    id="product_name"	name="证券名称"/>
		<item type="text"    id="rate"			name="比例(%)" default="15"/>
	</input>
    <service type="procedure" id="p_stock_gua_rate_control"  process="911" style="77">
		<param id="user_id"	      type="integer" way="in"/>
		<param id="cust_code"     type="string" way="in"/>
		<param id="market_code"   type="string"  way="in"/> 
		<param id="product_code"  type="string"  way="in"/>
		<param id="product_name"  type="string"  way="in"/>
		<param id="rate"		  type="double"  way="in"/>
    </service>
    <output export="true">
		<column type="number" id="institution_id"	name="营业部ID"   display="false"/>
		<column type="string" id="institution_code"	name="营业部代码" display="false"/>
		<column type="string" id="institution_name"	name="营业部名称"/>
		<column type="string" id="cust_code"	name="客户代码" />
		<column type="string" id="cust_name"	name="客户名称"/>
		<column type="string" id="product_code"	name="证券代码"/>
		<column type="string" id="product_name"	name="证券名称"/>
		<column type="number" id="stock_rate"	name="1、证券集中度比例(%)"	precision="8"/>
		<column type="number" id="gua_rate"	    name="2、维持担保比例(%)"	    precision="8"/>
		<column type="number" id="stock_gua_rate"	    name="1和2之比(%)"	    precision="8"/>
		<column type="string" id="warning_level"	name="报警级别"	level="true"	display="false"/>
    </output>
</function>