<?xml version="1.0" encoding="gbk" ?>
<function id="70044544" name="合约详细" >
	<input>	
        <item type="hidden" id="order_id"     name="合约ID"/>
		<item type="hidden" id="order_no"     name="合约no"/>
	</input>
	<service type="procedure" id="P70044544" process="922" style="77">
		<param type="integer" id="user_id" 	way="in" />
		<param type="integer" id="order_id" way="in" />
		<param type="string"  id="order_no" way="in"/>
	</service>
	<output>
        <column type="integer" id="order_id"   		name="合约id"   display="false"/>	
		<column type="string"  id="order_no"   		name="合约编号" display="false"/>
		<column type="integer" id="credit_num"   	name="申请最大交易额度"/>   
		<column type="string" id="credit_num_c"   	name="申请最大交易额度大写"/> 
		<column type="integer" id="approve_num"   	name="审批最大交易额度"/>
		<column type="string" id="approve_num_c"   	name="审批最大交易额度大写"/>
		<column type="integer" id="credit_num_net"   	name="申请网上最大交易额度"/>   
		<column type="string" id="credit_num_net_c"   	name="申请网上最大交易额度大写"/> 
		<column type="integer" id="approve_num_net"   	name="审批网上最大交易额度"/>
		<column type="string" id="approve_num_net_c"   	name="审批网上最大交易额度大写"/>
		<column type="string"  id="remark"  		name="备注"/>
		<column type="string"  id="manage_opinion"	name="处理意见"/>
		<column type="string"  id="approver_opinion" name="审核意见"/>
		<column type="string"  id="assessor_opinion" name="审批意见"/>
		<column type="integer" id="app_years"   	name="生效年限"/>
		<column type="string"  id="force_opinion"	name="复核意见"/>
		<column type="integer" id="cust_num"   		name="客户资产额度"/>
		<column type="string" id="cust_num_c"   	name="客户资产额度大写"/> 
		<column type="number" id="credit_num_rate"   	name="申请最大交易额度" precision="2"/>
		<column type="string"  id="credit_level_code"	name="信用级别代码"/>
		<column type="string"  id="credit_level_name"	name="信用级别名称"/>
		<column type="number" id="total_score"   	name="评级总分" precision="2"/>
		<column type="string"  id="end_date"	name="额度有效截止日"/>
	</output>
</function>