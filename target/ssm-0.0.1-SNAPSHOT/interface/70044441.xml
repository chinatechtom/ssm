		</group>  
	</input>
	
	<service type="procedure" id="P70044441" process="822" style="1">
		<param type="integer" id="user_id" way="in"/>
		<param type="integer" id="order_id" way="in" />
		<param type="string" id="approve_valuation_score" way="in"/>
		<param type="string" id="auto_credit_level" way="in"/>
		<param type="string" id="manage_idea" way="in"/>
	</service>
	
	<output/>
</function>