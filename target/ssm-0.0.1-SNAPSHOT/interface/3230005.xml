	<item type="text"   id="cust_id" 		name="客户ID" /> 
  </input>
  <service type="procedure" id="P10002" process="911" style="77">
	<param id="user_id" 	type="integer"/>
   	<param id="cust_id" 	type="integer"/>
  </service>
  <output >
	<column type="number" id="cust_amt"                name="现金"  />
    <column type="number" id="cust_intr"   	           name="利息及费用"       precision="4"/>
    <column type="number" id="gua_stock_convert_value" name="冲抵保证金证券折算价值" precision="4" />   
    <column type="number" id="financing_profit"   	   name="融资折算盈亏"     precision="4"/>
    <column type="number" id="borrow_stock_profit"     name="融券折算盈亏"     precision="4" />
    <column type="number" id="financing_occupy"   	   name="融资买入占用"     precision="4"/>
    <column type="number" id="borrow_stock_occupy"     name="融券卖出占用"     precision="4"/>
