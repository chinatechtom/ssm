		<item type="hidden"		id="cust_code"		name="客户代码"	/>
		<item type="grid"		id="recipients"		name="收件人"		source="32219"	/>	
		<item type="select"		id="message_type"	name="信息类型"		source="333"	nullable="false"	/>
		<item type="hidden"		id="recipient_type"	name="收件人类型"	default="2" />	
	</input>
	<service type="procedure"	id="p_manual_email_generate_by_type" process="811" style="77">
		<param id="user_id"			type="integer"	/>				
		<param id="cust_code"		type="string"	/>		
		<param id="message_type"	type="string"	/>
		<param id="recipient_type"	type="string"	/>
	</service>
	<output/>
</function>