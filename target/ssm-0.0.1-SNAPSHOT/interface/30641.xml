<?xml version="1.0" encoding="gbk" ?>
<function id="30641" name="查询客户资产评估" >
	<input>	
		<item type="text" id="cust_id" name="客户号" />
	</input>
	<service type="procedure" id="p_full_evaluate_query" process="911" style="77" implement="p_full_evaluate_queryABCD">
		<param id="user_id" type="integer" way="in" />
		<param id="cust_id" type="integer" way="in" />	
	</service>
	<output>    
		<column type="number" id="if_allow" name="是否允许"/>
		<column type="string" id="if_allow_name" name="是否允许"/>
		<column type="number" id="if_blacklist" name="是否黑名单"/>	
		<column type="string" id="if_blacklist_name" name="是否黑名单"/>
		<column type="number" id="fund_amt" name="资金余额" precision="2"/>
		<column type="number" id="stock_market_value" name="证券市值" precision="2"/>
		<column type="number" id="stock_convert_amt" name="证券折算金额" precision="2"/>
		<column type="number" id="fund_convert_amt" name="资金折算金额" precision="2"/>
		<column type="number" id="gage_value" name="担保品价值" precision="2"/>
		<column type="number" id="usable_num" name="可用额度" precision="2"/>
		<column type="number" id="financing_amt" name="融资总金额" precision="2"/>
		<column type="number" id="borrow_stock_amt" name="融券总市值" precision="2"/>
		<column type="number" id="fee_all" name="总费用" precision="2"/>
		<column type="number" id="guan_rate" name="维持担保比例" precision="2"/>
		<column type="number" id="total_asset" name="总资产" precision="2"/>
		<column type="number" id="total_debt" name="总负债" precision="2"/>
	</output>  
</function>