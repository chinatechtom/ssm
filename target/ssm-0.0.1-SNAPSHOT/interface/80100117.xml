<?xml version="1.0" encoding="gbk" ?>
<function id="80100117" name="更新融资融券交易">
	<description>确定要将选中记录同步到融资融券交易系统吗？</description>
	<input>
		<item type="hidden" id="cust_code"		name="客户号"/>
		<item type="hidden" id="institution_code"	name="机构号"/>
		<item type="hidden" id="guarantee_rate"		name="保证金比例"/> 
		<item type="hidden" id="opertype"		name="操作类别"/>
		<item type="hidden" id="instruction"		name="业务类别"/>
		<item type="hidden" id="user_code"		name="修改用户"/>	
		<item type="hidden" id="date"			name="修改日期"/>
		<item type="hidden" id="time"			name="修改时间"/>
		<item type="hidden" id="remark"			name="描述"/>
	</input>
	<service type="drtp" id="413000" process="611" style="77">
		<param id="cust_code"			field="scust_no"	type="string"/>
		<param id="institution_code"		field="sbranch_code0"	type="string"/>
		<param id="guarantee_rate"		field="damt0"		type="float"/> 
		<param id="opertype"			field="lvol1"		type="integer"/>
		<param id="instruction"			field="sholder_type"	type="string"/>
		<param id="user_code"			field="semp"		type="string"/>		
		<param id="date"			field="sdate0"		type="string"/>
		<param id="time"			field="stime0"		type="string"/>
		<param id="remark"			field="vsvarstr0"	type="string"/>
		<param id="lvol9" 			type="integer"   	default="130927"/>
	</service>
	<output/>
</function>