<?xml version="1.0" encoding="gbk"?>
<function id="31007" name="新增客户延期合同申请">
	<input>
		<item type="hidden"   id="order_no"            name="定单编号"/>
		<item type="hidden"   id="formerly_voucher_no" name="原始凭证号"/>
		<item type="hidden"   id="cust_id"             name="客户id"     nullable="false"/>
	    <item type="hidden"   id="bond_id"             name="合同id"     nullable="false"/>
		<item type="text"     id="cust_name"           name="客户姓名"   editable="false"/>
		<item type="text"     id="cust_code"           name="客户代码"   editable="false"/>
		<item type="text"     id="app_years"           name="延期年数"   nullable="false"/>
		<item type="textarea" id="remark"              name="备注说明"/>
	</input>
	<service type="produce" id="p_delay_bond_insert" process="811" style="1"  implement="p_delay_bond_insert31007">
		<param type="integer" id="user_id"/>
		<param type="string"  id="order_no"/>
		<param type="string"  id="formerly_voucher_no"/>
		<param type="integer" id="cust_id"/>
		<param type="integer" id="app_years"/>
		<param type="string"  id="remark"/>
		<param type="integer" id="bond_id"/>
	</service>
	<output/>
