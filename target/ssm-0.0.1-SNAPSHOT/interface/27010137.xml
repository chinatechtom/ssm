			<column type="number" id="THIS_YEAR_BS_BUY"	            name="本年买券还券交易量" precision="2" />
			<column type="number" id="THIS_YEAR_GUA_TRADE"			name="本年担保品买卖交易量"	  precision="2" 	 />
			<column type="number" id="THIS_YEAR_TRADE_AMT"			name="本年融资融券总交易量"	  precision="2" 	 />
			<column type="number" id="THIS_YEAR_TRADE_AMT_RATE"	    name="本年融资融券总交易量公司占比" precision="6" />
		</group>	
    </output>
</function>
