<?xml version="1.0" encoding="gbk" ?>
<function id="3064409" name="客户资产评估_海通授信" >
	<input>    
		<item type="text"  id="cust_id"  name="客户id"/> 
		<item type="text"  id="order_id" name="合约id"/> 
	</input>
	<service type="procedure" id="p_cust_credit_report_asset_info" process="911" style="77">
		<param id="user_id"  type="integer"/> 
		<param id="cust_id"  type="ingeter"/>
		<param id="order_id" type="ingeter"/>
	</service>
	<output >
		<column type="number" id="total_asset_y"    name="总资产(元)" precision="2"/>
		<column type="number" id="total_debt_y"     name="总负债(元)" precision="2"/>
		<column type="number" id="total_asset_wy"   name="总资产(万元)" precision="4"/>
		<column type="number" id="total_debt_wy"    name="总负债(万元)" precision="4"/>
	</output> 
</function>