<?xml version="1.0" encoding="gbk" ?>
<function id="32017" name="单客户单证券融资买入数量占流通股本监控">
	<input>
		<item type="text"	id="cust_code"		name="客户代码"/>
		<item type="text"	id="product_code" 	name="证券代码"/>
		<item type="text"	id="c1"				name="集中度(%)"	default="0.1"/>
		<item type="select"	id="market_code"	name="市场代码"		source="039"/>
	</input>
	<service type="procedure" id="p_share_index_query" process="911" style="77" implement="p_share_index_query32017">
		<param id="user_id"			type="integer"/>
		<param id="cust_code"		type="string"/>
		<param id="product_code"	type="string"/>
		<param id="market_code"		type="string"/>
		<param id="c1"				type="double"/>
	</service>
	<output export="true" print="true">
		<column type="string" id="institution_name"	name="营业部名称"/>	<!--MODIFY BY ZYT 20140825 SECREQ-1419-->
		<column type="string" id="cust_name"		name="客户姓名"/>	<!--MODIFY BY ZYT 20140825 SECREQ-1419-->
		<column type="string" id="cust_code"		name="客户代码"/>
		<column type="string" id="product_code"		name="证券代码"/>
		<column type="string" id="product_name"		name="证券名称"/>
		<column type="string" id="market_name"		name="市场名称"/>
		<column type="number" id="circulated_a"		name="流通总股本"		precision="0"/>
		<column type="number" id="c1"				name="集中度(%)"		precision="8"/>
		<column type="string" id="warning_level"	name="报警级别"			level="true"	display="false"/>
		<column type="number" id="guarantee_rate"	name="维持担保比例"		precision="6"/> <!--MODIFY BY ZYT 20140825 SECREQ-1419-->
		<column type="number" id="debt_amt"			name="负债"					precision="2"/> <!--MODIFY BY ZYT 20140825 SECREQ-1419-->
		<column type="number" id="trade_qty_1"		name="当日担保品买入数量"	precision="0"/> <!--MODIFY BY ZYT 20140825 SECREQ-1419-->
		<column type="number" id="trade_qty_2"		name="当日担保品卖出数量"	precision="0"/> <!--MODIFY BY ZYT 20140825 SECREQ-1419-->
		<column type="number" id="trade_qty_3"		name="当日融资买入数量"	precision="0"/> <!--MODIFY BY ZYT 20140825 SECREQ-1419-->
		<column type="number" id="trade_qty_4"		name="当日买券还券数量"	precision="0"/> <!--MODIFY BY ZYT 20140825 SECREQ-1419-->
		<column type="number" id="trade_qty_5"		name="当日卖券还款数量"	precision="0"/> <!--MODIFY BY ZYT 20140825 SECREQ-1419-->
	</output>
</function>
