<?xml version="1.0" encoding="gbk" ?>
<function id="30603" name="查看定单-信用上限" >
	<input>    
	 <item type="hidden"  id="order_id" name="客户id"/> 
  </input>
  <service type="procedure" id="p_order_credit_num_query" process="911" style="77">
	<param id="user_id" type="integer" way="in"/> 
	<param id="order_id" type="ingeter" way="in"/>
  </service>
  <output detail="true">
	<column type="number" id="order_id" name="合约id"	display="false"/>
	<column type="number" id="app_num" name="信用上限" precision="2"/>
	<column type="number" id="approve_num" name="审批额度" precision="2"/>
	<column type="string" id="approve_num_chn" name="审批额度大写" />	
	<column type="string" id="credit_date" name="申请日期" />
  </output> 
</function>