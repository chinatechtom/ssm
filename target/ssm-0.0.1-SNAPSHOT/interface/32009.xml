  <service type="procedure" id="p_business_report_32009_query" process="911" style="77">
	<param id="user_id" 		type="integer"	way="in"/>
	<param id="product_code" 	type="string"	way="in"/>
	<param id="produce_name" 	type="string"	way="in"/>	
