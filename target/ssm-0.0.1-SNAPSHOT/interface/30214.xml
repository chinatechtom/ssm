		<item type="text"  id="credit_amt_single"	name="单客户最大融资上限"		nullable="false"	number="true"/>
		<item type="text"  id="credit_frozen_amt"	name="冻结金额"		nullable="false"	number="true"/>
	</input>
    <service type="procedure" id="p_financing_quantum_set"  process="811" style="1">
		<param id="user_id"	            type="integer" way="in"/>
		<param id="all_credit_amt"	    type="double"  way="in"/>
		<param id="credit_amt_total"	type="double"  way="in"/>
		<param id="credit_amt_single"	type="double"  way="in"/>
		<param id="credit_frozen_amt"	type="double"  way="in"/>
    </service>
    <output/>   
</function>