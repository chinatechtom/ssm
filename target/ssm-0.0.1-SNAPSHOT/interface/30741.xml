<?xml version="1.0" encoding="gbk" ?>
<function id="30741" name="设置信用违约资料" >
	<input>
		<item type="hidden" id="record_id"	name="记录id" />
		<item type="text"   id="wyxwdm"		name="席位代码"			maxlen="6"		nullable="false"/>
		<item type="text"   id="wyzqmc"		name="券商名称"			maxlen="40"		nullable="false"/>
		<item type="text"   id="wygddm"		name="违约者证券账户"	maxlen="10"		nullable="false"/>	
		<item type="text"   id="wygdxm"		name="违约者姓名"		maxlen="60"		nullable="false"/>	
		<item type="text"   id="wysfzh"		name="身份证号"			maxlen="30"		nullable="false"/>	
		<item type="text"   id="wywyje"		name="违约金额"			number="true"	nullable="false"/>	
		<item type="select" id="wywylb"		name="违约类别"			source="327"	nullable="false"/>
		<item type="select" id="wysblb"		name="申报类别"			source="328"	nullable="false"/>
		<item type="date"   id="wysbrq"		name="申报日期"			default="now"	nullable="false"/>
		<item type="hidden" id="wymark"		name="备用" />
		<item type="select" id="wyscdm"		name="市场"				source="039"	nullable="false"/>
	</input>
	<service type="procedure" id="p_credit_violation_set"  process="911" style="1"> 
		<param id="user_id"		type="integer" way="in"/>
		<param id="record_id"   type="integer" way="in"/> 
		<param id="wyxwdm"		type="string"  way="in"/>  
		<param id="wyzqmc"		type="string"  way="in"/>
		<param id="wygddm"		type="string"  way="in"/> 
		<param id="wygdxm"		type="string"  way="in"/> 
		<param id="wysfzh"		type="string"  way="in"/> 
		<param id="wywyje"		type="double"  way="in"/> 
		<param id="wywylb"		type="string"  way="in"/> 
		<param id="wysblb"		type="string"  way="in"/> 
		<param id="wysbrq"		type="string"  way="in"/> 
		<param id="wymark"		type="string"  way="in"/> 
		<param id="wyscdm"		type="string"  way="in"/> 
	</service>
	<!--MODIFY BY SECREQ-501 zyt 20140624 <output/> -->
	<output  target="@func_no">
		<column type="string" id="func_no"           		name="功能号"/> 
		<column type="string" id="avai_data_item"	 		name="有效数据项"/>
		<column type="string" id="cust_code"         		name="客户代码"/>
		<column type="string" id="institution_code"	 		name="营业部代码"/>
		<column type="string" id="credit_level_code" 		name="信用级别代码"/>
		<column type="number" id="credit_num"		 		name="信用上限" precision="2"/>
		<column type="string" id="pc_mode"		 	 		name="平仓标志"/>
		<column type="string" id="credit_usemode"	 		name="信用上限使用模式"/>
		<column type="string" id="if_blacklist"	 	 		name="是否黑名单"/>
		<column type="string" id="hk_mode"					name="还款模式"/>
		<column type="string" id="bond_no"		  	 		name="合同编号"/>
		<column type="string" id="bond_status"		 		name="合同状态"/>
		<column type="string" id="begin_date"        		name="起始日期"/>
		<column type="string" id="begin_date"       		name="结束日期"/>
		<column type="string" id="product_code1"        	name="融资产品"/>
		<column type="string" id="product_code2"       		name="融券产品"/>
		<column type="number" id="credit_num2"				name="授信额度"/>
		<column type="number" id="financing_amt"			name="融资上限"/>
		<column type="number" id="bstock_amt"		    	name="融券上限"/>
		<column type="string" id="user_institution_code"	name="操作员机构代码"/>
		<column type="string" id="mac_address"				name="网卡地址"/>
		<column type="string" id="user_code"				name="操作员代码"/>
		<column type="string" id="spost_code"				name="参数代码"/> 
		<column type="string" id="adjuest_value"			name="参数值"/>
		<column type="string" id="safe_level"				name="交易鉴别调用来源"/>
		<param id="guarantee_rate" field="damt0"		type="float"/> 
		<param id="opertype"  field="lvol1"		type="integer"/>
		<param id="instruction" field="sholder_type"	type="string"/>
		<param id="user_code" field="semp"		type="string"/>		
		<param id="date" field="sdate0"		type="string"/>
		<param id="time" field="stime0"		type="string"/>
		<param id="remark" field="vsvarstr0"	type="string"/>
	</output>	
</function>