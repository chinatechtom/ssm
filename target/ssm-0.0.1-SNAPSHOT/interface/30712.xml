<?xml version="1.0" encoding="gbk" ?>
<function id="30712" name="单客户融券总额占流通股本监控" >	
  <input>
		<item type="text"	id="rate"	name="比例(%)"	nullable="false"	default="0.1"	number="true"/> 		
  </input>
  <service type="procedure" id="p_borrow_stock_all_query" implement="p_borrow_stock_all_query30711"  process="911" style="77"> 
		<param type="integer" id="user_id"	way="in"/>
		<param type="double"  id="rate"		way="in"/>			
  </service>
  <output refresh="120" export="true">
		<column type="number" id="cust_id"					name="客户id"		display="false"/>
		<column type="string" id="cust_code"				name="客户代码"/>
		<column type="string" id="cust_name"				name="客户姓名"/>
		<column type="string" id="market_code"				name="市场代码"/>
		<column type="number" id="product_id"				name="产品id"		display="false"/>
		<column type="string" id="product_code"				name="证券代码"/>
		<column type="number" id="asset_qty"				name="融券总额"		precision="0"/>
		<column type="number" id="circulated_a"				name="流通股"/>
		<column type="number" id="rate"						name="比例(%)"			precision="8"/>	
		<column type="string" id="warning_level"			name="报警级别"	level="true" display="false"/>
  </output>
  <assistant action="30103,3200301,30102,30104,30109,30111,30201,30202,30208,30601,30625,30667,30668,30669,30501,">
		<link src="cust_id" target="cust_id"/>
		<link src="product_id" target="product_id"/>
  </assistant> 
</function>