<?xml version="1.0" encoding="gbk" ?>
<function id="37815012" name="客户保证金比例调整时对应的各项评级因子" >
	<input>
		<item type="hidden" id="institution_code1"	name="机构代码"/>
		<item type="hidden" id="commness_cust_code"	name="普通资金账号" />
		<item type="hidden" id="date"				name="系统日期"/>
		<item type="hidden" id="user_code"			name="用户代码" />		
	</input>
	<service type="procedure" id="kingstar.P37815012" implement="P37815012_37815012" process="311" style="77">
		<param id="institution_code1"	type="string"		alias="branch_code"		way="in" />
		<param id="commness_cust_code"	type="string"		alias="capital_account" way="in" />
		<param id="date"				type="string"		alias="client_date"		 way="in" />
		<param id="user_code"			type="string"		way="in" />
	</service>
	<output> 
		<column type="number" id="abs_profit_rate"			name="年化收益率(%)"				precision="2"/>
		<column type="number" id="hs300_year_profit_rate"	name="同期沪深300年化收益率(%)"	precision="2"/>
		<column type="number" id="cp_profit_rate"			name="相对收益率(%)"				precision="2"/>
		<column type="number" id="cur_asset"				name="当前净资产(万元)"			precision="2"/>
		<column type="number" id="rzrq_commission"			name="信用交易佣金(万元)"			precision="2"/>
		<column type="number" id="rzrq_fee"					name="信用交易息费收入(万元)"		precision="2"/>
	</output>
</function>