<?xml version="1.0" encoding="gbk" ?>
<function id="30302" name="风险级别浏览" >
	<input>
		<item type="hidden"   id="product_risk_level_id"   name="风险级别id"/>	
	</input>
	<service type="procedure" id="p_product_risk_level_browse" implement="p_product_risk_level_browse302"  process="911" style="77"> 
		<param id="user_id" type="integer" way="in"/>
		<param id="product_risk_level_id" type="integer" way="in"/>
	</service>
	<output export="true" autoload="true">
		<column type="number" id="product_risk_level_id"   name="证券风险级别id" display="false"/>
		<column type="string" id="product_risk_level_no"   name="风险级别代码" />
		<column type="string" id="product_risk_level_name" name="风险级别名称" />
		<column type="string" id="if_estop"				   name="是否禁止1"	  display="false"/>
		<column type="string" id="if_estop_name"		   name="是否禁止" />
		<column type="string" id="if_financing"			   name="融资标的1" display="false"/>
		<column type="string" id="if_financing_name"	   name="融资标的" />
		<column type="string" id="if_borrow_stock"		   name="融券标的1" display="false"/>
		<column type="string" id="if_borrow_stock_name"	   name="融券标的" />
		<column type="string" id="if_guarantee"			   name="担保品标的1" display="false"/>
		<column type="string" id="if_guarantee_name"	   name="担保品标的" />
		<column type="number" id="stock_convert_rate"	   name="折算率(%)"  precision="4"/>	
		<column type="number" id="borrow_sec_rate"		   name="融券冻结率(%)"  precision="4"/>	
		<column type="number" id="fee_rate"				   name="融券浮动费率(%)"  precision="4"/>		
		<button type="add"    id="add"					   name="增加" action="30307" multiple="-1" />
		<button type="edit"   id="edit"					   name="修改" action="30307" multiple="0" />
		<button type="set"   id="set"					   name="批量更新该级别证券标的和折算率" action="3030701" multiple="0" />
	</output>
	<assistant action="30303">		
		<link src="product_risk_level_id" target="product_risk_level_id"/>
    </assistant>
</function>