<?xml version="1.0" encoding="gbk" ?>
<function id="3100202" name="风险类―警戒风险统计" >  
  <input>
  <item type="select" id="institution_id" name="营业部名称"  source="95000008"/>
	<item type="hidden" id="date" name="日期"  default="now"/>	
  </input>
   <service type="procedure" id="p_statistics_3100202_query" process="911" style="77">
	<param id="user_id" type="integer" way="in" /> 
	<param id="date"/>
	<param id="institution_id" />
  </service> 
  <output  print="true">
    <column type="string" id="c1" name="营业部名称"/>  
	<group name="融资融券实际使用额度（元）">
	<group name="100(万元)以下">
	<column type="number" id="c2" name="融资" precision="2"/>
	<column type="number" id="c3" name="融券" precision="2"/>
	</group>
	<group name="100-500(万元)">
	<column type="number" id="c4" name="融资" precision="2"/>
	<column type="number" id="c5" name="融券" precision="2"/>
	</group>
	<group name="500-1000(万元)">
	<column type="number" id="c6" name="融资" precision="2"/>
	<column type="number" id="c7" name="融券" precision="2"/>
	</group>
	<group name="1000(万元)以上">
	<column type="number" id="c8" name="融资" precision="2"/>
	<column type="number" id="c9" name="融券" precision="2"/>
	</group>
	</group>
	<column type="number" id="c10" name="合计" precision="2"/>
  </output>
</function>