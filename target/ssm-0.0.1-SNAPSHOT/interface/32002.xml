    <column type="number" id="c5"   						name="总资产"  precision="2" total="true"/>
    <column type="number" id="cust_debt"   			name="总负债"  precision="2" total="true"/>
    <column type="number" id="c7"   						name="利息费用"  precision="2" total="true"/>
    <column type="number" id="guarantee_rate"   name="维持担保比例"  precision="4" />
	</output> 	
</function>