<?xml version="1.0" encoding="gbk" ?>
<function id="3030103" name="风险证券设置" >	
	<input>		
		<item type="hidden" id="product_id"	      			  name="产品id"/>	
        <item type="text"   id="product_code"	      	      name="证券代码" editable="false"/>	
        <item type="text"   id="product_name"	      	      name="证券名称" editable="false"/>			
		<item type="select"	id="if_risk_stock"				  name="是否风险证券"       source="302" nullable="false"/>		
		<item type="select" id="if_high_concentration_stock"  name="是否高集中度证券"   source="302" nullable="false"/>
	</input>
    <service type="procedure" id="p_risk_product_flag_set" process="811" style="1"> 
		<param id="user_id"						 type="integer" way="in"/>		
		<param id="product_id"					 type="integer" way="in"/>		
		<param id="if_risk_stock"				 type="string"	way="in"/>	
		<param id="if_high_concentration_stock"	 type="string"	way="in"/>
	</service>	
</function>
