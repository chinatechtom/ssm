<?xml version="1.0" encoding="gbk" ?>
<function id="3220903" name="短信批量发送状态修改">
	<input>
	  <description>操作有风险,提交需谨慎!</description>
	  <group name="改动参数">
		<item type="select" 	id="set_send_flg"    name="发送标志"			source="334" nullable="false"/>
		<item type="text" 	    id="set_retry_times" name="重试次数"			nullable="false"/>
	  </group>
	  <group name="筛选条件">
	    <item type="date" 	    id="sc_date"           name="信息生成日期"	    nullable="false"/>
		<item type="select" 	id="msg_type"          name="信息类型"			source="333" nullable="false"/>
		<item type="select" 	id="send_flg"          name="发送标志"			source="334" nullable="false"/>
		<item type="text" 	    id="retry_times"       name="重试次数"			nullable="false"/>
		<item type="hidden" 	id="cont_meth"         name="通知类型"			nullable="false" default="2"/>
		<item type="text" 	    id="gua_rate_lower"    name="担保率下限≥"    	/>
		<item type="text" 	    id="gua_rate_upper"    name="担保率上限＜"  	/>
		<item type="button"     id="assistant"	       name="筛选预览"          action="3220904" multiple="0"/>
	  </group>
	</input>
	<service type="procedure" id="p_msg_status_batch_set" implement="p_msg_status_batch_set3220903" process="811" style="1">
		<param id="user_id" type="integer"/>		
		<param id="set_send_flg"    type="string"/>
		<param id="set_retry_times" type="integer"/>
		<param id="sc_date"         type="string"/>
		<param id="msg_type"        type="string"/>
		<param id="send_flg"        type="string"/>
		<param id="retry_times"     type="integer"/>
		<param id="gua_rate_lower"  type="string"/>
		<param id="gua_rate_upper"  type="string"/>
		<param id="cont_meth"    /> <!--1 邮件 2 短信 -->
	  </service>
</function>
		
		