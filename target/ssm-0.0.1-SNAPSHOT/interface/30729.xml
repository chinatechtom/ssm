<?xml version="1.0" encoding="gbk" ?>
<function id="30729" name="客户强制平仓记录监控" >
	<input>
		<item type="hidden"  id="record_id"	    name="记录id"/>	
		<item type="hidden"  id="cust_id"	    name="客户id"/>
		<item type="select" id="institution_id" name="营业部名称"  source="95000008"/>
		<item type="text"	 id="cust_code"	    name="客户代码"/>	
		<item type="text"    id="cust_name"	    name="客户姓名"/>
		<item type="select"	id="status"	name="处理状态"	source="352" nullable="false"/>
		<item type="date"    id="date1"			name="起始日期"/>
		<item type="date"    id="date2"			name="结束日期"/>
	</input>
    <service type="procedure" id="p_pc_record_browse"  process="911" style="77">
		<param id="user_id"	        type="integer" way="in"/> 
		<param id="record_id"		type="integer" way="in"/>
		<param id="institution_id"	type="integer" way="in"/>
		<param id="cust_id"			type="integer" way="in"/>
		<param id="cust_code"		type="string"  way="in"/>
		<param id="cust_name"		type="string"  way="in"/>
		<param id="date1"			type="string"  way="in"/>
		<param id="date2"			type="string"  way="in"/>
		<param id="status"			type="string"  way="in"/>
    </service>
    <output export="true" autoload="true" refresh="120">
		<column type="number" id="record_id"			name="记录id"			display="false"/>
		<column type="number" id="cust_id"				name="客户id"			display="false"/>
		<column type="number" id="institution_id"		name="营业部id"			display="false"/>
		<column type="string" id="institution_code"		name="营业部代码"/>
		<column type="string" id="institution_name"		name="营业部名称"/>
		<column type="string" id="cust_code"			name="客户代码"/>
		<column type="string" id="cust_name"			name="客户姓名"/>
		<column type="string" id="ddate"				name="平仓预案制定日期"/>
		<column type="string" id="ttime"				name="平仓预案制定时间"/>
		<column type="number" id="debt_amt"				name="客户总负债"		precision="3"/>
		<column type="number" id="financing_amt"		name="客户融资总额"		precision="3"/>
		<column type="number" id="borrow_stock_amt"		name="客户融券总价值"		precision="3"/>
		<column type="number" id="guarantee_amt"		name="客户担保品价值"		precision="3"/>
		<column type="number" id="credit_charge"		name="利息费用"			precision="3"/>
		<column type="number" id="asset_gap"			name="资产差额"			precision="3"/>	
		<column type="number" id="product_id"			name="产品id"			display="false"/>
		<column type="string" id="product_code"			name="信用产品代码"/>
		<column type="string" id="product_name"			name="信用产品名称"/>	
		<column type="string" id="product_sort"			name="信用产品类型1"		display="false"/>
		<column type="string" id="product_sort_name"	name="信用产品类型"/>
		<column type="string" id="product_end_date"		name="产品到期日期"/>
		<column type="number" id="bond_id"				name="合同id"			display="false"/>
		<column type="string" id="bond_no"				name="合同编号"/>
		<column type="string" id="bond_name"			name="合同名称"			display="false"/>	
		<column type="string" id="bond_end_date"		name="合同到期日期"/>		
		<column type="string" id="pc_mode"				name="平仓顺序1"			display="false"/>	
		<column type="string" id="pc_mode_name"			name="平仓顺序"/>	
		<column type="string" id="pc_reason"			name="平仓原因1"			display="false"/>	
		<column type="string" id="pc_reason_name"		name="平仓原因"/>	
		<column type="string" id="manage_status"		name="平仓处理状态1"		display="false"/>	
		<column type="string" id="manage_status_name"	name="平仓处理状态"/>	
		<column type="string" id="pc_result"			name="平仓结果1"			display="false"/>	
		<column type="string" id="pc_result_name"		name="平仓结果"/>	
		<column type="string" id="bc_begin_date"		name="补仓起始日期"/>	
		<column type="string" id="bc_end_date"			name="补仓截至日期"/>	
		<column type="string" id="today"				name="今天日期"/>	
		<column type="string" id="approver_opinion"		name="审批意见"/>	
		<column type="number" id="manage_id"			name="处理人员id"			display="false"/>
		<column type="string" id="manage_name"			name="处理人员"/>
		<column type="string" id="remark"				name="备注"/>	
		<column type="string" id="level"				name="级别"	level="true" display="false"/>
		<column type="number" id="guarantee_rate2"		name="触发时担保率"	precision="4"/>
		<column type="number" id="real_gua_rate"		name="实时担保率"	precision="4"/>
		<column type="number" id="credit_num"			name="信用额度" precision="2"/>
		<column type="number" id="margin_used"			name="已使用保证金" precision="2"/>
		<column type="number" id="margin_remain"		name="剩余保证金" precision="2"/>
		<column type="number" id="margin_total"			name="总保证金" precision="2"/>
		<column type="number" id="non_guarantee"		name="非担保品市值" precision="2"/>
		<column type="number" id="pc_amt"				name="强制平仓额度" precision="2"/>
		<column type="string" id="touch_date"			name="触发日期"/>
		<column type="string" id="pc_begin_date"		name="拟平仓日期"/>
		<column type="string" id="find_mode"			name="主联系方式1"	display="false"/>
		<column type="string" id="find_mode_value"		name="主联系方式"/>
		<column type="string" id="order_no"				name="合同编号"/>
		<button type="edit"   id="detail" name="平仓预案详情"	action="30734"		multiple="0"/>
		<button type="edit"   id="set"	  name="设置补仓日期"	action="30735"		multiple="0"/>
		<button type="ok"     id="ok"	  name="生成平仓报告"	action="30732"		multiple="0"/>
		<button type="edit"   id="cancel" name="作废平仓预案"	action="30733"		multiple="0"/>
    </output>	
</function>