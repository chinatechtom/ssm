<?xml version="1.0" encoding="gbk" ?>
<function id="30122" name="客户普通A股账户资产总值获取">
	<input>
		<item type="hidden" id="cust_id"	name="合约id"/>
		<item type="hidden" id="order_id"	name="客户id"/>
	</input>
	<service type="procedure" id="p_cust_common_fund_amt"  process="911" style="77">
		<param id="user_id"		type="integer"/>
		<param id="cust_id"		type="integer"/>
		<param id="order_id"	type="integer"/>
	</service>
	<output>
		<column type="number" id="total_asset"		name="总资产"						precision="2"/>
		<column type="number" id="total_debt"		name="总负债"						precision="2"/>
		<column type="number" id="common_fund_amt"	name="普通A股账户资产总值"			precision="2"/>
		<column type="number" id="other_fund_amt"	name="其他金融资产总值"			precision="4"/>
		<column type="number" id="all_debt_amt"		name="回购等负债总值"				precision="4"/>
		<column type="number" id="net_asset_amt"	name="在公司内净资产总值"			precision="4"/>
		<column type="number" id="net_asset_amt_wy"	name="在公司内净资产总值(万元)"	precision="4"/>
	</output>
</function>