  <item type="month"  id="month"  name="月份" nullable="false"/>
</input>
<service type="procedure" id="P36000028" implement="P3600002836000028" process="955" style="77">
  <param id="user_id" type="integer"/>
  <param id="institution_id" type="integer"/>
  <param id="year" type="string"/>
  <param id="month" type="string"/>
</service>
<output export="true" >
