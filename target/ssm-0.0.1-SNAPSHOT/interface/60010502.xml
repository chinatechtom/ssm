		<item type="hidden" id="date"     name="打印日期"/>	<!--从1888.xml获得的打印日期-->
		<item type="hidden" id="user_code"     name="打印柜员代码"/><!--从1888.xml获得的打印柜员代码-->
		<item type="hidden" id="user_name"     name="打印柜员名称"/><!--从1888.xml获得的打印柜员名称-->
		<item type="hidden" id="company"    name="券商名称"/><!--从1888.xml获得的券商名称-->			
		<group name="交易协议书信息">
			<item type="text" id="sbond_no" name="交易协议书编号" editable="false"/>
		    <item type="hidden" id ="related_sbond_id"	name="related_sbond_id"/>
			<item type="text" 	id ="related_sbond_no" 	name="关联(主)交易协议书编号"	editable="false"/>
			<item type="hidden" id ="hg_code" 		name="初始交易代码" 	editable="false"/>
			<item type="hidden" id ="hg_code2" 		name="购回交易代码" 	editable="false"/>
			<item type="select" id ="market_code" 	name="市场代码" 		editable="false" source="039"/>
			<item type="text" 	id ="product_code" 	name="标的证券代码" 	editable="false"/>
			<item type="text" 	id ="product_name" 	name="标的证券名称" 	editable="false"/>
			<item type="text" 	id ="product_type" 	name="标的证券类型" 	editable="false"/>
