<?xml version="1.0" encoding="gbk" ?>
<function id="30722" name="客户证券集中度超限监控" >
	<input>
		<item type="hidden"  id="cust_id"			name="客户id"/>
		<item type="text"    id="cust_code"			name="客户代码"/>	
		<item type="text"    id="cust_name"			name="客户姓名"/>
		<item type="hidden"  id="product_id"	    name="证券id"/>
		<item type="select"  id="market_code"	    name="市场代码"	source="039"/>
		<item type="text"    id="product_code"	    name="证券代码"/>	
		<item type="text"    id="product_name"	    name="证券名称"/>
		<item type="text"    id="rate"				name="比例(%)"	default="0"/>
	</input>
    <service type="procedure" id="p_cust_stock_centralization_beyond_limit_control"  process="911" style="77">
		<param id="user_id"	        type="integer" way="in"/> 
		<param id="cust_id"			type="integer" way="in"/>
		<param id="cust_code"	    type="string"  way="in"/>
		<param id="cust_name"	    type="string"  way="in"/>
		<param id="product_id"		type="integer" way="in"/>
		<param id="market_code"		type="string"  way="in"/>
		<param id="product_code"	type="string"  way="in"/>
		<param id="product_name"	type="string"  way="in"/>
		<param id="rate"			type="doubule" way="in"/>
    </service>
    <output export="true" autoload="true" refresh="120">
		<column type="number" id="cust_id"			name="客户id"		display="false"/>
		<column type="string" id="cust_code"		name="客户代码"/>
		<column type="string" id="cust_name"		name="客户姓名"/>
		<column type="number" id="product_id"		name="证券id"		display="false"/>
		<column type="string" id="product_code"		name="证券代码"/>
		<column type="string" id="product_name"		name="证券名称"/>
		<column type="number" id="asset_qty"		name="总量"		precision="0"/>
		<column type="number" id="circulated_a"		name="流通数量"	precision="0"/>
		<column type="number" id="rate"				name="比例(%)"	precision="8"/>
		<column type="string" id="warning_level"	name="报警级别"	level="true"	display="false"/>
    </output>
	<assistant action="30103,3200301,30102,30104,30109,30111,30201,30202,30208,30673,30672,30667,30668,30669">
		<link src="cust_id"		target="cust_id"/>
		<link src="product_id"	target="product_id"/>
    </assistant>
</function>