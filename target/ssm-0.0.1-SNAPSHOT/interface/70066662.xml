<?xml version="1.0" encoding="gbk" ?>
<function id="70066662" name="修改合同紧急联系人申请合约详情" >
  <input>
	<group name="合同信息" >
		<item type="hidden" id="cust_id" name="客户id" />			
		<item type="hidden" id="bond_no" name="合同编号" editable="false"/>
		<item type="text" id="cust_name" name="客户姓名" editable="false"/>
		<item type="text" id="cust_code" name="客户代码" editable="false"/> 	
	</group>
	<item type="hidden" id="order_id"	name="合约ID" action="70044562"/>			
	<item type="hidden" id="date"		name="打印日期"/>
	<item type="hidden" id="user_code"	name="打印柜员代码"/>
	<item type="hidden" id="user_name"	name="打印柜员名称"/>
	<item type="hidden" id="company"    name="券商名称"/>		
	<group name="紧急联系人信息">
	  <item type="text" 	id="emerg_contacts_man"		name="紧急联系人" editable="false"/>
	  <item type="text" 	id="emerg_contacts_tel"		name="紧急联系电话" editable="false"/>
	</group>
	<group name="意见">
		<item type="textarea" id ="remark" 			 name="申请备注" editable="false"/>
		<item type="textarea" id ="manage_opinion"	 name="处理意见" editable="false"/>
		<item type="textarea" id ="approver_opinion" name="审核意见" editable="false"/>
		<item type="textarea" id ="assessor_opinion" name="审批意见" editable="false"/>
	</group>
  </input>
</function>