		<column type="string" 	id="cust_type"			name="客户类型"	 display="false"/>
		<column type="string" 	id="cust_type_name"		name="客户类型"	/>
		<column type="string" 	id="market_code"		name="市场代码"	 display="false"/>
		<column type="string" 	id="market_name"		name="市场代码"	/>
		<column type="string" 	id="busi_type"			name="业务类型"	 display="false"/>
		<column type="string" 	id="busi_type_name"		name="业务类型"	/>
		<column type="string" 	id="limit_type"			name="限制类型"	 display="false"/>
		<column type="string" 	id="limit_type_name"	name="限制类型"	 />
		<column type="number" 	id="early_days"			name="提前天数" />
		<column type="string" 	id="limit_time"			name="截止时间" />
		<column type="string" 	id="limit_time_early"	name="开始时间" />
    	<button type="add"		id="add" 		  		name="新增"    action="70011309"  multiple="-1"/>
		<button type="edit"		id="edit" 				name="修改"    action="70011310"  multiple="0"/>
		<button type="delete"	id="delete"   			name="删除"    action="70011311"  multiple="0"/>
	</output>
</function>
		
		