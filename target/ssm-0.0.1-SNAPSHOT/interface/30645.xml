<?xml version="1.0" encoding="gbk" ?>
<function id="30645" name="查看合同-费率" >
  <input>
	<item type="hidden" id="bond_id" name="合同id"/>
  </input>
  <service type="procedure" id="p_bond_fl_query" process="911" style="77">
	<param type="integer" id="user_id"/>	
	<param type="integer" id="bond_id"/>
  </service>
  <output>    
    <column type="number" id="bond_id" name="合同id"   display="false"/>
    <column type="number" id="loan_interest_rate" name="融资利息率(%)"  precision="2"/>  
	<column type="number" id="borrow_stock_interest_rate" name="融券利息率(%)" precision="2"/>
    <column type="number" id="punish_interest_rate" name="罚息率(%)"  precision="2"/>  
	<column type="number" id="commission_charge" name="手续费" precision="2"/>
  </output> 
</function>