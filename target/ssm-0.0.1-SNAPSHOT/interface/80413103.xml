<?xml version="1.0" encoding="GBK" ?>
<function id="80413103" name="取客户资金">
  <input>
  	<item type="text" id="scust_no" name="客户号"/>
  </input>
  <service type="drtp" id="413103" process="711" style="77">
	  <param type="string" id="scust_no"/>
  </service>
  <output target="81413103">
    <column name="客户代码"     id="cust_code"           type="string" field="scust_no"      />
    <column name="客户姓名"     id="cust_name"           type="string" field="sname"         />
    <column name="账户代码"	    id="acc_code"	           type="string" field="sholder_ac_no" />
    <column name="日期"		      id="date"			           type="string" field="stime3"        />
    <column name="货币类型"	    id="currency_type"	     type="string" field="scurrency_type"/>
    <column name="资金余额"	    id="cash_amt"		         type="number" field="damt0"         />  
    <column name="存入金额"	    id="deposit"		         type="number" field="damt1"         />  
    <column name="取出金额"	    id="withdraw"		         type="number" field="damt2"         />  
    <column name="冻结资金"	    id="frozen_amt"		       type="number" field="damt33"         />  
    <column name="异常冻结资金"	id="abnormal_frozen_amt" type="number" field="damt5"         />  
    <column name="实际买入金额"	id="real_buy_amt"		     type="number" field="damt12"        />   
    <column name="买入冻结金额"	id="buy_frozen_amt"		   type="number" field="damt7"        />   
    <column name="卖出解冻金额"	id="sell_done_amt"	     type="number" field="damt8"        />   
    <column name="手工冻结金额"	id="manual_frzn_amt"	   type="number" field="damt9"        />   
    <column name="手工解冻金额"	id="manual_unfrzn_amt"   type="number" field="damt10"        />   
  </output>
</function>