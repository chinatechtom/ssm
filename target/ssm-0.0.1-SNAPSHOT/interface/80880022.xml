<?xml version="1.0" encoding="gbk" ?>
<function id="80880022" name="深交所报送数据查询">
	<input export="true">
		<item type="select"	id="institution_id"	name="营业部" source="97000001"/>	
		<item type="select"	id="market_code"	name="市场" nullable="false" source="039" default="2"/>	
		<item type="text"	id="guarantee_rate"	name="履约保障比例低于" default="2"/>
		<item type="text"	id="first_amt"		name="初始交易金额大于" default="30000000"/>
		<item type="select" id="order_type2"	name="业务类别" source="739" default="1" nullable="false"/>
		<group name="分页查询" page="true"> 		
			<item type="text"   id="page_num"	name="页码"     integer="true" default="1"/>
			<item type="select" id="rows_num"	name="每页行数" integer="true" source="95000012" nullable="false"/>
		</group>
	</input>
	<service type="procedure" id="P60880022" process="933" style="77">
		<param id="user_id"				type="integer"/>
		<param id="institution_id"		type="integer"/>
		<param id="market_code"			type="integer"/>
		<param id="guarantee_rate"		type="string"/>
		<param id="first_amt"			type="double"/>
		<param id="order_type2"			type="string"/>
		<param id="page_num"			type="integer"/>
		<param id="rows_num"			type="integer"/>
	</service>
	<output export="true">
		<column name="营业部代码"		type="string"	id="institution_code"/>
		<column name="营业部名称"		type="string"	id="institution_name"/>
		<column name="市场"				type="string"	id="market_code"/>
		<column name="客户号"			type="string"	id="cust_code"/>
		<column name="客户姓名"			type="string"	id="cust_name"/>
		<column name="交易协议书编号"	type="string"	id="sbond_no"/>
		<column name="履约保障比例"		type="number"	id="guarantee_rate" precision="4"/>
		<column name="初始交易合同序号"	type="string"	id="sqbh"/>
		<column name="会员简称"			type="string"	id="huiyuan_name"/>
		<column name="证券代码"			type="string"	id="product_code"/>
		<column name="证券简称"			type="string"	id="product_name"/>
		<column name="股份性质"			type="string"	id="stock_property"/>
		<column name="融出方名称"		type="string"	id="rcf_name"/>
		<column name="融入方名称"		type="string"	id="rrf_name"/>
		<column name="质押证券数量"		type="string"	id="curr_stk_vol" precision="0"/>
		<column name="融入方应付金额"	type="string"	id="rrf_should_pay"/>
		<column name="预警线"			type="number"	id="warning_rate" precision="4"/>
		<column name="平仓线"			type="number"	id="pc_rate" precision="4"/>
	</output>
</function>