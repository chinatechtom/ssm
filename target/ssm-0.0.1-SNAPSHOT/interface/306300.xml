<?xml version="1.0" encoding="gbk" ?>
<function id="306300" name="标准合同签署申请" >
	<input print="306300.jsp">
		<item type="hidden" id="cust_id" name="客户ID" action="30107,3042210,30631,1888,30108,3063405,30127"/>
		<group name="合同基本信息">
		    <item type="hidden" id="cust_id"                name="客户ID"/><!--从30107.xml中获取-->
		  	<item type="text"   id="cust_name"   			name="客户姓名"     editable="false"/><!--从30107.xml中获取-->
		    <item type="text"   id="cust_code"   			name="客户代码"     editable="false"/><!--从30107.xml中获取-->
			<item type="hidden"   id="commness_cust_code"   name="普通资金账户"/>
			<item type="hidden"   id="cust_type"            name="客户类型"    />
			<item type="hidden" id="order_no"        		name="合同编号" />
			<item type="hidden" id="formerly_voucher_no" 	name="原始凭证号"/>
			<item type="hidden" id="currency"    			name="货币类型"     nullable="false" editable="false"/>
			<item type="text"   id="currency_name"    		name="货币类型"     nullable="false" editable="false"/>
			<item type="hidden" id="dc_mode"     			name="信用使用模式" source="306" default="1"/>
			<item type="hidden" id="begin_time"  			name="起始日期"/><!--从上级页面即30628.xml中获取-->
			<item type="hidden" id="return_time" 			name="到期日期"/><!--从上级页面即30628.xml中获取-->
			<item type="hidden" id="over_time"   			name="延长日期"/><!--从上级页面即30628.xml中获取-->
			<item type="text"   id="app_years"   			name="申请年数"    nullable="false" integer="true" editable="false"/><!--从30127中获取-->	
			<item type="hidden" id="company"     			name="券商名称"/>	<!--从1888.xml获得的券商名称,用于打印回单-->	
			<item type="hidden" id="date"    		 		name="打印日期"/>	<!--从1888.xml获得的打印日期-->
			<item type="hidden" id="user_code"     			name="打印柜员代码"/><!--从1888.xml获得的打印柜员代码-->
			<item type="hidden" id="user_name"     			name="打印柜员名称"/><!--从1888.xml获得的打印柜员名称-->			
			<item type="hidden" id="remark1"     			name="备注说明"/>
			<item type="button" id="assistant"   			name="更多详情"    action="30107,3200301,30102,30104,30109,30111,30201,30202,30205,30206,30207,30215,30673,30672,30667,30668" multiple="0" />
		</group>
		<group name="联系资料"><!--此group下面都从30107.xml中获取-->
			<item type="text"   id="move_tel"           name="移动电话"       editable="false" nullable="false"/>
			<item type="text"   id="fix_tel"            name="固定电话"       editable="false" nullable="false"/>
			<item type="text"   id="meet_tel"           name="应急电话"       editable="false" />
			<item type="text"   id="family_address"     name="联系地址"       editable="false" nullable="false"/>
			<item type="text"   id="email"              name="电子邮件"       editable="false" nullable="false"/>
			<item type="text"   id="ps_code"            name="邮政编码"       editable="false" nullable="false"/>
			<item type="text"   id="fax"                name="传真"           editable="false" />
			<item type="select" id="find_mode"          name="主联系方式"     source="312"     nullable="false" default="1"/>
			<item type="text"   id="emerg_contacts_man" name="应急联系人"     editable="false" nullable="false"/>
			<item type="text"   id="emerg_contacts_tel" name="应急联系人电话" editable="false" nullable="false"/>
		</group>
		<group name="自然情况">
			<item type="grid" id="cust_id"         source="30422"       name="自然情况"/>
			<item type="text" id="total_score"     name="自然情况总分" editable="false" default="0.0"/> <!--从3042203.xml中获取-->
		</group>
		<group name="授信评估">
			<item type="text"     id="valuation_score"   	  name="评估总分"         number="true" editable="false"/>
			<item type="hidden"     id="total_capital"   	  name="普通账户总资产"         number="true" editable="false"/> <!--modify by SECREQ-1185-->
			<item type="select"   id="auto_credit_level" 	  name="信用级别"         source="90000002" editable="false"/>
			<item type="text"     id="auto_credit_num_wy"     name="授信额度(万元)"         number="true"     editable="false"/> <!--modify by SECREQ-1185-->
			<item type="textarea" id="chinese"           	  name="授信额度中文大写" editable="false"/>
			<item type="hidden"     id="auto_credit_num"   	  name="授信额度"         number="true"     editable="false"/> <!--modify by SECREQ-1185-->
		</group>
		<group name="授信额度申请">
		    <item type="hidden"   id="product_id1"   name="融资产品" nullable="false" editable="false"/>
			<item type="text"     id="product_name1" name="融资产品" nullable="false" editable="false"/>
			<item type="hidden"   id="product_id2"   name="融券产品" nullable="false" editable="false"/>
			<item type="text"     id="product_name2" name="融券产品" nullable="false" editable="false"/>
			<item type="button"   id="assistant"	 name="更多详情" action="3050102"   multiple="0"/>
		</group>
		<item type="hidden" id="paraid"	    name="杂项ID"         action="32310" default="1030"/>
		<item type="hidden" id="paravalue" 	name="杂项数值" 	  default="0"/>		
		<group name="授信额度有效天数">   
			<item type="text"  id="days"    name="有效天数"  filter="paravalue=1" integer="true"/>		   
		</group>
		<group name="客户参数">
			<item type="hidden"  id="credit_control_mode"     	name="信用控制模式"      source="128"  default="1"/>
			<item type="text"     id="pc_mode_name"          	name="平仓顺序" 		   nullable="false" editable="false"/>
			<item type="hidden"   id="pc_mode"               	name="平仓顺序" 		   nullable="false" editable="false"/>
			<item type="text"     id="return_mode_name"      	name="还款模式" 		   nullable="false" editable="false"/>
			<item type="hidden"   id="return_mode" 		     	name="还款模式" 		   nullable="false" editable="false"/>
			<item type="text"     id="borrow_stock_pg_name" 	name="融券复权模式" 	   nullable="false" editable="false"/>
			<item type="hidden"   id="borrow_stock_pg" 	     	name="融券复权模式" 	   nullable="false" editable="false"/>
			<item type="hidden"   id="borrow_stock_hg" 	     	name="融券红股模式"      source="123"  default="1"/>
			<item type="text"     id="borrow_stock_tspc_name" 	name="融券退市平仓日期"  nullable="false" editable="false"/>	
			<item type="hidden"   id="borrow_stock_tspc" 	 	name="融券退市平仓日期"  nullable="false" editable="false"/>	
		</group>  
		<group name="备    注">
			<item type="textarea" id="remarks" name="备注说明"/>
		</group>
	</input>
	<service type="procedure" id="p_qualifications_insert3" implement="p_qualifications_insert3306300" process="811" style="1">
		<param type="integer" id="user_id"             way="in"/>
		<param type="integer" id="cust_id"             way="in"/>
		<param type="string"  id="order_no"            way="in"/>
		<param type="string"  id="formerly_voucher_no" way="in"/>
		<param type="string"  id="dc_mode"             way="in"/>
		<param type="string"  id="currency"            way="in"/>
		<param type="string"  id="begin_time"          way="in"/>
		<param type="string"  id="return_time"         way="in"/>
		<param type="string"  id="over_time"           way="in"/>
		<param type="string"  id="remarks"             way="in"/>
		<param type="string"  id="credit_control_mode" way="in"/>
		<param type="string"  id="pc_mode"             way="in"/>
		<param type="string"  id="return_mode"         way="in"/>
		<param type="string"  id="borrow_stock_pg"     way="in"/>
		<param type="string"  id="borrow_stock_hg"      way="in"/>
		<param type="string"  id="borrow_stock_tspc"   way="in"/>
		<param type="string"  id="move_tel"            way="in"/>	
		<param type="string"  id="fix_tel"             way="in"/>
		<param type="string"  id="meet_tel"            way="in"/>
		<param type="string"  id="family_address"      way="in"/>
		<param type="string"  id="ps_code"             way="in"/>
		<param type="string"  id="email"               way="in"/>
		<param type="string"  id="fax"                 way="in"/>
		<param type="string"  id="find_mode"           way="in"/>
		<param type="string"  id="emerg_contacts_man"  way="in"/>
		<param type="string"  id="emerg_contacts_tel"  way="in"/>
		<param type="integer" id="product_id1"         way="in"/>
		<param type="integer" id="product_id2"         way="in"/>
		<param type="double"  id="auto_credit_num"     way="in"/>
		<param type="double"  id="total_score"         way="in"/>
		<param type="double"  id="valuation_score"     way="in"/>
		<param type="string"  id="auto_credit_level"   way="in"/>
		<param type="string"  id="if_allow"            way="in" default="1"/>
		<param type="string"  id="if_blacklist"        way="in" default="2"/>
		<param type="double"  id="fund_amt"            way="in" default="0.0"/>
		<param type="double"  id="stock_market_value"  way="in" default="0.0"/>
		<param type="double"  id="stock_convert_amt"   way="in" default="0.0"/>
		<param type="double"  id="fund_convert_amt"    way="in" default="0.0"/>
		<param type="double"  id="gage_value"          way="in" default="0.0"/>
		<param type="double"  id="usable_num"          way="in" default="0.0"/>
		<param type="string"  id="remark1"             way="in"/>
		<param type="integer" id="app_days"            way="in"/>
		<param type="double"  id="cust_factor"         way="in"/>
		<param type="integer" id="app_years"           way="in"/>
		<param type="double"  id="auto_credit_num"     way="in"/>
		<param type="double"  id="auto_credit_num"     way="in"/>
		<param type="double"  id="auto_credit_num"     way="in"/>
		<param type="integer" id="days"                way="in"/>
	</service>
	<output/>
</function>