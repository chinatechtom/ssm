<?xml version="1.0" encoding="gbk" ?>
<function id="70770008" name="中征码录入-查询" >
	<input>
		<item type="select" id="insitution_id" name="营业部名称" source="97000001"/>
		<item type="text" id="cust_code" name="客户代码"/>
		<item type="text" id="cust_name" name="客户姓名"/>
		<item type="select" id="cust_type" name="客户类别" source="027" editable="false" default="1"/>
