<?xml version="1.0" encoding="gbk" ?>
<function id="36000071" name="7.1.3营业部融资融券单客户大笔交易统计" >  
  <input>
  <item type="select" id="institution_id" name="营业部名称"  source="95000008"/>
  <item type="text"   id="product_code"   name="证券代码"  />
  <item type="select" id="market_code"    name="市场代码" source="039" />
  <item type="text"   id="num_line"       name="成交量阀值" default="300000" />
  <item type="text"   id="amt_line"       name="成交金额阀值" default="300000" />
  <item type="select"   id="sec_in_out"     name="买卖方向" source="006" />
 <item type="date"   id="begig_date"     name="起始日期"  default="last"/>
 <item type="date"   id="end_date"       name="结束日期"  default="now"/>
 <item type="select"  id="order_choice"    name="排序选择"  source="374"/>
  </input>
   <service type="procedure" id="P36000071" process="911" style="77">
 <param id="user_id"         type="integer" way="in" /> 
 <param id="institution_id"  type="integer" way="in" />
  <param id="product_code"    type="string" way="in" />
  <param id="market_code"     type="string" way="in" />
  <param id="num_line"        type="double" way="in" />
  <param id="amt_line"        type="double" way="in" />
  <param id="sec_in_out"      type="string" way="in" />
  <param id="begig_date"      type="string" way="in" />
  <param id="end_date"        type="string" way="in" />
  <param id="order_choice"     type="string"  way="in"/>
  </service> 
  <output  print="true">
    <column type="number" id="id"               name="序号"/>
    <column type="string" id="c1" name="营业部"/> 
    <column type="string" id="c2"        name="客户代码"/>  
   <column type="string" id="c3"        name="客户名称"/>
   <column type="string" id="c4"             name="日期"/>
    <column type="string" id="c5"     name="市场名称"   />
     <column type="string" id="c6"    name="证券代码"  />
     <column type="string" id="c7"    name="证券名称"  />
    <column type="string" id="c8"   name="交易类别" />
     <column type="number" id="c9"  name="成交金额"  precision="2" total="true"/> 
    <column type="number" id="c10"  name="成交数量"  precision="0" total="true"/>
    <column type="string" id="c11"  name="买卖方向"/>
  </output>
</function>