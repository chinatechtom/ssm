<?xml version="1.0" encoding="gbk" ?>
<function id="27030105" name="单证券融资集中度统计" >
     <input>
      <item type="select"  id="institution_id"        name="营业部名称" source="95000008"/> 
      <item type="text"  id="product_code"        name="证券代码*"/>
      <item type="date"  id="end_date"        name="日期*" default="now"/>                  
     </input>
  <service type="procedure" id="P27030105"  process="911" style="77">
      <param id="user_id"               type="integer" way="in"/> 
       <param id="institution_id"      type="integer" way="in"/>      
      <param id="product_code"      type="string" way="in"/> 
      <param id="end_date"      type="string" way="in"/>       
   </service>
 <output print="true">
        <column type="string" id="institution_name"   name="营业部名称"/>
        <column type="number" id="fin_num" name="融资数量" />        
        <column type="number" id="fin_amt"   name="融资金额" precision="2" />
        <column type="number" id="company_fin_amt"   name="公司融资总额" precision="2" />
        <column type="number" id="rate1" name="分布比例" precision="6" />   
        <column type="number" id="fin_acc_num"   name="融资买入该证券账户数量"/>    
</output>
</function>  