<?xml version="1.0" encoding="gbk" ?>
<function id="30750" name="融资明细到期监控" >
  <input>
    <item type="text"   id="cust_code"	name="客户代码" />
    <item type="text"   id="days"		name="剩余自然天数" default="30"/>	
	<item type="select"	id="if_sum"		name="汇总选择" source="506"/>
  </input>
  <service type="procedure" id="p_financing_detail_expire_control"  process="911" style="77"> 
    <param id="user_id"		type="integer"  way="in"/>
    <param id="cust_code"   type="string"   way="in"/>  
	<param id="days"		type="integer"  way="in"/>  
	<param id="if_sum"		type="integer"  way="in"/>  
  </service>
  <output>
	<column type="number" id="cust_id"				name="客户id" display="false"/>
	<column type="string" id="cust_code"		    name="客户代码"/>
	<column type="string" id="cust_name"		    name="客户姓名"/>			
	<column type="string" id="branch_code"		    name="营业部代码" display="false"/>
	<column type="string" id="institution_name"		name="营业部名称"/>
	<column type="string" id="occur_date"		    name="发生日期"/>
	<column type="string" id="serial_no"		    name="流水号"/>
	<column type="string" id="business_code"		name="业务代码"/>
	<column type="string" id="business_name"		name="业务名称"/>
	<column type="string" id="market_code"			name="市场代码"/>
	<column type="number" id="product_id"			name="产品id" display="false"/>
	<column type="string" id="sec_code"				name="证券代码"/>
	<column type="string" id="product_name"			name="证券名称"/>
	<column type="number" id="buy_vol"				name="买入数量"/>
	<column type="number" id="sell_vol"				name="卖出数量"/>
	<column type="string" id="last_sell_date"		name="最后卖出日期"/>
	<column type="string" id="currency_type"	    name="币种"/>
	<column type="number" id="occur_bal"			name="发生金额" precision="3"/>
	<column type="number" id="payback_bal"			name="偿还金额" precision="3"/>
	<column type="number" id="amt_bal"				name="余额" precision="3"/>
	<column type="string" id="last_payback_date"	name="最后偿还日期"/>
	<column type="number" id="interest"       name="已结利息" precision="3"/>
	<column type="number" id="interest_pay"   name="已付利息" precision="3"/>
	<column type="number" id="yjwf"          name="已结未付利息" precision="3"/>
	<column type="string" id="note"					name="备注"/>
	<column type="string" id="fee_flag"				name="计费标志" display="false"/>
	<column type="string" id="payback_flag"			name="还款标志" display="false"/>
	<column type="number" id="fee_sum"				name="合计费用" precision="3"/>
	<column type="number" id="non_pay_fee_sum"		name="未结费用" precision="3"/>
	<column type="number" id="availability_day_num"	name="剩余自然天数"/>
	<column type="number" id="reach_date"	        name="预估剩余交易天数"/>
	<column type="string" id="pc_reason"		    name="平仓原因" display="false"/>	
	<column type="string" id="pc_reason_name"		name="平仓原因1" display="false"/>	
	<column type="string" id="pc_mode"				name="平仓顺序" display="false"/>
	<column type="string" id="warning_level"		name="报警级别"	level="true" display="false"/>
	<column type="number" id="pc_amt"				name="强制平仓额度" precision="3"/>
	<column type="string" id="expire_date"			name="明细到期日期"/>
	<column type="string" id="touch_date"			name="触发平仓日期"/>
	<column type="string" id="ddate"				name="当前系统日期"/>
	<column type="string" id="pc_begin_date"		name="拟平仓日期"/>
	<column type="number" id="current_num"			name="触发时担保率" precision="3"/>
	<column type="number" id="cur_gua_rate"			name="实时担保率" precision="3"/>				
	<button type="edit"   id="edit" name="生成平仓预案" action="30726" multiple="0"/>
  </output>	
</function>