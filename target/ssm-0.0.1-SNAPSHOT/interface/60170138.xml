<?xml version="1.0" encoding="gbk" ?>
<function id="60170138" name="增加电话通知记录信息" >
	<input>	
		<item type="text" id="cust_code" name="客户代码" nullable="false"/>
		<item type="date" 	id="record_date" name="通知日期" nullable="false" default="now"/>
		<item type="text" id="record_times" name="通知次数" nullable="false"/>
		<item type="text" id="record_content" name="通知内容" nullable="false"/>
		<item type="select" id="if_connection" name="是否通知到客户" source="921"  nullable="false" default="0"/>
		<item type="text" id="remark" name="备注" />
	</input>   
	<service type="procedure" id="p60170138"  process="833" style="1"> 
		<param id="user_id"			type="integer" 	way="in"/>
		<param id="cust_code"   	type="string"  way="in"/>
		<param id="record_date"    	type="string" way="in"/>
		<param id="record_times"    type="integer" 	way="in"/>
		<param id="record_content"  type="string" way="in"/>
		<param id="if_connection"   type="string" 	way="in"/>  
		<param id="remark"			type="string"  	way="in"/>  	
	</service> 
	<output/>  
</function>    