		<item type="text"	id="cust_code"			name="客户代码"/>
		<item type="text"	id="commness_cust_code"	name="普通资金账号"/>			<!-- modify by SECREQ-1427 20140711 -->
		<item type="date"	id="begin_date"			name="开始日期" default="null"/>
		<item type="date"	id="end_date"			name="结束日期"	default="null"/>
		<item type="select" id="credit_type1"		name="业务类别"	source="388"/>
