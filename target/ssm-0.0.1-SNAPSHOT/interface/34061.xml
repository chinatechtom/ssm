<?xml version="1.0" encoding="gbk" ?>
<function id="34061" name="场外资产抵押折算记录查询">
	<input > 	
		<item type="text"   id="cust_code" name="客户号"/> 
		<item type="select" id="mortgage_type" name="质押物类别" source="262"/> 
		<item type="date"   id="begin_date" name="起始日期" default="last"/> 
		<item type="date"   id="end_date" name="结束日期" default="now"/>  
		<item type="select" id="status" name="记录状态" source="810" default="1"/> 
	</input>
	<service type="procedure" id="P17000053" implement="P17000053_34061" process="911" style="77">
		<param id="user_id" type="integer"/>
		<param id="cust_code" type="string"/> 
		<param id="mortgage_type" type="string"/> 
		<param id="begin_date" type="string"/> 
		<param id="end_date" type="string"/> 
		<param id="status" type="string"/> 
  </service> 
	<output>  
		<column type="string"	id="mortgage_id"    name="抵押记录ID"/>
		<column type="string"	id="begin_date"     name="起始日期"	/>
		<column type="string"	id="end_date"       name="结束日期"	/>
		<column type="string"	id="cust_code"      name="信用资金账号"	/>
		<column type="string"	id="cust_name"      name="客户名称"	/>
		<column type="string"	id="status"         name="记录状态" display="false"	/>
		<column type="string"	id="mortgage_value" name="抵押金额(万元)"	/>
		<column type="string"	id="mortgage_type"  name="抵押物类别" display="false"/>
		<column type="string"	id="remark"         name="备注"	/>
		<column type="string"	id="time"           name="时间"	/>
		<column type="string"	id="user_code"      name="操作员"	/>
		<column type="string"	id="status_name"    name="记录状态"	/>
		<column type="string"	id="mortgage_type_name" name="抵押物类别"	/> 
		<button type="set"	id="add"     name="新增场外抵押记录" action="34062" multiple="-1"/> 
		<button type="set"	id="modify"  name="修改场外抵押记录" action="34063" extendaction="status" multiple="0"/> 
		<button type="set"	id="delete"  name="删除场外抵押记录" action="34064" extendaction="status" multiple="0"/>
		</output>  
</function>
		
		
		