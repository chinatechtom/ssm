<?xml version="1.0" encoding="gbk" ?>
<function id="31002" name="证券统计" >  
  <input>
	<item type="date" id="date" name="日期"  default="now"/>
	<item type="select" id="property_type" name="资产种类"  source="129"/>
	<item type="select" id="mark_code" source="039" name="市场代码" />
  </input>
   <service type="procedure" id="p_stock_addup" process="911" style="77">
	<param id="user_id" type="integer"/> 
	<param id="date"/>
	<param id="property_type" />
	<param id="mark_code" />
  </service> 
  <output >
    <column type="string" id="date" name="日期"/>   
	<column type="number" id="institution_id" name="机构id" display="false"/>
	<column type="string" id="institution_code" name="机构代码"/>
    <column type="number" id="property_type"   name="资产种类" display="false"/>
	<column type="string" id="property_typename"   name="资产种类"/>
    <column type="number" id="currency_type"   name="货币类型" display="false"/>
	<column type="string" id="currency_typename"   name="货币类型"/>
    <column type="number" id="market_code"   name="市场代码" display="false"/>
	<column type="string" id="market_codename"   name="市场代码"/>
	<column type="number" id="product_type" name="产品类型" display="false"/>   
	<column type="string" id="product_typename" name="产品类型"/>   
    <column type="number" id="product_id"   name="产品id" display="false"/>
    <column type="string" id="product_code"   name="产品代码"/>
    <column type="number" id="asset_qty"   name="资产余额" precision="2"/>
	<column type="number" id="cur_buy_vol"  name="当日买入数量" precision="2"/>
    <column type="number" id="cur_buy_amt"   name="当日买入金额" precision="2"/>
    <column type="number" id="cur_sell_vol"   name="当日卖出数量" precision="2"/>
    <column type="number" id="cur_sell_amt"   name="当日卖出金额" precision="2"/>
	<column type="number" id="abnormal_frzn_vol"  name="异常冻结数量" precision="2"/>   
    <column type="number" id="buy_unfrzn_vol"   name="买入解冻数量" precision="2"/>
    <column type="number" id="sell_frozen_vol"   name="委托冻结数量" precision="2"/>
    <column type="number" id="manual_frozen_vol"   name="手工冻结数量" precision="2"/>
	<column type="number" id="manual_unfrzn_vol"  name="手工解冻数量" precision="2"/>   
    <column type="number" id="trin_bal"   name="存入数量" precision="2"/>
    <column type="number" id="trout_bal"   name="取出数量" precision="2"/>
    <column type="number" id="uncome_buy_vol"   name="买入未过户数量" precision="2"/>
	<column type="number" id="uncome_buy_amt"  name="买入未过户金额" precision="2"/>   
    <column type="number" id="uncome_sell_vol"   name="卖出未过户数量" precision="2"/>
    <column type="number" id="uncome_sell_amt"   name="卖出未过户金额" precision="2"/>
    <column type="number" id="taday_qty"   name="当日变动数量" precision="2"/>
	<column type="number" id="asset_value"  name="资产价值" precision="2"/>   
    <column type="number" id="buy_cost_price"   name="持仓成本" precision="2"/>
    <column type="number" id="profit_loss"   name="浮动盈亏" precision="2"/>
	<column type="number" id="stock_convert_rate"   name="股票折算率" precision="2"/>
	<column type="number" id="stock_convert_amt"  name="股票折算金额" precision="2"/>   
  </output>
</function>