<?xml version="1.0" encoding="gbk" ?>
<function id="30683" name="查询客户评级资料-交易历史" >
  <input>
	<item type="hidden" id="cust_id" name="id"/>
  </input>
  <service type="procedure" id="p_cust_trade_his" process="911" style="77">
	<param type="integer" id="user_id"/>	
	<param type="integer" id="cust_id"/>	
  </service>
  <output>    
    <column type="number" id="cust_id" name="合同id" display="false"/>
    <column type="number" id="trade_amt_all" name="总交易量" precision="3"/>  
	<column type="number" id="fee_amt" name="总佣金贡献" precision="3"/>
	<column type="number" id="velocity_rate" name="周转率"  precision="3"/>  
	<column type="number" id="profit_loss" name="总盈亏" precision="3"/>
	<column type="number" id="one_a_stkcode" name="交易量最大的A股"/>  
	<column type="number" id="match_one_amt" name="排名第一的证券成交额"  precision="3"/>
  </output> 
</function>