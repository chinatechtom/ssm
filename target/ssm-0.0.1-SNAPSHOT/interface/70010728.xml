		<item type="text" 	id ="prewarning_price" 		name="预警履约保障比率(%)"	editable="false"/>
		<item type="text" 	id ="lowest_price" 		name="最低履约保障比例(%)"	editable="false"/>
		<item type="text" 	id ="fix_rate" 		name="固定交易费用比例(%)"	editable="false"/>
		<item type="text" id="manual_chg"    name="购回金额手工调整" editable="false"/>
