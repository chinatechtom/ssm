<?xml version="1.0" encoding="gbk" ?>
<function id="30809" name="担保资金账户历史数据查询" >	
  <input>		
  	<item type="text" 	id="acc_code" 	name="账户代码"/>
		<item type="date" 	id="date"	name="起始日期" default="last" nullable="false"/>
  </input>  
  <service type="procedure" id="p_guafund_account_data_hisquery" process="911" style="77">
		<param id="user_id"	 type="integer" way="in" /> 
		<param id="acc_code" type="string"  way="in"/>		
		<param id="date"    type="string"  way="in"/>
  </service>  
  <output export="true">
			<column type="string" id="ACC_CODE"   	name="账户代码"/>
			<column type="number" id="BS_OUT_AMT"   name="融券卖出资金余额" precision="2"/>
			<column type="number" id="GUA_AMT"   		name="货币保证金余额"         precision="2"/>
    	<column type="string" id="DATE"   			name="日期"/>
  </output>
</function>