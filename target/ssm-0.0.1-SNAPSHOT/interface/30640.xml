	<item type="hidden" id="product_id" name="产品号" />
  </input>
  <service type="procedure" id="p_product_credit_query" process="911" style="77">
	<param id="user_id" type="integer" way="in" />
	<param id="product_id" type="integer" way="in" />	
  </service>
  <output>    
  	<column type="number" id="product_id"   name="产品id"  display="false" />
    <column type="string" id="cyc_unit"   name="周期单位"  />
    <column type="number" id="product_cyc"   name="产品周期"  />
    <column type="number" id="financing_interest_rate"   name="融资利息率(%)" /> 
    <column type="number" id="financing_interest_rate"     name="融券利息率(%)"  />
    <column type="string" id="punish_interest_rate" name="罚息率(%)"/>
	<column type="number" id="commission_charge"  name="手续费" />   
	<column type="string" id="guarantee_rate"  name="担保率" />   
	<column type="string" id="count_mode"  name="计费方式" />   
	<column type="number" id="charge_mode"  name="收费模式" />   
 </output>  
</function>