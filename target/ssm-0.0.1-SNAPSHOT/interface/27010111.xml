<?xml version="1.0" encoding="gbk" ?>
<function id="27010111" name="1-3融资业务收入状况表" >
  <input>
    <item type="select" id="insitution_id" name="营业部名称" source="95000008"/>
	<item type="date"   id="begin_date"    name="起始日期" default="last"/>  
    <item type="date"   id="end_date"      name="结束日期" default="now"/> 	
  </input>
  <service type="procedure" id="P27010111" implement="P2701011127010111" process="955" style="77">
    <param id="user_id"               type="integer" way="in"/>
    <param id="insitution_id"         type="integer" way="in"/>
    <param id="begin_date"            type="string" way="in"/>
    <param id="end_date"              type="string" way="in"/>	
  </service>
 <output print="true">
	<column type="number" id="id"   name="序号"/>	
	<column type="string" id="institution_code"   name="营业部代码"/>        
	<column type="string" id="institution_name"   name="营业部名称" />
	<group name="融资业务收入">
	  <group name="本期融资业务收入">
	    <group name="固定">
		  <column type="number" id="this_date_fix_benifit" name="已收"           precision="2" total="true"/>   
		  <column type="number" id="this_date_fix_benifit_sum" name="应收未收"   precision="2" total="true"/>   
		</group>
		<group name="非固定">
		  <column type="number" id="this_date_unfix_benifit" name="已收"           precision="2" total="true"/>   
		  <column type="number" id="this_date_unfix_benifit_sum" name="应收未收"   precision="2" total="true"/>   
		</group>
		<column type="number" id="this_date_fin_benifit" name="本期合计"     precision="2" total="true"/>   
	  </group>
	  <column type="number" id="RATE" name="本期融资业务收入占比"   precision="6"/>   
	  <group name="本年度累计融资业务收入">
	    <group name="固定">
		  <column type="number" id="this_year_fix_benifit" name="已收"       precision="2" total="true"/>   
		  <column type="number" id="this_year_fix_benifit_sum" name="应收未收"   precision="2" total="true"/>   
		</group>
		<group name="非固定">
		  <column type="number" id="this_year_unfix_benifit" name="已收"       precision="2" total="true"/>   
		  <column type="number" id="this_year_unfix_benifit_sum" name="应收未收"   precision="2" total="true"/>   
		</group>
		<column type="number" id="this_year_fin_benifit" name="本年度合计"     precision="2" total="true"/>   
	  </group>	  
    </group>		
</output>
</function>  