<?xml version="1.0" encoding="gbk" ?>
<function id="3130601" name="词典内容查询" >
	<!--<description>
		增加内容设置时注意选择词典分类名称，修改词典内容时词典内容代码和词典内容名称不能同时修改！否则为新增词典内容！
	</description>-->
	<input>
		<item type="hidden"	id="systemid"		name="系统id"/>
		<item type="text"	id="coding_sort"	name="词典分类代码"/>
		<item type="text"	id="coding_name"	name="词典分类名称"/>
	</input>
	<service type="procedure" id="p_dictionary_content_query" implement="p_dictionary_content_query3130601"  process="911" style="77">
		<param type="integer"	id="user_id"		way="in"/>
		<param type="integer"	id="systemid"		way="in"/>
		<param type="string"	id="coding_sort"	way="in"/>
		<param type="string"	id="coding_name"	way="in"/>
	</service>
	<output autoload="true">
		<column type="string"	id="coding_sort"	name="词典分类代码"/>
		<column type="string"	id="coding_name"	name="词典分类名称"/>
		<column type="string"	id="dick_type"		name="词典类型"/>
		<column type="string"	id="data_code"		name="词典内容代码"/>
		<column type="string"	id="data_name"		name="词典内容名称"/>
		<button type="add"		id="add"			name="增加词典内容" action="3130602" multiple="-1"/>
		<button type="edit"		id="edit"			name="修改词典内容" action="3130602" multiple="0"/>
	</output>
</function>