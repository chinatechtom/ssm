      <item type="text"  id="product_code"        name="证券代码*"/>              
     </input>
  <service type="procedure" id="P27030107"  process="911" style="77">
      <param id="user_id"               type="integer" way="in"/> 
       <param id="institution_id"      type="integer" way="in"/>      
      <param id="product_code"      type="string" way="in"/>      
   </service>
 <output print="true">
        <column type="string" id="institution_name"   name="营业部名称"/>
        <column type="number" id="bs_vol" name="融券数量" />        
        <column type="number" id="bs_amt"   name="融券市值" precision="2"/>
         <column type="number" id="company_bs_amt"   name="公司该证券融券数量" />
        <column type="number" id="rate1" name="分布比例" precision="6" />   
        <column type="number" id="bs_acc_num"   name="融券卖出该证券账户数量"  />    
</output>
</function>  