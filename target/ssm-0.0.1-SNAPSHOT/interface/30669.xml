<?xml version="1.0" encoding="gbk" ?>
<function id="30669" name="查询利息" >
  <input>
	<item type="hidden" id="cust_id" name="客户id"/>
	<item type="hidden" id="bond_id" name="合同id"/>
  </input>
  <service type="procedure" id="p_credit_charge_query" process="911" style="77">
	<param type="integer" id="user_id"/>	
	<param type="integer" id="cust_id"/>	
	<param type="integer" id="bond_id"/>
  </service>
  <output>    
    <column type="number" id="bond_id" name="合同id" display="false"/>
    <column type="number" id="cust_id" name="客户id" display="false"/>
    <column type="string" id="cust_code" name="客户代码"/>  
		<column type="string" id="cust_name" name="客户姓名"/>
		<column type="string" id="accout_interest_date"      name="计息日期"/>  
		<column type="number" id="FINANCING_INTEREST_SUM"    name="融资未结利息"   precision="2" total="true"/>
		<column type="number" id="BORROW_STOCK_INTEREST_SUM" name="融券未结利息"   precision="2" total="true"/>  
		<column type="number" id="FINANCING_FIX_SUM"         name="融资未结固定额度费" precision="2" total="true"/>
		<column type="number" id="BORROW_STOCK_FIX_SUM"      name="融券未结固定额度费" precision="2" total="true"/>  
		<column type="number" id="FINANCING_TAKEUP_SUM"      name="融资未结占用额度费" precision="2" total="true"/>
		<column type="number" id="BORROW_STOCK_TAKEUP_SUM"   name="融券未结占用额度费" precision="2" total="true"/>
		<column type="number" id="FINANCING_BEF_RTN_SUM"     name="融资未结提前还贷手续费" precision="2" total="true"/>  
		<column type="number" id="BORROW_STOCK_BEF_RTN_SUM"  name="融券未结提前还贷手续费" precision="2" total="true"/>
		<column type="number" id="FINANCING_PUNISH_SUM"      name="融资未结逾期罚息"   precision="2" total="true"/>  
		<column type="number" id="BORROW_STOCK_PUNISH_SUM"   name="融券未结逾期罚息"   precision="2" total="true"/>
		<column type="number" id="FINANCING_BAD_PUNISH_SUM"  name="融资未结坏账罚息"   precision="2" total="true"/>  
		<column type="number" id="BORROW_STOCK_BAD_PUNISH_SUM" name="融券未结坏账罚息" precision="2" total="true"/>		
  </output> 
</function>