<?xml version="1.0" encoding="gbk" ?>
<function id="27010127" name="客户融资融券额度分布" >
	<input>	
		<item type="select" id="insitution_id"	name="营业部名称" source="95000008"/>
		<item type="text"	id="cust_code"		name="客户代码" />
		<item type="date"	id="begin_date"		name="日期" />
	</input>
	<service type="procedure" id="p27010127" implement="p2701012727010127" process="955" style="77">
		<param id="user_id"			type="integer"	/>
		<param id="insitution_id"	type="integer"	/>
		<param id="cust_code"		type="string"	/>
		<param id="begin_date"		type="string"	/>
	</service>
	<output export="true">  
		<column type="string" id="row_no"   		name="序号"  />
		<column type="string" id="institution_id"   name="营业部ID"		display="false" />
		<column type="string" id="institution_code"	name="营业部代码"	/> 
		<column type="string" id="institution_name"	name="营业部"	/>
		<column type="string" id="cust_code"		name="客户号"	/>
		<column type="string" id="cust_name"		name="客户姓名"	/>
		<column type="number" id="approve_qty"		name="已批准融资融券额度"	precision="2"/>
		<group name="已批准融资额度 ">
			<column type="number" id="approve_fix_qty"		name="固定额度"		precision="2"/>
			<column type="number" id="approve_unfix_qty"	name="非固定额度"	precision="2"/>
		</group>		
		<group name="已使用融资金额">
			<column type="number" id="used_fix_qty"			name="固定额度"		precision="2"/>
			<column type="number" id="used_unfix_qty"		name="非固定额度"	precision="2"/>
		</group>		
		<group name="未使用融资额度">
			<column type="number" id="unuse_fix_qty"		name="固定额度"		precision="2"/>
			<column type="number" id="unuse_unfix_qty"		name="非固定额度"	precision="2"/>
		</group>
		<column type="number" id="approve_rq_qty"	name="已批准融券金额"	precision="2"/>
		<column type="number" id="used_rq_qty"		name="已使用融券金额"	precision="2"/>
		<column type="number" id="unuse_rq_qty"		name="未使用融券金额"	precision="2"/>		
		<group name="客户资产">
			<column type="number" id="fund_amt"					name="资金"				precision="2"	/>
			<column type="number" id="guarantee_market_value"	name="担保证券市值"	precision="2"	/>
			<column type="number" id="cust_asset"				name="总资产"			precision="2"	/>
			<column type="number" id="cust_asset_unit_percent"	name="占比"				precision="4"	/>
		</group>
	</output>
</function>