<?xml version="1.0" encoding="gbk" ?>
<function id="30202" name="客户证券查询" >
	<input export="true">	
    <item type="hidden"  id="select_method_id"	    name="查询方式" source="515" nullable="false"/>					
		<item type="select"  id="institution_id"	    name="营业部名称" source="95000008"/>
		<item type="hidden"  id="cust_id"	            name="客户id"/>
		<item type="text"    id="cust_code"	            name="客户代码"/>
		<item type="hidden"  id="acc_id"	            name="账户id"/>
		<item type="text"    id="acc_code"	            name="证券账户"/>
		<item type="text"    id="product_code"          name="证券代码"/>
        <group name="分页查询" page="true">			
			<item type="text"    id="page_num"       name="页码"     integer="true" default="1"/>
			<item type="select"  id="rows_num"       name="每页行数" integer="true" source="95000012" nullable="false"/>
		</group>
	</input>
    <service type="procedure" id="p_cust_acc_sec_query"  implement="p_cust_acc_sec_query30202"  process="911" style="77">
		<param id="user_id"	            type="integer" way="in"/> 
 		<param id="page_num"	        type="integer"  way="in"/> 			
 		<param id="rows_num"	        type="integer"  way="in"/> 		
 		<param id="select_method_id"	    type="integer" way="in"/> 		
		<param id="institution_id"	    type="integer" way="in"/> 
		<param id="cust_id"	            type="integer" way="in"/> 
		<param id="cust_code"	        type="string"  way="in"/> 
		<param id="acc_id"				type="integer" way="in"/> 
		<param id="acc_code"	        type="string"  way="in"/> 
		<param id="product_code"        type="string"  way="in"/>
    </service>
    <output export="true">
    <column type="string" id="institution_code"		  name="营业部代码"/>    	
		<column type="string"  id="institution_name"		name="营业部名称"/>
		<column type="string"  id="cust_code"				name="客户代码"/>
		<column type="string"  id="cust_name"				name="客户姓名"/>
		<column type="number"  id="acc_id"					name="账户id"	display="false"/>        
		<column type="string"  id="acc_code"				name="证券账户"/>        
		<column type="string"  id="date"				    name="日期" display="false"/>                    
		<column type="string"  id="property_type"			name="资产种类" display="false"/>                     
		<column type="string"  id="currency_type"			name="货币类型" display="false"/>
		<column type="string"  id="market_code"				name="市场"/>
		<column type="string"  id="product_type"			name="证券类型"	display="false"/>
		<column type="number"  id="product_id"				name="证券id"	display="false"/> 
		<column type="string"  id="product_code"			name="证券代码"/>	
		<column type="string"  id="product_name"			name="证券名称"/>
		<column type="number"  id="asset_qty" 				name="证券余量"		precision="0" total="true"/>	
		<column type="number"  id="guarantee_stock_vol" 	name="担保品数量"		precision="0" total="true"/>
		<column type="number"  id="ungua_stock_vol" 	    name="非担保品数量"		precision="0" total="true"/>			
		<column type="number"  id="margin_offset_vol" 		name="可充抵保证金数量"	precision="0" total="true" display="false"/>
		<column type="number"  id="fin_buy_vol" 		    name="融资买入数量"		precision="0" total="true" display="false"/>
		<column type="number"  id="cur_buy_vol"				name="当日买入数量"	precision="0" total="true"/>
		<column type="number"  id="cur_buy_amt"				name="当日买入金额"	precision="2" total="true"/>        
		<column type="number"  id="cur_sell_vol"			name="当日卖出数量"	precision="0" total="true"/>
		<column type="number"  id="cur_sell_amt"			name="当日卖出金额"	precision="2" total="true"/>           
		<column type="number"  id="abnormal_frzn_vol"		name="异常冻结数量"	display="false"/>          
		<column type="number"  id="buy_unfrzn_vol"			name="买入解冻数量"	precision="0" total="true" display="false"/>            
		<column type="number"  id="sell_frozen_vol"			name="卖出冻结数量"	precision="0" total="true" display="false"/>             
		<column type="number"  id="manual_frozen_vol"		name="手工冻结数量"	display="false"/>
		<column type="number"  id="manual_unfrzn_vol"		name="手工解冻数量"	display="false"/>
		<column type="number"  id="trin_bal"				name="当日转入数量"		precision="0" total="true"/>
		<column type="number"  id="trout_bal"				name="当日转出数量"		precision="0" total="true"/>
		<column type="number"  id="uncome_buy_vol"			name="买入未过户数量"	display="false"/>
		<column type="number"  id="uncome_buy_amt"			name="买入未过户金额"	display="false"/>
		<column type="number"  id="uncome_sell_vol"			name="卖出未过户数量"	display="false"/>
		<column type="number"  id="uncome_sell_amt"			name="卖出未过户金额"	display="false"/>
		<column type="number"  id="last_price"				name="最新价格"		precision="3"/>
		<column type="number"  id="asset_value"				name="证券市值"		precision="2" total="true"/>	
		<column type="number"  id="buy_cost_price"			name="持仓成本"		display="false"/>
		<column type="number"  id="profit_loss"				name="浮动盈亏"		display="false"/>
		<column type="number"  id="stock_convert_rate"		name="证券折算率"	display="false"/>
		<column type="number"  id="stock_convert_amt"		name="折算价值"		display="false"/>
    </output>
</function>
