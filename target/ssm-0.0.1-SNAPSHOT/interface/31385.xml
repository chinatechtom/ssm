<?xml version="1.0" encoding="gbk" ?>
<function id="31385" name="清算日志查询" >
	<input>
		<item type="date"    id="date"  name="交易日期"  nullable="false"/>
		<item type="select"  id="system"  name="系统"  nullable="false" source="608" default="3"/>
		<item type="select"  id="step"  name="清算步骤"  source="350"/>
	</input>
	<service type="procedure" id="p_log_liquidation_query" process="911" style="77">
		<param id="user_id"  type="integer"/>
		<param id="date"  type="string"/>
		<param id="system"  type="string"/>
		<param id="step"  type="string"/>
	</service>
	<output export="true">
		<column type="string" id="user_code"	name="操作员代码"/>
		<column type="string" id="user_name"	name="操作员名称"/>
		<column type="string" id="date"			name="交易日期"/>
		<column type="string" id="step"			name="清算步骤"/>
		<column type="string" id="begin_time"	name="开始时间"/>
		<column type="string" id="end_time"		name="结束时间"/>
		<column type="string" id="code"			name="返回代码"/>
		<column type="string" id="result"		name="清算结果"/>
	</output>
</function>