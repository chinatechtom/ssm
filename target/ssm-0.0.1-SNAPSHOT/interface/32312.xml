<?xml version="1.0" encoding="gbk"?>
<function id="32312" name="查询功能参数设置" >  
	<input>
		<item type="text"   id="function_no"	     name="功能代码"     source="340"/>
		<item type="text"   id="function_name"	     name="代码名称"     source="340"/>
		<item type="select" id="if_date_range"	     name="日期范围控制" source="340"/>
		<item type="select" id="if_time_range"	     name="时间范围控制" source="340"/>
		<item type="select" id="if_Liquidation_brow" name="清算查询控制" source="340"/>
		<item type="select" id="if_frequency_ctl"	 name="查询频率控制" source="340"/>
	</input>
	<service type="procedure" id="p_function_query_limit_brow"  process="911" style="77">
		<param id="user_id" type="integer"/>
		<param id="function_no"/>
		<param id="function_name"/>
		<param id="if_date_range"/>
		<param id="if_time_range"/>
		<param id="if_Liquidation_brow"/>
		<param id="if_frequency_ctl"/>
	</service>
	<output >
		<column type="string" id="function_no"   	name="功能代码"/>
		<column type="string" id="function_name"   	name="菜单名称"/>
		<column type="string" id="if_date_range"	name="日期范围控制" display="false"/>
		<column type="string" id="if_date_range_name"	name="日期范围控制"/>
		<column type="string" id="if_time_range"	name="时间范围控制" display="false"/>
		<column type="string" id="if_time_range_name"	name="时间范围控制"/>
		<column type="string" id="if_Liquidation_brow"	name="清算查询控制" display="false"/>
		<column type="string" id="if_liqu_brow_name"	name="清算查询控制"	/>
		<column type="string" id="if_frequency_ctl"		name="查询频率控制" display="false"/>
		<column type="string" id="if_fre_ctl_name"		name="查询频率控制"	/>
		<column type="number" id="limit_days"		name="查询限制天数(天)"	/>
		<column type="number" id="limit_frequency"	name="查询限制频率(秒)"	/>
		<column type="string" id="limit_stime"		name="禁查开始时间"	/>
		<column type="string" id="limit_etime"		name="禁查结束时间"			/>	
		<column type="string" id="time"			    name="设置时间"	    />
		<button type="add"    id="add"		        name="增加参数" action="3231201"  multiple="-1"/>
		<button type="add" 	  id="modify"		    name="修改参数" action="3231202"  multiple="0"/>
		<button type="add" 	  id="delete"		    name="删除参数" action="3231203"  multiple="0"/>
	</output>
</function>