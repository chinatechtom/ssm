<?xml version="1.0" encoding="gbk" ?>
<function id="3064405" name="授信额度报告" >
	<input>
		<item type="hidden" id="order_id" name="order_id"/>
	</input>
	<service type="procedure" id="p_order_credit_report_query" process="911" style="77">
		<param type="integer" id="user_id"/>
		<param type="integer" id="cust_id"/>	
		<param type="integer" id="order_id"/>
		
	</service>
	<output>
		
		<column type="number" id="net_assets" name="客户净资产(万元)"/>
		<column type="number" id="commness_stock_asset" name="普通资金账户资产(万元)"/>
		<column type="number" id="financial_asset" name="其他金融资产总值(万元)"/>
		<column type="number" id="financial_asset_half" name="金融资产的一半(万元)"/>
		<column type="number" id="asset_quarter" name="总资产的四分之一(万元)"/>
		<column type="number" id="max_asset_deal" name="MAX(资产)(万元)"/>
		<column type="number" id="estate_asset" name="房产价值(万元)"/>
		<column type="string" id="estate_condition" name="个人房产状况"/>
		<column type="string" id="important_info" name="重要信息补充"/>
		<column type="number" id="total_asset_wy" name="信用资金台账总资产(万元)"/>
		<column type="number" id="total_debt_wy" name="信用资金台账总负债(万元)"/>
		<column type="number" id="report_credit_num" name="客户信用上限(万元)"/>
		<column type="number" id="first_credit_limit" name="初步授信额度(万元)"/>
		<column type="number" id="first_financing_limit" name="初步融资上限(万元"/>
		<column type="number" id="first_bstock_limit" name="初步融券上限"/>
		<column type="number" id="registered_capital" name="注册资本(万元)"/>
		<column type="number" id="net_profit" name="净利润(万元)"/>
		<column type="number" id="net_cash_flow" name="现金净流量(万元)"/>
		<column type="number" id="year_all_asset" name="会记年度总资产(万元)"/>	
		<column type="string" id="asset_over_date" name="以上资产总值截止时间"/>
	</output>
</function>