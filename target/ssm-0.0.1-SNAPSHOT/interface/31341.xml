<?xml version="1.0" encoding="gbk" ?>
<function id="31341" name="日志明细" >
	<input>     
		<item type="hidden" id="sno"  name="系统流水号"/>
		<item type="hidden" id="operdate"  name="操作时间"/>
  </input>
  <service type="procedure" id="p_system_log_detail" process="911" style="77">
    <param id="user_id"	 type="integer"/>
    <param id="operdate" type="string"  way="in"/>
		<param id="sno"      type="integer" way="in"/>
  </service>
	<output export="true">
		<column type="number" id="sno"			    name="系统流水号" display="false"/>
		<column type="string" id="operdate"			name="操作日期"/>
		<column type="string" id="sysdate"			name="操作交易日期"/>
		<column type="string" id="keyvalue"			name="操作对象"/>
		<column type="string" id="oldvalue"			name="修改前值"/>
		<column type="string" id="newvalue"			name="修改后值"/>
		<column type="string" id="remark"			  name="备注信息"/>
		<column type="date"   id="time"			    name="系统时间"/>
  </output> 
</function>