	<item type="hidden" id="order_id" name="合约ID" action="1889"/>
	<item type="hidden" id="cust_id" name="客户ID" action="37424,1888,30110,37425"/>
		<group name="客户基本情况" >
			<item type="hidden"	id="user_code"						name="操作员代码"/>
			<item type="text"	id="cust_name"						name="客户姓名"				editable="false"/>
			<item type="hidden"	id="order_no"						name="合约编号"	/>
			<item type="hidden"	id="institution_id"					name="机构id"/>
			<item type="hidden"	id="institution_code"				name="机构代码"/>
			<item type="text"	id="institution_name"				name="营业部"				editable="false"/>
			<item type="text"	id="cust_code"						name="信用资金账户"		editable="false"/>
