<?xml version="1.0" encoding="gbk" ?>
<function id="3063401" name="根据客户评分自动确定信用等级" >
  <input>	
  <item type="hidden" id="valuation_score" name="评估客户总分" precision="2"/>
  <item type="hidden" id="approve_valuation_score" name="评估客户总分" precision="2"/>
  </input>
  <service type="procedure" id="p_cust_credit_level_auto_select" process="911" style="77">
	<param id="user_id" type="integer"/>
	<param id="valuation_score" type="double"/>
	<param id="approve_valuation_score" type="double"/>
  </service>
  <output export="true">  
  	<column type="number" id="auto_credit_level"         name="信用级别" />  
  	<column type="number" id="auto_credit_num"           name="信用上限" />  
	<column type="number" id="origin_guarantee_rate"     name="初始保证金比例" precision="2"/>
	</output>
</function>