<?xml version="1.0" encoding="gbk" ?>
<function id="70010792" name="延期购回申请" >
	<input>
		<item type="hidden" id="sbond_id" name="交易协议书id"/>
		<group name="合同信息" >
			<item type="hidden" id="cust_id" name="客户id" />			
			<item type="text" id="bond_no" name="合同编号" editable="false"/>
			<item type="text" id ="cust_name" name="客户姓名" editable="false"/>
			<item type="text" id ="cust_code" name="客户代码" editable="false"/>
			<item type="text" id="sbond_no" name="交易协议书编号" editable="false"/>
		</group>  
		<item type="hidden" id="date"     name="打印日期"/>	<!--从1888.xml获得的打印日期-->
		<item type="hidden" id="user_code"     name="打印柜员代码"/><!--从1888.xml获得的打印柜员代码-->
		<item type="hidden" id="user_name"     name="打印柜员名称"/><!--从1888.xml获得的打印柜员名称-->
		<item type="hidden" id="company"    name="券商名称"/><!--从1888.xml获得的券商名称-->			
		<group name="交易协议书信息">
			<item type="text" id="sbond_no" name="交易协议书编号" editable="false"/>
		    <item type="hidden" id ="related_sbond_id"	name="related_sbond_id"/>
			<item type="text" 	id ="related_sbond_no" 	name="关联(主)交易协议书编号"	editable="false"/>
			<item type="hidden" id ="hg_code" 		name="初始交易代码" 	editable="false"/>
			<item type="hidden" id ="hg_code2" 		name="购回交易代码" 	editable="false"/>
			<item type="select" id ="market_code" 	name="市场代码" 		editable="false" source="039"/>
			<item type="text" 	id ="product_code" 	name="标的证券代码" 	editable="false"/>
			<item type="text" 	id ="product_name" 	name="标的证券名称" 	editable="false"/>
			<item type="text" 	id ="product_type" 	name="标的证券类型" 	editable="false"/>
			<item type="text" 	id ="hg_num" 			name="标的证券数量" 	editable="false"/>
			<item type="textarea" 	id ="hg_num_big" 			name="标的证券数量大写" 	editable="false"/>
			<item type="hidden" 	id ="hg_convert_rate" 	name="折扣比率(%)" 		editable="false"/>
			<item type="hidden" id ="app_price" 			name="标的证券价格" 	editable="false"/>
			<item type="date" 	id ="engage_trade_date" 	name="初始交易日期" editable="false"/>
			<item type="date" 	id ="bb_date" 			name="原购回交易日期" editable="false"/>
			<item type="date" 	id ="bb_date2" 			name="最新购回交易日期" editable="false"/>
			<item type="text" 	id ="days"				name="原购回期限(天)" 	editable="false"/>
			<item type="text" 	id ="days2"				name="最新购回期限(天)" 	editable="false"/>
			<item type="text" 	id ="hg_rate" 			name="购回价格(每100元年收益)" editable="false"/>
			<item type="text" 	id ="first_amt"		name="初始交易金额" 	editable="false"/>
			<item type="textarea" 	id ="first_amt_big"		name="初始交易金额大写" 	editable="false"/>
			<item type="text" 	id ="second_amt"		name="原购回交易金额" 	editable="false"/>
			<item type="textarea" 	id ="second_amt_big"		name="原购回交易金额大写" 	editable="false"/>
			<item type="text" 	id ="second_amt2"		name="最新购回交易金额" 	editable="false"/>
			<item type="textarea" 	id ="second_amt_big2"		name="最新购回交易金额大写" 	editable="false"/>
			<item type="text" 	id ="penalty_rate" 	name="违约金比率(日利率%)" editable="false"/>
			<item type="text" 	id ="prewarning_price" 	name="预警履约保障比率(%)" editable="false"/>
			<item type="text" 	id ="lowest_price" 	name="最低履约保障比例(%)" editable="false"/>
			<item type="text" 	id ="fix_rate" 	name="固定交易费用比例(%)" editable="false"/>
			<item type="text" id="manual_chg"   name="购回金额手工调整" editable="false"/>
			<item type="textarea" id ="remark" 		name="备注" editable="false"/>
		</group>
		<group name="延期购回信息">
			<item type="select" id ="bb_reason" name="延期购回原因" nullable="false" source="718"/>
			<item type="textarea" id ="remark1" name="延期购回备注"/>
			<item type="select" id ="bb_together" name="一并延期关联协议书" nullable="false" source="723"/>
			<item type="date" id ="bb_date3" name="延期日期" nullable="false"/>
		</group>
	</input>
	<service type="procedure" id="P70010790" process="822" style="1">
		<param type="integer" id="user_id" way="in" />
		<param type="integer" id="cust_id" way="in" />
		<param type="integer" id="sbond_id" way="in" />
		<param type="string"  id="bb_reason"  way="in"/>
		<param type="string"  id="bb_together"  way="in"/>
		<param type="string"  id="remark1"  way="in"/>
		<param type="string"  id="order_type"  way="in" default="56"/>
		<param type="string"  id="bb_date3"  way="in"/>
	</service>
	<output/>
</function>