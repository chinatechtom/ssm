<?xml version="1.0" encoding="gbk" ?>
<function id="30655555203" name="合同详细">
	<input>
		<item type="hidden" id="bond_id"   name="合同id"  action="3042203,30672,30686"/>
		<group name="合同基本信息" >
			<item type="text"	id="bond_no"			name="合同编号" editable="false"/>
			<item type="hidden" id="bond_name"			name="合同名称" editable="false"/>
			<item type="hidden" id="cust_id"			name="客户id" editable="false"/>
			<item type="text"	id="cust_name"			name="客户姓名" editable="false"/>
			<item type="text"	id="date"				name="合同签署日期" editable="false"/>
			<item type="text"	id="time"				name="合同签署时间" editable="false"/>
			<item type="hidden" id="availability_time"	name="合同有效日期" editable="false"/>
			<item type="text"	id="begin_time"			name="合同起始日期" editable="false"/>
			<item type="text"	id="return_time"		name="合同到期日期" editable="false"/>
			<item type="hidden" id="over_time"			name="合同延长日期" editable="false"/>
			<item type="hidden" id="dc_mode"			name="借贷模式id" editable="false"/>
			<item type="hidden" id="dc_mode_name"		name="借贷模式" editable="false"/>
			<item type="hidden" id="currency_type"		name="货币类型" editable="false"/>
			<item type="text"	id="currency_type_name"	name="货币类型" editable="false"/>
		</group>
		<group name="融资申请情况" >
			<item type="grid" id="bond_id" source="30689" name="融资详细"/>
		</group>
		<group name="融券申请情况" scope="group">
			<item type="grid" id="bond_id" source="30690" name="融券详细"/>
		</group>
		<group name="信用产品" scope="group">
			<item type="grid"	id="cust_id"	source="305010203"	name="信用产品"/>
			<item type="button"	id="assistant"	name="更多详情"		action="3050102" multiple="0"/>
		</group>
		<group name="自然情况">
			<item type="grid" id="cust_id"		source="30422"			name="自然情况"/>
			<item type="text" id="total_score"	name="自然情况总分"	editable="false" default="0.0"/>
		</group>
		<group name="交易情况" scope="group">
		<item type="text"	id="trade_amt_all"		name="总交易量" editable="false"/>
		<item type="text"	id="fee_amt"			name="总佣金贡献" editable="false"/>
		<item type="text"	id="velocity_rate"		name="周转率" editable="false"/>  
		<item type="text"	id="profit_loss"		name="总盈亏" editable="false"/>
		<item type="text"	id="cust_fund_amt"		name="资金余额" editable="false"/>
		<item type="text"	id="asset_value"		name="证券市值" editable="false"/>
		<item type="hidden" id="match_one_amt"		name="排名第一的证券成交额" editable="false"/>
		<item type="text"	id="one_a_stkcode"		name="交易量最大证券" editable="false"/>
		<item type="text"	id="two_a_stkcode"		name="交易量第二证券" editable="false"/>
		<item type="text"	id="three_a_stkcode"	name="交易量第三证券" editable="false"/>
		<item type="text"	id="four_a_stkcode"		name="交易量第四证券" editable="false"/>
		<item type="text"	id="five_a_stkcode"		name="交易量第五证券" editable="false"/>
		</group>
	   <group name="信用评估">    
		<item type="text" id="valuation_score"		name="评估总分"	editable="false"/>
		<item type="text" id="credit_level_name"	name="信用级别"	editable="false"/>
		<item type="text" id="credit_num"			name="信用上限"	editable="false"/>
		</group> 
		<group name="合约参数">
		<item type="hidden" id="credit_control_mode"		name="信用控制模式" editable="false"/>
		<item type="hidden" id="credit_control_mode_name"	name="信用控制模式" editable="false"/>
		<item type="text"	id="return_mode_name"			name="还款模式" editable="false"/>
		<item type="hidden" id="pc_mode"					name="平仓顺序" editable="false"/>
		<item type="text"	id="pc_mode_name"				name="平仓顺序" editable="false"/>
		<item type="hidden" id="borrow_stock_pg"			name="融券复权模式" editable="false"/>
		<item type="text"	id="borrow_stock_pg_name"		name="融券复权模式" editable="false"/>
		<item type="hidden" id="borrow_stock_hg"			name="融券红股模式" editable="false"/>
		<item type="hidden" id="borrow_stock_hg_name"		name="融券红股模式" editable="false"/>
		<item type="hidden" id="borrow_stock_tspc"			name="融券退市平仓日期" editable="false"/>
		<item type="text"	id="borrow_stock_tspc_name"		name="融券退市平仓日期" editable="false"/>
		</group>
		<group name="资产评估" scope="group">
			<item type="hidden" id="cust_id"			name="客户id" action="30631"/>
			<item type="hidden" id="if_allow"			name="是否允许"/>
			<item type="hidden" id="if_allow_name"		name="是否允许" editable="false"/>
			<item type="hidden" id="if_blacklist"		name="是否黑名单"/>
			<item type="hidden" id="if_blacklist_name"	name="是否黑名单" editable="false"/>
			<item type="text"	id="total_asset"		name="总资产" editable="false"/>
			<item type="text"	id="fund_amt"			name="资金余额" editable="false"/>
			<item type="text"	id="stock_market_value"	name="证券市值" editable="false"/>
			<item type="text"	id="stock_convert_amt"	name="证券折算金额" editable="false"/>
			<item type="hidden"	id="fund_convert_amt"	name="资金折算金额" editable="false"/>
			<item type="text"	id="gage_value"			name="担保品折算价值" editable="false"/>
			<item type="hidden" id="usable_num"			name="评估信用上限" editable="false"/>
			<item type="hidden" id="credit_num"			name="信用上限" editable="false"/>
			<item type="text"	id="total_debt"			name="总负债" editable="false"/>
			<item type="text"	id="financing_amt"		name="实际融资总额" editable="false"/>
			<item type="text"	id="borrow_stock_amt"	name="实际融券总额" editable="false"/>
			<item type="text"	id="fee_all"			name="总费用" editable="false"/>
			<item type="text"	id="guan_rate"			name="维持担保比例" editable="false"/>
		</group>
		<group name="客户联系方式">
			<item type="text"	id="move_tel"		name="移动电话"		editable="false"/>
			<item type="text"	id="fix_tel"		name="固定电话"		editable="false"/>
			<item type="text"	id="meet_tel"		name="应急电话"		editable="false"/>
			<item type="text"	id="family_address"	name="联系地址"		editable="false"/>
			<item type="text"	id="ps_code"		name="邮政编码"		editable="false"/>
			<item type="text"	id="fax"			name="传真"			editable="false"/>
			<item type="text"	id="email"			name="电子邮件"		editable="false"/>
			<item type="text"	id="find_mode_name"	name="主联系方式"	editable="false"/>
			<item type="hidden"	id="find_mode"		name="主联系方式"	editable="false"/>
		</group>
		<group name="备注说明">
			<item type="textarea" id="manage_opinion"	name="处理意见"	editable="false"/>
			<item type="textarea" id="approver_opinion"	name="审核意见"	editable="false"/>
			<item type="textarea" id="assessor_opinion"	name="审批意见"	editable="false"/>
			<item type="textarea" id="remark"			name="备注说明"	editable="false"/>
		</group>
	</input> 
	<output/>
</function>