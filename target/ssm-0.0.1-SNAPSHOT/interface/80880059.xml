<?xml version="1.0" encoding="gbk" ?>
<function id="80880059" name="行权融资合同签署申请" >
  <input>
  	<item type="hidden" id="cust_id" name="客户id" action="70001888,70010430,97600018"/>	
    <group name="客户基本信息">
		<item type="text"   id="cust_name" 	name="客户姓名" editable="false"/>
		<item type="text"   id="full_name" 	name="客户全称" editable="false"/>
		<item type="text"   id="cust_code" 	name="客户代码" editable="false"/>
		<item type="text"   id="cust_type_name"		name="客户类型" editable="false"/>
		<item type="text"	id ="institution_name"	name="营业部名称" editable="false"/>
		<item type="text"   id="cert_type" 	name="证件类型" editable="false"/>
		<item type="text"   id="cert_no"   	name="证件号码" editable="false"/>
		<item type="text"   id="cert_start_date"   	name="证件生效日期" editable="false"/>
		<item type="text"   id="cert_end_date"   	name="证件失效日期" editable="false"/>
		<item type="text"   id="bond_no"    name="合同编号" editable="false"/>
		<item type="select" id="currency"   name="货币类型" source="018" editable="false" nullable="false" default="01"/>
		<item type="text"   id="app_years" 	name="申请年数" integer="true" nullable="false" default="99"/>
		<item type="date"   id="over_time" 	name="客户协议签署日期" nullable="false"/>
		<item type="hidden" id="date"       name="打印日期"/>
		<item type="hidden" id="user_code"  name="打印柜员代码"/>
		<item type="hidden" id="user_name"  name="打印柜员名称"/>
		<item type="hidden" id="company"    name="券商名称"/>
		<item type="hidden" id="remark1"    name="备注说明"/>
		<item type="text"   id="agent_name" 	name="代理人姓名" editable="false"/>
		<item type="text"   id="agent_cert_type" 	name="代理人证件类型" editable="false"/>
		<item type="text"   id="agent_cert_no"   	name="代理人证件号码" editable="false"/>
		<item type="text"   id="legal_man_name"   	name="法定代表人名称" editable="false"/>
		<item type="text" 	id="move_tel" name="移动电话"  editable="false"/>
		<item type="text" 	id="fix_tel" name="固定电话" editable="false"/>
		<item type="text" 	id="family_address" name="联系地址"  editable="false"/>
		<item type="text"   id="email" name="电子邮件" editable="false"/>
		<item type="text" 	id="ps_code" name="邮政编码"  editable="false"/>	
		<item type="text" 	id="fax" name="传    真" editable="false"/>	
        <item type="select" id="bond_type" name="合同申请类型" source="97600112" default="145" editable="false"/>  <!--默认选择行权融资合同-->
    </group>
    <group name="融出方信息">
		<item type="select" id="lender_id" name="融出方" source="98800003" nullable="false" action="60010101"/>
		<item type="text" id="lender_code" name="融出方代码" editable="false" maxlen="6"/>
		<item type="text" id="lender_name" name="融出方名称" editable="false" maxlen="100"/>
		<item type="select" id="lender_type" name="融出方类别" source="800" editable="false"/>
		<item type="select" id="if_valid" name="是否有效" source="802" editable="false" default="1"/>
		<item type="date" id="found_date" name="设立日期" editable="false"/>
    </group>     
    <group name="自然情况">
		<item type="grid" 	id="cust_id" source="70030422" name="自然情况"/>		
	</group>
	<group name="备    注">
		<item type="textarea" id="remark" name="备注说明" />
	</group>
  </input>
  <service type="procedure" id="p60010415" process="833" style="1">
	<param type="integer" 	id="user_id" way="in"/>
	<param type="string" 	id="order_no"  way="in"/>
	<param type="string" 	id="bond_no"  way="in"/>
	<param type="string" 	id="currency" way="in"/>
	<param type="integer" 	id="cust_id" way="in"/>
	<param type="string" 	id="remark" way="in"/>
	<param type="integer" 	id="app_years" way="in"/>
	<param type="integer" 	id="lender_id" way="in"/>
	<param type="string" 	id="over_time" way="in"/>
    <param type="string"    id="bond_type" way="in"/>
  </service>
  <output/>
</function>