<?xml version="1.0" encoding="gbk" ?>
<function id="30503" name="基准利率设置" >	
  <input> 
		<item type="hidden"	id="basis_interest_id"		 name="基准利率id" />
		<item type="text"	id="basis_interest_name"	 name="基准利率名称" />		
  </input>
  <service type="procedure" id="p_base_interest_brow" implement="p_base_interest_brow30503"  process="911" style="77"> 
		<param id="user_id"				type="integer" way="in"/>
		<param id="basis_interest_id"	type="integer" way="in"/>
		<param id="basis_interest_name"	type="string"  way="in"/>	
  </service>
  <output	autoload="true">
		<column type="number" id="basis_interest_id"	 name="基准利率id"	display="false"/>
		<column type="string" id="basis_interest_name"	 name="基准利率名称"/>
		<column type="number" id="basis_interest_rate"	 name="基准利率(年)(%)" precision="4"/>
		<button type="add"    id="add"					 name="新增" action="30504" multiple="-1"/>	
		<button type="edit"   id="edit"					 name="修改" action="30504" multiple="0"/>
  </output>		
</function>