			<column type="number" id="THIS_DAYS_BS_BUY"	            name="本期买券还券交易量" precision="2" />
			<column type="number" id="THIS_DAYS_GUA_TRADE"			name="本期担保品买卖交易量"	  precision="2" 	 />
			<column type="number" id="THIS_DAYS_TRADE_AMT"			name="本期融资融券总交易量"	  precision="2" 	 />
			<column type="number" id="THIS_DAYS_TRADE_AMT_RATE"	    name="本期融资融券总交易量公司占比" precision="6" />
		</group>
		<group name="本月占比">
			<column type="number" id="THIS_MONT_FIN_BUY"			name="本月融资买入交易量"	  precision="2" 	 />
			<column type="number" id="THIS_MONT_FIN_SELL"	        name="本月卖券还款交易量" precision="2" />
