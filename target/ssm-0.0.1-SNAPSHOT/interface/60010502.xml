	　　<group name="分段计息区间">
			<item type="grid" id="sbond_id" source="60660600" name="分段计息区间"/>	
		</group>
		<group name="新增违约申请">
			<item type="hidden" id ="sbond_id"	name="sbond_id"/>
			<item type="select" id="violate_type" name="违约类型" source="831" nullable="false"/>
			<item type="select" id="penalty_fee_type" name="违约金收取方式" source="1000" nullable="false" default="0"/>
			<item type="date" id="begin_date" name="违约起始日期" nullable="false"/>
			<item type="date" id="end_date" name="违约结束日期"	nullable="false"/>
			<item type="textarea" id ="remark1" name="违约备注" />
		</group>
