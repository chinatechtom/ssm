<?xml version="1.0" encoding="gbk" ?>
<function id="36110030" name="本月对单个客户融资融券规模与净资本比例居前五名及超过5%的客户情况" >
<input>
  <item type="select"  id="institution_id"  name="营业部名称" source="95000008"/> 
  <item type="text"   id="cust_num"         name="客户数量" default="5"/>
  <item type="text"   id="rate"             name="占净资本比例" default="0.05"/>
  <item type="year"  id="year"  name="年份" nullable="false"/>
  <item type="month"  id="month"  name="月份" nullable="false"/>
</input>
<service type="procedure" id="P36110030" process="911" style="77">
  <param id="user_id" type="integer"/>
  <param id="institution_id" type="integer"/>
  <param id="cust_num" type="double"/>
  <param id="rate" type="string"/>
  <param id="year"           type="integer"/>
  <param id="month"          type="integer"/>
</service>
<output export="true">
  <column type="number" id="institution_id" name="序号" display="false"/>
  <column type="string" id="institution_name" name="营业部"/>
  <column type="string" id="cust_num"       name="客户数量" />  
  <column type="number" id="rate"           name="占净资本比例" precision="6"/>
  <column type="string" id="year"           name="年份选择"  display="false"/>
  <column type="string" id="month"          name="月份选择"  display="false"/>
  <column type="string" id="print_date"   name="时间"  display="false"/>
  <button type="print" id="print" name="打印回单" action="36111030" multiple="1"/>
</output>
</function>