<?xml version="1.0" encoding="gbk"?>
<function id="3060168" name="征信任务明细查询">
	<input>
		<item type="text" id="cust_code" name="客户代码"/>
		<item type="text" id="task_id"   name="任务编号"/>
	</input>
	<service type="procedure" id="p_cust_zx_task_query" implement="p_cust_zx_task_query3060168" process="911" style="77">
		<param id="user_id"   type="integer" way="in"/>
		<param id="cust_code" type="string"  way="in"/>
		<param id="task_id"   type="integer" way="in"/>
	</service>
	<output>
		<column type="number" id="task_id" 			name="任务编号"/>
		<column type="string" id="cust_code" 		name="任务代码"/>
		<column type="string" id="begin_date" 		name="征信起始日期"/>
		<column type="string" id="end_date"  		name="征信结束日期"/>
		<column type="string" id="evaluate_date" 	name="评定日期"/>
		<column type="string" id="business_type" 	name="业务类别"/>
		<column type="string" id="app_num" 			name="申请额度"/>
		<column type="string" id="profit_clt_rate" 	name="亏损控制比率%"/>
		<column type="string" id="remark"  			name="备注"/>
	</output>
</function>