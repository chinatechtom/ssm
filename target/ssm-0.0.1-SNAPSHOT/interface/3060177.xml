<?xml version="1.0" encoding="gbk" ?>
<function id="3060177" name="客户其他资料录入查询" >
	<input>
		<item type="text"	id="cust_code"			name="客户代码"/>
		<item type="text"	id="commness_cust_code"	name="普通资金账号"/>
		<item type="text"	id="cust_name"			name="客户姓名"/>
		<item type="text"	id="cert_no"			name="证件号码"/>
	</input>
	<service type="procedure" id="p_cust_other_info_query" implement="p_cust_other_info_query3060177" process="911" style="77">
		<param id="user_id"			   			type="integer" way="in"/>
		<param id="cust_code"			   		type="string" way="in"/> 
		<param id="commness_cust_code"			type="string" way="in"/> 
		<param id="cust_name"			   		type="string" way="in"/> 
		<param id="cert_no"			   			type="string" way="in"/> 
	</service> 
	<output>
		<column id="cust_id"				type="number" name="客户ID" display="false"/>
		<column id="cust_code"				type="string" name="客户代码"/>
		<column id="cust_name"				type="string" name="客户姓名"/>
		<column id="commness_cust_code" 	type="string" name="普通资金账号"/>
		<column id="cert_no"				type="string" name="证件号码"/>
		<column id="cust_type_flag"			type="string" name="客户类型" display="false"/>
		<column id="cust_type_name"			type="string" name="客户类型"/>
		<column id="total_commness_asset"	type="number" name="普通账户总资产"/>
		<column id="already_open_month"		type="number" name="投资经验"/>
		<column id="age"					type="number" name="年龄"/>
		<column id="warning_level"			type="string" name="报警级别"	level="true" display="false"/>
		<button id="select"		type="select" name="其他资料录入" action="3060179" multiple="0"/>
	</output>
</function>