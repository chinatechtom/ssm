<?xml version="1.0" encoding="gbk" ?>
<function id="37526" name="保证金比例调整申请中评价项目详情" >	
  <input> 
		<item type="hidden"	id="cust_id"					name="客户id" />
		<item type="hidden"	id="order_id"					name="合约id" />
		<item type="hidden"	id="cust_code"					name="客户代码"/>	
  </input>
  <service type="procedure" id="p_grade_factor_query"  implement="p_grade_factor_query37526" process="911" style="77"> 
		<param id="user_id"						type="integer"/>		
		<param id="cust_id"						type="integer"/>
		<param id="order_id"					type="integer"/>
  </service>
  <output>
		<column type="number" id="abs_profit_rate"			name="绝对收益率(%)"				precision="2"/>
		<column type="number" id="abs_profit_score"			name="绝对收益率得分"				precision="0"/>
		<column type="number" id="hs300_year_profit_rate"	name="同期沪深300年化收益率(%)"	precision="2"/>
		<column type="number" id="hs300_year_profit_score"	name="同期沪深300年化收益率得分"	precision="0"/>
		<column type="number" id="cp_profit_rate"			name="相对收益率(%)"				precision="2"/>
		<column type="number" id="cp_profit_score"			name="相对收益率得分"				precision="0"/>
		<column type="number" id="cur_asset"				name="净资产(万元)"					precision="2"/>
		<column type="number" id="cur_asset_score"			name="净资产得分"					precision="0"/>
		<column type="number" id="rzrq_commission"			name="信用交易息费收入(万元)"		precision="2"/>
		<column type="number" id="rzrq_commission_score"	name="信用交易息费收入得分"		precision="0"/>
		<column type="number" id="rzrq_fee"					name="信用交易息费收入(万元)"		precision="2"/>
		<column type="number" id="rzrq_fee_score"			name="信用交易息费收入得分"		precision="0"/>
		<column type="number" id="add_score"				name="投资者属性额外加分项"		precision="0"/>	
		<column type="number" id="total_score"					name="综合得分"		precision="2"/>
		<column type="number" id="lower_guarantee_rate"			name="最低融资/融券保证金比例"		precision="2"/>
		<column type="number" id="suggest_lower_guarantee_rate"	name="建议融资/融券保证金比例"		precision="2"/>	
		<column type="number" id="adjuest_value"				name="调整值"		precision="2"/>	
  </output>		
</function>