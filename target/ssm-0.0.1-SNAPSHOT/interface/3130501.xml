<?xml version="1.0" encoding="gbk" ?>
<function id="3130501" name="词典分类查询" >
	<input>
		<item type="hidden"	id="systemid"		name="系统id"/>
		<item type="text"	id="coding_sort"	name="词典分类代码"/>
		<item type="text"	id="coding_name"	name="词典分类名称"/>
	</input>
	<service type="procedure" id="p_dictionary_sort_query" implement="p_dictionary_sort_query3130501"  process="911" style="77">
		<param type="integer"	id="user_id"		 way="in"/>
		<param type="integer"	id="systemid"		 way="in"/>
		<param type="string"	id="coding_sort"	 way="in"/>
		<param type="string"	id="coding_name"	 way="in"/>
	</service>
	<output autoload="true">
		<column type="number"	id="systemid"		name="系统id" display="false"/>
		<column type="string"	id="coding_sort"	name="词典分类代码"/>
		<column type="string"	id="coding_name"	name="词典分类名称"/>
		<column type="string"	id="dick_type"		name="词典类型"/>
		<button type="add"		id="add"			name="增加词典分类" action="3130502" multiple="-1"/>
		<button type="set"		id="update"			name="修改词典分类" action="3130503" multiple="0"/>
	</output>
	<assistant action="3130601">		
		<!--<link src="systemid"	target="systemid"/>-->
		<link src="coding_sort" target="coding_sort"/>
		<!--<link src="coding_name" target="coding_name"/>-->
    </assistant>
</function>