<?xml version="1.0" encoding="gbk"?>
<function id="3231102" name="杂项修改" >  
  <input>
		<item type="text"	 id="paraid" 			name="参数符号"	editable="false"/>
		<item type="text"	 id="paraname" 			name="参数名字"	editable="false"/>
		<item type="text" 	id="paravalue" 			name="参数值"/>
		<item type="hidden" id="datatype" 			name="参数数据类型"/>
		<item type="hidden" id="datalen" 			name="数据长度"/>
		<item type="hidden" id="declen"	 			name="小数点长度"/>
		<item type="hidden" id="charset" 			name="允许取值字符集"/>
		<item type="hidden" id="ctrltype" 			name="控件类型"/>
		<item type="hidden" id="Show" 				name="控件中显示内容"/>
		<item type="hidden" id="biztype" 			name="业务大类"/>
		<item type="hidden" id="paralevel" 			name="参数分类"/>
		<item type="hidden" id="status" 			name="参数状态"/>
		<item type="text"  	id="remark" 			name="备注信息"	editable="false"/>
		<item type="hidden" id="func_id" 			action="8801" 			default="940000002"/>
		<item type="hidden" id="func_need_check" 	name="是否支持复核" 	default="0"/>		
		<group name="复核">   
			<item type="text" 		id="chk_user_code" 	name="复核人员" filter="func_need_check=1"/>
			<item type="password"   id="chkuser_pwd" 	name="密码" 	filter="func_need_check=1"/>
		</group>
  </input>
  <service type="procedure" id="p_mixedconfig_set" implement="p_mixedconfig_set3231102" process="811" style="1">
		<param id="user_id" 		type="integer" way="in"/>
		<param id="paraid" 			type="string"  way="in"/>
		<param id="paraname" 		type="string"  way="in"/>
		<param id="paravalue" 		type="string"  way="in"/>
		<param id="datatype" 		type="string"  way="in"/>
		<param id="datalen" 		type="integer" way="in"/>
		<param id="declen" 			type="integer" way="in"/>
		<param id="charset" 		type="string"  way="in"/>
		<param id="ctrltype" 		type="string"  way="in"/>
		<param id="Show" 			type="string"  way="in"/>
		<param id="biztype" 		type="string"  way="in"/>
		<param id="paralevel" 		type="string"  way="in"/>
		<param id="status"			type="string"  way="in"/>
		<param id="remark"			type="string"  way="in"/>
		<param id="chk_user_code"	type="string"  way="in"/>
		<param id="chkuser_pwd"	  	type="string"  way="in"/>
		<param id="func_id"			type="integer" way="in"/>
		<param id="oper_type"		type="string"  way="in" default="2"/>
  </service>   
  <output/>
</function>