<?xml version="1.0" encoding="gbk" ?>
<function id="30800" name="客户历史资金查询" >
	<input export="true">
	    <item type="select"  id="statistics_type"	name="查询方式" source="515" nullable="false"/>
		<item type="select"  id="institution_id"	name="营业部名称" source="95000008"/>
		<item type="hidden"  id="cust_id"			name="客户id"/>		
		<item type="text"    id="cust_code"			name="客户代码"/>
		<item type="hidden"  id="acc_id"			name="账户id"/>
		<item type="hidden"  id="acc_code"	        name="资金账户"/>	
		<item type="date"    id="date"				name="日期" default="last" nullable="false"/>	
		<group name="分页查询" page="true">		
			<item type="text"   id="page_num"       name="页码"     integer="true" default="1"/>
			<item type="select" id="rows_num"       name="每页行数" integer="true" source="95000012" nullable="false"/>
		</group>		
	</input>
    <service type="procedure" id="p_cust_acc_money_query_his"   process="955" style="77">
		<param id="user_id"			type="integer" way="in"/> 
		<param id="statistics_type"	type="integer" way="in"/> 
		<param id="page_num"        type="integer" way="in"/>
		<param id="rows_num"        type="integer" way="in"/>		
		<param id="institution_id"	type="integer" way="in"/> 
		<param id="cust_id"			type="integer" way="in"/> 
		<param id="cust_code"		type="string"  way="in"/> 
		<param id="acc_id"			type="integer" way="in"/> 
		<param id="acc_code"		type="string"  way="in"/> 
		<param id="date"	        type="string"  way="in"/>	
    </service>
    <output export="true">
		<column type="string"  id="institution_name"		name="营业部名称"/>
		<column type="string"  id="cust_code"				name="客户代码"/>
		<column type="string"  id="cust_name"				name="客户姓名"/>
		<column type="number"  id="acc_id"					name="账户id"	display="false"/>        
		<column type="string"  id="acc_code"				name="资金账户" display="false"/>        
		<column type="string"  id="date"				    name="日期"		/>                    
		<column type="string"  id="property_type"			name="资产种类"  display="false"/>                     
		<column type="string"  id="currency_type"			name="货币类型"	/>                
		<column type="number"  id="fund_amt"   				name="资金余额"	  precision="2" total="true"/>  
		<!--<column type="string"  id="big_chinese"				name="资金余额中文大写"	/>-->
		<column type="number"  id="modify_fund_amt"   name="修正资金余额"	  precision="2" total="true"/>      
		<column type="number"  id="buy_frozen_amt"			name="买入冻结金额" precision="2" total="true" display="false"/>
		<column type="number"  id="frozen_amt"				name="冻结资金"	  display="false"/>
		<column type="number"  id="abnormal_frozen_amt"		name="异常冻结资金" precision="2" total="true" display="false"/> 
		<column type="number"  id="deposit"					name="当日存入"	  precision="2" total="true"/>             	
		<column type="number"  id="withdraw"				name="当日取出"	  precision="2" total="true"/> 
		<column type="number"  id="real_buy_amt"		    name="买入成交金额"	precision="2" total="true"/>   
		<column type="number"  id="sell_done_amt"			name="卖出解冻金额" precision="2" total="true"/>     
		<column type="number"  id="manual_frzn_amt"			name="手工冻结金额" display="false"/>
		<column type="number"  id="manual_unfrzn_amt"		name="手工解冻金额" display="false"/>
		<column type="number"  id="amend_amt"		    name="修正资金" precision="2" total="true"/>
		<column type="number"  id="credit"					name="信用额"	  display="false"/>
		<column type="number"  id="forbid_amt"				name="禁用资金"	  display="false"/>
		<column type="number"  id="forbid_asset"			name="禁取资产"	  display="false"/>
		<column type="number"  id="interest"				name="利息"		  display="false"/>
		<column type="number"  id="interest_count"			name="利息积数"   display="false"/>
		<column type="number"  id="punish_interest"			name="罚息"		  display="false"/>
		<column type="number"  id="punish_interest_count"   name="罚息积数"	  display="false"/>
		<column type="number"  id="crdt_sell_frzn_amt"			name="融券卖出冻结金额"		precision="2" total="true"/>
		<column type="number"  id="gua_fund_amt"            name="货币保证金余额"	    precision="2" total="true"/>				
    </output>
</function>
