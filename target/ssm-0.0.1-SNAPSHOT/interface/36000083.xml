<?xml version="1.0" encoding="gbk" ?>
<function id="36000083" name="8.2.4标的证券变动统计" >  
  <input>
   <item type="date"   id="begin_date"         name="起始日期"  default="last"/>
  <item type="date"   id="end_date"            name="结束日期"  default="now"/>
  <item type="text"   id="product_code"        name="证券代码"  />
  </input>
   <service type="procedure" id="P36000083" process="911" style="77">
   <param id="user_id" type="integer" way="in" /> 
   <param id="begin_date"    type="string" way="in" />
   <param id="end_date"    type="string" way="in" />
   <param id="product_code"    type="string" way="in" />
  </service> 
  <output  print="true">
    <column type="number" id="id"                 name="序号"/>
    <column type="string" id="product_code"       name="证券代码" /> 
    <column type="string" id="product_name"       name="证券名称" />  
   <column type="string"  id="bd_style"           name="标的类型" />
   <column type="string"  id="in_out"             name="调入调出" />
    <column type="string" id="date"               name="调整日期" /> 
   <column type="string"  id="change_reason"      name="变动原因" />
   <column type="string"  id="note"               name="备注" display="false"/>
  </output>
</function>