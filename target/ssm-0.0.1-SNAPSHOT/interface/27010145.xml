<?xml version="1.0" encoding="gbk" ?>
<function id="27010145" name="融券券源调拨申请表" >
     <input>     
      <item type="date"    id="date"             name="日期*"  />    
      <item type="text"    id="day_num"             name="天数(n)*"  default="3" />  
     </input>
  <service type="procedure" id="P27010145"  process="911" style="77">
      <param id="user_id"               type="integer" way="in"/> 
      <param id="date"             type="string"  way="in"/> 
       <param id="day_num"             type="integer"  way="in"/>            
   </service>
 <output print="true">
        <column type="string" id="date"   name="日期" display="false"/>
        <column type="string" id="day_num"   name="天数" display="false"/>
        <group name="T日提交的融券申请额度">
        <column type="string" id="product_name" name="证券名称" />        
        <column type="string" id="product_code"   name="证券代码" />
        <column type="number" id="product_num"   name="证券数量" />        
        </group>
                <group name="T+n日到期的融券合约额度">
        <column type="string" id="product_name_over" name="证券名称" />        
        <column type="string" id="product_code_over"   name="证券代码" />
        <column type="number" id="product_num_over"   name="证券数量" />        
        </group>
        <group name="T+n日拟申请的融券额度">
        <column type="string" id="product_name_bond" name="证券名称" />        
        <column type="string" id="product_code_bond"   name="证券代码" />
        <column type="number" id="product_num_bond"   name="证券数量" />        
        </group>     
</output>
</function>  