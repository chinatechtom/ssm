<?xml version="1.0" encoding="gbk" ?>
<function id="31393" name="清算日志查询" >
	<input>
		<item type="date"    id="date"               name="交易日期"        nullable="false"/>
		<item type="select"  id="bak_restore_point"  name="备份还原时间点"  source="516"/>
	</input>
	<service type="procedure" id="p_backup_restore_log_query" process="911" style="77">
		<param id="user_id"  type="integer"/>
		<param id="date"  type="string"/>
		<param id="bak_restore_point"  type="string"/>
	</service>
	<output export="true">
		<column type="string" id="sys_date"		name="交易日期"/>
		<column type="string" id="table_name"	name="处理表名"/>
		<column type="string" id="bak_restore_point"	name="操作时间点"/>
		<column type="string" id="backup_flag"	name="备份标志"/>
		<column type="string" id="restore_flag"	name="还原标志"/>
		<column type="string" id="begin_time"	name="开始时间"/>
		<column type="string" id="end_time"		name="结束时间"/>
		<column type="string" id="remark"		name="备注"/>
        <column type="string" id="user_code"	name="操作员代码"/>
		<column type="string" id="user_name"	name="操作员名称"/>
	</output>
</function>