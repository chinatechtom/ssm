<?xml version="1.0" encoding="gbk" ?>
<function id="60370602" name="分段计息区间浏览" >	
  <input> 
		<item type="hidden"	id="order_id" name="合约id" />
		<item type="hidden"	id="section_id" name="分段计息区间id" />
  </input>
  <service type="procedure" id="P60370602" process="977" style="77"  adapter="psr">
	    <param id="user_id"	type="integer"/>		
		<param id="order_id" type="integer"/>	
	    <param id="section_id" type="integer"/>
  </service>
  <output>
		<column type="number" field="section_id"        id="section_id"        name="区间ID"   display="false"/>
		<column type="string" field="section_name"      id="section_name"      name="区段"     display="false"/>
		<column type="string" field="begin_date"        id="section_begin_date"        name="计息起始日期"/>
		<column type="string" field="end_date"          id="section_end_date"          name="计息截至日期"/>
		<column type="number" field="days" 		        id="section_days" 		      name="区间天数"/>
		<column type="number" field="hg_rate"	        id="section_hg_rate"	      name="购回价格(每100元)"	 precision="4"/>
		<column type="number" field="interest_count"    id="section_interest_count"    name="计息积数" precision="2"/>
		<column type="number" field="interest"		    id="section_interest"		  name="未结利息"  precision="2"/>
		<column type="number" field="received_interest" id="section_received_interest" name="已收利息" display="false"/>
		<column type="string" field="remarks"           id="section_remarks"           name="备注"/>
  </output>		
</function>