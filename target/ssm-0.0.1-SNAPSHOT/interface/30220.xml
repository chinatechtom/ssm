<?xml version="1.0" encoding="gbk" ?>
<function id="30220" name="融资明细历史查询" >	
  <input>	
	<item type="hidden" id="in_cust_id" name="客户id"/>
	<item type="text"   id="in_cust_code"    name="客户代码" />
	<item type="text"   id="in_product_code" name="证券代码" />	
	<item type="date"   id="in_date" name="日期"  nullable="false" default="now"/>
  </input>  
  <service type="procedure" id="p_report_financing_his_query" process="911" style="77"> 
  <param id="user_id" type="integer" />
	<param id="in_cust_id" type="integer"/>
	<param id="in_cust_code"     type="string"	way="in"/>
	<param id="in_product_code"  type="string"  way="in"/>	
	<param id="in_date" type="string" way="in"/>
  </service>  
  <output export="true">  	
	<column type="string"  id="cust_code"                  name="客户代码"/>
	<column type="string"  id="cust_name"                  name="客户姓名"/>
	<column type="string"  id="product_code"               name="证券代码"/>    
	<column type="number"  id="yes_financing_amt"          name="前日融资余额"		precision="3"/>
	<column type="number"  id="today_financing_amt"        name="本日融资余额"		precision="3"/>
	<column type="number"  id="today_buy_amt"              name="本日融资买入额"		precision="3"/>
	<column type="number"  id="today_direct_return_amt"    name="本日融资直接还款额"	precision="3"/>	
	<column type="number"  id="today_retutn_amt"           name="本日融资偿还额"		precision="3"/>	
	<column type="number"  id="today_direct_return_amt"    name="本日融资直接还款额"	precision="3"/>
	<column type="number"  id="today_sell_return_amt"      name="本日融资卖券还款额"	precision="3"/>
	<column type="number"  id="today_pc_amt"               name="本日融资强制平仓额"	precision="3"/>
	<column type="string"  id="date"                       name="交易日期"/>
  </output>
</function>