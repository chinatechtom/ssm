<?xml version="1.0" encoding="gbk" ?>
<function id="30643" name="查看合同-担保品" >
  <input>
	<item type="hidden" id="bond_id" name="合同id"/>
  </input>
  <service type="procedure" id="p_bond_guarantee_query" process="911" style="77">
	<param type="integer" id="user_id"/>	
	<param type="integer" id="bond_id"/>
  </service>
  <output>    
    <column type="number" id="bond_id" name="合同id"   display="false"/>
    <column type="number" id="fund_amt" name="资金余额"  precision="2" />  
	<column type="number" id="stock_market_value" name="证券市值"  precision="2" />  
	<column type="number" id="stock_convert_amt" name="证券折算金额"  precision="2" /> 
	<column type="number" id="fund_convert_amt" name="资金折算金额"  precision="2" /> 
	<column type="number" id="gage_value" name="担保品价值"  precision="2" /> 
	<column type="number" id="use_num" name="已用额度"  precision="2" /> 
	<column type="number" id="frozen_num" name="冻结额度"  precision="2" /> 
	<column type="number" id="usable_num" name="可用额度"  precision="2" /> 
	<column type="number" id="restrict_amt" name="禁取资金"  precision="2" /> 
	<column type="number" id="most_amt" name="最大取款"  precision="2" /> 
  </output> 
</function>