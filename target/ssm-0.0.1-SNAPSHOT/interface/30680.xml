<?xml version="1.0" encoding="gbk" ?>
<function id="30680" name="查询融资申请" >
  <input>
		<item type="text" id="order_id" name="合同id"/>
  </input>
  <service type="procedure" id="p_order_fix_qty_query" process="911" style="77">
		<param type="integer" id="user_id"/>	
		<param type="integer" id="order_id"/>	
  </service>
  <output> 
  	<column type="number" id="institution_id" name="营业部ID" display="false"/>
  	<column type="string" id="institution_name" name="营业部名称" display="false"/>   
    <column type="number" id="order_id" name="合同id" display="false"/>
    <column type="number" id="order_fix_qty" name="融资数额" precision="2"/>
    <column type="string" id="big_fix_qty" name="中文大写融资数额"/>
    <column type="string" id="small_fix_qty" name="中文小写融资数额"/>
    <column type="number" id="order_fix_qty_all" name="总融资上限" precision="2"/>
    <column type="string" id="big_fix_qty_all" name="中文大写总融资数额"/>
    <column type="string" id="small_fix_qty_all" name="中文小写总融资数额"/>   
    <column type="number" id="bond_fix_qty"    name="原合同融资上限" precision="2" display="false"/>
	<column type="string" id="begin_time_origin" name="原合同融资起始时间" display="false"/>   
	<column type="string" id="return_time_oringin" name="原合同融资结束时间" display="false"/>   
	<column type="string" id="order_model_origin" name="原合同融资信用模式" display="false"/>   
    <column type="string" id="big_fix_qty_bond" name="中文大写合同融资" display="false"/>
    <column type="string" id="small_fix_qty_bond" name="中文小写合同融资" display="false"/>	
	<column type="string" id="order_model" name="模式" display="false"/> 
	<column type="string" id="order_model_name" name="信用使用模式"/> 
	<column type="string" id="order_begin_time" name="起始日期"/>
	<column type="string" id="order_return_time"  name="结束日期"/>
	<column type="string" id="begin_time11"   name="起始日期" display="false"/>
	<column type="string" id="return_time11"  name="结束日期" display="false"/>
	<column type="number" id="AVAILABILITY_DAY_NUM"  name="有效时间"/>
	<column type="string" id="application_time"  name="申请时间" />	
	<column type="number" id="cust_factor"  name="客户融资费率因子"/>	<!--add by zsj 20090216-->
	<column type="number" id="product_id"	name="融资产品ID" display="false"/>
	<column type="string" id="product_code"	name="融资产品代码"/>
	<column type="string" id="product_name"	name="融资产品名称"/>
	<column type="number" id="exist_fix_qty"			name="已申请融资上限"/>
  </output> 
</function>