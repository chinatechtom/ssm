<?xml version="1.0" encoding="gbk" ?>
<function id="27010141" name="标的证券变动统计" >
     <input>
       <item type="text"  id="product_code"      name="证券代码" />  
       <item type="date"  id="begin_date"        name="开始日期*" default="last"/>  
       <item type="date"  id="end_date"          name="结束日期*" default="now"/>                    
     </input>
  <service type="procedure" id="P27010141"  process="911" style="77">
      <param id="user_id"       type="integer" way="in"/> 
      <param id="product_code"  type="string" way="in"/> 
      <param id="begin_date"    type="string" way="in"/>      
      <param id="end_date"      type="string" way="in"/>      
   </service>
 <output print="true">
        <column type="string" id="row_id"     name="序号"/>
        <column type="string" id="chg_date"   name="调整日期" />
      <group name="调整项目">
      	<column type="string" id="change_in"   name="调入" />
      	<column type="string" id="change_out"  name="调出" />
      	<column type="string" id="change_rate" name="折算率" />
      </group>	
        <column type="string" id="product_code" name="证券代码" />        
        <column type="string" id="product_name"   name="证券名称"  />
        <column type="number" id="convert_rate_before"   name="调整前折算率"  precision="4"/>
        <column type="number" id="convert_rate_after"    name="调整后折算率"  precision="4"/>
        <column type="string" id="gua_type" name="标的类型" />             
        <column type="string" id="chg_reason"   name="变动原因" />     
</output>
</function>  