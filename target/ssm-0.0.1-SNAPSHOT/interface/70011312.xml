<?xml version="1.0" encoding="gbk" ?>
<function id="70011312" name="网上交易业务时间限制参数查询">
	<input>
		<item type="hidden" 	id="cust_id" 		name="客户id"  />
		<item type="text" 		id="cust_code" 		name="客户代码" />
		<item type="text" 		id="cust_name" 		name="客户名称" />
		<item type="select" 	id="market_code" 	name="市场代码"  	source="039" />
		<item type="select" 	id="busi_type" 		name="业务类型"  	source="743" />
		<item type="select" 	id="limit_type" 	name="限制类型"  	source="742" />
	</input>
	<service type="procedure" id="P70011312" process="922" style="77">
		<param id="user_id"		type="integer"/>
		<param id="cust_id"		type="integer"/>
		<param id="cust_code"	type="string"/>
		<param id="cust_name"	type="string"/>
		<param id="market_code"	type="string"/>
		<param id="busi_type"	type="string"/>	
		<param id="limit_type"	type="string"/>
	</service>
	<output export="true">
		<column type="number" 	id="cust_id"			name="客户ID"	 display="false"/>
		<column type="string" 	id="cust_code"			name="客户代码" />
