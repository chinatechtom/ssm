<?xml version="1.0" encoding="gbk" ?>
<function id="30305" name="查看证券行情" >
	<input>
		<item type="hidden" id="product_id"   name="证券id" />	
		<item type="text"   id="product_code" name="证券代码"/>
		<item type="text"   id="product_name" name="证券名称"/>
    </input>
    <service type="procedure" id="p_product_price_query" implement="p_product_price_query305"  process="911" style="77"> 
		<param id="user_id"		  type="integer" way="in"/>
		<param id="product_id"    type="integer" way="in"/>
		<param id="product_code"  type="string"  way="in"/>
		<param id="product_name"  type="string"  way="in"/>
	</service>
	<output export="true">
		<column type="number" id="product_id"			  name="证券id" display="false" />
		<column type="string" id="product_code"			  name="证券代码"/>
		<column type="string" id="product_name"			  name="证券名称"/>	
    	<column type="string" id="market_code"   		name="市场代码"/>		
		<column type="number" id="yesterday_close_price"  name="昨日收盘价" precision="4"/>
		<column type="number" id="today_open_price"	      name="今日开盘" precision="4" display="false"/>
		<column type="number" id="lately_price"	          name="最近成交" precision="4" display="false"/>
		<column type="number" id="last_price"	          name="最新价" precision="4"/>
		<column type="number" id="trade_qty"			  name="成交数量" display="false"/>
		<column type="number" id="trade_amt"			  name="成交金额" display="false"/>
		<column type="number" id="top_price"			  name="涨停价格" precision="4"/>
		<column type="number" id="low_price"			  name="跌停价格" precision="4"/>
		<column type="number" id="five_sale_qty"	      name="卖盘5量"  display="false"/>
		<column type="number" id="five_sale_price"	      name="卖盘5价"	 precision="4" display="false"/>
		<column type="number" id="four_sale_qty"	      name="卖盘4量"	 display="false"/>
		<column type="number" id="four_sale_price"	      name="卖盘4价"	 precision="4" display="false"/>
		<column type="number" id="three_sale_qty"	      name="卖盘3量"	display="false"/>
		<column type="number" id="three_sale_price"	      name="卖盘3价"	 precision="4" display="false"/>
		<column type="number" id="two_sale_qty"	          name="卖盘2量"	display="false"/>
		<column type="number" id="two_sale_price"	      name="卖盘2价"	 precision="4" display="false"/>
		<column type="number" id="one_sale_qty"	          name="卖盘1量"	display="false"/>
		<column type="number" id="one_sale_price"	      name="卖盘1价"	 precision="4" display="false"/>
		<column type="number" id="one_buy_qty"	          name="买盘1量"	display="false"/>
		<column type="number" id="one_buy_price"	      name="买盘1价"	 precision="4" display="false"/>
		<column type="number" id="two_buy_qty"	          name="买盘2量"	display="false"/>
		<column type="number" id="two_buy_price"	      name="买盘2价"	 precision="4" display="false"/>
		<column type="number" id="three_buy_qty"	      name="买盘3量"	 display="false"/>
		<column type="number" id="three_buy_price"	      name="买盘3价"	 precision="4" display="false" />
		<column type="number" id="four_buy_qty"	          name="买盘4量"	display="false"/>
		<column type="number" id="four_buy_price"	      name="买盘4价"	 precision="4" display="false"/>
		<column type="number" id="five_buy_qty"	          name="买盘5量"	display="false"/>
		<column type="number" id="five_buy_price"	      name="买盘5价"	 precision="4" display="false"/>
		<button type="edit"   id="edit"                   name="修改"        action="3030501" multiple="0"/>
	</output>
</function>