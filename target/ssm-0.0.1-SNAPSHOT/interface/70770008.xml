	</service>
	<output export="true">
		<column type="number" id="institution_id" name="营业部id"/>
		<column type="string" id="institution_name" name="营业部名称"/>
		<column type="number" id="cust_id"    name="客户号"  display="false"/>
		<column type="string" id="cust_code"  name="客户代码"/>
		<column type="string" id="cust_name"  name="客户姓名"/>
		<column type="string" id="full_name"  name="客户全称"/>
		<column type="string" id="loancard_code"   name="中征码"/>
		<column type="string" id="national_tax_no"  name="国税号"/>
		<column type="string" id="cust_type"   name="客户类型"/>
		<column type="string" id="cust_type_name"   name="客户类型"/>
		<column type="string" id="cust_sort_name"  name="客户类别"/>
		<column type="string" id="cert_type_name"  name="证件类型"/>
		<column type="string" id="cert_no"    name="证件号码"/>
		<column type="string" id="org_code"   name="组织机构代码"/>
		<column type="string" id="legal_man_name" name="法定代表人姓名"/>
		<column type="string" id="legal_man_type" name="法定代表人性质"/>
