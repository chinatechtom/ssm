<?xml version="1.0" encoding="gbk" ?>
<function id="70066676" name="回购补仓资金返还申请合约详细">
	<input>
		<item type="hidden" id="order_id"     name="合约ID" action="70044570"/>
		<group name="合同信息" >
			<item type="hidden" id="cust_id" name="客户id" action="70001888"/>			
			<item type="text" id="bond_no" name="合同编号" editable="false"/>
			<item type="text" id ="institution_name" name="营业部名称" editable="false"/>
			<item type="text" id ="institution_code" name="营业部代码" editable="false"/> 
			<item type="text" id ="cust_name" name="客户姓名" editable="false"/>
			<item type="text" id ="cust_code" name="客户代码" editable="false"/> 	
		</group>      			
		<item type="hidden" id="date"     name="打印日期"/>	<!--从1888.xml获得的打印日期-->
		<item type="hidden" id="user_code"     name="打印柜员代码"/><!--从1888.xml获得的打印柜员代码-->
		<item type="hidden" id="user_name"     name="打印柜员名称"/><!--从1888.xml获得的打印柜员名称-->
		<item type="hidden" id="company"    name="券商名称"/><!--从1888.xml获得的券商名称-->			
		<group name="交易协议书基本信息">    
			<item type="hidden" id="cust_id" name="客户id"/>
			<item type="text" id="sbond_no" name="交易协议书编号" editable="false" action="70010707"/>
			<item type="text" id="related_sbond_no" name="关联(主)交易协议书编号" editable="false"/>
			<item type="text" id ="cust_code" name="客户代码" editable="false"/>	
			<item type="text" id="cust_name" name="客户姓名" editable="false"/>	
			<item type="text" id="bond_status_name" name="合同状态" editable="false"/>			
			<item type="text" id="market_code_name" name="市场代码" editable="false"/>	
			<item type="text" id="product_code" name="证券代码" editable="false"/>
			<item type="text" id="product_name" name="证券名称" editable="false"/>
			<item type="text" id="product_type" name="证券类型" editable="false"/>
			<item type="text" id="hg_num" name="交易数量" editable="false"/>	
			<item type="text" id="first_amt" name="初始交易金额" editable="false"/>	
			<item type="text" id="second_amt" name="购回交易金额" editable="false"/>	
			<item type="text" id="engage_trade_date" name="初始交易日期" editable="false"/>
			<item type="text" id="days" name="购回期限(天)" editable="false"/>
			<item type="text" id="bb_date" name="购回交易日期" editable="false"/>
			<item type="text" id="sbond_status" name="交易协议书状态" editable="false"/>
			<item type="text" id="hg_rate" name="购回价格(每100元年收益)" editable="false"/>
			<item type="text" id="manual_chg"   name="购回金额手工调整" editable="false"/>
			<item type="text" id="penalty_rate" name="违约金比率(日利率%)" editable="false"/>
			<item type="text" id="fix_rate" name="固定交易费用比例(%)" editable="false"/>
			<item type="textarea" id ="zjyt" name="资金用途" editable="false"/>
			<item type="textarea" id ="remarks" name="备注" editable="false"/>
		</group>
		<group name="逐日盯市信息">
			<item type="text" id="curr_stk_vol" name="当前证券数量" editable="false"/>
			<item type="text" id="curr_debt_amt" name="当前负债金额" editable="false"/>
			<item type="text" id="days2" name="最新购回期限(天)" editable="false"/>
			<item type="text" id="bb_date2" name="最新约定购回日期" editable="false"/>
			<item type="text" id="real_interest" name="实时借款利息" editable="false"/>
			<item type="text" id="real_penalty" name="实时违约金" editable="false"/>
			<item type="text" id="real_hl_interest" name="实时红利利息" editable="false"/>
			<item type="textarea" id="mtm_remark" name="备注" editable="false"/>
		</group>
		<group name="补仓资金返还信息">
			<item type="text" id="fund_chg_amt" name="补仓资金金额"  editable="false"/>
			<item type="textarea" id="remark" name="备注"  editable="false"/>
		</group>
		<group name="备注说明">
			<item type="textarea" id="manage_opinion"   name="处理意见" editable="false"/>
			<item type="textarea" id="approver_opinion"   name="审核意见" editable="false"/>
			<item type="textarea" id="assessor_opinion"   name="审批意见" editable="false"/>
			<item type="textarea" id="double_check_opinion"   name="复核意见" editable="false"/>
		</group>
	</input>
</function>