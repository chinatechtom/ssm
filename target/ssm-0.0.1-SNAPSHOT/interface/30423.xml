<?xml version="1.0" encoding="gbk" ?>
<function id="30423" name="客户评级因子-自然情况浏览" >	
  <input> 
		<item type="hidden"	id="cust_id"	 name="客户id" />
		<item type="text"	id="cust_code"	 name="客户编号" />
  </input>
  <service type="procedure" id="p_cust_ratingfactor_sort_query"  process="911" style="77"> 
		<param id="user_id"			type="integer"/>		
		<param id="cust_id" type="integer"/>
		<param id="cust_code"/>
		<param id="ratingfactor_type" default=",1,"/>
  </service>
  <output>
        <column type="number" id="cust_id" name="客户id"	display="false"/>
		<column type="string" id="cust_name"  name="客户姓名" 	display="false"/>
		<column type="string" id="cust_code" name="客户编号"	display="false"/>
		<column type="number" id="ratingfactor_type" name="评级因子类型"	display="false"/>
		<column type="string" id="ratingfactor_type_name"  name="评级因子类型名称"/>
		<column type="number" id="ratingfactor_sort" name="评级因子类别" display="false"/>
		<column type="string" id="ratingfactor_name" name="评级因子名称"/>
		<column type="number" id="data_code"  name="数据编码" display="false"/>
		<column type="string" id="data_content"  name="数据内容" display="false"/>
		<column type="string" id="into_data"  name="客户数据内容"/>
		<column type="number" id="score"  name="客户参考分值"  precision="2" />
		<column type="number" id="total_score" name="客户参考总分值"  precision="2" display="false"/>
			<button type="edit" id="edit" name="设 置" action="3042201" multiple="0"/>
  </output>		
</function>