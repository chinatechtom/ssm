<?xml version="1.0" encoding="gbk" ?>
<function id="70010484" name="将任务明细同步到征信" >
	<input>
		<item type="hidden"	id="task_id"            name="任务代码"		/>
		<item type="hidden"	id="task_id_out"            name="任务代码out"		/>
		<item type="hidden"	id="institution_code"   name="营业部代码"  />
 		<item type="hidden"	id="commness_cust_code" name="普通资金账号" />
		<item type="hidden"	id="begin_date"         name="征信起始日期" />
		<item type="hidden"	id="end_date"           name="征信结束日期" />
		<item type="hidden"	id="evaluate_date"      name="评定日期"	 />
		<item type="hidden"	id="app_num"            name="申请额度"	 />
		<item type="hidden"	id="profit_clt_rate"    name="亏损控制比率%" />
	</input>
	<service type="procedure" id="zx_prod.SP_ZX_PARAMS" adapter="netezza" process="844" style="1">
		<param type="string"  id="task_id_out"   />
		<param type="string"  id="institution_code" />
		<param type="string"  id="commness_cust_code" />
		<param type="string"  id="begin_date" />
		<param type="string"  id="end_date" />
		<param type="string"  id="evaluate_date" />
		<param type="string"  id="app_num" />
		<param type="string"  id="profit_clt_rate" />
		<param type="string"  id="op_user" default="krb"/>
	</service>
</function>