<?xml version="1.0" encoding="gbk" ?>
<function id="30687" name="查询合同信用产品" >
  <input>
	<item type="hidden" id="bond_id" name="合同id"/>
	<item type="text" id="bond_no" name="合同编号"/>
  </input>
  <service type="procedure" id="p_bond_creditproduct_query" process="911" style="77">
	<param type="integer" id="user_id"/>	
	<param type="integer" id="bond_id"/>	
	<param id="bond_no"/>
  </service>
  <output>    
    <column type="number" id="bond_id" name="合同id" display="false"/>
    <column type="number" id="product_id" name="产品id" display="false"/>  
	<column type="string" id="product_name" name="信用产品名称"/>
	<column type="string" id="product_sort" name="产品类别" display="false"/>
	<column type="string" id="product_sort_name" name="产品类别" display="false"/>
	<column type="string" id="product_cyc" name="产品周期"  display="false"/>  
	<column type="string" id="cyc_unit" name="周期单位" display="false"/>
	<column type="string" id="availability_time" name="有效日期" display="false"/>  
	<column type="string" id="begin_time" name="起始日期"  display="false"/>
	<column type="string" id="return_time" name="到期日期"  display="false"/>  
	<column type="string" id="over_time" name="延长日期" display="false"/>	
  </output> 
</function>