<?xml version="1.0" encoding="gbk" ?>
<function id="36000007" name="4.2.2维持担保比例最低的十家客户明细" >
 <input>
  <item type="select"  id="institution_id" name="营业部名称" source="95000008"/> 
  <item type="date"    id="date"        name="查询日期"/> 
  <item type="text"    id="cust_num"        name="客户数量"  default="10"/>
  <item type="select"    id="order_choice"        name="排序选择"  source="366"/>  
 </input>
 <service type="procedure" id="P36000007" implement="P3600000736000007" process="955" style="77">
   <param id="user_id"         type="integer" way="in"/> 
   <param id="institution_id" type="integer" way="in"/>
   <param id="date"         type="string"  way="in"/> 
   <param id="cust_num"         type="number"  way="in"/>
   <param id="order_choice"         type="string"  way="in"/>
 </service>
 <output export="true" print="true" unit="元">
    <column type="string" id="cust_name"              name="客户姓名或名称"/>
    <column type="string" id="cust_code"              name="客户代码"            display="false"/>
  <group name="期末数">
    <column type="number" id="financing_scale"       name="融资规模"             precision="2"/>
    <column type="number" id="guarantee_value"       name="担保品市值"           precision="2"/>
    <column type="number" id="guarantee_rate"         name="维持担保比例"        precision="4"/>
  </group>
  <column type="number" id="average_guarantee_rate"  name="当月日均维持担保比例"    precision="4"/>
</output>
</function>