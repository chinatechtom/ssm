<?xml version="1.0" encoding="gbk" ?>
<function id="30815" name="单证券融资规模占净资本比例" >	
  <input>
		<item type="text"	id="rate"	name="比例(%)"	nullable="false"	default="2.5"	number="true"/> 		
  </input>
  <service type="procedure" id="p_stock_capital_percent_query" implement="p_stock_capital_percent_query"  process="911" style="77"> 
		<param type="integer" id="user_id"	way="in"/>
		<param type="double"  id="rate"		way="in"/>			
  </service>
  <output refresh="120" export="true">
		<column type="string" id="market_code"				name="市场代码"		display="false"/>
		<column type="string" id="market_name"				name="市场名称"/>
		<column type="string" id="sec_code"					name="证券代码"/>
		<column type="string" id="product_name"				name="证券名称"/>
		<column type="number" id="fund_amt"					name="净资本"		precision="2"/>
		<column type="number" id="FINANCING_SCALE"			name="融资规模"		precision="2"/>
		<column type="number" id="rate"						name="比例(%)"		precision="8"/>	
		<column type="string" id="warning_level"			name="报警级别"	level="true"	display="false"/>
  </output>
</function>