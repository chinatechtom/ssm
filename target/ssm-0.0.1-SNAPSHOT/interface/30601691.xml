<?xml version="1.0" encoding="gbk" ?>
<function id="30601691" name="信息申报" >  
  <input>
	<item type="hidden" id="cust_id"			  name="客户ID" action="306016911"/>
	<item type="select" id="system_flag"		  name="系统标志" source="608" nullable="false"/>
	<item type="text"	id="cust_name"			  name="客户姓名/名称"/>
	<item type="text"	id="commness_cust_code"	  name="普通资金账户"/>
	<item type="text"	id="cert_no"			  name="证件号码"/>
	<item type="select"	id="cert_type"			  name="证件类型" source="029" nullable="false"/>
	<item type="text"	id="security_code_sh"	  name="沪市A股证券账户"/>
	<item type="text"	id="security_code_sz"	  name="深市A股证券账户"/>
	<group name="本人接受的投资者教育(A、B项为必须通过项)">
		<item type="textarea" id="content_e" name="教育项内容" default="A:面对面讲解合同、风险揭示书及交易规则;
			B:融资融券投资者教育测试并通过测试;
			C:培训讲座、资料派发、宣传栏、视频、网站等宣传及其它教育方式" editable="false" />
		<item type="text" id="accept_edu" name="教育项(分号分隔)"/>
	</group>
	<group name="持有关联证券账户">
		<item type="text" id="hold_rela_account_name" name="持有关联账户名称"/>
		<item type="text" id="hold_rela_commness_cust_code" name="持有关联普通资金账户"/>
	</group>
	<group name="关联人证券账户">
		<item type="text" id="rela_account_name" name="关联人账户名称"/>
		<item type="text" id="rela_commness_cust_code" name="关联人普通资金账号"/>
	</group>
	<group name="海通证券大股东(持股5%以上)">
		<item type="text" id="hold_product_amt" name="持有海通证券股数(股)" number="true"/>
	</group>
	<group name="海通证券关联人">
		<item type="text" id="rela_relation" name="与海通证券的关系"/>
	</group>
	<group name="一致行动人">
		<item type="text" id="fit_action_name" name="一致行动人"/>
	</group>
	<group name="上市公司董监高、大股东(持股5%以上)、关联人">
		<item type="text" id="listed_company_name"			name="上市公司名称"/>
		<item type="text" id="listed_company_product_code"	name="上市公司股份代码"/>
		<item type="textarea" id="content_pos" name="职位内容" default="0:董事;1:监事;2:高级管理人员;3:大股东" editable="false" />
		<item type="text" id="position" name="职位(分号分隔)"/>
		<item type="text" id="hold_stock_ratio"	name="持股比例" number="true"/>
		<item type="select" id="if_relation"	name="是否为关联人" source="611"/>
		<item type="text" id="listed_company_rela_relation"	name="关联关系"/>
	</group>
	<group name="与其他证券公司签订融资融券合同">
		<item type="text" id="other_bond_sign_company" name="签订融资融券合同的公司"/>
	</group>
	<group name="已在其他证券公司开立信用资金账户">
		<item type="text" id="other_credit_security_code_sh" name="沪市信用证券账户"/>
		<item type="text" id="other_credit_security_code_sz" name="深市信用证券账户"/>
	</group>
	<group name="持有限售流通股或解除限售流通股">
		<item type="text"	id="product_name" name="股票名称"/>
		<item type="text"	id="product_code" name="股票代码"/>
		<item type="text"	id="stock_amt"    name="股份数量" number="true"/>
		<item type="select" id="product_type" name="股票类型" source="600"/>
		<item type="text"	id="relieve_date" name="解禁日期"/>
		<item type="select" id="market_code"  name="市场代码" source="039"/>
		<item type="button"	id="add"	name="增加" action="3060175" multiple="0"/>
		<item type="button"	id="delete" name="删除" action="3060176" multiple="0"/>
		<item type="button"	id="view"	name="查询" action="306016913" multiple="0"/>
	</group>
  </input>
   <service type="procedure" id="p_cust_info_declare_insert" implement="p_cust_info_declare_insert30601691" process="811" style="1">
	<param id="user_id"						  type="integer" way="in"/> 
	<param id="cust_id"						  type="integer" way="in"/> 
	<param id="system_flag"					  type="string" way="in"/>
	<param id="cust_name"					  type="string" way="in"/>
	<param id="commness_cust_code"			  type="string" way="in"/>
	<param id="cert_no"						  type="string" way="in"/>
	<param id="cert_type"					  type="string" way="in"/>
	<param id="security_code_sh"			  type="string" way="in"/>
	<param id="security_code_sz"			  type="string" way="in"/>
	<param id="accept_edu"					  type="string" way="in"/>
	<param id="hold_rela_account_name"		  type="string" way="in"/>
	<param id="hold_rela_commness_cust_code"  type="string" way="in"/>
	<param id="rela_account_name"			  type="string" way="in"/>
	<param id="rela_commness_cust_code"		  type="string" way="in"/>
	<param id="hold_product_amt"			  type="double" way="in"/>
	<param id="rela_relation"				  type="string" way="in"/>
	<param id="fit_action_name"			  	  type="string" way="in"/>
	<param id="listed_company_name"			  type="string" way="in"/>
	<param id="listed_company_product_code"   type="string" way="in"/>
	<param id="position"					  type="string" way="in"/>
	<param id="hold_stock_ratio"			  type="double" way="in"/>
	<param id="if_relation"					  type="string" way="in"/>
	<param id="listed_company_rela_relation"  type="string" way="in"/>
	<param id="other_bond_sign_company"		  type="string" way="in"/>
	<param id="other_credit_security_code_sh" type="string" way="in"/>
	<param id="other_credit_security_code_sz" type="string" way="in"/>	
  </service> 
  <output/>
</function>