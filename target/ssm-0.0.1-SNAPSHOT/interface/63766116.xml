<?xml version="1.0" encoding="gbk" ?>
<function id="63766116" name="修改交易所认证信息申请合约详情" >
  <input>
	<group name="合同信息" >
		<item type="hidden" id="cust_id" name="客户id" />
		<item type="hidden" id="order_id" 	name="合约id"	action="63744116"/>
		<item type="hidden" id="bond_no" name="合同编号" editable="false"/>
		<item type="text" id="cust_name" name="客户姓名" editable="false"/>
		<item type="text" id="cust_code" name="客户代码" editable="false"/> 	
	</group>
	<item type="select" id="market_code" name="市场代码" source="039"  editable="false"/>
