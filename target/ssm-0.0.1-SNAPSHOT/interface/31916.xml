<?xml version="1.0" encoding="gbk" ?>
<function id="31916" name="客户授信额度申请查询" >
  <input export="true">
  <item type="select" id="institution_id" name="营业部名称" source="95000008"/>
	<item type="hidden" id="cust_id" name="客户id"/>
	<item type="text" 	id="cust_code" name="客户代码"/>
	<item type="hidden" id="bond_id" name="合同id"/>
	<group name="分页查询" page="true">		
		<item type="text"   id="page_num"       name="页码"     integer="true" default="1"/>
		<item type="select" id="rows_num"       name="每页行数" integer="true" source="95000012" nullable="false"/>
	</group>	
  </input>
  <service type="procedure" id="p_cust_credit_num2_query" process="911" style="77" implement="p_cust_credit_num2_queryABC">
	<param type="integer" id="user_id"/>	
	<param type="integer" id="page_num"/>
	<param type="integer" id="rows_num"/>		
	<param type="integer" id="institution_id"/>	
	<param type="integer" id="cust_id"/>
	<param type="string" 	id="cust_code"/>	
	<param type="integer" id="bond_id"/>
  </service>
   <output >    
   	<column type="number" id="institution_id" name="机构ID" display="false"/>
   	<column type="string" id="institution_name" name="营业部名称"/>
   	<column type="string" id="institution_code" name="营业部代码"/>
    <column type="number" id="bond_id" name="合同id" display="false"/>
	  <column type="string" id="cust_code" name="客户代码"/>
	  <column type="string" id="cust_name1" name="客户姓名"/>
	  <column type="number" id="credit_num2" name="授信额度" precision="2"/>  
	  <column type="string" id="begin_time"  name="起始日期" display="false"/>
	  <column type="string" id="return_time" name="结束日期" display="false"/>
    <column type="number" id="fix_qty" name="融资上限" precision="2"/>	  
	  <column type="string" id="financing_product_id" name="融资产品ID" display="false"/>
	  <column type="string" id="financing_product_code" name="融资信用产品代码"/>
	  <column type="string" id="financing_product_name" name="融资信用产品名称"/>
    <column type="number" id="application_borrow_qty" name="融券上限" precision="2"/>	  
    <column type="string" id="bs_product_id"   name="融券产品ID" display="false"/>    
	  <column type="string" id="bs_product_code" name="融资信用产品代码"/>
	  <column type="string" id="bs_product_name" name="融资信用产品名称"/>	  
  </output> 
</function>