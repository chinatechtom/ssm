<?xml version="1.0" encoding="gbk" ?>
<function id="3063226" name="最高授信额度查询" >
	<input>
		<item type="hidden" id="cust_id"			name="客户ID"/>
		<item type="hidden" id="credit_level_id"	name="信用级别"/>
		<item type="hidden"	id="comm_credit_total_asset"		name="普通账户实时总资产"/>
	</input>
	<service type="procedure" id="p_top_credit_num_query" implement="p_top_credit_num_query3063226"  process="911" style="77">
		<param id="user_id"					type="integer"	way="in"/>
		<param id="cust_id"					type="integer"	way="in"/>
		<param id="credit_level_id"			type="integer"	way="in"/>
		<param id="comm_credit_total_asset"	type="double"	way="in"/>
	</service>
	<output>
		<column type="number" id="top_credit_num"		name="最高授信额度"		precision="2"/>
		<column type="number" id="top_credit_num_wy"	name="最高授信额度(万元)"	precision="2"/>
		<column type="number" id="credit_total_net"		name="客户信用净资产"		precision="2"/>
	</output>
</function>