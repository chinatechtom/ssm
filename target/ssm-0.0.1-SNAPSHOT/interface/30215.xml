<?xml version="1.0" encoding="gbk" ?>
<function id="30215" name="客户实时成交">
	<input export="true">		
		<item type="select"  id="institution_id"	    name="营业部名称" source="95000008"/>
		<item type="hidden"  id="cust_id"	            name="客户id"/>
		<item type="text"    id="cust_code"	            name="客户代码"/>
		<item type="hidden"  id="acc_id"	            name="账户id"/>
		<item type="text"    id="acc_code"	            name="证券账户"/>
		<item type="text"    id="product_code"          name="证券代码"/>
		<item type="date"    id="begin_date"	        name="起始日期*"  default="now"/>	
		<item type="date"    id="end_date"	            name="终止日期*" default="now"/>	
        <group name="分页查询" page="true">		
			<item type="text"    id="page_num"       name="页码"     integer="true" default="1"/>
			<item type="select"  id="rows_num"       name="每页行数" integer="true" source="95000012" nullable="false"/>
		</group>

	</input>
    <service type="procedure" id="p_cust_real_trade_query"  process="911" style="77">
		<param id="user_id"	        type="integer" way="in"/> 
		<param id="page_num"	        type="integer"  way="in"/> 			
		<param id="rows_num"	        type="integer"  way="in"/>
		<param id="institution_id"	type="integer" way="in"/>
		<param id="cust_id"	        type="integer" way="in"/>
		<param id="cust_code"	    type="string"  way="in"/> 
		<param id="acc_id"	        type="integer" way="in"/> 
		<param id="acc_code"		type="string"  way="in"/>
		<param id="product_code"    type="string"  way="in"/>
		<param id="begin_date"		type="string"  way="in"/>
		<param id="end_date"		type="string"  way="in"/>
    </service>
    <output export="true">
    <column type="string" id="institution_code"	  name="营业部代码"/>    	    	
    <column type="string" id="institution_name"		name="营业部名称"/>
		<column type="string" id="cust_code"		name="客户代码"/>
		<column type="string" id="cust_name"		name="客户姓名"/>
		<column type="number" id="acc_id"			name="账户id"		display="false"/>
		<column type="string" id="acc_code"			name="证券账户"/>
		<column type="string" id="fund_acc_code"	name="资金账号"		display="false"/>
		<column type="string" id="date"				name="成交日期"/>
		<column type="string" id="time"				name="成交时间"/>
		<column type="string" id="product_code"		name="证券代码"/>
		<column type="string" id="credit_flag"		name="信用交易标志1"	display="false"/>
		<column type="string" id="credit_flag_name"	name="信用交易标志"/>
		<column type="string" id="bs"				name="买卖"/>
		<column type="number" id="trade_qty"		name="成交数量"		precision="2" total="true"/>
		<column type="number" id="trade_price"		name="成交价格"		precision="3" />
		<column type="string" id="entrust_type"		name="委托种类"		display="false"/>
		<column type="string" id="entrust_no"		name="委托序号"/>
		<column type="string" id="contract_no"		name="合同号码"/>
		<column type="string" id="done_no"			name="成交编号"/>
		<column type="string" id="market_code"		name="市场"/>
		<column type="string" id="currency_type"	name="货币类型"	display="false"/>
    <column type="string" id="currency_typename"	name="货币类型"  />
		<column type="number" id="product_id"		name="产品id"		display="false"/>		
		<column type="string" id="product_type"		name="证券类型"/>
		<column type="string" id="mark"				name="标志"			display="false"/>
		<column type="string" id="sec_in_out"		name="买卖方向"		display="false"/>		
		<column type="string" id="report_seat"		name="报盘席位"/>
		<column type="string" id="sec_acc"			name="报盘代码"		display="false"/>
		<column type="string" id="report_contract"	name="报盘合同号"/>
		<column type="number" id="entrust_qty"		name="委托数量"		precision="2" total="true" display="false"/>
		<column type="number" id="entrust_amt"		name="委托金额"		precision="2" total="true" display="false"/>
		<column type="number" id="last_qty"			name="上次余额"		precision="2" total="true" display="false"/>	
		<column type="number" id="trade_amt"		name="成交金额"		precision="2" total="true" display="false"/>
		<column type="number" id="trade_counts"		name="成交笔数"		precision="2" total="true" display="false"/>
		<column type="number" id="this_qty"			name="本次余额"		precision="2" total="true" display="false"/>
		<column type="number" id="frst_fee"			name="一级费用"		precision="2" total="true" display="false"/>
		<column type="number" id="frst_clr_amt"		name="一级清算金额"	precision="2" total="true" display="false"/>
		<column type="number" id="secont_fee"		name="二级费用"		precision="2" total="true" display="false"/>
		<column type="number" id="second_clr_amt"	name="二级清算金额"	precision="2" total="true" display="false"/>
		<column type="number" id="net_amt"			name="成交净额"		precision="2" total="true"/>
		<column type="number" id="fund_chg"			name="资金发生"		precision="2" total="true" display="false"/>
		<column type="string" id="force_flag"		name="强制平仓标志1"	 display="false"/>
		<column type="string" id="force_flag_name"	name="强制平仓"/>
    </output>
</function>
