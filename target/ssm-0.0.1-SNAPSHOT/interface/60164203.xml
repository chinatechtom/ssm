<?xml version="1.0" encoding="gbk"?>
<function id="60164203" name="股票质押式回购应收利息增减变动及余额汇总表">
	<input>   
	 <item type="date"    id="trade_date"  name="查询日期"/>
	 <item type="select"  id="lender_id"   name="融出方" source="97600001"/>
  </input>
  <service type="procedure" id="p60164203" process="933" style="77" adapter="psr">
	<param id="user_id" type="integer"/>  
	<param id="trade_date" type="string"/>
	<param id="lender_id" type="integer"/>
  </service>
  <output export="true">
    <column type="string" id="trade_date" name="报表日期"/>
	<column type="number" id="lender_id" name="融出方ID" display="false"/>
	<column type="string" id="lender_name" name="融出方" display="false"/>
    <column type="string" id="report_items" name="报告项目" display="false"/>
	<column type="string" id="report_items_name" name="项目"/>
	<column type="number" id="money" name="金额" precision="2"/>
	<column type="string" id="remarks" name="备注"/>
	<button type="print" id="print" name="打印" action="60164223" multiple="0"/> 
  </output>
</function>
