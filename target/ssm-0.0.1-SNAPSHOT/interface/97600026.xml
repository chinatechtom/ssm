	<param id="sdate1" type="string"/>
	<param id="sect_interest_mode" type="string"/>
	<param id="hg_rate" type="double"/>
	<param id="convert_rate" type="double"/>
  </service>
  <output export="true">
	<column type="number" id="damt4" name="购回价格(%)" precision="2"/>	
	<column type="number" id="damt2" name="折算率(%)" precision="2"/>	
	<column type="number" id="app_price" name="标的证券价格" precision="2"/>	
	<column type="number" id="days" name="购回期限"/>	
	<column type="number" id="section_days1" name="购回期限"/>	
</output>
</function>