<?xml version="1.0" encoding="gbk" ?>
<function id="3042603" name="融资融券密码校验" >		
	<input> 		
		<item type="hidden" id="cust_code" name="客户代码"/>	
		<item type="hidden" id="paravalue2" name="是否需要校验密码"/>		
		<item type="hidden" id="chk_cust_pwd"   name="客户交易密码"/>	
	</input>	
	<service type="procedure" id="p_trade_pwd_query"  process="911" style="1"> 		
		<param id="user_id" type="integer" way="in"/>	
		<param id="cust_code" type="string"  way="in"/>	
		<param id="paravalue2" type="string"  way="in"/>		
		<param id="chk_cust_pwd" type="string"  way="in"/>	
	</service>	<output target="@func_no">		
		<column name="功能号" 		        		id="func_no"       	type="string"/>		
		<column name="客户代码"		  				id="scust_no"  		type="string"/>		
		<column name="客户交易密码"		  	        id="stx_pwd"   		type="string"/>		
		<column name="密码类型"	  				    id="lvol0"		    type="number"  precision="0"/> 
	</output>		</function>