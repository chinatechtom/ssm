<?xml version="1.0" encoding="gbk" ?>
<function id="3221003" name="邮件内容查询(自定义内容)" >
	<input>
		<item type="hidden"	id="c1"	name="信息编号"	/>		
	</input>
	<service type="procedure" id="p_msg_manual_content_query" process="911" style="77">
		<param id="user_id"	type="integer"  way="in"/>
		<param id="c1"	type="integer"  way="in"/>
	</service>
	<output>
		<column type="number"	id="msg_id"				name="信息ID"		display="true"	/>
		<column type="string"	id="subject"			name="主题"			display="true"	/>
		<column type="string"	id="content"			name="内容"			display="true"	/>		
	</output>
</function>