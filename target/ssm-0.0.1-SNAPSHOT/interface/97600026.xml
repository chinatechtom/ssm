<?xml version="1.0" encoding="gbk" ?>
<function id="97600026" name="协议书要素查询" >
  <input>
	<item type="text" id="stx_pwd" name="产品代码"/>
	<item type="text" id="order_type" name="指令类别"/>
	<item type="text"   id="sstock_code"   name="证券代码"/>
	<item type="select" id="smarket_code"    name="市场"/>
	<item type="date" 	id ="sdate0" name="初始交易日期"/>
	<item type="date" 	id ="sdate1" name="购回交易日期"/>
	<item type="text"   id="sect_interest_mode"   name="分段计息模式"/>
	<item type="text"   id="hg_rate"   name="默认利率"/>
	<item type="text"   id="convert_rate"   name="加成折算率"/>
