<?xml version="1.0" encoding="gbk" ?>
<function id="306444448" name="处理客户结清利息申请" >
  <input> 
	<group name="利息费用详细" scope="group">
	  <item type="text" id ="cust_name" name="客户姓名" editable="false"/>
	  <item type="text" id ="cust_code" name="客户代码" editable="false"/>
	  <item type="hidden" id="cust_id" action="30225" name="客户id"/>
		<item type="text" id="account_interest_date"     name="计息日期" editable="false" />
		<!--<item type="text" id="over_interest_flag"    name="结息标志" editable="false"/>-->
		<item type="text" id="financing_interest_sum"    name="融资利息合计" editable="false"/> 
		<item type="text" id="borrow_stock_interest_sum" name="融券利息合计"  editable="false"/> 
		<item type="text" id="financing_fix_sum"         name="融资固定额度费合计" editable="false"/> 
		<item type="text" id="borrow_stock_fix_sum"      name="融券固定额度费合计"  editable="false"/> 
		<item type="text" id="financing_takeup_sum"      name="融资占用额度费合计"  editable="false"/> 
		<item type="text" id="borrow_stock_takeup_sum"   name="融券占用额度费合计" editable="false"/> 
		<item type="text" id="financing_bef_rtn_sum"     name="融券违约手续费合计" editable="false"/> 
		<item type="text" id="borrow_stock_bef_rtn_sum"  name="提前还贷手续费合计"  editable="false"/> 
    <item type="text" id="financing_punish_sum"      name="融资预期罚息合计"  editable="false"/> 
    <item type="text" id="borrow_stock_punish_sum"   name="融券逾期罚息合计"  editable="false"/> 
    <item type="text" id="financing_bad_punish_sum"  name="融资坏账罚息合计"  editable="false"/> 
    <item type="text" id="borrow_stock_bad_punish_sum" name="融券坏账罚息合计"  editable="false"/> 
  </group>
  <group name="处理客户结清利息申请">
	<item type="hidden" id="order_id" name="合约id" action="30673"/>		
	<item type="text" id="order_no" name="合约编号" editable="false"/>
	<item type="hidden" id="formerly_voucher_no" name="原始凭证号" editable="false"/>
	<item type="textarea" id="remark" name="备注" editable="false"/>
  </group>
  <group name="处理意见">	
	<item type="textarea" id="manage_idea" name="处理意见"/>
  </group>
  </input>
  <service type="procedure" id="p_compute_interest_modi" process="811" style="1">
	<param type="integer" id="user_id" way="in"/>	
	<param type="integer" id="order_id"  way="in"  />
	<param type="string" id="manage_idea"  way="in"  />
  </service>
  <output/>
</function>