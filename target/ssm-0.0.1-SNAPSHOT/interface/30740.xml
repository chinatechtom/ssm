<?xml version="1.0" encoding="gbk" ?>
<function id="30740" name="信用违约资料查询" >
  <input export="true">
    <item type="date"   id="begin_date"   name="开始时间" />
    <item type="date"   id="end_date"	  name="结束时间" />	
	<item type="text"   id="cert_no"	  name="证件号码" />
	<group name="分页查询" page="true">			
		<item type="text"    id="page_num"       name="页码"     integer="true" default="1"/>
		<item type="select"  id="rows_num"       name="每页行数" integer="true" source="95000012" nullable="false"/>
	</group>
  </input>
  <service type="procedure" id="p_credit_violation_search"  process="911" style="77"> 
    <param id="user_id"		 type="integer" way="in"/>
	<param id="page_num"     type="integer" way="in"/> 			
 	<param id="rows_num"     type="integer" way="in"/> 
    <param id="begin_date"   type="string"  way="in"/>  
	<param id="end_date"	 type="string"  way="in"/>  
	<param id="cert_no"	     type="string"  way="in"/>  
  </service>
	<output>
		<column type="number" id="record_id"	   name="记录id" display="false"/>
		<column type="string" id="wyxwdm"		   name="席位代码"/>
		<column type="string" id="wyzqmc"		   name="券商名称"/>			
		<column type="string" id="wygddm"		   name="违约者证券账户"/>
		<column type="string" id="wygdxm"		   name="违约者姓名"/>
		<column type="string" id="wysfzh"		   name="身份证号"/>
		<column type="number" id="wywyje"		   name="违约金额"/>
		<column type="string" id="wywylb"		   name="违约类别1"	display="false"/>
		<column type="string" id="wywylb_name"	   name="违约类别"/>
		<column type="string" id="wysblb"		   name="申报类别1"	display="false"/>
		<column type="string" id="wysblb_name"	   name="申报类别"/>
		<column type="string" id="wysbrq"		   name="申报日期"/>
		<column type="string" id="wymark"		   name="备用"		display="false"/>
		<column type="string" id="wyscdm"		   name="市场代码1"	display="false"/>
		<column type="string" id="wyscdm_name"	   name="市场代码"/>
		<button type="add"    id="add"             name="新增信用违约资料" action="30741" multiple="-1"/>
		<button type="add"    id="add2"             name="批量新增信用违约资料" action="34039" multiple="-1"/>
		<button type="edit"   id="edit"            name="修改信用违约资料" action="30741" multiple="0"/>
		<button type="delete" id="delete"          name="删除信用违约资料" action="3074102" multiple="0"/>		
	</output>	
</function>