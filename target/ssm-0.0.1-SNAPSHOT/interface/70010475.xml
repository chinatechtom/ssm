<?xml version="1.0" encoding="gbk" ?>
<function id="70010475" name="任务执行状态查询" >
	<input>
		<description>是否更新执行状态?
		</description>
		<item type="hidden"	id="task_id" name="任务代码"		/>
		<item type="hidden"	id="task_id_out" name="任务代码out"		/>
	</input>
	<service type="procedure" id="zx_prod.SP_ZX_STATUS" adapter="netezza" process="944" style="1">
		<param type="string" id="task_id_out"/>
	</service>
	<output target="70010476">
		<column type="string" id="task_id"     name="任务代码"	/>  	
		<column type="string" field="stat_cd"  id="task_status" name="返回结果"	/>  		
	</output>
</function>