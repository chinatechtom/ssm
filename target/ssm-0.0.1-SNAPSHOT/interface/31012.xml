<?xml version="1.0" encoding="gbk" ?>
<function id="31012" name="历史浮动授信变更明细查询">
	<input>
		<item type="text"	id="cust_code"		name="客户代码"/>
		<item type="select"	id="business_code"	name="业务代码"	source="627"/>
		<item type="date"	id="begin_date"		name="起始日期"	default="last"/>
		<item type="date"	id="end_date"		name="终止日期"	default="now"/>
	</input>
	<service type="procedure" id="p_his_floafing_credit_num_select" implement="p_his_floafing_credit_num_select31012" process="955" style="77">
		<param id="user_id"			type="integer"	way="in"/>
		<param id="cust_code"		type="string"	way="in"/>
		<param id="business_code"	type="string"	way="in"/>
		<param id="begin_date"		type="string"	way="in"/>
		<param id="end_date"		type="string"	way="in"/>
	</service>
	<output export="true">
		<column type="number" id="cust_id"				name="客户ID"		display="false"/>
		<column type="string" id="branch_code"			name="营业部代码"	display="false"/>
		<column type="string" id="branch_name"			name="营业部名称"/>
		<column type="string" id="date"					name="变动日期"/>
		<column type="number" id="serial_no"			name="证券ID"/>
		<column type="string" id="cust_code"			name="客户代码"/>
		<column type="string" id="cust_name"			name="客户姓名"/>
		<column type="number" id="bond_id"				name="合同ID"		display="false"/>
		<column type="string" id="bond_no"				name="合同编号"/>
		<column type="string" id="bond_name"			name="合同名称"/>
		<column type="string" id="business_code"		name="业务代码"/>
		<column type="string" id="business_name"		name="业务名称"/>
		<column type="string" id="business_type"		name="业务类别"		display="false"/>
		<column type="string" id="business_type_name"	name="业务类别"/>
		<column type="string" id="last_value"			name="操作前内容"/>
		<column type="string" id="this_value"			name="操作后内容"/>
	</output>
</function>
