<?xml version="1.0" encoding="gbk" ?>
<function id="3062931" name="信用客户浮动授信查询">
	<input>
		<item type="text"	id="cust_code"	name="客户代码"/>
		<item type="text"	id="cust_name"	name="客户姓名"/>
		<item type="text"	id="cert_no"	name="证件号码"/>
		<item type="text"	id="bond_no"	name="合同编号"/>
	</input>
	<service type="procedure" id="p_float_credit_num_query" implement="p_float_credit_num_query3062931" process="911" style="77">
		<param id="user_id"		type="integer"	way="in"/>
		<param id="cust_code"	type="string"	way="in"/>
		<param id="cust_name"	type="string"	way="in"/>
		<param id="cert_no"		type="string"	way="in"/>
		<param id="bond_no"		type="string"	way="in"/>
	</service>
	<output>
		<column type="number"	id="cust_id"			name="客户ID" display="false"/>
		<column type="string"	id="cust_code"			name="客户代码"/>
		<column type="string"	id="cust_name"			name="客户姓名"/>
		<column type="number"	id="bond_id"			name="合同ID" display="false"/>
		<column type="string"	id="bond_no"			name="合同编号"/>
		<column type="string"	id="bond_name"			name="合同名称"	display="false"/>
		<column type="string"	id="float_flag"			name="浮动授信标识" display="false"/>
		<column type="string"	id="float_flag_name"	name="浮动授信标识"/>
		<column type="number"	id="float_credit_limit"	name="浮动授信额度(万元)"/>
		<column type="string"	id="if_black_list"		name="是否黑名单" display="false" level="true"/>
		<column type="string"	id="if_black_list_name"	name="是否黑名单"/>
		<button type="edit"		id="edit"	name="签署浮动授信"				action="31010"	multiple="0"/>
		<button type="edit"		id="edit"	name="取消浮动授信"				action="31011"	multiple="0"/>
	</output>
</function>