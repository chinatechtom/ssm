<?xml version="1.0" encoding="gbk" ?>
<function id="340694" name="客户平仓线预警线查询" >
	<input>
		<item type="text"	id="cust_code" name="信用账号"  />  
		<item type="text"	id="commness_cust_code"	name="普通账户" />  
	</input>
	<service type="procedure" id="P17000093" process="911" style="77">
	<param id="user_id"      type="integer" way="in" />  
	<param id="cust_code"    type="string" way="in" />
	<param id="commness_cust_code" type="string" way="in" /> 
  </service> 	
  <output> 
		<column type="number" id="cust_id"   name="客户ID" display="false"/>	
    <column type="string" id="institution_code"  name="营业部代码"/>
		<column type="string" id="cust_code"  name="客户代码"/>
		<column type="string" id="commness_cust_code"  name="普通账户"/>
		<column type="string" id="cust_name"  name="客户姓名"/> 
    <column type="string" id="guarantee_rate_name"     name="维保比例类型"/>  
    <column type="hidden" id="instruction"     name="类型"/>  
		<column type="number" id="guarantee_rate"   name="维保比例"  precision="4"/>	 
		<column type="string" id="cert_type_name"  name="证件类型"/> 
		<column type="string" id="cert_no"    name="证件号码"/>  
		<column type="string" id="user_code"  name="设置员工"/>   
		<column type="string" id="remark"     name="备注"/>
		<column type="string" id="date"       name="更新日期"/>
		<column type="string" id="time"       name="更新时间"/>
		<column type="string" id="opertype"   name="操作类型" display="false"/>
		<button type="add" id="add"   name="增加客户" action="340695" multiple="-1"/> 
		<button type="mod" id="mod"   name="修改客户" action="340696" multiple="0"/>
		<button type="del" id="add"   name="删除客户" action="340697" multiple="0"/> 
		<button type="qry" id="qry"   name="同步到交易" action="80100117" multiple="0"/> 
	</output>
</function>