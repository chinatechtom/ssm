<?xml version="1.0" encoding="gbk" ?>
<function id="3065006" name="生成大写中文" >
	<input>	
		<item type="hidden"	id="rz_or_rq"			name="交易种类"	default="1"/>
		<item type="hidden"	id="chinese_type"		name="是否大写"	default="1"/>	
		<item type="hidden"	id="auto_credit_num"	name="信用上限"/>
	</input>
	<service type="procedure" id="p_digit_convert_chinese" implement="p_digit_convert_chinese3065006" process="911" style="77">
		<param type="integer"	id="user_id" />
		<param type="integer"	id="rz_or_rq"		default="1"/>
		<param type="integer"	id="chinese_type"	default="1"/>
		<param type="double"	id="auto_credit_num"/>
	</service>
	<output export="true">  
		<column type="number" id="credit_num_wy"	name="授信额度(万元)"	precision="2"/>
		<column type="string" id="chinese"			name="大写中文"			display="false"/>  <!--modify by SECREQ-1823 20141111-->
	</output>
</function>