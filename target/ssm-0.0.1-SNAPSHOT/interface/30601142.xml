		<param id="begin_date"/> 
		<param id="end_date"/>
		<param id="credit_type1"/>
		<param id="status1"/>
		<param id="if_warning_open"/>		<!-- modify by SECREQ-1427 20140711 -->
		<param id="source_flag"/>			<!-- modify by SECREQ-1427 20140711 -->
		<param id="total_asset"/> <!--huet add 20141106-->
	</service> 
	<output autoload="true" refresh="180">
	    <column type="number" id="versionid"			name="评级因子编号"/>
