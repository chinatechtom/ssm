<?xml version="1.0" encoding="gbk" ?>
<function id="36000076" name="8.1.2客户融资授信集中度(前X名客户)" >  
  <input>
 <item type="date"   id="date"           name="日期"  default="now"/>
 <item type="text"   id="cust_num"       name="客户数量" />
 <item type="select" id="institution_id" name="营业部名称"  source="95000008"/>
 <item type="select"  id="order_choice"    name="排序选择"  source="378"/>
  </input>
   <service type="procedure" id="P36000076" process="911" style="77">
 <param id="user_id"   type="integer" way="in" /> 
  <param id="date"      type="string"  way="in" />
  <param id="cust_num"  type="double" way="in" /> 
  <param id="institution_id"  type="integer" way="in" /> 
  <param id="order_choice"     type="string"  way="in"/>
  </service> 
  <output  print="true">
    <column type="number" id="id"                  name="序号"/>
    <column type="string" id="institution_name"    name="营业部"/> 
    <column type="string" id="cust_code"    name="客户代码"/>
    <column type="string" id="cust_name"    name="客户名称"/>
    <group name="已授融资额度">
    <column type="number" id="given_float_quota"  name="已授非固定融资额度" precision="2" total="true"/>
    <column type="number" id="given_fix_quota"    name="已授固定融资额度"   precision="2" total="true"/>
   </group>
    <column type="number" id="rate1"   name="占已授总额比例"  precision="4" />
     <column type="number" id="used_financing_total"   name="已用额度" precision="2" total="true"/>
     <column type="number" id="rate2"    name="已用占已授"   precision="4" />
  </output>
</function>