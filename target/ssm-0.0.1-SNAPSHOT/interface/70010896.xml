	<param id="end_date"			type="string"/>
	<param id="engage_trade_date"	type="string"/>
	<param id="market_code"			type="string"/>
	<param id="product_code"		type="string"/>
    <param id="order_type"			type="string"/>
	<param id="manage_status"		type="string" default=",03,"/>
	<param id="order_id"			type="string"/>
  </service>
  <output export="true">
    <column type="number" id="institution_id"			name="机构id"	display="false"/>
	<column type="string" id="institution_name"			name="营业部名称"/>	
	<column type="number" id="cust_id"					name="客户id"	display="false"/>
	<column type="string" id="cust_code"				name="客户代码"/>
	<column type="string" id="cust_name"				name="客户姓名"/>	
	<column type="string" id="order_type"	            name="合约类型id" display="false"/>
