<?xml version="1.0" encoding="gbk" ?>
<function id="31390" name="清算备份数据还原">	
	<input>
		<item type="date"	id="date"		name="日期"/>	
	</input>		
	<service type="procedure" id="p_database_backup_record_query" implement="p_database_backup_record_query31390"  process="911" style="77">
		<param id="user_id"		type="integer"/>
        <param id="id"		    type="integer"/>		
		<param id="date"		type="string"/>	
	</service>
	<output>
		<column type="string" id="ddate"                name="备份日期"/>
		<column type="string" id="system_date"          name="系统日期"/>
		<column type="string" id="ttime"                name="备份时间"/>
		<column type="string" id="backup_file_name"     field="name" name="备份名称"/>
		<column type="string" id="system_status_name"	name="当前系统状态"/>
		<column type="string" id="system_status"	    name="当前系统状态" display="false"/>
		<column type="string" id="remark"               name="备注"/>
		<column type="string" id="database_name"        name="备份数据库名称" display="false"/>
		<button type="edit"   id="forward"              name="清算前备份还原" action="3139001"  multiple="0"/>
		<button type="edit"   id="backward"             name="清算后备份还原" action="3139002"  multiple="0"/>
  </output>
</function>