<?xml version="1.0" encoding="gbk" ?>
<function id="70010452" name="信用级别设置" >	
  <input> 				
		<item type="hidden" id="credit_level_id"	name="信用级别id"/>
		<item type="text"	id="credit_level_code"	name="信用级别代码" nullable="false"	 maxlen="10"/>	
		<item type="text"	id="credit_level_name"	name="信用级别名称" nullable="false"	 maxlen="40"/>		
		<item type="select"	id="risk_level"			name="风险级别" source="152" nullable="false"/>			
		<item type="select"	id="if_estop"			name="是否禁止" source="002" nullable="false"/>			
		<item type="text"	id="lower_limit"		name="评估总分下限"  nullable="false"/>
		<item type="text"	id="high_limit" 		name="评估总分上限"  nullable="false"/>
		<item type="hidden"	id="credit_num" 		name="默认信用上限"  default="0"/>
		<item type="text"	id="credit_num_rate" 	name="最大交易额度系数"  nullable="false"/>
  </input>
  <service type="procedure" id="p70010452"  process="822" style="1"> 
		<param id="user_id"				type="integer" way="in"/>
		<param id="credit_level_id"		type="integer" way="in"/>
		<param id="credit_level_code"	type="string"  way="in"/>			
		<param id="credit_level_name"	type="string"  way="in"/>		
		<param id="risk_level"			type="string"  way="in"/>			
		<param id="if_estop"			type="double"  way="in"/>			
		<param id="lower_limit"			type="double"  way="in"/>			
		<param id="high_limit"			type="double"  way="in"/>		
		<param id="credit_num"			type="double"  way="in"/>
		<param id="credit_num_rate"		type="double"  way="in"/>
  </service>
  <output/>
</function>