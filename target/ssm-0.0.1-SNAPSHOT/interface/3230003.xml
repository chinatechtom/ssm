<?xml version="1.0" encoding="gbk" ?>
<function id="3230003" name="证券资产明细" >
  <input>	
	<item type="text"   id="cust_id" 		name="客户ID" /> 
  </input>
  <service type="procedure" id="P10003" process="911" style="77">
	<param id="user_id" 	type="integer"/>
   	<param id="cust_id" 	type="integer"/>
  </service>
  <output >
    <column type="string" id="row_id"       name="序号"  />
	<column type="string" id="cust_id"      name="客户ID"     display="false"/>
    <column type="string" id="cust_code"   	name="客户代码"   display="false"/>
    <column type="string" id="cust_name"   	name="客户名称"  />   
    <column type="string" id="product_code"   	name="证券代码"     />
    <column type="string" id="product_name"     name="证券名称"     />
	<column type="number" id="gua_asset_qty"   	name="冲抵保证金"  precision="2"/>
	<column type="number" id="fin_buy_qty"      name="非冲抵保证金"    precision="2"/>
	<column type="number" id="asset_qty"   		name="当前库存"        precision="2"/>
    <column type="number" id="cost_price"   	name="参考成本价"      precision="3"/>
    <column type="number" id="last_price"   	name="最新价"           precision="3"/>
	<column type="number" id="market_value"   	name="市值"             precision="2"/>
	<column type="number" id="profit_loss"   	name="浮动盈亏"         precision="2"/>
  </output>
</function>