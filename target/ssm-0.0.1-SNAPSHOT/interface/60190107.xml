<?xml version="1.0" encoding="gbk" ?>
<function id="60190107" name="客户股票质押利息预估" >
	<input>	
		<item type="select" id="insitution_id" name="营业部名称" source="97000001"/>
		<item type="text" id="cust_code" name="客户代码" />
		<item type="text" id="cust_name" name="客户名称" />
        <item type="text"  id="sbond_no" name="交易协议书编号"/>
        <item type="select" id="sbond_status" name="交易协议书状态" source="700"/>
		<item type="date" id="fix_date" name="查询日期" nullable="false"/>
	</input>
	<service type="procedure" id="p60190107" process="933" style="77">
		<param id="user_id"   type="integer"/>
		<param id="insitution_id" type="integer"/>
		<param id="cust_code" type="string"/>
		<param id="cust_name" type="string"/>
        <param type="string"  id="sbond_no"/>
        <param type="string"  id="sbond_status"/>
		<param id="fix_date"  type="string"/>
	</service>
	<output export="true">
		<column type="number" id="institution_id"   name="institution_id" display="false"/>
		<column type="string" id="institution_name" name="营业部名称"/>  
		<column type="string" id="institution_code" name="营业部代码"/>
		<column type="string" id="cust_name" name="客户名称"/>
		<column type="string" id="cust_code" name="客户代码"/>  
		<column type="string" id="bond_no" name="合同编号"/>
		<column type="string" id="sbond_no" name="交易协议书编号"/>
        <column type="string" id="sbond_status"   name="交易协议书状态"/>
        <column type="string" id="product_code"   name="证券代码"/>
        <column type="number" id="curr_stk_vol"   name="当前证券数量" precision="0"/>
        <column type="float" id="first_amt"   name="初始交易金额" precision="2"/>
        <column type="string" id="engage_trade_date"   name="初始交易日期"/>
		<column type="string" id="last_bb_date" name="最新购回日期"/>
        <column type="float" id="received_interest" name="已收利息"/>  	
        <column type="float" id="unreceived_interest" name="已结未收利息"/>  	
        <column type="float" id="curr_interest" name="当前利息"/>  	
        <column type="float" id="interest_end" name="截止指定日利息"/>  		
	</output>
</function>