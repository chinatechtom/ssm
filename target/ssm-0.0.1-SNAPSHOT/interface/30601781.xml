<?xml version="1.0" encoding="gbk" ?>
<function id="30601781" name="个人基本情况查询">
	<input>
		<item type="hidden"	id="cust_id"		name="客户ID"/>
		<item type="hidden"	id="cust_type_flag"	name="客户类型"/>
	</input>
	<service type="procedure" id="p_basic_personal_info_select" implement="p_basic_personal_info_select30601781" process="911" style="1">
		<param id="user_id"				type="integer"	way="in"/>
		<param id="cust_id"				type="integer"	way="in"/>
		<param id="cust_type_flag"		type="string"	way="in"/>
	</service>
	<output>
		<column id="cust_id"					type="number"	name="客户ID"/>
		<column id="post"						type="string"	name="职务"/>
		<column id="post_data_code"				type="string"	name="职务"/>
		<column id="work_company"				type="string"	name="工作单位" display="false"/>
		<column id="work_company_name"			type="string"	name="工作单位"/>
		<column id="work_company_detail"		type="string"	name="工作单位详情"/>
		<column id="edu_degree"					type="string"	name="文化程度" />
		<column id="family_state"				type="string"	name="家庭状况" display="false"/>
		<column id="family_state_name"			type="string"	name="家庭状况"/>
		<column id="part_business"				type="string"	name="曾参与业务"/>
		<column id="part_business_name"			type="string"	name="曾参与业务"/>
		<column id="if_pro_credit_report"		type="string"	name="是否提供信用报告" display="false"/>
		<column id="if_pro_credit_report_name"	type="string"	name="是否提供信用报告"/>
		<column id="after_tax_income"			type="string"	name="税后年收入"/>
		<column id="finance_asset_wy"			type="string"	name="金融资产(不含海通资金账户内资产)"/>
		<column id="property_situation"			type="string"	name="房产情况"/>
		<column id="bank_debt_wy"				type="string"	name="银行负债"/>
		<column id="other_debt_wy"				type="string"	name="其他负债"/>
		<column id="asset_oledge_state"			type="string"	name="个人资产抵押情况"/>
		<column id="other_gura_state"			type="string"	name="个人其他对外担保情况"/>
		<column id="credit_limit" 				type="number"   name="授信额度" precision="0" /> 
		<column id="credit_limit_c"    			type="string"   name="授信额度中文" display="false"/>
		<column id="system_date"    			type="string"   name="系统日期" />
		<column id="explain_begin_date"    		type="string"   name="讲解开始日期" />
		<column id="explain_begin_time"    		type="string"   name="讲解开始时间" />
		<column id="explain_end_date"    		type="string"   name="讲解结束日期" />
		<column id="explain_end_time"    		type="string"   name="讲解结束时间" />
	</output>
</function>