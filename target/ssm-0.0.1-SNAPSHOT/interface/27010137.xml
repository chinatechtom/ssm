<?xml version="1.0" encoding="gbk" ?>
<function id="27010137" name="2-2营业部融资融券交易量占比">
	<input>
		<item type="hidden"  id="cust_id"	            name="客户id" />
		<item type="select"	 id="insitution_id"			name="营业部名称" source="95000008"/>
		<item type="year"   id="begin_year"     name="起始年度" nullable="false"/>
		<item type="month"  id="begin_month"    name="起始月份" nullable="false"/>
		<item type="year"   id="end_year"       name="结束年度" nullable="false"/>
		<item type="month"  id="end_month"      name="结束月份" nullable="false"/>	
	</input>
    <service type="procedure" id="P27010137"  process="955" style="77" implement="P2701013727010137">
		<param id="user_id"	        type="integer" way="in"/>
		<param id="insitution_id"	type="integer" way="in"/>
		<param id="begin_year"     type="string"	way="in"/>
		<param id="begin_month"    type="string"	way="in"/>
		<param id="end_year"       type="string"	way="in"/>
		<param id="end_month"      type="string"	way="in"/>
    </service>
    <output export="true" print="true">
		<column type="string" id="institution_id"				name="营业部id"		display = "false"/>
		<column type="string" id="institution_code"				name="营业部分支号"/>
		<column type="string" id="institution_name"				name="营业部名称"/>
		<group name="本期占比">
			<column type="number" id="THIS_DAYS_FIN_BUY"			name="本期融资买入交易量"	  precision="2" 	 />
			<column type="number" id="THIS_DAYS_FIN_SELL"	        name="本期卖券还款交易量" precision="2" />
