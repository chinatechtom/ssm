<?xml version="1.0" encoding="gbk" ?>
<function id="70010716" name="券商主动调整客户信用级别" >
  <input print="70010716.jsp">	
	<item type="hidden" 	id="bond_id" 			name="合同id"/>
	<item type="hidden" 	id="cust_id" 			name="客户id" 	 		action="70001888"/>
	<item type="text"		id="cust_name"			name="客户姓名" 		editable="false"/>
	<item type="text"		id="cust_code"			name="客户代码" 		editable="false"/>
	<group name="信用评估">    
	  <item type="text"		id="consult_score"		name="参考客户总分"	editable="false"/>	
	  <item type="text"		id="valuation_score"	name="参考评估总分"	editable="false"/>	
	  <item type="text"		id="credit_level_name"	name="参考信用级别"	editable="false"/>	
	  <item type="text"		id="credit_num" 		name="最大交易额度上限"	editable="false"/>
    </group> 
    <group name="客户信用级别申请与调整">
      <item type="hidden"	id="order_no"			name="合约编号"/>
      <item type="hidden"	id="cust_id"			name="客户ID" />
	  <item type="text"		id="valuation_score" alias="approve_valuation_score"	name="评估客户总分" action="97000024" nullable="false" number="true"/>	
	  <item type="select" 	id="credit_level_id" name="信用级别"  		source="97000015"	 nullable="false"/>
	  <item type="textarea"	id="remark"				name="备注说明" />
	</group>
    <group name="自然情况"	scope="group">
	  <item type="grid"		id="cust_id"			name="自然情况" 	 	source="70030422" />
	</group>
	<item type="hidden" 	id="date" 				name="打印日期"/><!--从1888.xml获得的打印日期-->
	<item type="hidden" 	id="user_code" 			name="打印柜员代码"/><!--从1888.xml获得的打印柜员代码-->
	<item type="hidden" 	id="user_name" 			name="打印柜员名称"/><!--从1888.xml获得的打印柜员名称-->
	<item type="hidden" 	id="company" 			name="券商名称"/><!--从1888.xml获得的券商名称-->	
  </input>
  <service type="procedure"	id="P70010716" process="822" style="1">
	<param type="integer" 	id="user_id" way="in"/>
	<param type="string"	id="order_no" way="in"/>
	<param type="integer" 	id="cust_id" way="in"/>
	<param type="string"	id="remark" way="in"/>
	<param type="integer" 	id="bond_id" way="in"/>
	<param type="string" 	id="consult_score" way="in"/>
	<param type="string"	id="approve_valuation_score" way="in"/>
	<param type="string"	id="credit_level_id" way="in"/>
  </service>
  <output/>
</function>