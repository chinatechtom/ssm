<?xml version="1.0" encoding="gbk" ?>
<function id="31360" name="部门浏览">
	<input>
		<item type="hidden"	id="dept_id"	 name="部门id"/>
		<item type="text"   id="dept_code"   name="部门代码"	nullable="false"/>
		<item type="text"   id="dept_name"   name="部门名称"	nullable="false"/>
	</input>
	<service type="procedure" id="p_department_browse"   process="911" style="77">
		<param id="user_id"		type="integer"  way="in"/>
		<param id="dept_id"		type="integer"  way="in"/>
		<param id="dept_code"	type="string"   way="in"/>
		<param id="dept_name"	type="string"   way="in"/>
	</service>
	<output autoload="true">
		<column type="number"  id="dept_id"		name="部门id" display="false"/>
		<column type="string"  id="dept_code"	name="部门代码"/>
		<column type="string"  id="dept_name"	name="部门名称"/>	
		<button type="add" id="add"	name="增加" action="31361"	multiple="-1"/>
		<button type="edit" id="edit"	name="修改" action="31361" multiple="0"/>
	</output>
</function>