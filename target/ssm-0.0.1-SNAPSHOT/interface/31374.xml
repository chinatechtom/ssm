<?xml version="1.0" encoding="gbk" ?>
<function id="31374" name="客户联系资料修改查询">
	<input>
		<item type="select"	id="institution_id"		name="营业部名称" source="95000008"/>
		<item type="text"   id="cust_code"		    name="客户代码"/>	
        <item type="date"   id="begin_date"		    name="起始日期" default="last"/>			
		<item type="date"   id="end_date"		    name="结束日期" default="now"/>			
	</input>
	<service type="procedure" id="p_cust_connect_info_chg_brow"   process="911" style="77">
		<param id="user_id"			type="integer"  way="in"/>
		<param id="institution_id"  type="integer"  way="in"/>
		<param id="cust_code"		type="string"   way="in"/> 
		<param id="begin_date"		type="string"   way="in"/>	
        <param id="end_date"		type="string"   way="in"/>			
	</service>
	<output>
		<column type="number"  id="INSTITUTION_ID"			name="营业部ID" display="false"/>
		<column type="string"  id="INSTITUTION_NAME"		name="营业部名称"/>
		<column type="number"  id="CUST_ID"	                name="客户ID"  display="false"/>
		<column type="string"  id="CUST_CODE"		        name="客户代码"   />	
		<column type="string"  id="CUST_NAME"	            name="客户名称" />		
		<column type="string"  id="MOVE_TEL_BEFORE"         name="原移动电话" />	
		<column type="string"  id="MOVE_TEL_AFTER"          name="现移动电话" />	
		<column type="string"  id="FIX_TEL_BEFORE"          name="原固定电话" />		
		<column type="string"  id="FIX_TEL_AFTER"           name="现固定电话" />	
		<column type="string"  id="FAMILY_ADDRESS_BEFORE"   name="原联系地址" />	
		<column type="string"  id="FAMILY_ADDRESS_AFTER"    name="现联系地址" />		
		<column type="string"  id="PS_CODE_BEFORE"          name="原邮编" />	
		<column type="string"  id="PS_CODE_AFTER"           name="现邮编" />	
		<column type="string"  id="EMAIL_BEFORE"            name="原电子邮件" />		
		<column type="string"  id="EMAIL_AFTER"             name="现电子邮件" />	
		<column type="string"  id="FAX_BEFORE"		        name="原传真" />	
		<column type="string"  id="FAX_AFTER"	            name="现传真" />		
		<column type="string"  id="CHANGE_RESOURCE_NAME"    name="变更原因" />	
		<column type="string"  id="DATE"	                name="变更日期" />		
		<column type="string"  id="TIME"                    name="变更时间" />	
	</output>
</function>