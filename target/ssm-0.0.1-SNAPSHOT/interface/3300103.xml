<?xml version="1.0" encoding="gbk" ?>
<function id="3300103" name="修改重点客户" >
	<input>	
		<item type="text" 	id="record_id" 	name="记录编号"	    nullable="false" editable="false"/>	
		<item type="text" 	id="cust_name" 	name="客户姓名" 	nullable="false"/>
		<item type="select" id="cert_type" 	name="证件类型" 	nullable="false" source="029"/>		
		<item type="text" 	id="cert_no" 	name="证件号码" 	nullable="false"/>
		<item type="text" 	id="area_code" 	name="地址"			nullable="false"/>
		<item type="select" id="risk_level" name="风险级别" 	nullable="false" source="337"/>
		<item type="select" id="reason_code"	name="类别"  source="96000004" nullable="false"/>
		<item type="text"	id="in_value"	name="进入数值"	number="true"/>
		<item type="date"	id="in_date"	name="进入日期"/>		
		<item type="date"	id="out_date"	name="退出日期"/>
		<item type="hidden" id="issyscust"	name="是否系统客户"  source="391"/>
		<item type="select" id="is_valid"	name="是否有效"  source="392"/>
		<item type="textarea"	id="remark" 	name="备注"/>
	</input>
	<service type="procedure" id="p_risk_cust_list_modify" process="811" style="1">
		<param id="user_id" type="integer"/>
		<param id="record_id" type="integer"/>	
		<param id="cust_name"/>
		<param id="cert_type"/>
		<param id="cert_no"/>
		<param id="area_code"/>
		<param id="risk_level"/>
		<param id="in_date"/>
		<param id="reason_code"/>
		<param id="in_value"	type="double"/>
		<param id="out_date"/>
		<param id="issyscust"/>
		<param id="is_valid"/>
		<param id="remark"/>
	</service>
	<output/>
</function>
