<?xml version="1.0" encoding="gbk" ?>
<function id="60045105" name="合同终止申请处理" >
	<input print="60045105.jsp">
		<item type="hidden" id="order_id"     name="合约ID" action="60044105"/>
		<group name="合同信息" >
			<item type="text" id="bond_no" name="合同编号" editable="false"/>
			<item type="text" id ="institution_name" name="营业部名称" editable="false"/>
			<item type="text" id ="institution_code" name="营业部代码" editable="false"/> 
			<item type="text" id ="cust_name" name="客户姓名" editable="false"/>
			<item type="text" id ="cust_code" name="客户代码" editable="false"/> 	
		</group>
		<group name="融出方信息">
			<item type="select" id="lender_id" name="融出方" source="97600001" editable="false" action="60010102"/>
			<item type="text" id="lender_code" name="融出方代码" editable="false" maxlen="6"/>
			<item type="text" id="lender_name" name="融出方名称" editable="false" maxlen="100"/>
			<item type="text" id="lender_cust_code" name="融出方资金帐号" editable="false" maxlen="100"/>
			<item type="text" id="plan_contract" name="资产管理合同号" editable="false" maxlen="100"/>
			<item type="select" id="lender_type" name="融出方类别" source="800" editable="false"/>
			<item type="select" id="if_valid" name="是否有效" source="802" editable="false" default="1"/>
			<item type="date" id="found_date" name="设立日期" editable="false"/>
			<item type="text" id="lender_acc_sh"   name="融出方沪A证券账户" editable="false"/>
			<item type="text" id="lender_seat_sh" name="融出方沪A席位代码" editable="false" maxlen="100"/>
			<item type="text" id="lender_acc_sz"   name="融出方深A证券账户" editable="false"/>
			<item type="text" id="lender_seat_sz" name="融出方深A席位代码" editable="false" maxlen="100"/>
			<item type="text" id="mortgagee_name" name="质权人名称" editable="false" maxlen="100"/>
		</group>        			
			<item type="hidden" id="cust_id"	name="cust_id"      action="70010102,70001888"/>
			<item type="hidden" id="stock_holder_sh"  name="沪A证券帐户" editable="false"/>
			<item type="hidden" id="stock_holder_sz"  name="深A证券帐户" editable="false"/>
			<item type="hidden" id="seat_code_sh"  name="沪A席位代码" editable="false"/>
			<item type="hidden" id="seat_code_sz"  name="深A席位代码" editable="false"/>	
		<item type="hidden" id="date"     name="打印日期"/>	<!--从1888.xml获得的打印日期-->
		<item type="hidden" id="user_code"     name="打印柜员代码"/><!--从1888.xml获得的打印柜员代码-->
		<item type="hidden" id="user_name"     name="打印柜员名称"/><!--从1888.xml获得的打印柜员名称-->
		<item type="hidden" id="company"    name="券商名称"/><!--从1888.xml获得的券商名称-->			
		<group name="处理信息">
			<item type="textarea" id ="remark2" name="处理意见"/>
		</group>
	</input>
	<service type="procedure" id="P60045105" process="833" style="1">
		<param type="integer" id="user_id" way="in"/>
		<param type="integer" id="order_id" way="in"/>
		<param type="string" id="remark2"  way="in"/>
	</service>
	<output/>
</function>