 	</input>
   	<service type="procedure" id="p_credit_limit_query2" implement="p_credit_limit_query23063211" process="911" style="77">
		<param id="user_id" 			type="integer" way="in"/> 
		<param id="cust_id" 			type="integer" way="in"/> 
		<param id="top_credit_limit"	type="double"  way="in"/>
		<param id="credit_limit"		type="double" way="in"/>
  	</service> 
  	<output>
		<column type="number" id="apply_credit_limit"       name="可授信额度"/>
	    <column type="number" id="approval_credit_limit"    name="审批额度" />
 	</output>
</function>