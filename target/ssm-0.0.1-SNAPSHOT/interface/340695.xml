<?xml version="1.0" encoding="gbk" ?>
<function id="340695" name="新增客户平仓线预警线" >
	<input>
		<item type="text"	id="cust_code" name="信用账号" nullable="false" action="30101"/> 
    <item type="text"	id="commness_cust_code" name="普通账户" editable="false"/> 
		<item type="text"	id="cust_name" name="客户姓名" editable="false"/>  
		<item type="text"	id="cert_type" name="证件类型" editable="false"/>  
		<item type="text"	id="cert_number" name="证件号码" editable="false"/>  
		<item type="text"	id="pc_line" name="平仓线(%)" nullable="false"/> 
		<item type="text"	id="yj_line" name="风控线(%)" nullable="false"/> 
		<item type="text"	id="bc_line" name="补仓线(%)" nullable="false"/> 
		<item type="textarea"	id="remark" name="备注"/>  
		<item type="hidden" id="func_id" action="8801" default="340695"/>
		<item type="hidden" id="func_need_check" 	name="是否支持复核" 	default="0"/>		
		<group name="复核">   
			<item type="text" 		id="chk_user_code" 	name="复核人员" filter="func_need_check=1"/>
			<item type="password"   id="chkuser_pwd" 	name="密码" 	filter="func_need_check=1"/>
		</group>
	</input>
	<service type="procedure" id="P17000094" process="911" style="1">
		<param id="user_id" type="integer" way="in" /> 
		<param id="cust_code" type="string" way="in" />  
		<param id="pc_line" type="number" way="in" /> 
		<param id="yj_line"  type="number" way="in" />  
		<param id="bc_line"  type="number" way="in" />  
		<param id="remark"  type="string" way="in" />  
		<param id="chk_user_code"  type="string" way="in" />  
		<param id="chkuser_pwd"  type="string" way="in" />  
		<param id="func_id"			type="integer" way="in"/>
	</service> 	
  <output target="80100117">
  	<column type="number" id="cust_id"   name="客户ID" display="false"/>	
    <column type="string" id="institution_code"  name="营业部代码"/>
		<column type="string" id="cust_code"  name="客户代码"/>
		<column type="string" id="commness_cust_code"  name="普通账户"/>
		<column type="string" id="cust_name"  name="客户姓名"/> 
    <column type="string" id="instruction"     name="维保比例类型"/>  
		<column type="number" id="guarantee_rate"   name="维保比例"  precision="4"/>	 
		<column type="string" id="cert_type_name"  name="证件类型"/> 
		<column type="string" id="cert_no"    name="证件号码"/>  
		<column type="string" id="user_code"  name="设置员工"/>   
		<column type="string" id="remark"     name="备注"/>
		<column type="string" id="date"       name="更新日期"/>
		<column type="string" id="time"       name="更新时间"/>
		<column type="string" id="opertype"   name="操作类型" display="false"/>
	</output>
</function>