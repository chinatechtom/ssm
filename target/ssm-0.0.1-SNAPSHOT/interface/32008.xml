	<column type="string" id="c3"   name="客户姓名"  />
    <column type="number" id="c4"   name="账户维持担保比例"  precision="2"/>
    <column type="number" id="c5"   name="剔除所选股票后的维持担保品比例"  precision="2"/>
    <button type="add" 	  id="add"  	name="剔除/取消剔除"		action="3200801"  multiple="-1"/>
    <button type="send"   id="send"		name="恢复所有已剔除的股票"	action="3200803"  multiple="-1"/>
	</output> 	
</function>