<?xml version="1.0" encoding="gbk"?>
<function id="3021101" name="机构净资本设置" >  
  <input>
	<item type="hidden" id="institution_id" name="机构号"  />
  </input>
  <service type="procedure" id="p_institution_net_asset_query"  process="911" style="77">
	<param id="user_id" type="integer"/>
	<param id="institution_id" type="integer"/>
  </service>
  <output export="true">
    <column type="number" id="institution_id"		name="机构号"	display="false"/>
    <column type="string" id="date"					name="日期"/>
    <column type="string" id="currency_type"		name="货币类型"  display="false"/>
    <column type="string" id="currency_typename"	name="货币类型"  />
    <column type="number" id="yestoday_amt"			name="上日余额"  precision="2"/>
    <column type="number" id="fund_amt"				name="净资本余额"    precision="2"/>
    <column type="number" id="deposit"				name="单笔融资占比(%)"    precision="2"/>
    <column type="number" id="withdraw"				name="单个客户融资占比(%)"    precision="2"/>
	<column type="number" id="frozen_amt"   name="总融资占比(%)"    precision="2"/>
	<column type="number" id="bs_amt"			name="融券总规模"			precision="2"/>
	<button type="add"    id="add"			name="机构净资本设置" action="30212" multiple="0"/>
  </output>
</function>