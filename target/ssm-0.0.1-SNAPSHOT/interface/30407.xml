<?xml version="1.0" encoding="gbk" ?>
<function id="30407" name="定义某个客户的信用级别" >	
	<input>
		<item type="hidden" id="cust_id"				name="客户id" />
		<item type="date"   id="date"				    name="日期" default="last"/>
		<item type="select" id="before_credit_level_id" name="变更前信用级别" source="90000002" nullable="false"/>
		<item type="select" id="after_credit_level_id"  name="变更后信用级别" source="90000002" nullable="false"/>
		<item type="text"   id="remark"				    name="备注说明" />
		<item type="select" id="manage_status"		    name="处理状态" source="102"/>
		<item type="hidden" id="accept_id"			    name="受理员id" />
		<item type="date"   id="accept_time"		    name="受理时间" default="last"/>
	</input>
	<service type="procedure" id="p_cust_credit_level_change_define"  implement="p_cust_credit_level_change_define407"  process="811" style="1">
		<param id="user_id"					  type="integer" way="in" />	
		<param id="cust_id"					  type="integer" way="in"/>
		<param id="date"					  type="string"  way="in"/>
		<param id="before_credit_level_id"	  type="integer" way="in"/>
		<param id="after_credit_level_id"	  type="integer" way="in"/>
		<param id="remark"					  type="string"  way="in"/>
		<param id="manage_status"			  type="string"  way="in"/>
		<param id="accept_id"				  type="integer" way="in"/>
		<param id="accept_time"				  type="string"  way="in"/>
    </service>
	<output/> 
</function>