<?xml version="1.0" encoding="gbk" ?>
<function id="34037" name="客户信用交易数据" >
	<input> 
		<item type="date"	id="client_date" name="客户日期" />
		<item type="text"	id="commness_cust_code"	name="普通资金账号" nullable="true"/>
		<item type="date"	id="credit_begin_date" name="起始日期" />
		<item type="date"	id="credit_end_date" name="结束日期"/>
	</input>
	<service type="procedure" id="kingstar.p37819011_rzrq" adapter="newzhengxin" process="155" style="77"> 
		<param type="string"  id="commness_cust_code"/><!--普通资金账号-->
		<param type="string"  id="client_date"/>
		<param type="string"  id="credit_begin_date"/>
		<param type="string"  id="credit_end_date"/>
		<param type="string"  id="user_code" default="rzrqgl"/>
	</service>
	<output target="34007">
		<column type="string"	id="branch_code"		        name="营业部代码"	/>
		<column type="string"	id="branch_abbrev"		        name="营业部名称"	/>
		<column type="string"	id="capital_account"			name="信用资金账号"	/>
		<column type="string"	id="related_capital_account"	name="普通资金账号"	/>
		<column type="string"	id="client_name"				name="客户名称"	/>
		<column type="string"	id="credit_year_yield" field="year_yield"	name="信用交易绝对收益率"/>
		<column type="string"	id="hs300_yield_year"			name="沪深300年化收益率"	/>
		<column type="string"	id="credit_use_rate"			name="日均授信额度使用率"	/>
		<column type="string"	id="credit_turn_over_rate"		name="授信额度换手率"	/>
		<column type="string"	id="total_rzrq_commission"		name="信用交易佣金"	/>
		<column type="string"	id="itst_ctb"					name="信用交易息费收入"	/>
		<column type="string"	id="asset_pt_pdr"					name="普通资金账户资产"	/>
		<column type="string"	id="start_date"					name="开始日期"/>
		<column type="string"	id="end_date"					name="截止日期"/>
	</output>
</function>