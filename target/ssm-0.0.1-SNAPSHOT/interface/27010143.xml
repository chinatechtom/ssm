<?xml version="1.0" encoding="gbk" ?>
<function id="27010143" name="融资资金调拨申请表" >
     <input>     
      <item type="date"    id="date"             name="日期*"  />    
      <item type="text"    id="day_num"             name="天数(n)*"  default="3"/>  
     </input>
  <service type="procedure" id="P27010143"  process="911" style="77">
      <param id="user_id"               type="integer" way="in"/> 
      <param id="date"             type="string"  way="in"/> 
       <param id="day_num"             type="integer"  way="in"/>            
   </service>
 <output print="true">
        <column type="string" id="date"   name="日期" display="false"/>
        <column type="string" id="day_num"   name="天数" display="false"/>
        <group name="T日提交的融资申请额度">
        <column type="number" id="fix_model" name="固定模式" precision="2"/>        
        <column type="number" id="unfix_model"   name="非固定模式" precision="2"/>
        <column type="number" id="total"   name="合计" precision="2"/>        
        </group>
        <column type="number" id="overday_num" name="T+n日到期融资合约金额" precision="2"/>        
        <column type="number" id="bond_fin"   name="T+n日拟申请使用融资额" precision="2"/>
</output>
</function>  