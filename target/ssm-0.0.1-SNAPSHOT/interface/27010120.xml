	  <column type="number" id="fin_cmisn_crge_year" name="本年度累计融资佣金收入" precision="2" total="true"/>   
	  <column type="number" id="bs_cmisn_crge_date"  name="融券佣金收入"       precision="2" total="true"/> 
	  <column type="number" id="bs_rate"             name="融券佣金收入占比"   precision="6"/>   
