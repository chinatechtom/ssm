		<item name="客户类别"	id="cust_sort"   type="text"	editable="false"/>                                                           
		<item name="性别"    	id="gender"      type="hidden"	editable="false"/>   
		<item name="性别"    	id="gender_name" type="text"	editable="false"/>  		
		<item name="证件类型"	id="cert_type"   type="hidden"	editable="false"/>   
