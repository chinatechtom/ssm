<?xml version="1.0" encoding="gbk" ?>
<function id="32013" name="收入分析表" >
  <input>	
	<item type="select" id="institution_id"     name="营业部名称"   	source="95000008" />
	<item type="text"   id="cust_code"	        name="客户代码" />
	<item type="date" 	id="begin_date" 		name="起始时间" 		default="last"/>
	<item type="date" 	id="end_date" 			name="截止时间" 		default="now"/>
  </input>
  <service type="procedure" id="p_business_report_32013_query" process="911" style="77">
	<param id="user_id" 			type="integer"   way="in"/>
	<param id="institution_id" 		type="integer"	way="in"/>
	<param id="cust_code"			type="string"	way="in"/>
	<param id="begin_date" 			type="datetime"	way="in"/>	
	<param id="end_date" 			type="datetime"	way="in"/>
  </service>
  <output export="true">    
  	<column type="string" id="c1"   name="营业部名称"  />
    <column type="string" id="c2"   name="客户信用账户号"  />
	<column type="number" id="c3"   name="客户交易量"  	precision="2"/>
    <column type="number" id="c4"   name="佣金"  		precision="2"/>
    <column type="number" id="c5"   name="利息费用收入"  	precision="2"/>
    <column type="number" id="c6"   name="小计"  		precision="2"/>
    <column type="string" id="c7"   name="起始时间"  />
    <column type="string" id="c8"   name="结束时间"  />
    </output>
</function>