<?xml version="1.0" encoding="gbk" ?>
<function id="67120111" name="解冻客户资金信息同步">
	<description>确实同步客户解冻资金信息吗?</description>
	<input>
		<item type="hidden" id="sbranch_code0" name="开户营业部代码"/>
		<item type="hidden" id="sbranch_code1" name="发生营业部代码"/>
		<item type="hidden" id="scust_no" name="客户号"/>
		<item type="hidden" id="scurrency_type" name="货币类型"/>
		<item type="hidden" id="lsafe_level" name="标志"/>
		<item type="hidden" id="damt9" name="临时解冻金额"/>
		<item type="hidden" id="snote" name="备注"/>
		<item type="hidden" id="sstatus0" name="委托方式"/>
	</input>    
	<service type="drtp" id="120111" process="711" style="77">
		<param type="string" id="sbranch_code0" />
		<param type="string" id="sbranch_code1" />
		<param type="string" id="scust_no" />
		<param type="string" id="scurrency_type"/>
		<param type="string" id="scurrency_type2" default="01"/>
		<param type="string" id="lsafe_level" default="1"/>
		<param type="number" id="damt9"	/>
		<param type="string" id="snote"	/>
		<param type="string" id="semp" default="SYSTEM"/>
		<param type="string" id="lwithdraw_flag" default="1"/>
		<param type="string" id="sstatus0" default="d"/>
	</service>
	<output target="69120111">
		<column name="发生日期"	id="tradedate"   type="string"  field="sdate0"/>		
		<column name="流水号"	id="serial_no"  type="string"  field="sserial0"/>
		<column name="客户号"	id="cust_code"  type="string"  field="scust_no"/>
		<column name="备注"		id="sbond_no"  type="string"  field="snote"/>
		<column name="后临时解冻金额"	id="unfrn_amt"  type="number"  field="damt1"/>
	</output>
</function>
