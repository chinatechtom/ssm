<?xml version="1.0" encoding="gbk" ?>
<function id="30304" name="查看某个证券的风险级别" >
	<input>
		<item type="hidden" id="product_id" name="产品id" />		
    </input>
	<service type="procedure" id="p_product_risk_level_02_query"  implement="p_product_risk_level_02_query304"  process="911" style="77"> 
		<param id="user_id"    type="integer" way="in"/>
		<param id="product_id" type="integer" way="in"/>
	</service>
	<output export="true" detail="true">
		<column type="number" id="product_id" name="证券id" display="false" />
		<column type="string" id="product_code" name="证券代码" />
		<column type="string" id="product_name" name="证券名称" />
		<column type="number" id="product_risk_level_id" name="证券风险级别id" display="false" />
		<column type="string" id="product_risk_level_no" name="证券风险级别代码" />	
		<column type="string" id="product_risk_level_name" name="证券风险级别名称" />	
	</output>
</function>