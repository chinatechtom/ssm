<?xml version="1.0" encoding="gbk" ?>
<function id="31313" name="设置用户密码">
	<input>
		<item type="text"		id="user_code"	name="用户代码" editable="false"/>
		<item type="text"		id="user_name"	name="用户名称" editable="false"/>
		<item type="password"	id="password"	name="用户密码" nullable="false" complexity="true" minlen="8"/>
		<item type="password"	id="password2"	name="密码校验" nullable="false" complexity="true" minlen="8"/>
		<item type="hidden" id="func_id" 							 action="8801" 					default="940000017"/>
    <item type="hidden" 	id="func_need_check" 		 name="是否支持复核" 	  default="0"/>		
		<group name="复核">   
    <item type="text" 		id="chk_user_code" 				name="复核人员" filter="func_need_check=1"/>
    <item type="password" id="CHKUSER_PWD" 					name="密码" 	filter="func_need_check=1"/>
    </group>		
	</input>
	<service type="procedure" id="p_password_set" implement="p_password_set31313"  process="811" style="1">
		<param id="user_id"		type="integer"  way="in"/>
		<param id="chk_user_code"				type="string"  way="in"/>
		<param id="CHKUSER_PWD"	  			type="string"  way="in"/>
		<param id="func_id"							type="integer" way="in"/>
		<param id="user_code"	type="string"   way="in"/>
		<param id="user_name"	type="string"   way="in"/>
		<param id="password"	type="string"   way="in"/>
		<param id="password2"	type="string"   way="in"/>
	</service>
	<output/>
</function>