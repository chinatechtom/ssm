<?xml version="1.0" encoding="gbk" ?>
<function id="70160124" name="本期约定购回证券交易明细" >
  <input>	
	<item type="text"   id="cust_id" 		name="客户ID" /> 
	<item type="date" 	id="begin_date"  name="起始日期" default="last" nullable="false"/>
	<item type="date" 	id="end_date" 	 name="结束日期" default="now"  nullable="false"/>
  </input>
  <service type="procedure" id="P70160124" process="922" style="77">
	<param id="user_id" 	type="integer"/>
   	<param id="cust_id" 	type="integer"/>
	<param id="begin_date" 	        type="string"/>
	<param id="end_date" 	        type="string"/>
  </service>
  <output >
    <column type="number" id="cust_id"      name="客户ID"     display="false"/>
    <column type="string" id="sbond_no"   	name="交易协议书编号"   />
    <column type="string" id="trade_date"      name="交易日期" />
	<column type="string" id="product_code"   	name="证券代码"     />
    <column type="string" id="product_name"     name="证券名称"     />
	<column type="number" id="done_qty"   	name="标的证券数量" total="true" precision="2"/>
	<column type="string" id="bs"   		name="买卖方向"      />
    <column type="number" id="done_amt"   	name="交易金额"   total="true"   precision="2"/>
    <column type="string" id="trade_type"   	name="交易类型"        />
	<column type="number" id="fees"   	name="佣金及其它交易费用"   total="true"  precision="2"/>
  </output>
</function>