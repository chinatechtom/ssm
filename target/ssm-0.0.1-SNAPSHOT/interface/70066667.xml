<?xml version="1.0" encoding="gbk" ?>
<function id="70066667" name="调整客户活期利率合约详情" >
  <input>
	<group name="合同信息" >
		<item type="hidden" id="cust_id" name="客户id" />
		<item type="hidden" id="order_id" 	name="合约id"	action="70044567"/>
		<item type="hidden" id="bond_no" name="合同编号" editable="false"/>
		<item type="text" id="institution_name" name="营业部名称" editable="false"/>
		<item type="text" id="institution_code" name="营业部代码" editable="false"/> 
		<item type="text" id="cust_name" name="客户姓名" editable="false"/>
		<item type="text" id="cust_code" name="客户代码" editable="false"/> 	
	</group>
	<group name="合约信息">
		<item type="select" id="hq_code" name="活期利率代码" source="97000027"  editable="false" action="70040301"/>
		<item type="text" id="hq_rate" name="活期年利率(%)" editable="false"/>
		<item type="text" id="hq_code2" name="原活期利率代码" editable="false"/>
	</group>
	<item type="hidden" id="date"		name="打印日期"/><!--从1888.xml获得的打印日期-->
	<item type="hidden" id="user_code"	name="打印柜员代码"/><!--从1888.xml获得的打印柜员代码-->
	<item type="hidden" id="user_name"	name="打印柜员名称"/><!--从1888.xml获得的打印柜员名称-->
	<item type="hidden" id="company"    name="券商名称"/><!--从1888.xml获得的券商名称-->			
	<group name="意见">
		<item type="textarea" id ="remark" 			 name="申请备注"/>
		<item type="textarea" id ="manage_opinion"	 name="处理意见"/>
		<item type="textarea" id ="approver_opinion" name="审核意见"/>
		<item type="textarea" id ="assessor_opinion" name="审批意见"/>
	</group>
  </input>
</function>