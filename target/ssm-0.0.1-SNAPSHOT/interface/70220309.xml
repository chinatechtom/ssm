<?xml version="1.0" encoding="gbk" ?>
<function id="70220309" name="证券公司约定购回式证券交易" >
  <input>
	<item type="year" id="year" name="查询年份" nullable="false" default="now"/>
	<item type="month" id="month" name="查询月份" nullable="false" default="last"/>
  </input>
  <service type="procedure" id="p70220309" process="922" style="77">
	<param type="integer" id="user_id"/>
	<param type="string"  id="year"/>
	<param type="string"  id="month"/>
  </service>
  <output  export="true">  
	<column type="string" id="company"   name="公司" />
	<column type="number" id="total_num"   name="签约客户数" />
	<column type="number" id="org_num"   name="其中：机构户" precision="0"/>
	<column type="number" id="per_num"   name="其中：个人户" precision="0"/>	
	<column type="number" id="credit_num"   name="授信额度（亿元）" precision="4"/>
	<column type="number" id="last_first_amt"   name="上月末待购回交易金额（万元）" precision="4"/>
	<column type="number" id="curr_first_amt"   name="本月初始交易金额（万元）" precision="4"/>
	<column type="number" id="curr_bb_amt"   name="本月购回交易金额（万元）" precision="4"/>	
	<column type="number" id="curr_unbb_amt"   name="本月末待购回交易金额（万元）" precision="4"/>
	<column type="number" id="special_account_balance"   name="月末专户市值余额（万元）" precision="4"/>
	<column type="number" id="total_gua_rate"   name="整体履约保障比例" precision="2"/>
	<column type="number" id="interest_balance"   name="本年累计息费收入（万元）" precision="4"/>	
	<column type="number" id="interest"   name="其中：利息收入（万元）" precision="4"/>
	<column type="number" id="comm_fee"   name="其中：佣金收入（万元）" precision="4"/>
	<column type="string" id="hg_rate_band"   name="1年期约定购回的利率区间（%）" precision="2"/>
	<column type="number" id="violate_num"   name="本期客户违约数量" precision="0"/>	
	<column type="number" id="curr_fault_num"   name="本期系统故障次数" precision="0"/>
	<column type="number" id="curr_complain_num"   name="本期客户投诉数量" precision="0"/>
	<column type="string" id="curr_others"   name="参与投资者的账户资金要求、开户期限要求及其它适当性要求" />
  </output>
</function>