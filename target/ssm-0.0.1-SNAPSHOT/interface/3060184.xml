<?xml version="1.0" encoding="gbk" ?>
<function id="3060184" name="客户申报明细查询" >
 <input>
	<item type="hidden" id="cust_id"			name="客户ID" action="306016911"/>
	<item type="text"	id="cust_name"			name="客户姓名/名称" editable="false"/>
	<item type="text"	id="commness_cust_code" name="普通资金账户"	editable="false"/>
	<item type="text"	id="cert_no"			name="证件号码" editable="false"/>
	<item type="select" id="cert_type"			name="证件类型" source="029" editable="false"/>
	<item type="text"	id="security_code_sh"	name="沪市A股证券账户" editable="false"/>
	<item type="text"	id="security_code_sz"	name="深市A股证券账户" editable="false"/>
	<group name="本人接受的投资者教育(A、B项为必须通过项)">
		<item type="textarea" id="content_e" name="教育项内容" default="A:面对面讲解合同、风险揭示书及交易规则;
			B:融资融券投资者教育测试并通过测试;
			C:培训讲座、资料派发、宣传栏、视频、网站等宣传及其它教育方式" editable="false" />
		<item type="text" id="accept_edu" name="教育项(分号分隔)" editable="false"/>
	</group>
	<group name="持有关联证券账户">
		<item type="text" id="hold_rela_account_name"		name="持有关联账户名称" editable="false"/>
		<item type="text" id="hold_rela_commness_cust_code" name="持有关联普通资金账户" editable="false"/>
	</group>
	<group name="关联人证券账户">
		<item type="text" id="rela_account_name"	   name="关联人账户名称" editable="false"/>
		<item type="text" id="rela_commness_cust_code" name="关联人普通资金账号" editable="false"/>
	</group>
	<group name="海通证券大股东(持股5%以上)">
		<item type="text" id="hold_product_amt" name="持有海通证券股数(股)" editable="false"/>
	</group>
	<group name="海通证券关联人">
		<item type="text" id="rela_relation" name="与海通证券的关系" editable="false"/>
	</group>
	<group name="一致行动人">
		<item type="text" id="fit_action_name" name="一致行动人"	editable="false"/>
	</group>
	<group name="上市公司董监高、大股东(持股5%以上)、关联人(职位以逗号分隔)">
		<item type="text" id="listed_company_name"			name="上市公司名称" editable="false"/>
		<item type="text" id="listed_company_product_code"	name="上市公司股份代码" editable="false"/>
		<item type="textarea" id="content_pos" name="职位内容" default="0:董事;1:监事;2:高级管理人员;3:大股东" editable="false" />
		<item type="text" id="position" name="职位(分号分隔)"	editable="false"/>
		<item type="text" id="hold_stock_ratio"				name="持股比例" editable="false"/>
		<item type="select" id="if_relation"				name="是否为关联人" source="611"  editable="false" nullable="false" default="0"/>
		<item type="text" id="listed_company_rela_relation"	name="关联关系" editable="false"/>
	</group>
	<group name="与其他证券公司签订融资融券合同">
		<item type="text" id="other_bond_sign_company" name="签订融资融券合同的公司" editable="false"/>
	</group>
	<group name="已在其他证券公司开立信用资金账户">
		<item type="text" id="other_credit_security_code_sh" name="沪市信用证券账户" editable="false"/>
		<item type="text" id="other_credit_security_code_sz" name="深市信用证券账户" editable="false"/>
	</group>
	<group name="持有限售流通股或解除限售流通股">
		<item type="grid"   id="cust_id" name="持有限售流通股或解除限售流通股" source="306016912"/>
	</group>
  </input>
  <output/>
</function>