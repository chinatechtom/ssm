	<service type="procedure" id="P60010488" process="833" style="1">
		<param type="integer" id="user_id" way="in"/>
		<param type="integer" id="cust_id" way="in"/>
		<param type="integer" id="sbond_id" way="in"/>
		<param type="string" id="market_code1" way="in"/>
		<param type="string" id="product_code1" way="in"/>
		<param type="integer" id="stock_vol1" way="in" default="0"/>
		<param id="remark1" type="string" way="in"/>
	</service>
	<output/>
