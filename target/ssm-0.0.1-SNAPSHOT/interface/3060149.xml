<?xml version="1.0" encoding="gbk" ?>
<function id="3060149" name="证券负债利率查询" >
  <input>
	<item type="text"   id="product_code"     name="证券代码"/>
    <item type="text"   id="product_name"     name="证券名称"/>
	<item type="select" id="market_code"      name="市场选择"	        source="039"/>
	<item type="text"   id="beta_value_range" name="证券利率范围(%)"	default="0"/>
  </input>
  <service type="procedure" id="p_stock_beta_value_query" implement="p_stock_beta_value_query3060149"  process="911" style="77"> 
    <param id="user_id"				type="integer" way="in"/> 
	<param id="product_code"		type="string"  way="in"/>
    <param id="product_name"	    type="string"  way="in"/>
    <param id="market_code"	        type="string"  way="in"/>
	<param id="beta_value_range"	type="double"  way="in"/>	
  </service>
  <output >	
    <column type="string" id="market_code"  name="市场"     />
	<column type="string" id="PRODUCT_TYPE" name="证券类型" />
	<column type="string" id="product_code" name="证券代码" />
	<column type="string" id="product_name" name="证券名称"	 />
	<column type="string" id="beta_value"   name="证券负债利率" precision="2"/>
	<button type="edit"   id="import"       name="证券负债利率导入" action="306014901"  multiple="-1"/>
  </output>
</function>