<?xml version="1.0" encoding="gbk" ?>
<function id="3042208" name="客户评级因子浏览_海通" >	
  <input> 
		<item type="hidden"	id="cust_id"	 name="客户id" />
		<item type="hidden"	id="cust_code"	 name="客户代码" />
		<item type="hidden"	id="versionid"	 name="评级因子编号" />
		<item type="hidden"	id="credit_type" name="业务类别" />
  </input>
  <service type="procedure" id="p_cust_ratingfactor_query_ht"  process="911" style="77"> 
		<param id="user_id" type="integer"/>		
		<param id="cust_id" type="integer"/>
		<param id="cust_code" type="string"/>
		<param id="versionid" type="integer"/>
		<param id="credit_type" type="string"/>
  </service>
  <output>
        <column type="number" id="cust_id" name="客户id"	display="false"/>
		<column type="string" id="cust_name"  name="客户姓名" 	display="false"/>
		<column type="string" id="cust_code" name="客户代码"	display="false"/>
		<column type="string" id="ratingfactor_type_name"  name="评级因子类型名称" display="false"/>
		<column type="number" id="ratingfactor_sort" name="评级因子类别" display="false"/>
		<column type="string" id="ratingfactor_name" name="评级因子名称"/>
		<column type="number" id="data_code"  name="数据编码" display="false"/>
		<column type="string" id="data_content"  name="客户数据内容"/>
		<column type="string" id="into_data"  name="客户实际信息" />
		<column type="number" id="score"      name="分值"  precision="2" />
  </output>		
</function>