	<item type="hidden" id="cust_id"   name="客户号"/>
	<item type="hidden" id="cust_code" name="客户代码" />
	<item type="hidden" id="cust_name" name="客户姓名" />
	<item type="hidden" id="cert_no"   name="证件号码" />
  </input>
  <service type="procedure" id="p_cust_info_query" process="911" style="77" implement="p_cust_info_query30103">
	<param id="user_id" type="integer" way="in" />
	<param id="cust_id" type="integer" way="in" />
	<param id="acc_code"/>
	<param id="cust_name"/>
	<param id="cert_no" />
  </service>
  <output detail="true">    
