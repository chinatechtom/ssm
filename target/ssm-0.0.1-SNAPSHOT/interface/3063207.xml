<?xml version="1.0" encoding="gbk" ?>
<function id="3063207" name="授信额度查询" >
	<input export="true">
		<item type="select" id="institution_id" name="营业部名称" source="95000008"/>
		<item type="hidden" id="cust_id" name="客户id"/>
		<item type="text" 	id="cust_code" name="客户代码"/>
		<item type="hidden" id="bond_id" name="合同id"/>
		<group name="分页查询" page="true">		
			<item type="text"   id="page_num"       name="页码"     integer="true" default="1"/>
			<item type="select" id="rows_num"       name="每页行数" integer="true" source="95000012" nullable="false"/>
		</group>
	</input>
	<service type="procedure" id="p_cust_credit_num2_query" implement="p_cust_credit_num2_query3063207" process="911" style="77">
		<param type="integer" id="user_id"/>	
		<param type="integer"   id="page_num"/>
		<param type="integer"   id="rows_num"/>			
		<param type="integer" id="institution_id"/>	
		<param type="integer" id="cust_id"/>
		<param type="string"  id="cust_code"/>	
		<param type="integer" id="bond_id"/>
	</service>
	<output>
		<column type="number" id="bond_product_id2"   name="融资产品ID" display="false"/>
		<column type="string" id="bond_product_code2" name="融资产品代码"/>
		<column type="string" id="bond_product_name2" name="融资产品名称"/>
		<column type="number" id="bond_product_id3"   name="融券产品ID" display="false"/>
		<column type="string" id="bond_product_code3" name="融券产品代码"/>
		<column type="string" id="bond_product_name3" name="融券产品名称"/>
		<column type="number" id="bond_credit_limit"  name="授信额度" precision="0"/> 
		<column type="number" id="bond_financing_limit" name="融资上限" precision="0"/> 
		<column type="number" id="bond_bstock_limit"  name="融券上限" precision="0"/>
		<column type="string" id="bond_credit_limit_c"    name="授信额度中文"/>
		<column type="string" id="bond_financing_limit_c" name="融资上限中文"/>
		<column type="string" id="bond_bstock_limit_c"    name="融券上限中文"/> 	
		<column type="number" id="bond_days"              name="申请天数" precision="0"/>	
		<column type="string" id="begin_time"             name="起始日期" />	
		<column type="string" id="return_time"            name="结束日期" />	
	</output>
</function>