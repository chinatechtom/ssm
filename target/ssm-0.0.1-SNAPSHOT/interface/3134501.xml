		<item type="hidden" id="sno"		name="系统流水号"/>
		<item type="hidden" id="operdate"	name="操作时间"/>
	</input>
	<service type="procedure" id="p_system_log_detail_his" imlement="p_system_log_detail_his3134501" process="955" style="77">
