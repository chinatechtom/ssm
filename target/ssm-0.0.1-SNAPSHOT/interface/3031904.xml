		<item type="select" id="institution_id" 	name="营业部名称" source="95000008" nullable="false"/>
		<item type="select" id="type" 	name="融资融券" source="500"/>
		<item type="select" id="if_stop" 	name="开启暂停" source="501" nullable="false"/>
	</input>
    <service type="procedure" id="p_start_or_stop_institution_rzrq" process="911" style="1"> 
		<param id="user_id"	type="integer" way="in"/>
		<param id="institution_id"	type="string" way="in"/>
		<param id="type"	type="string" way="in"/>
		<param id="if_stop"	type="string" way="in"/>
	</service>
	<output target="@func_no">
		<column type="string" id="avai_data_item"	 name="有效数据项"/>
