<?xml version="1.0" encoding="gbk" ?>
<function id="36000087" name="8.3.4全部客户资产盈亏汇总" >  
  <input>
   <item type="select" id="institution_id" name="营业部名称"  source="95000008"/>
  <item type="date"   id="date"           name="日期"  default="now"/>
  </input>
   <service type="procedure" id="P36000087" process="911" style="77">
   <param id="user_id" type="integer" way="in" />    
   <param id="date"    type="string" way="in" />
   <param id="institution_id" type="integer" way="in" />
  </service> 
  <output  print="true">
    <column type="number" id="id"                 name="序号"/>
    <column type="string" id="institution_name"   name="营业部"/> 
    <column type="number" id="guarantee_amt"      name="保证金"  precision="2" total="true"/> 
    <group name="担保证券"> 
    <column type="number" id="gua_stock_cost"     name="成本"  precision="2" total="true"/>
    <column type="number" id="gua_market_value"   name="市值"  precision="2"  total="true"/>
    <column type="number" id="profit_loss"        name="浮动盈亏" precision="2" total="true"/>
    </group>
    <column type="number" id="fund_amt"           name="资产总额" precision="2" total="true"/>
    <column type="number" id="debt_amt"           name="负债总额" precision="2" total="true"/>
    <column type="number" id="violation_amt"      name="违约金额" precision="2" total="true"/>
  </output>
</function>