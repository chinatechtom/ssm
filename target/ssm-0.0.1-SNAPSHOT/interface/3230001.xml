    	<item type="grid" id="cust_id,sdate,edate" 	source="3191501" name="查询负债资金流水"/>
    </group>
    <group name="查询资金流水">
    	<item type="grid" id="cust_id,sdate,edate" 	source="3011101" name="查询资金流水"/>
    </group>
	</input>
	<output/>
</function>
	