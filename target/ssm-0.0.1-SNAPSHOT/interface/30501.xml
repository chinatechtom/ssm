<?xml version="1.0" encoding="gbk" ?>
<function id="30501" name="信用产品浏览" >	
  <input> 
		<item type="hidden"	id="product_id"		 name="产品id" />
		<item type="text"	id="product_code"	 name="产品代码" />
		<item type="text"	id="product_name"	 name="产品名称" />
  </input>
  <service type="procedure" id="p_credit_product_brow" implement="p_credit_product_brow30501"  process="911" style="77"> 
		<param id="user_id"			type="integer" way="in"/>
		<param id="product_id"		type="integer" way="in"/>
		<param id="product_code"	type="string"  way="in"/>
		<param id="product_name"	type="string"  way="in"/>
  </service>
  <output>
  	<column type="number" id="product_id"						 name="产品id"			display="false"/>
	<column type="string" id="product_code"						 name="产品代码"/>
	<column type="string" id="product_name"						 name="产品名称"/>
	<column type="string" id="product_sort"						 name="产品类型1"			display="false"/>
	<column type="string" id="product_sort_name"				 name="产品类型"/>
	<column type="number" id="financing_interest_rate"			 name="融资利息率"		precision="4"/>
	<column type="number" id="borrow_stock_interest_rate"		 name="融券利息率"		precision="4"/>
	<column type="number" id="fix_borrow_fund_rate"				 name="融资固定额度费率"	precision="4"/>
	<column type="number" id="fix_borrow_stock_rate"			 name="融券固定额度费率"	precision="4"/>
	<column type="number" id="takeup_borrow_fund_rate"			 name="融资占用额度费率"	precision="4"/>
	<column type="number" id="takeup_borrow_stock_rate"			 name="融券占用额度费率"	precision="4"/>
	<column type="number" id="before_return_commission_charge"	 name="融资提前还贷手续费"	precision="4"/>
	<column type="number" id="bs_bef_return_commission_charge"	 name="融券提前还贷手续费"	precision="4"/>
	<column type="number" id="punish_interest_rate"				 name="融资合约罚息率"			precision="4"/>
	<column type="number" id="bs_punish_interest_rate"		     name="融券合约罚息率"			precision="4"/>
	<column type="number" id="financing_bill_punish_interate"	 name="融资仓单罚息率"			precision="4"/>
	<column type="number" id="bs_bill_punish_interate"           name="融券仓单罚息率"			precision="4"/>
	<column type="number" id="bad_punish_interest_rate"			 name="坏帐罚息率"		precision="4"/>
 </output>		
</function>