<?xml version="1.0" encoding="gbk" ?>
<function id="31901" name="信用客户资料查询">
	<input export="true">
		<item type="select" id="institution_id" name="营业部名称" source="95000008"/>
		<item type="hidden" id="cust_id"        name="客户id"/>
		<item type="text"   id="cust_code"      name="客户代码"/>
		<item type="text"   id="cust_name"      name="客户姓名"/>
		<item type="text"   id="cert_no"        name="证件号码"/>
		<group name="分页查询" page="true">		
			<item type="text"   id="page_num"       name="页码"     integer="true" default="1"/>
			<item type="select" id="rows_num"       name="每页行数" integer="true" source="95000012" nullable="false"/>
		</group>
	</input>
	<service type="procedure" id="p_cust_credit_info_query" process="911" style="77" implement="p_cust_credit_info_query31901">
		<param type="integer"   id="user_id"/>
		<param type="integer"   id="page_num"/>
		<param type="integer"   id="rows_num"/>
		<param type="integer"   id="institution_id"/>
		<param type="integer"   id="cust_id"/>
		<param type="string"    id="cust_code"/>
		<param type="string"    id="cust_name"/>
		<param type="string"    id="cert_no"/>
	</service>
	<output>
		<column type="string"	id="avai_data_item"			name="有效数据项"	display="false"/>
		<column type="string"	id="institution_name"		name="营业部名称"/>
		<column type="string"	id="institution_code"		name="营业部代码"/>
		<column type="string"	id="cust_code"				name="客户代码"/>
		<column type="string"	id="cust_name"				name="客户姓名"/>
		<column type="string"	id="credit_level_code"		name="信用级别代码"/>
		<column type="string"	id="credit_level_name"		name="信用级别名称"/>
		<column type="string"	id="pc_mode"				name="平仓标志1"	display="false"/>
		<column type="string"	id="pc_mode_name"			name="平仓标志"/>
		<column type="string"	id="credit_usemode"			name="信用上限使用模式1"	display="false"/>
		<column type="string"	id="credit_usemode_name"	name="信用上限使用模式"	display="false"/>
		<column type="string"	id="if_blacklist"			name="是否黑名单1"			display="false"/>
		<column type="string"	id="if_blacklist_name"		name="是否黑名单"/>  
		<column type="string"	id="hk_mode"				name="还款模式1"			display="false"/> 
		<column type="string"	id="hk_mode_name"			name="还款模式"/>
		<column type="string"	id="bond_no"				name="合同编号"/>
		<column type="string"	id="bond_status"			name="合同状态1"			display="false"/>
		<column type="string"	id="bond_satus_name"		name="合同状态"/>
		<column type="string"	id="begin_date"				name="起始日期"/>
		<column type="string"	id="end_date"				name="终止日期"/>
		<column type="string"	id="product_code1"			name="融资产品"/>
		<column type="string"	id="product_code2"			name="融券产品"/>
		<column type="number"	id="credit_num2"			name="信用上限"	precision="2" total="true"/>
		<column type="number"	id="credit_num"				name="授信额度"	total="true"/>
		<column type="number"	id="financing_amt"			name="融资上限"	total="true"/>
		<column type="number"	id="bstock_amt"				name="融券上限"	total="true"/>
		<column type="number"	id="score"					name="核定信用评分"	precision="2" total="true"/>
		<column type="number"	id="cust_id"				name="客户ID"			display="false"/> 
		<column type="string"	id="if_risk"				name="是否风险客户"/>
		<column type="string"	id="pvt_ctl_stauts"			name="私用额度控制状态"/>
		<column type="number"	id="pvt_fin_lmt_modulus"	name="私用转融资比例系数"	precision="2"/>
		<column type="number"	id="pvt_bs_lmt_modulus"		name="私用转融券比例系数"	precision="2"/>
		<column type="number"	id="pvt_crd_lmt_modulus"	name="私用转融通授信比例系数"	precision="2"/>
		<column type="string"	id="pvt_fin_product_code"	name="私用转融资信用产品"/>
		<column type="string"	id="pvt_bs_product_code"	name="私用转融券信用产品"/>
		<button type="edit"		id="edit" name="同步" action="81413105"  multiple="0"/>     
	</output>
</function>