<?xml version="1.0" encoding="gbk" ?>
<function id="340671" name="特殊客户公允价格处理-查询">
	<input > 	
		<item type="text"   id="cust_code" name="客户号"/> 
    <item type="select"   id="market_code" name="市场代码" source="039"/> 
		<item type="text"   id="stock_code" name="证券代码" /> 
		<item type="select" id="status" name="记录状态" source="810" default="1"/>
		<item type="date"   id="begin_date" name="起始日期" default="last"/> 
		<item type="date"   id="end_date" name="结束日期" default="now"/>  
		 
	</input>
	<service type="procedure" id="P17000070"  process="911" style="77">
		<param id="user_id" type="integer"/>
		<param id="cust_code" type="string"/> 
		<param id="market_code" type="string"/> 
		<param id="stock_code" type="string"/>  
		<param id="status" type="string"/> 
		<param id="begin_date" type="string"/> 
		<param id="end_date" type="string"/>
  </service> 
	<output>  
		<column type="string"	id="record_id"    name="记录ID" display="false"/>
		<column type="string"	id="date"         name="新增日期"	/> 
		<column type="string"	id="time"         name="更新时间"	/> 
		<column type="string"	id="cust_code"    name="信用资金账号"	/>
		<column type="string"	id="cust_name"    name="客户名称"	/>
		<column type="string"	id="status"       name="记录状态" display="false"	/>
		<column type="string"	id="status_name"  name="记录状态"	/>
		<column type="string"	id="market_code_name"  name="市场"/>
		<column type="string"	id="market_code"  name="市场代码" display="false"/>
		<column type="string"	id="product_code"   name="证券代码"	/>
		<column type="string"	id="product_name"   name="证券名称"	/> 
		<column type="string"	id="remark"         name="备注"	/> 
		<column type="string"	id="user_code"      name="操作员"	/>  
		<column type="string"	id="last_price"      name="公允价格/市价"	/>  
		<column type="string"	id="last_price_bak"      name="市价"	/>  
		<button type="set"	id="add"     name="新增记录" action="340672" multiple="-1"/> 
		<button type="set"	id="modify"  name="修改状态" action="340673" multiple="0"/>  
		</output>  
</function>
		
		
		