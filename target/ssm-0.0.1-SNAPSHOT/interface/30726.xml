			<item type="text"	id="margin_remain"		name="剩余保证金"	editable="false"/>
			<item type="text"	id="margin_total"		name="总保证金"	editable="false"/>
			<item type="grid"	id="financing_asset"	name="融资明细"	source="3190501"/>
			<item type="grid"	id="bstock_asset"	name="融券明细"	source="3190601"/>
		</group>
		<group name="平仓方案">
			<item type="select"	id="pc_mode"	name="平仓顺序"	source="121" editable="false"/> 			
			<item type="text"	id="pc_reason_name"	name="平仓原因" editable="false"/> 
			<item type="hidden"	id="pc_reason"	name="平仓原因代码"	editable="false"/> 
			<item type="grid"	id="sell_stock"	name="应平证券"	source="30730"/> 
			<item type="grid"	id="buy_stock"	name="应买证券"	source="30731"/> 
		</group>
		<group name="审批结果">
			<item type="textarea" id="approve_idea" name="审批意见"	maxlen="256"/>
			<item type="text"	id="bc_begin_date"	name="补仓起始日期" editable="false"/>
			<item type="text"	id="bc_end_date"	name="补仓截止日期" editable="false"/>
		</group>
		<item type="hidden"	id="product_id"	name="产品id"/> 
		<item type="hidden"	id="bond_id"	name="合同id"/> 
		<!--<item type="hidden"	id="guarantee_rate_id_ctl"	name="补仓线id1"	display="false"/> 
		<item type="hidden"	id="pc_reason"	name="平仓原因1"/>--> 			
