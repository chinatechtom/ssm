	<service type="procedure" id="P70770008" implement="P70770008_70770008" process="922" style="77">
		<param id="user_id" type="integer"/>
		<param id="insitution_id" type="integer"/>	
		<param id="cust_code" type="string"/>
		<param id="cust_name" type="string"/>
		<param id="cust_type" type="string"/>
		<param id="operation_type" type="string"/>
		<param id="page_num"  type="integer" way="in"/>
