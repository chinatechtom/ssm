<?xml version="1.0" encoding="gbk" ?>
<function id="30602" name="查看定单-担保率" >
	<input>    
	 <item type="hidden"  id="order_id" name="客户id"/> 
  </input>
  <service type="procedure" id="p_order_guarantee_query" process="911" style="77">
	<param id="user_id"				type="integer" way="in"/> 
	<param id="order_id"				type="ingeter" way="in"/>
  </service>
  <output detail="true">
	<column type="number" id="order_id" name="合约id"	display="false"/>
	<column type="number" id="guarantee_rate" name="担保率(%)" precision="4"/>
	<column type="number" id="approve_guarantee_rate" name="审批担保率(%)" precision="4"/>
  </output> 
</function>