<?xml version="1.0" encoding="gbk" ?>
<function id="30306" name="查看证券财务数据" >	
  <input>	
	<item type="hidden" id="product_id"   name="证券id" />
	<item type="text"   id="product_code" name="证券代码"/>
	<item type="text"   id="product_name" name="证券名称"/>
  </input>
  <service type="procedure" id="p_product_finance_data_query" implement="p_product_finance_data_query306"  process="911" style="77"> 
	<param id="user_id"       type="integer" way="in"/>
	<param id="product_id"    type="integer" way="in"/>
	<param id="product_code"  type="string"  way="in"/>
	<param id="product_name"  type="string"  way="in"/>
  </service>
  <output export="true">  	
	<column type="number"  id="product_id"                 name="证券id" 	display="false" />
    <column type="string"  id="market_code"                name="市场代码" display='false'/>
    <column type="string"  id="market_name"                name="市场" />
    <column type="string"  id="product_code"               name="证券代码" 	   />
    <column type="string"  id="product_name"               name="证券名称" 	  />
    <column type="string"  id="address"                    name="地点" 				display="false"/>
    <column type="string"  id="into_date"                  name="上市日期"      />
    <column type="string"  id="allot_date"                 name="分配日期"    display="false"/>
    <column type="number"  id="all_stock"                  name="总股本"        />
    <column type="number"  id="circulated_a"               name="流通A"         />
    <column type="number"  id="circulated_b"               name="流通B"        />
    <column type="number"  id="all_assets"                 name="总资产"       display="false" />	
    <column type="number"  id="capital_assets"             name="固定资产"     display="false" />	
    <column type="number"  id="current_assets"             name="流动资产"     display="false" />
    <column type="number"  id="clean_assets"               name="净资产"        display="false"/>	
    <column type="number"  id="all_liabilities"            name="总负债"        display="false"/>	
    <column type="number"  id="long_time_liabilities"      name="长期负债"      display="false"/>	
    <column type="number"  id="asset_liabilities"          name="资产负债"      display="false"/>	
    <column type="number"  id="current_liabilities"        name="流动负债"      display="false"/>
    <column type="number"  id="market_profit_rate"         name="市盈率"      precision="2"   display="false"/>
    <column type="number"  id="yield"                      name="收益率"      precision="2"   display="false"/>
    <column type="number"  id="metaphase_profit"           name="中期每股收益" precision="2"  display="false"/>
    <column type="number"  id="year_profit"                name="年度每股收益" precision="2"  display="false"/>
    <column type="number"  id="every_clean_assets"         name="每股净资产"   precision="2"  display="false"/>
    <column type="number"  id="every_accumulation_fund"    name="每股公积金"   precision="2"  display="false"/>
    <column type="number"  id="all_margin"                 name="利润总额"      display="false"/>	
    <column type="number"  id="clean_margin"               name="净利润"        display="false"/>	
    <column type="number"  id="no_allot_margin"            name="未分配利润"    display="false"/>	
    <column type="number"  id="send_off_10"                name="每10股送股"   precision="2" display="false"/>
    <column type="number"  id="allot_10"                   name="每10股配股"   precision="2" display="false"/>
    <column type="number"  id="allot_max_price"            name="配股最高价格"  precision="3" display="false"/>
    <column type="number"  id="allot_min_price"            name="配股最低价"   precision="3" display="false"/>
    <column type="number"  id="every_bonus"                name="每股红利"     precision="3"  display="false"/>
    <column type="number"  id="main_earning"               name="主营收入"      display="false"/>	
    <column type="number"  id="subordinate_earning"        name="非主营收入"    display="false"/>
    <column type="number"  id="stockholder_interests"      name="股东权益"     precision="2"   display="false"/>
    <column type="number"  id="stockholder_interests_rate" name="股东权益比"   precision="2"  display="false"/>
    <column type="number"  id="liquidity_rate"             name="流动比率"     precision="2" display="false"/>
    <column type="number"  id="fast_ratio"                 name="速动比率"     precision="2" display="false"/>
    <column type="number"  id="capital_accumulation_fund"  name="资本公积金"   display="false" />	
    <column type="number"  id="immateriality_pass_assets"  name="无形递延资产"  display="false"/>
	<button type="edit"    id="edit"	name="修改"	action="3030601" multiple="0"/>
	<button type="add"	   id="import"	name="导入财务数据"	action="3030603" multiple="-1"/>
	<button type="add"	   id="set"	name="同步所有证券模板"	action="80310428" multiple="-1"/>
	<button type="add"	   id="set"	name="推送股本信息至交易系统"	action="34034" multiple="-1"/>
  </output>
</function>