<?xml version="1.0" encoding="gbk" ?>
<function id="32217" name="用户分组查询">
	<input>
		<item type="text" 	id="group_code" 	name="组代码" />
		<item type="text" 	id="group_name" 	name="组名称" />		
		<item type="select"	id="group_type"		name="组类别"		source="386" />
		<item type="select" id="institution_id"	name="所属营业部"	source="95000008" />
		<item type="text"	id="group_member"	name="组成员"	/>
	</input>
	<service type="procedure" id="p_group_query" process="911" style="77">
		<param id="user_id"			type="integer"	/>
		<param id="group_code"		type="string"	/>
		<param id="group_name"		type="string"	/>
		<param id="group_type"		type="integer"	/>
		<param id="institution_id" 	type="integer"	/>
		<param id="group_member"	type="string"	/>
	</service>
	<output export="true">
		<column type="number" 	id="group_id"			name="组ID"		display="false" />
		<column type="string" 	id="group_code"			name="组代码"	/>
    	<column type="string" 	id="group_name"			name="组名称"	/>
		<column type="string" 	id="group_type"			name="组类别"	display="false"	/>
		<column type="string" 	id="group_type_name"	name="组类别"	/>
		<column type="number" 	id="institution_id"		name="营业部ID"	display="false" />
		<column type="string" 	id="institution_name"	name="营业部名称"	/>
		<column type="string" 	id="group_desc"			name="组描述"	/>
		<column type="string"	id="group_member"		name="组成员"	display="false" />		
		<button type="edit"		id="create_group"		name="新建组" 	action="3221701"	multiple="-1"	/>
		<button	type="view"		id="show_group_member"	name="查询成员"	action="3221703"	multiple="0"	/>
		<button type="edit"		id="modify_group"		name="修改组"	action="3221702"	multiple="0"	/>		
	</output>
</function>
		
		