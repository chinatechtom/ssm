<?xml version="1.0" encoding="gbk" ?>
<function id="36200055" name="6.2.1客户资产负债情况表" >  
  <input>
    <item type="select" id="institution_id" name="营业部名称" source="95000008"/> 
   <item type="text"  id="cust_code"   name="客户代码" />
   <item type="hidden" id="acc_code"   name="信誉账号" />
   <item type="hidden" id="instruction_id" name="维持担保品比例" />
   <item type="text"  id="rate_low"   name="担保率下限" />
   <item type="text"  id="rate_high"   name="担保率上限" /> 
   <item type="date"  id="date"        name="查询日期" default="now"/>   
   <item type="select"    id="order_choice"          name="排序选择"  source="384"/>
  </input>
 <service type="procedure" id="P36200055" process="911" style="77">
   <param id="user_id"      type="integer"/>
   <param id="cust_code"    type="string" />
   <param id="acc_code"     type="string"/> 
   <param id="institution_id"  type="integer"/>
   <param id="rate_high" type="number"/>
   <param id="rate_low"  type="number"/>
   <param id="date"  type="string"/>
   <param id="order_choice"  type="integer"/>
  </service> 
  <output  print="true">
     <column type="number" id="id"              name="序号"/> 
     <column type="string" id="c1"              name="营业部"/> 
     <column type="string" id="c2"              name="客户代码"     />  
     <column type="string" id="c3"              name="客户名称" /> 
     <column type="string" id="c12"             name="信用级别"     />  
     <column type="number" id="c5"              name="总资产"       precision="2" total="true"/> 
     <column type="number" id="c6"              name="总负债"       precision="2" total="true"/> 
     <column type="number" id="c8"              name="资产折算价值" precision="2" total="true"/> 
     <column type="number" id="c9"              name="维持担保比例" precision="4" total="true"/> 
  </output>
</function>