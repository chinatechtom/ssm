<?xml version="1.0" encoding="gbk" ?>
<function id="60190103" name="质押品汇总查询" >
	<input>
		<item type="select"  id="lender_id"	name="融出方" source="97600001"/>
		<item type="select"  id="market_code"	name="市场"	   source="039"/>
		<item type="text"    id="product_code"	name="证券代码"/>
		<item type="text"    id="product_name"	name="证券名称"/>
	</input>
    <service type="procedure" id="p60190103"  process="933" style="77">
		<param id="user_id"	   	  type="integer" way="in"/>
		<param id="lender_id"     type="integer"  way="in"/> 
		<param id="market_code"   type="string"  way="in"/> 
		<param id="product_code"  type="string"  way="in"/>
		<param id="product_name"  type="string"  way="in"/>
    </service>
    <output export="true">
		<column type="number" id="lender_id"	name="融出方id"		display="false"/>
		<column type="string" id="lender_code"	name="融出方代码"/>
		<column type="string" id="lender_name"	name="融出方名称"/>
		<column type="string" id="acc_code"		name="融出方账户"/>
		<column type="string" id="seat_code"	name="融出方席位代码"/>
		<column type="string" id="market_code"	name="市场" display="false"/>
		<column type="string" id="market_code_name"	name="市场名称"/>
		<column type="number" id="product_id"	name="证券id"		display="false"/>
		<column type="string" id="product_code"	name="证券代码"/>
		<column type="string" id="product_name"	name="证券名称"/>
		<column type="string" id="product_type"	name="证券类型" display="false"/>
		<column type="string" id="product_type_name"	name="证券类型"/>
		<column type="number" id="curr_stk_vol"			name="证券数量"	/>
		<column type="number" id="market_value"			name="证券市值"  precision="2"/>
		<column type="number" id="circulated_a"			name="流通股"/>
		<column type="number" id="all_stock"			name="总股本"/>
		<column type="number" id="rate_cir"				name="占流通股比例（%）"	precision="4"/>
		<column type="number" id="rate_all" 			name="占总股本比例（%）"	precision="4"/>
    </output>
</function>