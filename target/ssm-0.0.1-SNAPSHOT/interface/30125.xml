<?xml version="1.0" encoding="gbk" ?>
<function id="30125" name="客户普通账户信息查询" >
  <input>	
	<item type="hidden" id="cust_id" name="客户号"/>
	<item type="text"   id="cust_code" name="客户代码" />
	<item type="text"   id="cust_name" name="客户姓名" />
	<item type="text"   id="pt_cust_code" name="普通资金账户" />
  </input>
  <service type="procedure" id="p_cust_acc_pt_query" process="911" style="77" implement="p_cust_acc_pt_query30125">
	<param id="user_id" type="integer"/>
	<param id="cust_id" type="integer"/>	
	<param id="cust_code"/>
	<param id="cust_name"/>
	<param id="pt_cust_code"/>
  </service>
  <output export="true">  
  	<column type="number" id="cust_id"       name="客户ID"  display="false" />
    <column type="string" id="cust_code"     name="信用资金账户"  />
    <column type="string" id="cust_name"     name="客户姓名"  />
    <column type="string" id="pt_acc_code"   name="普通资金账户"  />
    <column type="string" id="sh_acc_code"   name="普通沪A证券账户"/>
    <column type="string" id="sz_acc_code"   name="普通深A证券账户"  />
    <column type="string" id="open_date"     name="普通账户开户日期"  />
	</output>
</function>