<?xml version="1.0" encoding="gbk" ?>
<function id="70011303" name="设置定价/议价申请报价参数信息" >
	<input>
		<item type="text" 	id="product_code"	name="证券代码" nullable="false" minlen="6" maxlen="6" editable="false"/>
		<item type="select" id="market_code"	name="市场代码" source="039" nullable = "false" editable="false"/>
		<item type="select" id="busi_type"	name="业务类别"  source="743" editable="false" nullable = "false"/>		
		<item type="select" id="order_type"	name="指令类别"  source="739" editable="false" nullable="false"/>
		<item type="text" id="lower_limit"	name="期限下限(含)" editable="false"/>	
		<item type="text" id="upper_limit"	name="期限上限" editable="false"/>	
		<item type="text" id="hg_rate"	name="购回价格默认值(%)" nullable = "false"/>	
		<item type="text" id="hg_rate_lower"	name="购回价格下限(%)" nullable="false"/>	
		<item type="text" id="hg_rate_upper"	name="购回价格上限(%)" nullable="false"/>	
		<item type="text" id="app_price"	name="交易价格默认值" nullable = "false"/>	
		<item type="text" id="app_price_lower"	name="交易价格下限" nullable="false"/>	
		<item type="text" id="app_price_upper"	name="交易价格上限" nullable="false"/>	
		<item type="text" id="hg_convert_rate"	name="标的折扣率默认值(%)" nullable = "false"/>	
		<item type="text" id="convert_rate_lower"	name="标的折扣率下限(%)" nullable="false"/>	
		<item type="text" id="convert_rate_upper"	name="标的折扣率上限(%)" nullable="false"/>	
	</input>   
	<service type="procedure" id="p70011303"  process="822" style="1"> 
		<param id="user_id"			type="integer" way="in"/>
		<param id="product_code"	type="string" way="in"/>
		<param id="market_code"		type="string" way="in"/>
		<param id="busi_type"		type="string"  way="in"/> 
		<param id="order_type"		type="string"  way="in"/>
		<param id="lower_limit"		type="integer"  way="in"/> 
		<param id="upper_limit"		type="integer"  way="in"/>  	 
		<param id="hg_rate"		type="float"  way="in"/>  
		<param id="hg_rate_lower"	type="float"  way="in"/>  
		<param id="hg_rate_upper"		type="float"  way="in"/>
		<param id="app_price"			type="float"  way="in"/>
		<param id="app_price_lower"			type="float"  way="in"/>
		<param id="app_price_upper"	type="float"  way="in"/> 
		<param id="hg_convert_rate"		type="float"  way="in"/>
		<param id="convert_rate_lower"		type="float"  way="in"/>
		<param id="convert_rate_upper"	type="float"  way="in"/>
	</service> 
	<output/>  
</function>    