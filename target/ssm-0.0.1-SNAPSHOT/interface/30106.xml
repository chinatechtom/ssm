<?xml version="1.0" encoding="gbk" ?>
<function id="30106" name="机构账户资料" >
  <input>
	<item type="hidden" id="institution_id" name="机构id	" />
	<item type="hidden" id="acc_id" name="账户号" />
	<item type="text" id="acc_code" name="账户代码" />
  </input>
  <service type="procedure" id="p_institution_acc_query" process="911" style="77">
	<param id="user_id" type="integer"/>
	<param id="institution_id" type="integer"/>
	<param id="acc_id"/>
	<param id="acc_code"/>
  </service>
  <output  export="true">  
    <column type="number" id="acc_id"   name="账户号" display="false" />   
    <column type="number" id="institution_id"   name="机构id"  display="false" /> 
	<column type="string" id="acc_type"   name="账户类型"  />
    <column type="string" id="acc_sort"    name="账户类别"  />
    <column type="string" id="acc_code"   name="账户代码"  />    
	<column type="string" id="cert_type"    name="证件类型"  />	
    <column type="string" id="cert_no"   name="证件号码"  />
    <column type="string" id="estop_mark"   name="禁止标志"  />
    <column type="string" id="monit_mark"    name="监控标志"  />
    <column type="string" id="mostly_mark" name="主登录标志"  />
    <column type="string" id="acc_status"  name="账户状态"  />
	<column type="string" id="log_psw"  name="登陆密码"  display="false"/>	
    <column type="string" id="open_date"    name="开户日期"  />	    
    <column type="string" id="open_oper"  name="开户人员" />
    <column type="string" id="close_date"  name="销户日期" />
    <column type="string" id="close_oper"  name="销户人员" />  	
	<column type="number" id="user_id"  name="操作员id" display="false"/>
	<column type="string" id="time"  name="时间"  display="false"/>
   </output>
  </function>