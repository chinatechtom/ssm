<?xml version="1.0" encoding="gbk" ?>
<function id="36000039" name="4.1.2融资买入证券余额统计表" >
<input>
  <item type="date" id="date"          name="查询日期"             default="now"/>
  <item type="text" id="scale_rate"    name="市值占融资总规模比例" />
  <item type="text" id="fund_rate"     name="市值占净资本比例" /> 
  <item type="select"    id="order_choice"        name="排序选择"  source="381"/>
</input>
<service type="procedure" id="P36000039" process="911" style="77">
  <param id="user_id"    type="integer"/>
  <param id="date"       type="string"/>
  <param id="scale_rate" type="double"/>
  <param id="fund_rate"  type="double"/>
  <param id="order_choice" type="string"  way="in"/>
</service>
<output export="true" print="true">
  <column type="string" id="id"           name="序号"  />
  <column type="string" id="product_id"   name="标的证券"  display="false"/>
  <column type="string" id="product_code" name="证券代码"  />
  <column type="string" id="product_name" name="证券名称"  />
  <column type="number" id="product_num"  name="证券数量"  total="true" precision="0"/>
  <column type="number" id="product_market_value"   name="证券市值"  precision="2"  total="true"/>
  <column type="number" id="fin_scale_rate"   name="市值占融资总规模比例"  precision="4" />
  <column type="number" id="fund_rate"        name="市值占净资本比例"  precision="6" />
  <column type="number" id="mission"          name="市值与买入金额之差     "  precision="2" total="true"/>
</output>
</function>