<?xml version="1.0" encoding="gbk" ?>
<function id="7001049100" name="设置任务明细" >
	<input>
		<item type="text" id="task_id"       name="任务编号" editable="false"/>
		<item type="date" id="begin_date"    name="征信起始日期" nullable="false"/>
		<item type="date" id="end_date"      name="征信结束日期" nullable="false"/>
		<item type="hidden" id="evaluate_date" name="评定日期"   />
		<item type="hidden" id="business_type" name="业务类别"     />
		<item type="text" id="app_num"       name="申请额度"     />
		<item type="text" id="profit_clt_rate" name="亏损控制比率%"     />
		<group name="可用半角逗号为分隔符输入多个客户号">
			<item type="bigtextarea" id="cust_code" name="客户代码"     />
		</group>
	</input>
	<service type="procedure" id="p70010491"  process="822" style="1">
		<param id="user_id" 					type="integer" 	way="in"/>
		<param id="task_id" 			        type="string" 	way="in"/>
		<param id="cust_code" 			        type="string" 	way="in"/>
		<param id="begin_date" 			        type="string" 	way="in"/>
		<param id="end_date" 			        type="string" 	way="in"/>
		<param id="evaluate_date" 			    type="string" 	way="in"/>
		<param id="business_type" 			    type="string" 	way="in"/>
		<param id="app_num" 			        type="double" 	way="in"/>
		<param id="profit_clt_rate" 			type="double" 	way="in"/>
		<param id="operation_type" 			    type="double" 	way="in" default="1"/>
	</service>
	<output/>
</function>