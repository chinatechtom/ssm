<?xml version="1.0" encoding="gbk" ?>
<function id="32014" name="申报文件内容查询" >
  <input>	
		<item type="hidden" id="product_id"     name="证券代码"    />
		<item type="date" 	id="date" 					name="日期" 		default="now"/>
  </input>
  <service type="procedure" id="p_data_declare_query" process="911" style="77">
		<param id="user_id" 					type="integer"   	way="in"/>
		<param id="product_id" 				type="integer"		way="in"/>
		<param id="date"							type="string"			way="in"/>
  </service>
  <output export="true">    
  	<column type="string" id="XYZQDM"   			name="证券代码"  								 						/>
    <column type="number" id="XYZRRZYE"   		name="前日融资余额"  					precision="2"	/>
		<column type="number" id="XYJRRZYE"   		name="本日融资余额"  					precision="2"	/>
    <column type="number" id="XYJRRZMR"   		name="本日融资买入额"  				precision="2"	/>
    <column type="number" id="XYJRRZCH"   		name="本日融资偿还额"  				precision="2"	/>
    <column type="number" id="XYJRZJHK"   		name="本日直接还款额"  				precision="2"	/>
    <column type="number" id="XYJRMJHK"   		name="本日卖券还款额"  				precision="2"	/>
		<column type="number" id="XYRZPC"   			name="本日融资强制平仓额"  		precision="2"	/>
    <column type="number" id="XYJRRZZQ"   		name="本日融资正权益调整量"  	precision="2"	/>
    <column type="number" id="XYJRRZFQ"   		name="本日融资负权益调整量"  	precision="2"	/>
    <column type="number" id="XYZRRJYE"   		name="前日融券余额"  					precision="2"	/>
    <column type="number" id="XYJRRJYL"   		name="本日融券余额"  					precision="2"	/>
		<column type="number" id="XYJRRJMC"   		name="本日融券卖出数量"  			precision="2"	/>
    <column type="number" id="XYJRRJCH"   		name="本日融券偿还量"  				precision="2"	/>
    <column type="number" id="XYJRRJMR"   		name="本日融券买入数量"  			precision="2"	/>
    <column type="number" id="XYJRXJCH"   		name="本日现券偿还数量"  			precision="2"	/>  
    <column type="number" id="XYRJPC"   			name="本日融券强制平仓量"  		precision="2"	/>
    <column type="number" id="XYJRYJHZ"   		name="本日余券划转量"  				precision="2"	/>
    <column type="number" id="XYJRRJZQ"   		name="本日融券正权益调整量"  	precision="2"	/>	
    <column type="number" id="XYJRRJFQ"   		name="本日融券负权益调整量"  	precision="2"	/>
    <column type="number" id="XYJRRJYE"   			name="本日融券余量金额"  			precision="2"	/>	
    <column type="string" id="XYRJDW"   			name="融券报送单位"  					display="false"	/>
    <column type="string" id="xyrjdw_name"   	name="融券报送单位"  								 				/>
    <column type="string" id="XYJYRQ"   			name="信用交易发生日期"  									 	/>
    <column type="string" id="xyscdm"   			name="市场代码"  							display="false"/>
    <column type="string" id="xyscdm_name"   	name="市场名称"  														/>
    </output>
</function>