			<item type="text" id="add_score"					name="投资者属性额外加分项" 		default="0" integer="true" action="37426"/>
			<item type="text" id="total_score"					name="综合得分" 					editable="false"/>
		</group> 
		<group name="处理意见">
			<item type="textarea"	id="manage_opinion"	name="处理意见"/>
			<item type="hidden"		id="manage_date"	name="处理日期"/> <!--add 20140904-->
			<item type="hidden"		id="manage_name"	name="处理员"/>	<!--add 20140910-->
		</group>
    </input>
    <service type="procedure" id="p_guarantee_rate_adjust" process="811" style="1" implement="p_guarantee_rate_adjust3064444481">
	    	<param type="integer" id="user_id"					way="in"/>	
	   	<param type="integer" id="order_id"					way="in"/>		
		<param type="double" id="cur_asset"					way="in"/>
		<param type="double" id="cur_asset_score"				way="in"/>
		<param type="double" id="hs300_year_profit_rate" 			way="in"/>
		<param type="double" id="hs300_year_profit_score"			way="in"/>
		<param type="double" id="abs_profit_rate"				way="in"/>
		<param type="double" id="abs_profit_score"				way="in"/>
		<param type="double" id="cp_profit_rate"				way="in"/>
		<param type="double" id="cp_profit_score"				way="in"/>
		<param type="double" id="rzrq_commission"				way="in"/>
		<param type="double" id="rzrq_commission_score"				way="in"/>
		<param type="double" id="rzrq_fee"					way="in"/>		
		<param type="double" id="rzrq_fee_score"				way="in"/>	
		<param type="double"  id="now_guarantee_rate_13"			way="in"/>
		<param type="double"  id="now_guarantee_rate_5"				way="in"/>
		<param type="double"  id="now_guarantee_rate_6"				way="in"/>
		<param type="double"  id="advise_guarantee_rate_13"			way="in"/>
		<param type="double"  id="advise_guarantee_rate_5"			way="in"/>
		<param type="double"  id="advise_guarantee_rate_6"			way="in"/>
		<param type="double"  id="add_score"					way="in"/>
		<param type="double"  id="total_score"					way="in"/>
		<param type="double"  id="lower_guarantee_rate"				way="in"/>
		<param type="double"  id="suggest_lower_guarantee_rate"			way="in"/>
		<param type="double"  id="adjuest_value"				way="in"/>
		<param type="string"  id="remark"					way="in"/>
	    	<param type="string"  id="manage_opinion"				way="in"/>
    </service>
    <output/>
</function>