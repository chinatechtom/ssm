	<group name="交易协议书详细信息">
		<item type="text" 	id ="hg_num" 			name="标的证券数量" 	editable="false"/>
		<item type="textarea" 	id ="hg_num_big" 			name="标的证券数量大写" 	editable="false"/>
		<item type="text" 	id ="hg_convert_rate" 	name="折扣比率(%)" 		editable="false"/>
		<item type="hidden" id ="app_price" 		name="标的证券价格" 		editable="false"/>
		<item type="date" 	id ="engage_trade_date"	name="初始交易日期" 	editable="false"/>
		<item type="text" 	id ="days" 				name="购回期限(天)" 	editable="false"/>
		<item type="date" 	id ="bb_date"			name="购回交易日期" 	editable="false"/>
		<item type="text" 	id ="hg_rate" 			name="购回价格(每100元年收益)"	editable="false"/>
		<item type="text" 	id ="first_amt" 		name="初始交易金额" 	editable="false"/>
