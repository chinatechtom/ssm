	<column type="number" id="interest_over_rate" name="利息逾期违约金率(%)" precision="3"/>
	<column type="number" id="after_rate" name="本金逾期违约金率(%)" precision="3"/>	
	<column type="number" id="convert_rate" name="加成折算率(%)" precision="2"/>	
