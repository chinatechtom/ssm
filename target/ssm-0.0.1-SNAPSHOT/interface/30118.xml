<?xml version="1.0" encoding="gbk" ?>
<function id="30118" name="客户维持担保比例查询" >
	<input>	
		<item type="hidden"  id="cust_id"	            name="客户id"/>	
	</input>
    <service type="procedure" id="p_cust_assure_rate_query"  process="911" style="77">
		<param id="user_id"	            type="integer" way="in"/> 
		<param id="cust_id"	            type="integer" way="in"/> 						
    </service>
    <output export="true">
		<column type="number" id="assure_rate" name="维持担保比例(计入场外担保品)" 	precision="6" />	
		<column type="number" id="assure_rate_nomortgage" name="维持担保比例(不计入场外担保品)" 	precision="6" />	
    </output>
</function>
