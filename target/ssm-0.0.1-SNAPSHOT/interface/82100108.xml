  	</input>
  	<service type="drtp" id="413000" process="711" style="77">
		  <param type="string"	id="sbranch_code0" default="000"/>
		  <param type="string"	id="scust_no"/>
		  <param type="string"	id="semp"	default="000000"/>
		  <param type="integer" id="lvol9"  default="100108"/>
  	</service>
  	<output target="83100108">
		<column id="cust_code"		name="客户代码"			type="string"	field="scust_no"/>
		<column id="cert_depart"	name="发证机关"			type="string"	field="semail2"/>
		<column id="org_code"		name="组织机构代码"		type="string"	field="scard0"/>
		<column id="tax_reg_code"	name="税务登记证号码"	type="string"	field="scusttypes"/>
		<column id="holder_kind"	name="法人性质"			type="string"	field="scust_type2"/>
  	</output>
</function>