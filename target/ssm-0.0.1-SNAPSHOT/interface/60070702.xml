<?xml version="1.0" encoding="gbk" ?>
<function id="60070702" name="证券标的设置" >
  <input>
	<item type="hidden" id="product_id" name="productid"/>
	<item type="text" 	id="product_code" name="产品代码" editable="false"/>
	<item type="text" 	id="product_name" name="产品名称" editable="false"/>
	<item type="select" id="if_zy_target" name="是否质押标的证券" source="804"/>
	<group name="质押标的证券设置"> 
	  <item type="text" id="zy_convert_rate" name="质押折算率(%)" number="true" filter="if_zy_target=1"/>
	  <item type="text" id="zy_concentration_rate" name="质押集中度(%)占总股本" number="true" filter="if_zy_target=1"/>	  
	  <item type="text" id="zy_stock_pca" name="质押集中度(%)占流通A" number="true" filter="if_zy_target=1"/>
	  <item type="text" id="zy_stock_pna" name="质押集中度(%)占净资本" number="true" filter="if_zy_target=1"/>
	</group>  
  </input>
  <service type="procedure" id="P60070702" process="833" style="1">
	<param id="user_id" 				type="integer"/> 			
	<param id="product_id" 				type="integer"/> 
	<param id="if_zy_target" 			type="string"/> 
	<param id="zy_convert_rate" 		type="float"/>
	<param id="zy_concentration_rate"	type="float"/>
	<param id="zy_stock_pca"			type="float"/>
	<param id="zy_stock_pna"			type="float"/>
  </service>
  <output/>
</function>